    </dxcn:RenderTemplate.Triggers>
  </dxcn:RenderTemplate>
  <!-- Separator -->
  <dxcn:RenderTemplate x:Key="{dxdst:DropDownButtonThemeKey ResourceKey=SeparatorBorderTemplate}">
    <dxcn:RenderPanel />
  </dxcn:RenderTemplate>
  <dx:WpfSvgPalette x:Key="{dxdst:BrushesThemeKey ResourceKey=ContrastSvgPalette}" />
  <dx:WpfSvgPalette x:Key="{dxdst:BrushesThemeKey ResourceKey=SvgPalette}">
    <SolidColorBrush x:Key="BlackColor" Color="#FF333333" />
    <SolidColorBrush x:Key="WhiteColor" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="RedColor" Color="#FFF8274D" />
    <SolidColorBrush x:Key="YellowColor" Color="#FFB3751B" />
    <SolidColorBrush x:Key="GreenColor" Color="#FF3F961E" />
    <SolidColorBrush x:Key="BlueColor" Color="#FF4C88C9" />
    <dx:WpfSvgPalette.States>
      <dx:WpfSvgPalette x:Key="Hover">
        <SolidColorBrush x:Key="BlackColor" Color="#FF333333" />
        <SolidColorBrush x:Key="WhiteColor" Color="#FFFFFFFF" />
        <SolidColorBrush x:Key="RedColor" Color="#FFF8274D" />
        <SolidColorBrush x:Key="YellowColor" Color="#FFB3751B" />
        <SolidColorBrush x:Key="GreenColor" Color="#FF3F961E" />
        <SolidColorBrush x:Key="BlueColor" Color="#FF4C88C9" />
      </dx:WpfSvgPalette>
      <dx:WpfSvgPalette x:Key="Pressed">
        <SolidColorBrush x:Key="BlackColor" Color="#FFFFFFFF" />
        <SolidColorBrush x:Key="WhiteColor" Color="#FFF78A09" />
        <SolidColorBrush x:Key="RedColor" Color="#FFFFD0B5" />
        <SolidColorBrush x:Key="YellowColor" Color="#FFFFEC91" />
        <SolidColorBrush x:Key="GreenColor" Color="#FFA5FF8A" />
        <SolidColorBrush x:Key="BlueColor" Color="#FFA9FFFF" />
