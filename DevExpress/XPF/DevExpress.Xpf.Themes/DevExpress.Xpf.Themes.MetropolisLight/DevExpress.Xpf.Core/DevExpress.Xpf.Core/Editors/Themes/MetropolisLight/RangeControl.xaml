<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dxer="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <ResourceDictionary.MergedDictionaries>
    <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.MetropolisLight.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/RangeControl.xaml" />
  </ResourceDictionary.MergedDictionaries>
  <SolidColorBrush x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarClientForegroundBrush}" Color="#FFFFFFFF" />
  <Thickness x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftLabelMargin}" Bottom="0" Left="0" Right="9" Top="0" />
  <Thickness x:Key="{dxt:RangeControlThemeKey ResourceKey=RightLabelMargin}" Bottom="0" Left="9" Right="0" Top="0" />
  <!-- CalendarSelectionMarker -->
  <DataTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarSelectionMarkerTemplate}">
    <Ellipse Fill="{DynamicResource {dxt:CalendarClientThemeKey ResourceKey=CalendarClientForegroundBrush}}" Height="20" HorizontalAlignment="Center" VerticalAlignment="Center" Width="20" />
  </DataTemplate>
  <!-- ToolTips -->
  <DataTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LabelTemplate}">
    <Border Background="#FFB9B9B9" MinHeight="22" MinWidth="80" VerticalAlignment="Top">
      <TextBlock Foreground="#FFFFFFFF" HorizontalAlignment="Center" Margin="12,2,12,2" Text="{Binding}" VerticalAlignment="Center" />
    </Border>
  </DataTemplate>
  <!-- Left & Right Navigation Buttons -->
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftNavigationButtonTemplate}" TargetType="{x:Type Button}">
    <Grid Background="Transparent" Margin="12,0,0,0" Opacity="0.9">
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="CommonStates">
          <VisualState x:Name="Normal">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="#FFB9B9B9" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="MouseOver">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="#FFF78A09" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Pressed">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="#FFF78A09" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
      <Ellipse Fill="#FFFFFFFF" Height="34" Stretch="Fill" Width="34" />
      <Path x:Name="Glyph" Data="F1 M 210,314L 210,312L 218,304L 221,304L 213,312L 230,312L 230,314L 213,314L 221,322L 218,322L 210,314 Z M 220,297C 228.837,297 236,304.164 236,313C 236,321.837 228.836,329 220,329C 211.163,329 204,321.837 204,313C 204,304.164 211.163,297 220,297 Z M 220,299C 212.268,299 206,305.268 206,313C 206,320.732 212.268,327 220,327C 227.732,327 234,320.732 234,313C 234,305.268 227.732,299 220,299 Z " Fill="#FFFFFFFF" Height="32" Stretch="Fill" Width="32" />
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RightNavigationButtonTemplate}" TargetType="{x:Type Button}">
    <Grid Background="Transparent" Margin="0,0,12,0" Opacity="0.9">
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="CommonStates">
          <VisualState x:Name="Normal">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="#FFB9B9B9" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="MouseOver">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="#FFF78A09" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Pressed">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="#FFF78A09" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
      <Ellipse Fill="#FFFFFFFF" Height="34" Stretch="Fill" Width="34" />
      <Path x:Name="Glyph" Data="F1 M 260,328L 252,336L 249,336L 257,328L 240,328L 240,326L 257,326L 249,318L 252,318L 260,326L 260,328 Z M 250,311C 258.837,311 266,318.164 266,327C 266,335.837 258.836,343 250,343C 241.163,343 234,335.837 234,327C 234,318.164 241.163,311 250,311 Z M 250,313C 242.268,313 236,319.268 236,327C 236,334.732 242.268,341 250,341C 257.732,341 264,334.732 264,327C 264,319.268 257.732,313 250,313 Z " Fill="#FFFFFFFF" Height="32" Stretch="Fill" Width="32" />
    </Grid>
  </ControlTemplate>
  <!-- Calendar -->
  <ControlTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarGroupItemTemplate}" TargetType="{x:Type dxer:CalendarGroupItem}">
    <Border Background="#FFB9B9B9">
      <TextBlock Margin="20,0" Text="{TemplateBinding Text}" TextAlignment="Center" VerticalAlignment="Center" />
    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarItemTemplate}" TargetType="{x:Type dxer:CalendarItem}">
    <Border x:Name="Border" Background="#FFB9B9B9" BorderThickness="0">
      <TextBlock x:Name="Text" HorizontalAlignment="Center" Margin="0,10,0,10" Text="{TemplateBinding Text}" TextAlignment="Center" />
    </Border>
    <ControlTemplate.Triggers>
      <Trigger Property="IsSelected" Value="True">
        <Setter Property="Background" TargetName="Border" Value="#FFF78A09" />
        <Setter Property="Foreground" TargetName="Text" Value="#FFFFFFFF" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <!-- Range Bar -->
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
    <Border Background="#FFFFFFFF" BorderBrush="#FFB9B9B9" BorderThickness="{Binding Path=(dxer:RangeControl.OwnerRangeControl).BorderThickness, RelativeSource={RelativeSource Self}}">
      <ContentPresenter Margin="1" />
    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarRootBorderTemplate}" TargetType="{x:Type ContentControl}">
    <Border Background="#FFFFFFFF" BorderBrush="#FFB9B9B9" BorderThickness="0,1,0,0" Margin="-1,1,-1,-1" Padding="1">
      <ContentPresenter />
    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarLeftResizeThumbTemplate}">
    <Border Background="Transparent" Padding="4,2,2,2">
      <Path Data="F1 M 0,0L 3,0L 3,1L 0,1L 0,0 Z M 3,2L 3,3L 0,3L 0,2L 3,2 Z M 0,4L 3,4L 3,5L 0,5L 0,4 Z M 3,6L 3,7L 0,7L 0,6L 3,6 Z M 0,8L 3,8L 3,9L 0,9L 0,8 Z " Fill="#FF999999" Height="9" Stretch="Fill" Width="3" />
    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarRightResizeThumbTemplate}">
    <Border Background="Transparent" Padding="2,2,4,2">
      <Path Data="F1 M 0,0L 3,0L 3,1L 0,1L 0,0 Z M 3,2L 3,3L 0,3L 0,2L 3,2 Z M 0,4L 3,4L 3,5L 0,5L 0,4 Z M 3,6L 3,7L 0,7L 0,6L 3,6 Z M 0,8L 3,8L 3,9L 0,9L 0,8 Z " Fill="#FF999999" Height="9" Stretch="Fill" Width="3" />
    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftSelectionThumbTemplate}" TargetType="{x:Type Thumb}">
    <Grid Background="Transparent" HorizontalAlignment="Left" Width="15">
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="Auto" />
      </Grid.ColumnDefinitions>
      <Border x:Name="Button" Background="#FFB9B9B9" BorderBrush="#FFFFFFFF" BorderThickness="0,0,1,0" Grid.Column="1" Width="8">
        <Path Data="F1 M 0,0L 3,0L 3,1L 0,1L 0,0 Z M 3,2L 3,3L 0,3L 0,2L 3,2 Z M 0,4L 3,4L 3,5L 0,5L 0,4 Z M 3,6L 3,7L 0,7L 0,6L 3,6 Z M 0,8L 3,8L 3,9L 0,9L 0,8 Z " Fill="#FFFFFFFF" Height="9" Stretch="Fill" Width="3" />
      </Border>
    </Grid>
    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding Path=(dxer:RangeControl.OwnerRangeControl).IsSelectionMoving, RelativeSource={RelativeSource Self}}" Value="True">
        <Setter Property="Background" TargetName="Button" Value="#FFB9B9B9" />
      </DataTrigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RightSelectionThumbTemplate}" TargetType="{x:Type Thumb}">
    <Grid Background="Transparent" HorizontalAlignment="Right" Width="15">
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>
      <Border x:Name="Button" Background="#FFB9B9B9" BorderBrush="#FFFFFFFF" BorderThickness="1,0,0,0" Grid.Column="0" Width="8">
        <Path Data="F1 M 0,0L 3,0L 3,1L 0,1L 0,0 Z M 3,2L 3,3L 0,3L 0,2L 3,2 Z M 0,4L 3,4L 3,5L 0,5L 0,4 Z M 3,6L 3,7L 0,7L 0,6L 3,6 Z M 0,8L 3,8L 3,9L 0,9L 0,8 Z " Fill="#FFFFFFFF" Height="9" Stretch="Fill" Width="3" />
      </Border>
    </Grid>
    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding Path=(dxer:RangeControl.OwnerRangeControl).IsSelectionMoving, RelativeSource={RelativeSource Self}}" Value="True">
        <Setter Property="Background" TargetName="Button" Value="#FFB9B9B9" />
      </DataTrigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <!-- Styles -->
  <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=SelectionRectangleStyle}" TargetType="{x:Type Border}">
    <Setter Property="Background" Value="#7FFFFFFF" />
  </Style>
  <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=OutOfRangeAreaBorderStyle}" TargetType="{x:Type Border}">
    <Setter Property="Background" Value="#7FE0E0E0" />
  </Style>
  <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarOutOfRangeBorderStyle}" TargetType="{x:Type Border}">
    <Setter Property="Background" Value="#BFB9B9B9" />
  </Style>
  <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarOutOfSelectionBorderStyle}" TargetType="{x:Type Border}">
    <Setter Property="Background" Value="#FFE0E0E0" />
  </Style>
  <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarSelectionBorderStyle}" TargetType="{x:Type Border}">
    <Setter Property="Background" Value="#FFFFFFFF" />
  </Style>
  <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarBottomBorderStyle}" TargetType="{x:Type Border}">
    <Setter Property="Background" Value="#FFFFFFFF" />
    <Setter Property="Height" Value="13" />
  </Style>
</ResourceDictionary>