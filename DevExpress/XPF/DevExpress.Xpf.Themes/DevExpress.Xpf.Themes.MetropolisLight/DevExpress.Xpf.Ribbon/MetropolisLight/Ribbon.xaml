  <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=MinimizationButtonTemplateInRibbonAeroWindow}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
    <Grid Background="#00000000" Height="22" SnapsToDevicePixels="True" Width="21">
      <Grid x:Name="Glyph" Height="7" HorizontalAlignment="Center" RenderTransformOrigin="0.5,0.5" VerticalAlignment="Center" Width="9">
