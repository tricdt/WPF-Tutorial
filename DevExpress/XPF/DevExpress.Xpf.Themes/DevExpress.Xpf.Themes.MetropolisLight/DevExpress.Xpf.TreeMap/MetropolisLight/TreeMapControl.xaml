<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/treemap/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- General -->
  <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ControlBorderBrush}" Color="#FFB9B9B9" />
  <!-- Group -->
  <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupBorderBrush}" Color="#FFB9B9B9" />
  <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupBackground}" Color="#FFFFFFFF" />
  <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupForeground}" Color="#FF373A3D" />
  <sys:Double x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupFontSize}">12</sys:Double>
  <FontWeight x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupFontWeight}">Bold</FontWeight>
  <!-- SubMenu -->
  <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupBackground}" Color="#FFF0F0F0" />
  <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupForeground}" Color="#FF373A3D" />
  <sys:Double x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupFontSize}">12</sys:Double>
  <FontWeight x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupFontWeight}">Bold</FontWeight>
  <!-- ToolTip -->
  <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipBackground}" Color="#FFF78A09" />
  <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipForeground}" Color="#FFFFFFFF" />
  <sys:Double x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipFontSize}">12</sys:Double>
  <FontWeight x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipFontWeight}">Bold</FontWeight>
</ResourceDictionary>