      </dxn:RenderPanel>
      <dxn:RenderPanel Name="PART_Remove" Height="12" Opacity="0.5" Visibility="Collapsed" Width="12">
        <dxn:RenderBorder Background="#00000000" CornerRadius="256" Margin="1" />
        <dxn:RenderPath Name="Path" Data="F1 M 1.75736,1.75736C 4.1005,-0.585785 7.89949,-0.585785 10.2426,1.75736C 12.5858,4.10051 12.5858,7.8995 10.2426,10.2426C 7.89949,12.5858 4.1005,12.5858 1.75736,10.2426C -0.585787,7.8995 -0.585787,4.10051 1.75736,1.75736 Z M 2.46447,8.12132L 3.87868,9.53553L 6,7.41421L 8.12132,9.53553L 9.53553,8.12132L 7.41421,6L 9.53553,3.87868L 8.12132,2.46447L 6,4.58579L 3.87868,2.46447L 2.46447,3.87868L 4.58579,6L 2.46447,8.12132 Z " Fill="#FF666666" Stretch="Fill" />
      </dxn:RenderPanel>
    </dxn:RenderPanel>
    <dxn:RenderTemplate.Triggers>
      <dxn:RenderStateGroup Name="CommonStates">
        <dxn:RenderStateTrigger Name="Normal" />
        <dxn:RenderStateTrigger Name="MouseOver">
          <dxn:RenderSetter Property="Opacity" TargetName="PART_Add" Value="1" />
          <dxn:RenderSetter Property="Opacity" TargetName="PART_Remove" Value="0.75" />
          <dxn:RenderSetter Property="Fill" TargetName="Path" Value="#FF990000" />
        </dxn:RenderStateTrigger>
      </dxn:RenderStateGroup>
      <dxn:RenderStateGroup Name="ButtonKind">
        <dxn:RenderStateTrigger Name="Add" />
        <dxn:RenderStateTrigger Name="Remove">
          <dxn:RenderSetter Property="Visibility" TargetName="PART_Remove" Value="Visible" />
          <dxn:RenderSetter Property="Visibility" TargetName="PART_Add" Value="Collapsed" />
        </dxn:RenderStateTrigger>
      </dxn:RenderStateGroup>
    </dxn:RenderTemplate.Triggers>
  </dxn:RenderTemplate>
  <dxn:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=Expander}">
    <dxn:RenderPanel>
      <dxn:RenderPath Name="GlyphCollapsed" Data="F1 M 232.5,17.0001C 228.91,17.0001 226,19.9102 226,23.5C 226,27.0898 228.91,30 232.5,30C 236.09,30 239,27.0898 239,23.5C 239,19.9102 236.09,17.0001 232.5,17.0001 Z M 232.5,18.0004C 235.538,18.0004 238,20.4628 238,23.5004C 238,26.538 235.538,29.0004 232.5,29.0004C 229.463,29.0004 227,26.538 227,23.5004C 227,20.4628 229.463,18.0004 232.5,18.0004 Z M 236,24.0001L 236,24.0001L 236,23.0004L 236,23L 233,20L 231,20L 234,23L 229,23.0004L 229,24.0001L 234,24L 231,26.9999L 233,26.9999L 236,24.0001 Z " Fill="#FF666666" Height="13" Stretch="Fill" Width="13" />
      <dxn:RenderPath Name="GlyphExpanded" Data="F1 M 372.5,127C 376.09,127 379,129.91 379,133.5C 379,137.09 376.09,140 372.5,140C 368.91,140 366,137.09 366,133.5C 366,129.91 368.91,127 372.5,127 Z M 372.5,128C 369.462,128 367,130.463 367,133.5C 367,136.538 369.462,139 372.5,139C 375.537,139 378,136.538 378,133.5C 378,130.463 375.537,128 372.5,128 Z M 370,136L 370,135L 373,135L 370,132L 370,131L 371,131L 374,134L 374,131L 375,131L 375,135L 375,136L 375,136L 374,136L 370,136 Z " Fill="#FF666666" Height="13" Stretch="Fill" Visibility="Hidden" Width="13" />
    </dxn:RenderPanel>
    <dxn:RenderTemplate.Triggers>
      <dxn:RenderStateGroup Name="MouseStates">
        <dxn:RenderStateTrigger Name="Normal" />
        <dxn:RenderStateTrigger Name="MouseOver">
          <dxn:RenderSetter Property="Fill" TargetName="GlyphExpanded" Value="#FF373A3D" />
          <dxn:RenderSetter Property="Fill" TargetName="GlyphCollapsed" Value="#FF373A3D" />
        </dxn:RenderStateTrigger>
        <dxn:RenderStateTrigger Name="Pressed">
          <dxn:RenderSetter Property="Fill" TargetName="GlyphExpanded" Value="#FF373A3D" />
          <dxn:RenderSetter Property="Fill" TargetName="GlyphCollapsed" Value="#FF373A3D" />
        </dxn:RenderStateTrigger>
      </dxn:RenderStateGroup>
      <dxn:RenderStateGroup Name="CheckedStates">
        <dxn:RenderStateTrigger Name="Checked">
          <dxn:RenderSetter Property="Visibility" TargetName="GlyphExpanded" Value="Visible" />
          <dxn:RenderSetter Property="Visibility" TargetName="GlyphCollapsed" Value="Hidden" />
        </dxn:RenderStateTrigger>
        <dxn:RenderStateTrigger Name="Unchecked" />
      </dxn:RenderStateGroup>
    </dxn:RenderTemplate.Triggers>
  </dxn:RenderTemplate>
</ResourceDictionary>