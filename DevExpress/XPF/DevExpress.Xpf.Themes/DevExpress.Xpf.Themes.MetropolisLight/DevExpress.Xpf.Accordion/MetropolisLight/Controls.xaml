<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dxa="http://schemas.devexpress.com/winfx/2008/xaml/accordion" xmlns:dxan="http://schemas.devexpress.com/winfx/2008/xaml/accordion/internal" xmlns:dxat="http://schemas.devexpress.com/winfx/2008/xaml/accordion/themekeys" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- Resource -->
  <sys:Double x:Key="{dxat:AccordionItemThemeKeys ResourceKey=RootItemExpandButtonSize}">22</sys:Double>
  <sys:Double x:Key="{dxat:AccordionItemThemeKeys ResourceKey=ItemExpandButtonSize}">14</sys:Double>
  <sys:Double x:Key="{dxat:AccordionItemThemeKeys ResourceKey=RootItemHeaderHeight}">32</sys:Double>
  <sys:Double x:Key="{dxat:AccordionItemThemeKeys ResourceKey=ItemHeaderHeight}">22</sys:Double>
  <sys:Double x:Key="{dxat:AccordionItemThemeKeys ResourceKey=ItemOffset}">18</sys:Double>
  <sys:Double x:Key="{dxat:AccordionItemThemeKeys ResourceKey=ExpandButtonContainerMinWidth}">18</sys:Double>
  <Thickness x:Key="{dxat:AccordionItemThemeKeys ResourceKey=HeaderElementsMargin}">3,3,0,3</Thickness>
  <Thickness x:Key="{dxat:AccordionItemThemeKeys ResourceKey=RootItemContainerMargin}">0,1,0,0</Thickness>
  <Thickness x:Key="{dxat:AccordionItemThemeKeys ResourceKey=RootItemHeaderMargin}">4,0,4,0</Thickness>
  <Thickness x:Key="{dxat:AccordionItemThemeKeys ResourceKey=ItemHeaderMargin}">3,0,3,0</Thickness>
  <Thickness x:Key="{dxat:AccordionItemThemeKeys ResourceKey=CollapsedItemGlyphMargin}">3,0,3,0</Thickness>
  <Thickness x:Key="{dxat:AccordionItemThemeKeys ResourceKey=ItemHeaderContainerMargin}">0,1,0,0</Thickness>
  <sys:Double x:Key="{dxat:AccordionItemThemeKeys ResourceKey=DisabledItemOpacity}">0.35</sys:Double>
  <sys:Double x:Key="{dxat:AccordionItemThemeKeys ResourceKey=RootItemHeaderFontSize}">12</sys:Double>
  <sys:Double x:Key="{dxat:AccordionItemThemeKeys ResourceKey=ItemHeaderFontSize}">12</sys:Double>
  <FontWeight x:Key="{dxat:AccordionItemThemeKeys ResourceKey=ItemHeaderFontWeight}">Normal</FontWeight>
  <FontWeight x:Key="{dxat:AccordionItemThemeKeys ResourceKey=RootItemHeaderFontWeight}">Bold</FontWeight>
  <Thickness x:Key="{dxat:AccordionThemeKeys ResourceKey=BorderThickness}">0</Thickness>
  <Thickness x:Key="{dxat:AccordionThemeKeys ResourceKey=CollapseButtonMargin}">7,5,7,5</Thickness>
  <Thickness x:Key="{dxat:AccordionThemeKeys ResourceKey=CollapseButtonCheckedMargin}">7,5,7,5</Thickness>
  <Thickness x:Key="{dxat:AccordionThemeKeys ResourceKey=SearchControlMargin}">8,5,8,5</Thickness>
  <sys:Double x:Key="{dxat:AccordionThemeKeys ResourceKey=SearchControlMinHeight}">22</sys:Double>
  <Thickness x:Key="{dxat:AccordionThemeKeys ResourceKey=OverflowItemMargin}">0,0,1,1</Thickness>
  <Thickness x:Key="{dxat:AccordionThemeKeys ResourceKey=OverflowBackgroundPadding}">0,1,0,0</Thickness>
  <sys:Double x:Key="{dxat:AccordionThemeKeys ResourceKey=HeaderFontSize}">12</sys:Double>
  <FontWeight x:Key="{dxat:AccordionThemeKeys ResourceKey=HeaderFontWeight}">Bold</FontWeight>
  <sys:Double x:Key="{dxat:AccordionThemeKeys ResourceKey=CollapsedItemFontSize}">12</sys:Double>
  <FontWeight x:Key="{dxat:AccordionThemeKeys ResourceKey=CollapsedItemFontWeight}">Bold</FontWeight>
  <sys:Double x:Key="{dxat:AccordionThemeKeys ResourceKey=SearchControlFontSize}">12</sys:Double>
  <!-- Expand/Collapse Button -->
  <dxmvvm:ObjectToObjectConverter x:Key="FlipHorizontalConverter" DefaultTarget="1">
    <dxmvvm:MapItem Source="True" Target="-1" />
  </dxmvvm:ObjectToObjectConverter>
  <ControlTemplate x:Key="{dxat:AccordionItemThemeKeys ResourceKey=ExpanderButtonTemplate}" TargetType="dxa:AccordionExpandButton">
    <Grid x:Name="Root" Background="Transparent">
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="CheckStates">
          <VisualState x:Name="Unchecked" />
          <VisualState x:Name="Checked">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="IsUnchecked" Storyboard.TargetProperty="(UIElement.Visibility)">
                <DiscreteObjectKeyFrame KeyTime="0">
                  <DiscreteObjectKeyFrame.Value>
                    <Visibility>Collapsed</Visibility>
                  </DiscreteObjectKeyFrame.Value>
                </DiscreteObjectKeyFrame>
              </ObjectAnimationUsingKeyFrames>
              <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="IsChecked" Storyboard.TargetProperty="(UIElement.Visibility)">
                <DiscreteObjectKeyFrame KeyTime="0">
                  <DiscreteObjectKeyFrame.Value>
                    <Visibility>Visible</Visibility>
                  </DiscreteObjectKeyFrame.Value>
                </DiscreteObjectKeyFrame>
              </ObjectAnimationUsingKeyFrames>
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Indeterminate" />
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
      <dxa:ExpandButtonContentPresenter x:Name="IsUnchecked" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" ItemType="{TemplateBinding ItemType}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
        <dxa:ExpandButtonContentPresenter.ItemContent>
          <Path x:Name="ItemContentCollapse" Data="F1 M 232.5,17.0001C 228.91,17.0001 226,19.9102 226,23.5C 226,27.0898 228.91,30 232.5,30C 236.09,30 239,27.0898 239,23.5C 239,19.9102 236.09,17.0001 232.5,17.0001 Z M 232.5,18.0004C 235.538,18.0004 238,20.4628 238,23.5004C 238,26.538 235.538,29.0004 232.5,29.0004C 229.463,29.0004 227,26.538 227,23.5004C 227,20.4628 229.463,18.0004 232.5,18.0004 Z M 236,24.0001L 236,24.0001L 236,23.0004L 236,23L 233,20L 231,20L 234,23L 229,23.0004L 229,24.0001L 234,24L 231,26.9999L 233,26.9999L 236,24.0001 Z " Fill="{TemplateBinding Foreground}" Height="13" RenderTransformOrigin="0.5,0.5" Stretch="Fill" Width="13">
            <Path.RenderTransform>
              <ScaleTransform ScaleX="{Binding IsRightExpandButton, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource FlipHorizontalConverter}}" />
            </Path.RenderTransform>
          </Path>
        </dxa:ExpandButtonContentPresenter.ItemContent>
        <dxa:ExpandButtonContentPresenter.RootItemContent>
          <Path x:Name="RootItemContentCollapse" Data="F1 M 433,248L 431,248L 431,259L 425,253L 425,256L 431,262L 433,262L 439,256L 439,253L 433,259L 433,248 Z M 432,266C 438.075,266 443,261.075 443,255C 443,248.925 438.075,244 432,244C 425.925,244 421,248.925 421,255C 421,261.075 425.925,266 432,266 Z M 432,264C 427.029,264 423,259.971 423,255C 423,250.029 427.03,246 432,246C 436.971,246 441,250.029 441,255C 441,259.971 436.971,264 432,264 Z " Fill="#FFB9B9B9" Height="22" Stretch="Fill" Width="22" />
        </dxa:ExpandButtonContentPresenter.RootItemContent>
      </dxa:ExpandButtonContentPresenter>
      <dxa:ExpandButtonContentPresenter x:Name="IsChecked" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" ItemType="{TemplateBinding ItemType}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Visibility="Hidden">
        <dxa:ExpandButtonContentPresenter.ItemContent>
          <Path x:Name="ItemContentExpand" Data="F1 M 372.5,127C 376.09,127 379,129.91 379,133.5C 379,137.09 376.09,140 372.5,140C 368.91,140 366,137.09 366,133.5C 366,129.91 368.91,127 372.5,127 Z M 372.5,128C 369.462,128 367,130.463 367,133.5C 367,136.538 369.462,139 372.5,139C 375.537,139 378,136.538 378,133.5C 378,130.463 375.537,128 372.5,128 Z M 370,136L 370,135L 373,135L 370,132L 370,131L 371,131L 374,134L 374,131L 375,131L 375,135L 375,136L 375,136L 374,136L 370,136 Z " Fill="{TemplateBinding Foreground}" Height="13" RenderTransformOrigin="0.5,0.5" Stretch="Fill" Width="13">
            <Path.RenderTransform>
              <ScaleTransform ScaleX="{Binding IsRightExpandButton, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource FlipHorizontalConverter}}" />
            </Path.RenderTransform>
          </Path>
        </dxa:ExpandButtonContentPresenter.ItemContent>
        <dxa:ExpandButtonContentPresenter.RootItemContent>
          <Path x:Name="RootItemContentExpand" Data="F1 M 403,275L 401,275L 401,264L 395,270L 395,267L 401,261L 403,261L 409,267L 409,270L 403,264L 403,275 Z M 402,257C 408.075,257 413,261.925 413,268C 413,274.075 408.075,279 402,279C 395.925,279 391,274.075 391,268C 391,261.925 395.925,257 402,257 Z M 402,259C 397.029,259 393,263.029 393,268C 393,272.971 397.029,277 402,277C 406.97,277 411,272.971 411,268C 411,263.029 406.971,259 402,259 Z " Fill="#FFB9B9B9" Height="22" Stretch="Fill" Width="22" />
        </dxa:ExpandButtonContentPresenter.RootItemContent>
      </dxa:ExpandButtonContentPresenter>
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="ItemState" Value="Default" />
      <Trigger Property="ItemState" Value="Hover" />
      <Trigger Property="ItemState" Value="Pressed" />
      <Trigger Property="ItemState" Value="Disabled" />
      <MultiTrigger>
        <MultiTrigger.Conditions>
          <Condition Property="ItemType" Value="RootItem" />
          <Condition Property="ItemState" Value="Default" />
        </MultiTrigger.Conditions>
      </MultiTrigger>
      <MultiTrigger>
        <MultiTrigger.Conditions>
          <Condition Property="ItemType" Value="RootItem" />
          <Condition Property="IsMouseOver" Value="True" />
        </MultiTrigger.Conditions>
        <Setter Property="Fill" TargetName="RootItemContentExpand" Value="#FF373A3D" />
        <Setter Property="Fill" TargetName="RootItemContentCollapse" Value="#FF373A3D" />
        <Setter Property="Fill" TargetName="ItemContentCollapse" Value="#FF373A3D" />
        <Setter Property="Fill" TargetName="ItemContentCollapse" Value="#FF373A3D" />
      </MultiTrigger>
      <MultiTrigger>
        <MultiTrigger.Conditions>
          <Condition Property="ItemType" Value="RootItem" />
          <Condition Property="IsPressed" Value="True" />
        </MultiTrigger.Conditions>
        <Setter Property="Fill" TargetName="RootItemContentExpand" Value="#FF373A3D" />
        <Setter Property="Fill" TargetName="RootItemContentCollapse" Value="#FF373A3D" />
        <Setter Property="Fill" TargetName="ItemContentCollapse" Value="#FFFFFFFF" />
        <Setter Property="Fill" TargetName="ItemContentCollapse" Value="#FFFFFFFF" />
      </MultiTrigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <!-- RootItem and Item -->
  <ControlTemplate x:Key="{dxat:AccordionItemThemeKeys ResourceKey=ItemBackgroundTemplate}" TargetType="dxa:AccordionItemBackgroundControl">
    <Grid x:Name="Root">
      <Border x:Name="RootItemDefault" Background="#FFFFFFFF" BorderBrush="#FFB9B9B9" BorderThickness="0,0,0,1" Opacity="0" />
      <Border x:Name="CustomBackground" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"></Border>
      <Border x:Name="ItemHover" Background="#FFE3E3E3" Opacity="0" />
      <Border x:Name="ItemPressed" Background="#FFF78A09" Opacity="0" />
      <Border x:Name="ItemSelected" Background="#FFF78A09" Opacity="0" />
      <Border x:Name="RootItemHover" Background="#FFF0F0F0" BorderBrush="#FFB9B9B9" BorderThickness="0,0,0,1" Opacity="0" />
      <Border x:Name="RootItemPressed" Background="#FFF0F0F0" BorderBrush="#FFB9B9B9" BorderThickness="0,0,0,1" Opacity="0" />
      <Border x:Name="RootItemSelected" Background="#FFF0F0F0" BorderBrush="#FFB9B9B9" BorderThickness="0,0,0,1" Opacity="0" />
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="ItemState" Value="Default" />
      <Trigger Property="ItemState" Value="Hover">
        <Setter Property="Opacity" TargetName="ItemHover" Value="1" />
      </Trigger>
      <Trigger Property="ItemState" Value="Pressed">
        <Setter Property="Opacity" TargetName="ItemPressed" Value="1" />
      </Trigger>
      <Trigger Property="ItemState" Value="Selected">
        <Setter Property="Opacity" TargetName="ItemSelected" Value="1" />
      </Trigger>
      <Trigger Property="ItemState" Value="SelectedInactive">
        <Setter Property="Opacity" TargetName="ItemHover" Value="1" />
      </Trigger>
      <Trigger Property="ItemState" Value="Focused">
        <Setter Property="Opacity" TargetName="ItemHover" Value="1" />
      </Trigger>
      <Trigger Property="ItemState" Value="Disabled" />
      <MultiTrigger>
        <MultiTrigger.Conditions>
          <Condition Property="ItemType" Value="RootItem" />
          <Condition Property="ItemState" Value="Default" />
        </MultiTrigger.Conditions>
        <Setter Property="Opacity" TargetName="RootItemDefault" Value="1" />
      </MultiTrigger>
      <MultiTrigger>
        <MultiTrigger.Conditions>
          <Condition Property="ItemType" Value="RootItem" />
          <Condition Property="ItemState" Value="Hover" />
        </MultiTrigger.Conditions>
        <Setter Property="Opacity" TargetName="RootItemHover" Value="1" />
      </MultiTrigger>
      <MultiTrigger>
        <MultiTrigger.Conditions>
          <Condition Property="ItemType" Value="RootItem" />
          <Condition Property="ItemState" Value="Pressed" />
        </MultiTrigger.Conditions>
        <Setter Property="Opacity" TargetName="RootItemPressed" Value="1" />
      </MultiTrigger>
      <MultiTrigger>
        <MultiTrigger.Conditions>
          <Condition Property="ItemType" Value="RootItem" />
          <Condition Property="ItemState" Value="Selected" />
        </MultiTrigger.Conditions>
        <Setter Property="Opacity" TargetName="RootItemSelected" Value="1" />
      </MultiTrigger>
      <MultiTrigger>
        <MultiTrigger.Conditions>
          <Condition Property="ItemType" Value="RootItem" />
          <Condition Property="ItemState" Value="SelectedInactive" />
        </MultiTrigger.Conditions>
        <Setter Property="Opacity" TargetName="RootItemHover" Value="1" />
      </MultiTrigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxat:AccordionThemeKeys ResourceKey=CollapseButtonTemplate}" TargetType="ToggleButton">
    <Grid Background="Transparent" Height="{TemplateBinding Height}" Width="{TemplateBinding Width}">
      <Border x:Name="glyphBorder" HorizontalAlignment="Center" RenderTransformOrigin="0.5,0.5" VerticalAlignment="Center">
        <Path x:Name="glyph" Data="F1 M 360,189L 360,187L 349,187L 355,181L 352,181L 346,187L 346,189L 352,195L 355,195L 349,189L 360,189 Z M 342,188C 342,194.075 346.925,199 353,199C 359.075,199 364,194.075 364,188C 364,181.925 359.075,177 353,177C 346.925,177 342,181.925 342,188 Z M 344,188C 344,183.029 348.029,179 353,179C 357.971,179 362,183.029 362,188C 362,192.971 357.971,197 353,197C 348.029,197 344,192.971 344,188 Z " Fill="#FFB9B9B9" Height="22" Stretch="Fill" Width="22"></Path>
      </Border>
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="IsChecked" Value="False">
        <Setter Property="RenderTransform" TargetName="glyphBorder">
          <Setter.Value>
            <ScaleTransform ScaleX="-1" />
          </Setter.Value>
        </Setter>
      </Trigger>
      <Trigger Property="IsMouseOver" Value="True">
        <Setter Property="Fill" TargetName="glyph">
          <Setter.Value>
            <SolidColorBrush Color="#FF373A3D" />
          </Setter.Value>
        </Setter>
      </Trigger>
      <Trigger Property="IsPressed" Value="True">
        <Setter Property="Fill" TargetName="glyph">
          <Setter.Value>
            <SolidColorBrush Color="#FF373A3D" />
          </Setter.Value>
        </Setter>
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxat:AccordionThemeKeys ResourceKey=PopupWindowFrameTemplate}" TargetType="ContentControl">
    <Border Background="#FFFFFFFF" BorderBrush="#FFB9B9B9" BorderThickness="1">
      <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" />
    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxat:AccordionThemeKeys ResourceKey=CollapsedItemTemplate}" TargetType="{x:Type dxe:ListBoxEditItem}">
    <Grid x:Name="Root">
      <Border x:Name="CustomBackground" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" />
      <Rectangle x:Name="FocusVisual" Opacity="0" Stroke="#FFFFFFFF" StrokeDashArray="1 2" StrokeThickness="1" />
      <Rectangle x:Name="FocusedState" Opacity="0" Stroke="#FFF78A09" StrokeDashArray="1 2" StrokeThickness="1" />
      <Border x:Name="InactiveSelectedState" Background="#FFE3E3E3" Opacity="0" />
      <Border x:Name="HoverState" Background="#FFE3E3E3" Opacity="0" />
      <Border x:Name="SelectedState" Background="#FFF78A09" Opacity="0" />
      <ContentControl x:Name="content" Focusable="False" FocusVisualStyle="{x:Null}" FontSize="{TemplateBinding FontSize}" FontWeight="{TemplateBinding FontWeight}" Foreground="{DynamicResource {dxat:AccordionItemThemeKeys ResourceKey=ItemHeaderDefaultForeground}}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
        <ContentPresenter x:Name="contentPresenter" Height="Auto" Margin="0" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" Width="Auto" />
      </ContentControl>
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="IsKeyboardFocused" Value="True">
        <Setter Property="Opacity" TargetName="SelectedState" Value="0" />
        <Setter Property="Opacity" TargetName="HoverState" Value="0" />
        <Setter Property="Opacity" TargetName="InactiveSelectedState" Value="0" />
        <Setter Property="Opacity" TargetName="FocusVisual" Value="1" />
      </Trigger>
      <MultiDataTrigger>
        <MultiDataTrigger.Conditions>
          <Condition Binding="{Binding Path=IsKeyboardFocused, RelativeSource={RelativeSource Self}}" Value="True" />
          <Condition Binding="{Binding Path=IsSingleItemVisible, RelativeSource={RelativeSource AncestorType=dxan:CollapsedAccordionVirtualizingPanel}, FallbackValue=False}" Value="True" />
        </MultiDataTrigger.Conditions>
        <Setter Property="Opacity" TargetName="FocusVisual" Value="0" />
      </MultiDataTrigger>
      <MultiDataTrigger>
        <MultiDataTrigger.Conditions>
          <Condition Binding="{Binding Path=IsMouseOver, RelativeSource={RelativeSource Self}}" Value="True" />
          <Condition Binding="{Binding Path=Owner.AllowItemHighlighting, RelativeSource={RelativeSource Self}}" Value="True" />
        </MultiDataTrigger.Conditions>
        <Setter Property="Foreground" TargetName="content" Value="{DynamicResource {dxat:AccordionItemThemeKeys ResourceKey=ItemHeaderHoverForeground}}" />
        <Setter Property="Opacity" TargetName="SelectedState" Value="0" />
        <Setter Property="Opacity" TargetName="HoverState" Value="1" />
        <Setter Property="Opacity" TargetName="InactiveSelectedState" Value="0" />
        <Setter Property="Opacity" TargetName="FocusVisual" Value="0" />
      </MultiDataTrigger>
      <Trigger Property="IsSelected" Value="True">
        <Setter Property="Foreground" TargetName="content" Value="{DynamicResource {dxat:AccordionItemThemeKeys ResourceKey=ItemHeaderSelectedForeground}}" />
        <Setter Property="Opacity" TargetName="SelectedState" Value="1" />
        <Setter Property="Opacity" TargetName="HoverState" Value="0" />
        <Setter Property="Opacity" TargetName="InactiveSelectedState" Value="0" />
        <Setter Property="Opacity" TargetName="FocusVisual" Value="0" />
      </Trigger>
      <MultiTrigger>
        <MultiTrigger.Conditions>
          <Condition Property="IsSelected" Value="True" />
          <Condition Property="IsFocused" Value="False" />
        </MultiTrigger.Conditions>
        <Setter Property="Foreground" TargetName="content" Value="{DynamicResource {dxat:AccordionItemThemeKeys ResourceKey=ItemHeaderSelectedInactiveForeground}}" />
        <Setter Property="Opacity" TargetName="SelectedState" Value="0" />
        <Setter Property="Opacity" TargetName="HoverState" Value="0" />
        <Setter Property="Opacity" TargetName="InactiveSelectedState" Value="1" />
        <Setter Property="Opacity" TargetName="FocusVisual" Value="0" />
      </MultiTrigger>
      <Trigger Property="IsEnabled" Value="False">
        <Setter Property="Opacity" TargetName="content" Value="0.35" />
        <Setter Property="Foreground" TargetName="content" Value="{DynamicResource {dxat:AccordionItemThemeKeys ResourceKey=ItemHeaderDisabledForeground}}" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxat:AccordionThemeKeys ResourceKey=SplitterTemplate}" TargetType="dxa:AccordionSplitter">
    <Border Background="Transparent">
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="CommonStates">
          <VisualState x:Name="Normal" />
          <VisualState x:Name="MouseOver" />
        </VisualStateGroup>
        <VisualStateGroup x:Name="OrientationStates">
          <VisualState x:Name="Vertical" />
          <VisualState x:Name="Horizontal" />
        </VisualStateGroup>
        <VisualStateGroup x:Name="DraggingStates">
          <VisualState x:Name="NotDragging" />
          <VisualState x:Name="Dragging" />
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
      <Border x:Name="Border" BorderBrush="#FFB9B9B9" BorderThickness="1" Height="2" Margin="0,3,0,3" />
    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxat:AccordionThemeKeys ResourceKey=DropDownMenuButtonTemplate}" TargetType="ToggleButton">
    <Grid Background="Transparent" MinWidth="32">
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup Name="CommonStates">
          <VisualState Name="MouseOver">
            <Storyboard>
              <DoubleAnimation Duration="0" Storyboard.TargetName="HoverState" Storyboard.TargetProperty="Opacity" To="1" />
              <ColorAnimation Duration="0" Storyboard.TargetName="Path2" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="#FF888888" />
            </Storyboard>
          </VisualState>
          <VisualState Name="Pressed">
            <Storyboard>
              <DoubleAnimation Duration="0" Storyboard.TargetName="PressedState" Storyboard.TargetProperty="Opacity" To="1" />
              <ColorAnimation Duration="0" Storyboard.TargetName="Path2" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="#FFFFFFFF" />
            </Storyboard>
          </VisualState>
          <VisualState Name="Normal" />
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
      <Grid x:Name="HoverState" Opacity="0">
        <Border Background="#FFE3E3E3" />
      </Grid>
      <Grid x:Name="PressedState" Opacity="0">
        <Border Background="#FFF78A09" BorderBrush="#FFF78A09" BorderThickness="1" />
      </Grid>
      <Grid Height="4" Margin="10,0,10,0" SnapsToDevicePixels="True" Width="8">
        <Path x:Name="Path2" Data="F1 M 50.5001,31.5001L 40.5,31.5001L 45.5,36.5001L 50.5001,31.5001 Z" Fill="#FFB9B9B9" Stretch="Fill" />
      </Grid>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxat:AccordionThemeKeys ResourceKey=OverflowBackgroundTemplate}" TargetType="ContentControl">
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
      <Border Background="Transparent" BorderBrush="#FFB9B9B9" BorderThickness="0,1,0,0" />
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxat:AccordionThemeKeys ResourceKey=HeaderBackgroundTemplate}" TargetType="ContentControl">
    <Grid Margin="0,0,0,2" MinHeight="32">
      <Border Background="Transparent" BorderBrush="#FFB9B9B9" BorderThickness="0,0,0,1" />
      <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
        <ContentPresenter />
      </Border>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxat:AccordionItemThemeKeys ResourceKey=SelectionIndicatorTemplate}" TargetType="{x:Type dxa:AccordionItemBackgroundControl}">
    <Border Visibility="Collapsed" />
  </ControlTemplate>
  <dxa:ExpandButtonPosition x:Key="{dxat:AccordionThemeKeys ResourceKey=DefaultSubItemExpandButtonPosition}">Left</dxa:ExpandButtonPosition>
</ResourceDictionary>