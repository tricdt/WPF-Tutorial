<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking" xmlns:dxdop="http://schemas.devexpress.com/winfx/2008/xaml/docking/platform" xmlns:dxdot="http://schemas.devexpress.com/winfx/2008/xaml/docking/themekeys" xmlns:dxdove="http://schemas.devexpress.com/winfx/2008/xaml/docking/visualelements" xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <ResourceDictionary.MergedDictionaries>
    <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.VS2017Dark.v24.1;component/Docking/VS2017Dark/UICore.xaml" />
  </ResourceDictionary.MergedDictionaries>
  <!-- General -->
  <ControlTemplate x:Key="{dxdot:FloatPaneElementsThemeKey ResourceKey=SingleBorderTemplate}" TargetType="{x:Type dxdove:FormBorderControl}">
    <Grid x:Name="Root">
      <Border x:Name="Background" Background="{DynamicResource {dxdot:BrushElementsThemeKey ResourceKey=PanelBackground}}" dxdove:DockPane.HitTestType="Border" Margin="2" />
      <Border x:Name="PART_Content" Margin="{TemplateBinding dxdove:FormBorderControl.ActualContentMargin}">
        <dxdove:LayoutItemsControl x:Name="Items" dxdove:DockPane.HitTestType="Content" ItemsSource="{Binding ItemsInternal}" Margin="{Binding ActualMargin}" Orientation="{Binding Orientation}" />
      </Border>
      <dxdove:ResizeBoundsControl x:Name="PART_ResizeBorders" />
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="SizeStates">
          <VisualState x:Name="EmptySizeState" />
          <VisualState x:Name="Maximized">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames dxi:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="PART_ResizeBorders" Storyboard.TargetProperty="Visibility" />
              <ObjectAnimationUsingKeyFrames dxi:ValueSetter.Thickness="0" Storyboard.TargetName="Background" Storyboard.TargetProperty="Margin" />
              <ObjectAnimationUsingKeyFrames dxi:ValueSetter.Thickness="0" Storyboard.TargetName="Items" Storyboard.TargetProperty="Margin" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxdot:FloatPaneElementsThemeKey ResourceKey=EmptyBorderTemplate}" TargetType="{x:Type dxdove:FormBorderControl}">
    <Grid>
      <Border x:Name="PART_Content" Margin="{Binding ActualContentMargin, RelativeSource={RelativeSource Mode=TemplatedParent}}">
        <dxdove:LayoutItemsControl dxdove:DockPane.HitTestType="Content" ItemsSource="{Binding ItemsInternal}" Margin="{Binding ActualMargin}" Orientation="{Binding Orientation}" />
      </Border>
      <dxdove:ResizeBoundsControl x:Name="PART_ResizeBorders" />
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="SizeStates">
          <VisualState x:Name="EmptySizeState" />
          <VisualState x:Name="Maximized">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames dxi:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="PART_ResizeBorders" Storyboard.TargetProperty="Visibility" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxdot:FloatPaneElementsThemeKey ResourceKey=FormBorderTemplate}" TargetType="{x:Type dxdove:FormBorderControl}">
    <Grid>
      <dxdove:FormBorderContentControl Padding="{TemplateBinding dxdove:FormBorderControl.ActualBorderMargin}">
        <dxdove:LayoutItemsControl dxdove:DockPane.HitTestType="Content" ItemsSource="{Binding ItemsInternal}" Margin="{Binding ActualMargin}" Orientation="{Binding Orientation}" />
      </dxdove:FormBorderContentControl>
      <dxdove:ResizeBoundsControl x:Name="PART_ResizeBorders" />
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="SizeStates">
          <VisualState x:Name="EmptySizeState" />
          <VisualState x:Name="Maximized">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames dxi:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="PART_ResizeBorders" Storyboard.TargetProperty="Visibility" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
  <!-- Content -->
  <ControlTemplate x:Key="{dxdot:FloatPaneElementsThemeKey ResourceKey=FormBorderContentTemplate}" TargetType="{x:Type dxdove:FormBorderContentControl}">
    <Grid x:Name="Root">
      <Border x:Name="Window" Background="{DynamicResource {dxdot:BrushElementsThemeKey ResourceKey=PanelBackground}}" BorderBrush="#FF3F3F46" BorderThickness="1" dxdove:DockPane.HitTestType="Border" cs:id="Border_0000" x:Uid="@BorderBrush=Border">
        <Grid>
          <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition />
          </Grid.RowDefinitions>
          <dxdove:FloatingDragWidget Background="Transparent" dxdove:DockPane.HitTestType="Header" cs:id="FloatingDragWidget_0000">
            <Grid>
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
              </Grid.ColumnDefinitions>
              <dxdove:TemplatedCaptionControl x:Name="PART_CaptionControlPresenter" FontWeight="Bold" LayoutItem="{Binding}" Margin="3" />
              <dxdove:WindowControlBoxControl x:Name="PART_ControlBox" ButtonHeight="16" ButtonStyle="{DynamicResource {dxdot:FloatPaneElementsThemeKey ResourceKey=FloatPaneButtonStyle}}" ButtonWidth="16" CloseButtonTemplate="{DynamicResource {dxdot:FloatPaneElementsThemeKey ResourceKey=CloseButton}}" Grid.Column="1" LayoutItem="{Binding}" Margin="3" MaximizeButtonTemplate="{DynamicResource {dxdot:FloatPaneElementsThemeKey ResourceKey=MaximizeButton}}" MinimizeButtonTemplate="{DynamicResource {dxdot:FloatPaneElementsThemeKey ResourceKey=MinimizeButton}}" RestoreButtonTemplate="{DynamicResource {dxdot:FloatPaneElementsThemeKey ResourceKey=RestoreButton}}" />
            </Grid>
          </dxdove:FloatingDragWidget>
          <Border x:Name="PART_Content" BorderBrush="{DynamicResource {dxdot:DockPaneElementsThemeKey ResourceKey=BorderBrush}}" dxdove:DockPane.HitTestType="Content" Grid.Row="1">
            <ContentPresenter />
          </Border>
        </Grid>
      </Border>
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="ActiveStates">
          <VisualState x:Name="Active">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Window" Storyboard.TargetProperty="BorderBrush">
                <DiscreteObjectKeyFrame KeyTime="0">
                  <DiscreteObjectKeyFrame.Value>
                    <SolidColorBrush Color="#FF007ACC" cs:id="SolidColorBrush_0000" x:Uid="@Color=Focused" />
                  </DiscreteObjectKeyFrame.Value>
                </DiscreteObjectKeyFrame>
              </ObjectAnimationUsingKeyFrames>
              <DoubleAnimation Duration="0" Storyboard.TargetName="PART_CaptionControlPresenter" Storyboard.TargetProperty="Opacity" To="1" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Inactive">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Window" Storyboard.TargetProperty="BorderBrush">
                <DiscreteObjectKeyFrame KeyTime="0">
                  <DiscreteObjectKeyFrame.Value>
                    <SolidColorBrush Color="#FF3F3F46" cs:id="SolidColorBrush_0002" x:Uid="@Color=Border" />
                  </DiscreteObjectKeyFrame.Value>
                </DiscreteObjectKeyFrame>
              </ObjectAnimationUsingKeyFrames>
              <DoubleAnimation Duration="0" Storyboard.TargetName="PART_CaptionControlPresenter" Storyboard.TargetProperty="Opacity" To="0.62" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding Path=(dxdo:DockLayoutManager.DockLayoutManager).ViewStyle, RelativeSource={RelativeSource Self}}" Value="Light">
        <Setter Property="BorderThickness" TargetName="PART_Content" Value="0,1,0,0" />
      </DataTrigger>
      <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
        <Setter Property="Margin" TargetName="PART_CaptionControlPresenter" Value="14" />
        <Setter Property="MinHeight" TargetName="PART_CaptionControlPresenter" Value="44" />
        <Setter Property="ButtonHeight" TargetName="PART_ControlBox" Value="44" />
        <Setter Property="ButtonWidth" TargetName="PART_ControlBox" Value="44" />
        <Setter Property="Margin" TargetName="PART_ControlBox" Value="0" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <!-- Minimized Elements -->
  <ControlTemplate x:Key="{dxdot:MinimizedFloatGroupsItemElementsThemeKey ResourceKey=Template}" TargetType="{x:Type dxdove:MinimizedFloatGroupsItem}">
    <Grid x:Name="PART_CaptionBackground" dxdove:DockPane.HitTestType="Header">
      <Grid.ColumnDefinitions>
        <ColumnDefinition />
        <ColumnDefinition Width="Auto" />
      </Grid.ColumnDefinitions>
      <Border x:Name="Window" Background="{DynamicResource {dxdot:BrushElementsThemeKey ResourceKey=PanelBackground}}" BorderBrush="#FF3F3F46" BorderThickness="1" Grid.ColumnSpan="2" cs:id="Border_0000" x:Uid="@BorderBrush=Border" />
      <dxdove:TemplatedCaptionControl DataContext="{TemplateBinding ActualLayoutItem}" LayoutItem="{Binding}" Margin="12,3,12,3" />
      <dxdove:MinimizedItemControlBoxControl x:Name="PART_ControlBox" ButtonHeight="16" ButtonStyle="{DynamicResource {dxdot:DockPaneElementsThemeKey ResourceKey=DockPaneButtonStyle}}" ButtonWidth="16" CloseButtonTemplate="{DynamicResource {dxdot:DockPaneElementsThemeKey ResourceKey=CloseButton}}" Grid.Column="1" LayoutItem="{TemplateBinding ActualLayoutItem}" Margin="4" MaximizeButtonTemplate="{DynamicResource {dxdot:DockPaneElementsThemeKey ResourceKey=MaximizeButton}}" RestoreButtonTemplate="{DynamicResource {dxdot:DockPaneElementsThemeKey ResourceKey=RestoreButton}}" />
    </Grid>
  </ControlTemplate>
</ResourceDictionary>