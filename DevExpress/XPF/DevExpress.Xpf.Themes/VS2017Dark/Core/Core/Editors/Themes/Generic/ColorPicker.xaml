      <TextBlock x:Name="PART_HTextBlock" VerticalAlignment="Center" Text="H:" Margin="0,4,0,0" />
      <dxe:SpinEdit Grid.Row="0" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="360" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" EditValue="{Binding Path=H, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
      <TextBlock x:Name="PART_LTextBlock" VerticalAlignment="Center" Text="L:" Margin="0,4,0,0" Grid.Row="1" />
      <dxe:SpinEdit Grid.Row="1" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="100" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" EditValue="{Binding Path=L, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
      <TextBlock x:Name="PART_STextBlock" VerticalAlignment="Center" Text="S:" Margin="0,4,0,0" Grid.Row="2" />
      <dxe:SpinEdit Grid.Row="2" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="100" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" EditValue="{Binding Path=S, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
      <TextBlock x:Name="PART_ATextBlock" VerticalAlignment="Center" Text="A:" Margin="0,4,0,0" Grid.Row="3" Visibility="{Binding Path=ShowAlphaChannel, RelativeSource={RelativeSource FindAncestor, AncestorType=dxei:ColorPicker}, Converter={StaticResource BoolToVisibilityConverter}}" />
      <dxe:SpinEdit Grid.Row="3" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="255" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" Visibility="{Binding Path=ShowAlphaChannel, RelativeSource={RelativeSource FindAncestor, AncestorType=dxei:ColorPicker}, Converter={StaticResource BoolToVisibilityConverter}}" EditValue="{Binding Path=A, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
    </Grid>
  </DataTemplate>
  <DataTemplate x:Key="{dxet:ColorPickerThemeKey ResourceKey=HSBDataTemplate}">
    <Grid>
      <Grid.RowDefinitions>
        <RowDefinition />
        <RowDefinition />
        <RowDefinition />
        <RowDefinition />
      </Grid.RowDefinitions>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="20" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>
      <TextBlock x:Name="PART_HTextBlock" VerticalAlignment="Center" Text="H:" Margin="0,4,0,0" />
      <dxe:SpinEdit Grid.Row="0" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="360" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" EditValue="{Binding Path=H, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
      <TextBlock x:Name="PART_STextBlock" VerticalAlignment="Center" Text="S:" Margin="0,4,0,0" Grid.Row="1" />
      <dxe:SpinEdit Grid.Row="1" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="100" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" EditValue="{Binding Path=S, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
      <TextBlock x:Name="PART_BTextBlock" VerticalAlignment="Center" Text="B:" Margin="0,4,0,0" Grid.Row="2" />
      <dxe:SpinEdit Grid.Row="2" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="100" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" EditValue="{Binding Path=B, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
      <TextBlock x:Name="PART_ATextBlock" VerticalAlignment="Center" Text="A:" Margin="0,4,0,0" Grid.Row="3" Visibility="{Binding Path=ShowAlphaChannel, RelativeSource={RelativeSource FindAncestor, AncestorType=dxei:ColorPicker}, Converter={StaticResource BoolToVisibilityConverter}}" />
      <dxe:SpinEdit Grid.Row="3" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="255" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" Visibility="{Binding Path=ShowAlphaChannel, RelativeSource={RelativeSource FindAncestor, AncestorType=dxei:ColorPicker}, Converter={StaticResource BoolToVisibilityConverter}}" EditValue="{Binding Path=A, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
    </Grid>
