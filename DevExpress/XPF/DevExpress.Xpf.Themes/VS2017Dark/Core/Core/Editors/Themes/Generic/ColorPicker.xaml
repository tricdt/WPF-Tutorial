  <DataTemplate x:Key="{dxet:ColorPickerThemeKey ResourceKey=RGBDataTemplate}">
    <Grid>
      <Grid.RowDefinitions>
        <RowDefinition />
        <RowDefinition />
        <RowDefinition />
        <RowDefinition />
      </Grid.RowDefinitions>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="20" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>
      <TextBlock x:Name="PART_RTextBlock" VerticalAlignment="Center" Text="R:" Margin="0,4,0,0" />
      <dxe:SpinEdit Grid.Row="0" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="255" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" EditValue="{Binding Path=R, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
      <TextBlock x:Name="PART_GTextBlock" VerticalAlignment="Center" Text="G:" Margin="0,4,0,0" Grid.Row="1" Grid.Column="0" />
      <dxe:SpinEdit Grid.Row="1" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="255" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" EditValue="{Binding Path=G, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
      <TextBlock x:Name="PART_BTextBlock" VerticalAlignment="Center" Text="B:" Margin="0,4,0,0" Grid.Row="2" Grid.Column="0" />
      <dxe:SpinEdit Grid.Row="2" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="255" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" EditValue="{Binding Path=B, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
      <TextBlock x:Name="PART_ATextBlock" VerticalAlignment="Center" Text="A:" Margin="0,4,0,0" Grid.Row="3" Grid.Column="0" Visibility="{Binding Path=ShowAlphaChannel, RelativeSource={RelativeSource FindAncestor, AncestorType=dxei:ColorPicker}, Converter={StaticResource BoolToVisibilityConverter}}" />
      <dxe:SpinEdit Grid.Row="3" Grid.Column="1" Margin="0,4,0,0" EditMode="{Binding Path=EditMode}" AllowDefaultButton="False" AllowNullInput="True" IsFloatValue="False" MinValue="0" MaxValue="255" AllowRoundOutOfRangeValue="True" HorizontalAlignment="Stretch" SelectAllOnMouseUp="true" Visibility="{Binding Path=ShowAlphaChannel, RelativeSource={RelativeSource FindAncestor, AncestorType=dxei:ColorPicker}, Converter={StaticResource BoolToVisibilityConverter}}" EditValue="{Binding Path=A, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource DecimalToIntConverter}}" />
    </Grid>
  </DataTemplate>
