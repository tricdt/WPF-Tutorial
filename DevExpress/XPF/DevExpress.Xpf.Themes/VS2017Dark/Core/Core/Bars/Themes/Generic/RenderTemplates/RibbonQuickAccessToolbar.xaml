          <dxcn:RenderContentControl Name="PART_Content" PreferRenderTemplate="False" Dock="Bottom" HorizontalAlignment="Center" HorizontalContentAlignment="Center" />
        </dxcn:RenderPanel>
      </dxcn:RenderPanel>
    </dxcn:RenderBorder>
    <dxcn:RenderTemplate.Triggers>
      <dxcn:RenderTriggersCollection>
        <dxcn:RenderTriggersCollection.MergedTriggers>
