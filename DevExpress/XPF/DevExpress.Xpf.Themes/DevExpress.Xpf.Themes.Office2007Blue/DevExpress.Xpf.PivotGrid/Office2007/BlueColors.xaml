<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxpgi="http://schemas.devexpress.com/winfx/2008/xaml/pivotgrid/internal"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FilterPanelForeground}" Color="Black"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=ResizingIndicatorBrush}" Color="Black"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=DataAreaCuttedTextForeground}" Color="#064DAE"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=PivotBackground}" Color="#FFE3EFFF"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=PivotBorderBrush}" Color="#FF6593CF"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=HeaderEmptyTextForeground}" Color="#FF89A9D2"/>

    <LinearGradientBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=HeaderNormalGradient}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFEFEFF" Offset="0"/>
        <GradientStop Color="#FFC5DEFF" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=HeaderActiveGradient}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFEFEFF" Offset="1"/>
        <GradientStop Color="#FFC5DEFF" Offset="0"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=HeaderControlsColor}" Color="#FF9AC6FF"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=HeaderForeground}" Color="#FF000000"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=HeaderMouseOverForeground}" Color="#FF000000"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FilterColor}" Color="#FF9AC6FF"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FilterMouseOverColor}" Color="#FF5181BF"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldTotalBrush}" Color="#FFDCEAFB"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldValueSelectedBrush}" Color="#FFDBE3F4"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=ValueTotalSelectedBrush}" Color="#FFDBE3F4"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=ValueSelectedForegroundBrush}" Color="#000000"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=CellForegroundBrush}" Color="#000000"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=CellTotalForegroundBrush}" Color="#000000"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=CellTotalSelectedForegroundBrush}" Color="#000000"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=CellSelectedForegroundBrush}" Color="#000000"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=ValueForegroundBrush}" Color="#000000"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=ValueTotalForegroundBrush}" Color="#000000"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=ValueTotalSelectedForegroundBrush}" Color="#000000"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldValueBorderBrush}" Color="#FFC6D7ED"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldValueOuterBorderBrush}" Color="#FFEFF5FE"/>

    <SolidColorBrush x:Key="{dxpgi:PivotGridThemeKey ResourceKey=FieldListBackgroundBrush}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldValueBackground}" Color="#FFEFF5FE"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=ExpandButtonColor}" Color="#FF304860"/>

    <LinearGradientBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=ExpandButtonBackground}"
                         EndPoint="0.78,0.914"
                         StartPoint="0.22,0.086">
        <GradientStop Color="#FFA0B0C0" Offset="0"/>
        <GradientStop Color="#FF304860" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=ExpandButtonInnerBackground}"
                         EndPoint="0.829,0.877"
                         StartPoint="0.171,0.123">
        <GradientStop Color="#FFFFFFFF" Offset="0"/>
        <GradientStop Color="#FFB0C8F0" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=CellValueBrush}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=CellValueBorderBrush}" Color="#FFC6D7ED"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=CellTotalBrush}" Color="#FFF4F8FE"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=CellSelectedBrush}" Color="#FFEAECF5"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=CellTotalSelectedBrush}" Color="#FFE0E6F4"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FocusedDataItemStroke}" Color="#FF969696"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldListDragTextForeground}" Color="#FF8E9399"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=GroupBorderShadowColor}" Color="#FFF1F7FF"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=GroupButtonFill}" Color="#FF567DB1"/>

    <SolidColorBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=GroupBorderBrush}" Color="#FFC1D0DE"/>

    <RadialGradientBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=ScrollingCornerGradient}">
        <RadialGradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="#FFFBFBFB" Offset="0.041"/>
                <GradientStop Color="#FFEBEDEF" Offset="1"/>
                <GradientStop Color="#FFF0F0F0" Offset="0.213"/>
                <GradientStop Color="#FFEFEFEF" Offset="0.476"/>
                <GradientStop Color="#FFEBEDEF" Offset="0.04"/>
                <GradientStop Color="#FFEBEDEF" Offset="0"/>
            </GradientStopCollection>
        </RadialGradientBrush.GradientStops>
        <RadialGradientBrush.RelativeTransform>
            <TransformGroup>
                <ScaleTransform CenterX="0.5"
                                CenterY="0.5"
                                ScaleX="2.671"
                                ScaleY="2.671"/>
                <SkewTransform AngleX="0"
                               AngleY="0"
                               CenterX="0.5"
                               CenterY="0.5"/>
                <RotateTransform Angle="90"
                                 CenterX="0.5"
                                 CenterY="0.5"/>
                <TranslateTransform X="-0.496" Y="-0.496"/>
            </TransformGroup>
        </RadialGradientBrush.RelativeTransform>
    </RadialGradientBrush>

    <LinearGradientBrush x:Key="{dxpgi:AppearanceThemeKey ResourceKey=AreaSeparatorBrush}"
                         EndPoint="0,0.5"
                         StartPoint="1,0.5">
        <GradientStop Color="#FFC6D7ED" Offset="0.5"/>
        <GradientStop Color="#00C6D7ED" Offset="1"/>
        <GradientStop Color="#00C6D7ED" Offset="0"/>
    </LinearGradientBrush>

    <Style x:Key="{dxpgi:AppearanceThemeKey ResourceKey=EmptyAreaHighlighterStyle}" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="#FFC9DAEE"/>
        <Setter Property="Margin" Value="-3"/>
    </Style>

    <Style x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldListDragOverBorderStyle}" TargetType="{x:Type Border}">
        <Setter Property="BorderBrush" Value="#FFFFC200"/>
        <Setter Property="CornerRadius" Value="4"/>
        <Setter Property="Opacity" Value="0.35"/>
    </Style>
    <sys:Double x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldListHiddenFieldsShowGridMinHeight}">16</sys:Double>
    <sys:Double x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldListUpdateButtonMinHeight}">24</sys:Double>
    <sys:Double x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldListUpdateButtonMinWidth}">78</sys:Double>

</ResourceDictionary>