        <GradientStop Color="#FFF0F0F0" Offset="0.676"/>
        <GradientStop Color="#FFF4F4F4" Offset="0.218"/>
        <GradientStop Color="#FFEFEFEF" Offset="0.288"/>
        <GradientStop Color="#FFEBEDEF" Offset="0"/>
        <GradientStop Color="#FFEBEDEF" Offset="0.08"/>
