                    <GradientStop Color="{Binding Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorOverlayConveter}, ConverterParameter=#FFFFFFFF}" Offset="1"/>
                    <GradientStop Color="{Binding Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorOverlayConveter}, ConverterParameter=#FFE0E0E0}" Offset="0.5"/>
                    <GradientStop Color="{Binding Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorOverlayConveter}, ConverterParameter=#FFD0D0D0}" Offset="0.5"/>
                </LinearGradientBrush>
                <SolidColorBrush x:Key="Color.TabSelectorItem.SelectedState.Border1" Color="#FF6593CF"/>
                <LinearGradientBrush x:Key="Color.TabSelectorItem.SelectedState.Border2"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0">
                    <GradientStop Color="{Binding Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorOverlayConveter}, ConverterParameter=#FFFFFFFF}" Offset="0"/>
                    <GradientStop Color="{Binding Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorOverlayConveter}, ConverterParameter=#FFF6F6F6}" Offset="0.35"/>
                    <GradientStop Color="{Binding Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorOverlayConveter}, ConverterParameter=#FFF6F6F6}" Offset="1"/>
                </LinearGradientBrush>
                <SolidColorBrush x:Key="Color.TabSelectorItem.MouseHoverState.Border1" Color="#FF6593CF"/>
                <LinearGradientBrush x:Key="Color.TabSelectorItem.MouseHoverState.Border2"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0">
                    <GradientStop Color="#FFFFFEFB" Offset="0"/>
                    <GradientStop Color="#FFFFF5D7" Offset="1"/>
                    <GradientStop Color="#FFFFF6DC" Offset="0.5"/>
                </LinearGradientBrush>
                <LinearGradientBrush x:Key="Color.TabSelectorItem.MouseHoverState.Border3"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0">
                    <GradientStop Color="#FFFFFBE5" Offset="0"/>
                    <GradientStop Color="#FFFCEBB7" Offset="1"/>
                    <GradientStop Color="#FFFADF8F" Offset="0.5"/>
                    <GradientStop Color="#FFF4C233" Offset="0.5"/>
                </LinearGradientBrush>
            </Grid.Resources>
            <Grid Margin="0,1,0,-1">
                <Grid x:Name="UnselectedState" Visibility="{TemplateBinding State, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Normal}">
                    <Border Background="{StaticResource Color.TabSelectorItem.UnselectedState.Border1}" Padding="0,0,1,0">
                        <Border Background="{StaticResource Color.TabSelectorItem.UnselectedState.Border2}" Padding="1,0,1,1">
                            <Border Background="{StaticResource Color.TabSelectorItem.UnselectedState.Border3}"/>
                        </Border>
                    </Border>
                </Grid>
                <Grid x:Name="HoverState" Opacity="0">
                    <Border Background="{StaticResource Color.TabSelectorItem.MouseHoverState.Border1}" Padding="0,0,1,0">
                        <Border Background="{StaticResource Color.TabSelectorItem.MouseHoverState.Border2}" Padding="1,0,1,1">
                            <Border Background="{StaticResource Color.TabSelectorItem.MouseHoverState.Border3}"/>
                        </Border>
                    </Border>
                </Grid>
                <Grid x:Name="CheckedState" Visibility="{TemplateBinding State, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Selected}">
                    <Border Background="{StaticResource Color.TabSelectorItem.UnselectedState.Border1}" Padding="0,0,1,0">
                        <Border Background="{StaticResource Color.TabSelectorItem.UnselectedState.Border2}" Padding="1,0,1,1">
                            <Border Background="{StaticResource Color.TabSelectorItem.UnselectedState.Border3}"/>
                        </Border>
                    </Border>
                </Grid>
                <Grid x:Name="SelectedState" Visibility="{TemplateBinding State, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Active}">
                    <Border Background="{StaticResource Color.TabSelectorItem.SelectedState.Border1}"
                            Margin="0,-1,0,0"
                            Padding="0,0,1,0">
                        <Border Background="{StaticResource Color.TabSelectorItem.SelectedState.Border2}" Padding="1,0,1,1"></Border>
                    </Border>
                </Grid>
            </Grid>
            <TextBlock Focusable="False"
                       Margin="12,0,12,0"
                       Text="{TemplateBinding Text}"
                       TextAlignment="Center"
                       VerticalAlignment="Center"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

