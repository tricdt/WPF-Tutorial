<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxret="http://schemas.devexpress.com/winfx/2008/xaml/richedit/themekeys"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--RichEditControlBackgroundBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RichEditControlBackgroundBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF9FBEE7" Offset="0"/>
        <GradientStop Color="#FF6591CD" Offset="1"/>
        <GradientStop Color="#FF587FB2" Offset="0.75"/>
    </LinearGradientBrush>

    <!--RichEditControlBackgroundPatternBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RichEditControlBackgroundPatternBrush}" Color="#27FFFFFF"/>

    <!--RichEditControlBorderBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RichEditControlBorderBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF6593CF" Offset="0"/>
        <GradientStop Color="#FF4F6DB6" Offset="1"/>
    </LinearGradientBrush>

    <!--PageShadowBackgroundBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=PageShadowBackgroundBrush}" Color="#3F000000"/>

    <!--PageBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=PageBorderBrush}" Color="#59000000"/>

    <!--RulerBackgroundBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBackgroundBrush}" Color="#7F275CC6"/>

    <!--RulerBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBorderBrush}" Color="#FF355AA5"/>

    <!--RulerTabHotZoneBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTabHotZoneBrush}" Color="#FF5379AC"/>

    <!--RulerIndentHotZoneBackgroundBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerIndentHotZoneBackgroundBrush}"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#FFE4EDF9" Offset="0"/>
        <GradientStop Color="#FFABBED8" Offset="1"/>
    </LinearGradientBrush>

    <!--RulerIndentHotZoneBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerIndentHotZoneBorderBrush}" Color="#FF5379AC"/>

    <!--RulerTableHotZoneBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTableHotZoneBorderBrush}" Color="#5A0A418A"/>

    <!--RulerTableHotZoneBackgroundBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTableHotZoneBackgroundBrush}" Color="#FFF8F8F8"/>

    <!--RulerTickMarkBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTickMarkBrush}" Color="#FF5A616C"/>

    <!--RulerDefaultTabMarkBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerDefaultTabMarkBrush}" Color="#7F5A616C"/>

    <!--RulerSpaceAreaBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerSpaceAreaBrush}" Color="#FFC8DDF9"/>

    <!--TabTypeToggleHotZoneControlTemplateBackgroundBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=TabTypeToggleHotZoneControlTemplateBackgroundBrush}"
                         EndPoint="0.774,0.918"
                         StartPoint="0.226,0.082">
        <GradientStop Color="#FFEAF2FF"/>
        <GradientStop Color="#FFC6DCFF" Offset="1"/>
    </LinearGradientBrush>

</ResourceDictionary>