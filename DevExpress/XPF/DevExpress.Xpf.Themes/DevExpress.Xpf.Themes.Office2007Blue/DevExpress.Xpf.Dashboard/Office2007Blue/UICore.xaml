<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdash="http://schemas.devexpress.com/winfx/2008/xaml/dashboard"
                    xmlns:dxdashi="http://schemas.devexpress.com/winfx/2008/xaml/dashboardi"
                    xmlns:dxdasht="http://schemas.devexpress.com/winfx/2008/xaml/dashboard/themekeys"
                    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:wn="clr-namespace:System.Windows;assembly=PresentationCore"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2007Blue.v24.1;component/DevExpress.Xpf.Dashboard/Office2007Blue/UICustom.xaml"/>
        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2007Blue.v24.1;component/DevExpress.Xpf.Dashboard/Office2007Blue/UIPreset.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!-- DashboardControl -->

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SelectedItemBackground}"
                     Color="#FFF8CD99"
                     Opacity="0.5"/>

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SelectedItemBorder}"
                     Color="#FFF6B386"
                     Opacity="0.5"/>

    <sys:Boolean x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=IsDarkTheme}">False</sys:Boolean>

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=Background}" Color="#FFE3EFFF"/>

    <Thickness x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=BorderThickness}">1</Thickness>

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=TitleForeground}" Color="#FF15428B"/>

    <sys:Double x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=TitleFontSize}">12</sys:Double>

    <FontWeight x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=TitleFontWeight}">Bold</FontWeight>

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SubTitleForeground}"
                     Color="#FF000000"
                     Opacity="0.62"/>

    <sys:Double x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SubTitleFontSize}">12</sys:Double>

    <FontWeight x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SubTitleFontWeight}">Normal</FontWeight>

    <Color x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=MasterFilterIconColor}">#FF4C607A</Color>

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=FormatConditionBarDrawAxisBrush}" Color="#FFFB8D00"/>

    <!-- DashboardLayoutItem -->

    <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=Background}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=BorderBrush}" Color="#FF8EA7C4"/>

    <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=CaptionBackground}" Color="#FFEBF3FF"/>

    <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=CaptionForeground}" Color="#FF15428B"/>

    <sys:Double x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=CaptionFontSize}">12</sys:Double>

    <FontWeight x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=CaptionFontWeight}">Bold</FontWeight>

    <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=WaitIndicatorForeground}" Color="#FFFB8D00"/>

    <!-- CardItem -->

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=Background}">Transparent</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=MainTextForeground}">#FF000000</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=CardItemBackground}">Transparent</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=CardItemBorderBrush}">#FF8EA7C4</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=HighlightedBackground}"
                     Color="#FFF8CD99"
                     Opacity="0.3"/>

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=HighlightedBorderBrush}"
                     Color="#FFF6B386"
                     Opacity="0.3"/>

    <CornerRadius x:Key="{dxdasht:CardItemThemeKey ResourceKey=CardBackgroundCornerRadius}">2</CornerRadius>

    <!-- DeltaValue -->

    <SolidColorBrush x:Key="{dxdasht:DeltaValuePresenterThemeKey ResourceKey=GoodForeground}">#FF5F957D</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:DeltaValuePresenterThemeKey ResourceKey=BadForeground}">#FFBA4D51</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:DeltaValuePresenterThemeKey ResourceKey=WarningForeground}">#FFAF8A53</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:DeltaValuePresenterThemeKey ResourceKey=NeutralForeground}">#FF666666</SolidColorBrush>

    <!-- GaugeItem -->

    <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeHighlightedBackgroundBrush}"
                     Color="#FFF8CD99"
                     Opacity="0.3"/>

    <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeHighlightedBorderBrush}"
                     Color="#FFF6B386"
                     Opacity="0.3"/>

    <CornerRadius x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeBackgroundCornerRadius}">2</CornerRadius>

    <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeCaptionForeground}" Color="#FF15428B"/>

    <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeScaleLabelForeground}" Color="#FF15428B"/>

    <sys:Double x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeCaptionFontSize}">12</sys:Double>

    <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeSubCaptionForeground}"
                     Color="#FF000000"
                     Opacity="0.62"/>

    <sys:Double x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeSubCaptionFontSize}">12</sys:Double>

    <!-- MapItem -->

    <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointBackgroundBrush}" Color="#FF8EA7C4"/>

    <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointHighLightedBackgroundBrush}" Color="#FFFB8D00"/>

    <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointBorderBrush}" Color="#FFFFFFFF"/>

    <sys:Double x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointDisplayTextFontSize}">12</sys:Double>

    <wn:FontWeight x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointDisplayTextFontWeight}">Bold</wn:FontWeight>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=GradientColorStart}">#FF7482AA</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=GradientColorEnd}">#FFAA7474</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=DeltaColorGood}">#FF5F957D</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=DeltaColorBad}">#FFBA4D51</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=DeltaColorWarning}">#FFAF8A53</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorTens}">#FF5F6595</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorHundreds}">#FF007E65</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorThousands}">#FFAF8A53</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorMillions}">#FFBA4D51</Color>

    <!-- PieItem -->

    <SolidColorBrush x:Key="{dxdasht:PieItemThemeKey ResourceKey=PieHighlightedBackgroundBrush}"
                     Color="#FFF8CD99"
                     Opacity="0.3"/>

    <SolidColorBrush x:Key="{dxdasht:PieItemThemeKey ResourceKey=PieHighlightedBorderBrush}"
                     Color="#FFF6B386"
                     Opacity="0.3"/>

    <CornerRadius x:Key="{dxdasht:PieItemThemeKey ResourceKey=PieBackgroundCornerRadius}">2</CornerRadius>

    <!-- ChartItem -->

    <SolidColorBrush x:Key="{dxdasht:ChartItemThemeKey ResourceKey=LegendBackgroundBrush}">#FFFFFFFF</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:ChartItemThemeKey ResourceKey=LegendForegroundBrush}">#FF27282C</SolidColorBrush>

    <!-- TreemapItem -->

    <SolidColorBrush x:Key="{dxdasht:TreemapItemThemeKey ResourceKey=ToolTipForeground}">#FFE3EFFF</SolidColorBrush>

    <sys:Double x:Key="{dxdasht:TreemapItemThemeKey ResourceKey=ToolTipFontSize}">12</sys:Double>

    <FontWeight x:Key="{dxdasht:TreemapItemThemeKey ResourceKey=ToolTipFontWeight}">Bold</FontWeight>

    <!-- ListBox -->

    <SolidColorBrush x:Key="{dxdasht:ListBoxItemThemeKey ResourceKey=Foreground}" Color="#FF000000"/>

    <!-- Other -->

    <Color x:Key="{dxdasht:PivotItemThemeKey ResourceKey=DefaultBackgroundColor}">#FFFFFFFF</Color>

    <Color x:Key="{dxdasht:GridItemThemeKey ResourceKey=DefaultBackgroundColor}">#FFFFFFFF</Color>

    <Color x:Key="{dxdasht:CardItemThemeKey ResourceKey=DefaultBackgroundColor}">#FFFFFFFF</Color>

    <Color x:Key="{dxdasht:ChartItemThemeKey ResourceKey=DefaultBackgroundColor}">#FFFFFFFF</Color>

</ResourceDictionary>