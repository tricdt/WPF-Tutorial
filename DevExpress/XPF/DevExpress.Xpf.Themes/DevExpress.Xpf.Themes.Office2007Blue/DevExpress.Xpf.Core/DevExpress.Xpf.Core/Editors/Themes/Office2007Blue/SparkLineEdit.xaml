    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = MinPointBrush}" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = StartPointBrush}" Color="#FF567DB1"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = EndPointBrush}" Color="#FF567DB1"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = NegativePointBrush}" Color="#FF12C6E6"/>

