    <ControlTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=VerticalBackgroundTemplate}" TargetType="ContentControl">
        <Grid x:Name="TrackBackground"
              Background="{x:Null}"
              Height="Auto"
              HorizontalAlignment="Center"
              Margin="0,1,0,1"
              VerticalAlignment="Stretch"
              Width="8">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="InternalFocusStates">
                    <VisualState x:Name="InternalFocused">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="FocusElement"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.35"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="InternalUnfocused"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="FocusElement"
                    Background="#FFFFC200"
                    CornerRadius="1"
                    HorizontalAlignment="Center"
                    Margin="0,-1,1,-1"
                    Opacity="0"
                    Width="3"/>
            <Border Background="#FF7496C2 "
                    BorderBrush="#7FFFFFFF "
                    BorderThickness="0,0,1,0"
                    Height="Auto"
                    HorizontalAlignment="Center"
                    Margin="0,0,0,0"
                    VerticalAlignment="Stretch"
                    Width="2"/>
            <Border x:Name="border"
                    Background="#FF7496C2 "
                    BorderBrush="{x:Null}"
                    BorderThickness="0,0,0,0"
                    Height="1"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,1,0"
                    VerticalAlignment="Center"
                    Width="Auto"/>
        </Grid>
    </ControlTemplate>

