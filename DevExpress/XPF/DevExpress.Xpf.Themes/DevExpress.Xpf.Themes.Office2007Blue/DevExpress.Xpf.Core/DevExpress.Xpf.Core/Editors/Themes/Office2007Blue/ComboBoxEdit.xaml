            </Grid>
            <Grid x:Name="PART_Pressed" Opacity="0.0">
                <Border x:Name="border_Copy"
                        CornerRadius="2"
                        Padding="1"
                        Visibility="Visible">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FF9F7559 " Offset="0"/>
                            <GradientStop Color="#FFC6C0B1 " Offset="1"/>
                            <GradientStop Color="#FF8E8165 " Offset="0.1"/>
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border x:Name="border3"
                            CornerRadius="1"
                            Padding="1"
                            Visibility="Visible">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFBB9F7C " Offset="0"/>
                                <GradientStop Color="#FFFFDE70 " Offset="1"/>
                                <GradientStop Color="#FFF8AD61 " Offset="0.5"/>
                                <GradientStop Color="#FFF7952F " Offset="0.5"/>
                                <GradientStop Color="#FFFDD3A8 " Offset="0.1"/>
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <ContentPresenter x:Name="contentPresenter" Margin="{TemplateBinding Padding}"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="False">
                <Setter Property="Opacity"
                        TargetName="PART_Hover"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="PART_Pressed"
                        Value="0"/>
                <Setter Property="Foreground" Value="#FF000000"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity"
                        TargetName="PART_Hover"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="PART_Pressed"
                        Value="0"/>
                <Setter Property="Foreground" Value="#FF000000"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Opacity"
                        TargetName="PART_Hover"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="PART_Pressed"
                        Value="1"/>
                <Setter Property="Foreground" Value="#FF000000"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" Value="0.36"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

</ResourceDictionary>