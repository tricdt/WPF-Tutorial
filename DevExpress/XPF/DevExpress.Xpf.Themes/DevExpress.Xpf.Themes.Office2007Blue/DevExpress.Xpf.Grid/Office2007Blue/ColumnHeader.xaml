    <ControlTemplate x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=ControlTemplate}" TargetType="{x:Type dxg:GridColumnHeaderBase}">
        <Grid x:Name="PART_Root">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="headerLeftSeparator"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="0,0,1,0"
                    Grid.Column="0"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    Visibility="Collapsed"/>
            <Border x:Name="headerRightSeparator"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="0"
                    Grid.Column="2"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    Visibility="Collapsed"/>
            <Border x:Name="PART_Border"
                    Focusable="True"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="0,0,0,1"
                    Grid.Column="1"/>
            <Rectangle x:Name="Separator"
                       Fill="#FF9AC6FF"
                       Grid.Column="1"
                       HorizontalAlignment="Right"
                       Margin="0,3,0,4"
                       Stroke="{x:Null}"
                       StrokeThickness="0"
                       Width="1"
                       cs:id="Rectangle_0000"/>
            <Rectangle x:Name="SeparatorBorderRight"
                       Fill="{DynamicResource {dxgt:GridColumnHeaderThemeKey ResourceKey=HeadersVerticalLineBrush}}"
                       Grid.Column="1"
                       HorizontalAlignment="Right"
                       Stroke="{x:Null}"
                       StrokeThickness="0"
                       Visibility="Collapsed"
                       Width="1"/>
            <Rectangle x:Name="SeparatorBorderLeft"
                       Fill="{DynamicResource {dxgt:GridColumnHeaderThemeKey ResourceKey=HeadersVerticalLineBrush}}"
                       Grid.Column="0"
                       HorizontalAlignment="Left"
                       Margin="-1,0,0,0"
                       Stroke="{x:Null}"
                       StrokeThickness="0"
                       Visibility="Collapsed"
                       Width="1"/>
            <dxg:ColumnHeaderLayout x:Name="PART_Layout"
                                    Grid.Column="1"
                                    Padding="{StaticResource {dxgt:GridColumnHeaderThemeKey ResourceKey=ContentMargin}}"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="DataColumnPosition" Value="Left">
                <Setter Property="Visibility"
                        TargetName="headerRightSeparator"
                        Value="Visible"/>
                <Setter Property="Width"
                        TargetName="headerRightSeparator"
                        Value="{Binding ActualBandRightSeparatorWidth}"/>
                <Setter Property="Background" TargetName="headerRightSeparator">
                    <Setter.Value>
                        <MultiBinding>
                            <MultiBinding.Bindings>
                                <Binding Path="ActualBandHeaderRightSeparatorColor"/>
                                <Binding Path="BorderBrush" RelativeSource="{RelativeSource TemplatedParent}"/>
                            </MultiBinding.Bindings>
                            <MultiBinding.Converter>
                                <dxg:BandSeparatorColorConverter/>
                            </MultiBinding.Converter>
                        </MultiBinding>
                    </Setter.Value>
                </Setter>
            </Trigger>
            <Trigger Property="DataColumnPosition" Value="Middle">
                <Setter Property="Width"
                        TargetName="headerRightSeparator"
                        Value="{Binding ActualBandRightSeparatorWidth}"/>
                <Setter Property="Visibility" TargetName="headerRightSeparator">
                    <Setter.Value>
                        <Binding Converter="{StaticResource IntToVisibilityConverter}" Path="ActualBandRightSeparatorWidth"/>
                    </Setter.Value>
                </Setter>
                <Setter Property="Visibility" TargetName="SeparatorBorderRight">
                    <Setter.Value>
                        <Binding Converter="{StaticResource IntToVisibilityConverter}" Path="ActualBandRightSeparatorWidth"/>
                    </Setter.Value>
                </Setter>
                <Setter Property="Visibility" TargetName="SeparatorBorderLeft">
                    <Setter.Value>
                        <Binding Converter="{StaticResource IntToVisibilityConverter}" Path="ActualBandLeftSeparatorWidth"/>
                    </Setter.Value>
                </Setter>
                <Setter Property="Background" TargetName="headerRightSeparator">
                    <Setter.Value>
                        <MultiBinding>
                            <MultiBinding.Bindings>
                                <Binding Path="ActualBandHeaderRightSeparatorColor"/>
                                <Binding Path="BorderBrush" RelativeSource="{RelativeSource TemplatedParent}"/>
                            </MultiBinding.Bindings>
                            <MultiBinding.Converter>
                                <dxg:BandSeparatorColorConverter/>
                            </MultiBinding.Converter>
                        </MultiBinding>
                    </Setter.Value>
                </Setter>
                <Setter Property="Visibility"
                        TargetName="headerLeftSeparator"
                        Value="Visible"/>
                <Setter Property="Width"
                        TargetName="headerLeftSeparator"
                        Value="{Binding ActualBandLeftSeparatorWidth}"/>
                <Setter Property="Background" TargetName="headerLeftSeparator">
                    <Setter.Value>
                        <MultiBinding>
                            <MultiBinding.Bindings>
                                <Binding Path="ActualBandHeaderLeftSeparatorColor"/>
                                <Binding Path="BorderBrush" RelativeSource="{RelativeSource TemplatedParent}"/>
                            </MultiBinding.Bindings>
                            <MultiBinding.Converter>
                                <dxg:BandSeparatorColorConverter/>
                            </MultiBinding.Converter>
                        </MultiBinding>
                    </Setter.Value>
                </Setter>
                <Setter Property="Visibility" TargetName="Separator">
                    <Setter.Value>
                        <Binding Converter="{StaticResource IntToVisibilityConverterInvert}" Path="ActualBandRightSeparatorWidth"/>
                    </Setter.Value>
                </Setter>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="HasRightSibling" Value="True"/>
                    <Condition Property="HasTopElement" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter Property="BorderThickness"
                        TargetName="headerRightSeparator"
                        Value="0,0,1,0"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="HasRightSibling" Value="True"/>
                    <Condition Property="HasTopElement" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="BorderThickness"
                        TargetName="headerRightSeparator"
                        Value="0,0,1,0"/>
            </MultiTrigger>
            <Trigger Property="HasRightSibling" Value="False">
                <Setter Property="Visibility"
                        TargetName="Separator"
                        Value="Collapsed"/>
            </Trigger>
            <Trigger Property="HasLeftSibling" Value="True">
                <Setter Property="BorderThickness"
                        TargetName="PART_Border"
                        Value="1,0,0,1"/>
            </Trigger>
            <Trigger Property="ColumnPosition" Value="Standalone">
                <Setter Property="Visibility"
                        TargetName="Separator"
                        Value="Collapsed"/>
                <Setter Property="Padding"
                        TargetName="PART_Layout"
                        Value="5,4"/>
                <Setter Property="BorderThickness"
                        TargetName="PART_Border"
                        Value="1"/>
            </Trigger>
            <Trigger Property="ColumnPosition" Value="Right">
                <Setter Property="HorizontalAlignment"
                        TargetName="Separator"
                        Value="Left"/>
            </Trigger>
            <Trigger Property="dxg:ColumnBase.HeaderPresenterType" Value="GroupPanel">
                <Setter Property="Margin" TargetName="PART_Border">
                    <Setter.Value>
                        <Binding Path="(dxg:GridColumn.MergeWithPreviousGroup)">
                            <Binding.Converter>
                                <dx:BoolToObjectConverter FalseValue="2,0,2,0" TrueValue="-3,0,2,0"/>
                            </Binding.Converter>
                        </Binding>
                    </Setter.Value>
                </Setter>
                <Setter Property="Padding"
                        TargetName="PART_Layout"
                        Value="7,4"/>
                <Setter Property="Margin" TargetName="PART_Root">
                    <Setter.Value>
                        <Binding Path="(dxg:GridColumn.GroupLevel)">
                            <Binding.Converter>
                                <dxg:GroupPanelPaddingConverter Offset="5"/>
                            </Binding.Converter>
                        </Binding>
                    </Setter.Value>
                </Setter>
                <Setter Property="VerticalAlignment"
                        TargetName="PART_Root"
                        Value="Top"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

