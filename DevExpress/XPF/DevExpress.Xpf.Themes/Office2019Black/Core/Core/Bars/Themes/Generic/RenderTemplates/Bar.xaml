          <dxcn:RenderContentControl Name="PART_Content" Dock="Bottom" PreferRenderTemplate="False" HorizontalAlignment="Center" HorizontalContentAlignment="Center" />
        </dxcn:RenderPanel>
      </dxcn:RenderPanel>
    </dxcn:RenderBorder>
    <dxcn:RenderTemplate.Triggers>
      <dxcn:RenderTriggersCollection>
