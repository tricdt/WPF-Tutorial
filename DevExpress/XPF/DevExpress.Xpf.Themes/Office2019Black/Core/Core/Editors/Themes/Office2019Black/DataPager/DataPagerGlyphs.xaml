  </ResourceDictionary.MergedDictionaries>
  <!-- General -->
  <DataTemplate x:Key="{dxet:DataPagerButtonThemeKey ResourceKey=FirstPageGlyph}">
    <Grid x:Name="Root" Height="20" Width="16">
      <Path Fill="#FFDADADA" Style="{StaticResource UIShape.Navigation.First}" cs:id="Path_0000" x:Uid="@Fill=ControlLight.Foreground" />
    </Grid>
    <DataTemplate.Triggers>
      <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
        <Setter Property="MinHeight" TargetName="Root" Value="42" />
        <Setter Property="MinWidth" TargetName="Root" Value="42" />
      </Trigger>
    </DataTemplate.Triggers>
  </DataTemplate>
