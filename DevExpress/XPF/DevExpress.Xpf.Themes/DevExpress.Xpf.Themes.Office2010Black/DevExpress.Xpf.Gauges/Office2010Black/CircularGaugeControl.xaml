<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxga="http://schemas.devexpress.com/winfx/2008/xaml/gauges"
                    xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/gauges/themekeys"
                    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- ArcScaleNeedle -->

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=NeedleFill}"
                     Color="#FFFFFFFF"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF484E5A"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=ForegroundBrush}"
                     Color="#FFFFFFFF"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF000000"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=ScaleElementsBrush}"
                     Color="#FF4F4F4F"
                     cs:Office2010Blue.Color="#FF515D7A"
                     cs:Office2010Silver.Color="#FF80838F"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=ScaleBackgroundBrush}"
                     Color="#FF646464"
                     cs:Office2010Blue.Color="#FF94ADCC"
                     cs:Office2010Silver.Color="#FFA5ACB5"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=ScaleInnerBackgroundBrush}"
                     Color="#FF838383"
                     Opacity="0.62"
                     cs:Office2010Blue.Color="#FFCFDDEE"
                     cs:Office2010Silver.Color="#FFE9EDF1"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=SpindleCapBackgroundBrush}"
                     Color="#FFFFFFFF"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF484E5A"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=SpindleCapForegroundBrush}"
                     Color="#FF838383"
                     cs:Office2010Blue.Color="#FFB8CBE2"
                     cs:Office2010Silver.Color="#FFE9EDF1"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=MarkerBackgroundBrush}"
                     Color="#FF838383"
                     cs:Office2010Blue.Color="#FFCFDDEE"
                     cs:Office2010Silver.Color="#FFE9EDF1"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=MarkerForegroundBrush}"
                     Color="#FFFFFFFF"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF484E5A"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=RangeGreenBrush}"
                     Color="#FF5B8C76"
                     cs:Office2010Blue.Color="#FF6C9D87"
                     cs:Office2010Silver.Color="#FF6C9D87"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=RangeYellowBrush}"
                     Color="#FFA49575"
                     cs:Office2010Blue.Color="#FFB3A383"
                     cs:Office2010Silver.Color="#FFB3A383"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=RangeRedBrush}"
                     Color="#FFA76865"
                     cs:Office2010Blue.Color="#FFBA7975"
                     cs:Office2010Silver.Color="#FFBA7975"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=GaugeBackgroundBrush}" Color="Transparent"/>

</ResourceDictionary>