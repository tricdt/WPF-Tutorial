<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Core.ThemedBorder}" TargetType="{x:Type dx:ThemedBorder}">
    <Setter Property="Background" Value="{DynamicResource {dxt:ThemedBorderThemeKey ResourceKey=BackgroundBrush}}" />
    <Setter Property="BorderBrush" Value="{DynamicResource {dxt:ThemedBorderThemeKey ResourceKey=BorderBrush}}" />
  </Style>
</ResourceDictionary>