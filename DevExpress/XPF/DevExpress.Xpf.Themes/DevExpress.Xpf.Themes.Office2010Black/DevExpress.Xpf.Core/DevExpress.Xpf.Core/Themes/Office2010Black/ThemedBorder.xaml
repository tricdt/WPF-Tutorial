<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="{dxt:ThemedBorderThemeKey ResourceKey=BackgroundBrush}"
                     Color="#FF838383"
                     cs:Office2010Blue.Color="#FFCFDDEE"
                     cs:Office2010Silver.Color="#FFE9EDF1"/>

    <SolidColorBrush x:Key="{dxt:ThemedBorderThemeKey ResourceKey=BorderBrush}"
                     Color="#FF4E4E4E"
                     cs:Office2010Blue.Color="#FF8BA0BB"
                     cs:Office2010Silver.Color="#FF8B9097"/>

</ResourceDictionary>