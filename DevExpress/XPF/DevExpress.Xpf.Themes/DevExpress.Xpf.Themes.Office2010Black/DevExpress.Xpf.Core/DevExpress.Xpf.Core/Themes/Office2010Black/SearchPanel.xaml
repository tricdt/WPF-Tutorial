<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxci="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:themes="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../StandardControlStyles.XAML"/>
    </ResourceDictionary.MergedDictionaries>

    <!--END WPF-->

    <SolidColorBrush x:Key="ThemeColorsThemeKey_WhiteForegroundColor"
                     Color="#FFFFFFFF"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="ThemeColorsThemeKey_BlackForegroundColor"
                     Color="#FF000000"
                     cs:Name="SolidColorBrush_0002"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

    <!--#include \..\..\..\..\Themes.SL\StandardControls\Brushes.xaml-->

    <!--#include \..\..\..\..\Themes.SL\StandardControls\SharedResources.xaml-->

    <!--#include \..\..\..\..\Themes.SL\StandardControls\Button.xaml-->

    <!--SearchPanelBorderBrush-->

    <SolidColorBrush x:Key="{themes:SearchPanelThemeKey ResourceKey=SearchPanelBorderBrush}"
                     Color="#FF484848"
                     cs:Name="SolidColorBrush_0003"
                     cs:Office2010Blue.Color="#FF94ADCC"
                     cs:Office2010Silver.Color="#FFA5ACB5"/>

    <!--SearchPanelBackgroundBrush-->

    <SolidColorBrush x:Key="{themes:SearchPanelThemeKey ResourceKey=SearchPanelBackgroundBrush}"
                     Color="#FF838383"
                     cs:Name="SolidColorBrush_0004"
                     cs:Office2010Blue.Color="#FFCFDDEE"
                     cs:Office2010Silver.Color="#FFE9EDF1"/>

    <!--SearchPanelTextBrush-->

    <SolidColorBrush x:Key="{themes:SearchPanelThemeKey ResourceKey=SearchPanelTextBrush}"
                     Color="#FFFFFFFF"
                     cs:Name="SolidColorBrush_0005"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

    <!--SearchPanelGlyphBrush-->

    <SolidColorBrush x:Key="{themes:SearchPanelThemeKey ResourceKey=SearchPanelGlyphBrush}"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0006"
                     cs:Office2010Blue.Color="#FF515D7A"
                     cs:Office2010Silver.Color="#FF80838F"/>

    <!--ButtonControlTemplate-->

    <ControlTemplate x:Key="{themes:SearchPanelThemeKey ResourceKey=ButtonControlTemplate}" TargetType="{x:Type Button}">
        <Grid SnapsToDevicePixels="True" UseLayoutRounding="True">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="00:00:00.1" To="MouseOver"/>
                        <VisualTransition GeneratedDuration="00:00:00.1" To="Pressed"/>
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0:0:0.01"
                                            Storyboard.TargetName="gs1"
                                            Storyboard.TargetProperty="Color"
                                            To="#FFCECECE"
                                            cs:Office2010Blue.To="#FFD5DEE8"
                                            cs:Office2010Silver.To="#FFCED4DB"
                                           />
                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                            Duration="0:0:0.01"
                                            Storyboard.TargetName="gs2"
                                            Storyboard.TargetProperty="Color"
                                            To="#FFB3B3B3"
                                            cs:Office2010Blue.To="#FFE1EDFA"
                                            cs:Office2010Silver.To="#FFF3F4F5"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0003"
                                            Duration="0:0:0.01"
                                            Storyboard.TargetName="gs1"
                                            Storyboard.TargetProperty="Color"
                                            To="#FF939393"
                                            cs:Office2010Blue.To="#FFD5DEE8"
                                            cs:Office2010Silver.To="#FFCED4DB"
                                           />
                            <ColorAnimation cs:Name="ColorAnimation_0004"
                                            Duration="0:0:0.01"
                                            Storyboard.TargetName="gs2"
                                            Storyboard.TargetProperty="Color"
                                            To="#FF939393"
                                            cs:Office2010Blue.To="#FFD5DEE8"
                                            cs:Office2010Silver.To="#FFCED4DB"
                                           />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border CornerRadius="8,8,8,8"
                    SnapsToDevicePixels="True"
                    UseLayoutRounding="True">
                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0041"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop x:Name="gs1"
                                      Color="#FFEDEDED"
                                      cs:Name="GradientStop_0001"
                                      cs:Office2010Blue.Color="#FFEDEDED"
                                      cs:Office2010Silver.Color="#FFEDEDED"
                                     />
                        <GradientStop x:Name="gs2"
                                      Color="#FFE1E1E1"
                                      cs:Name="GradientStop_0002"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#FFFFFFFF"
                                      cs:Office2010Silver.Color="#FFFFFFFF"
                                     />
                    </LinearGradientBrush>
                </Border.Background>
                <ContentPresenter x:Name="contentPresenter"
                                  Content="{TemplateBinding Content}"
                                  ContentTemplate="{TemplateBinding ContentTemplate}"
                                  HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                  Margin="{TemplateBinding Padding}"
                                  SnapsToDevicePixels="True"
                                  UseLayoutRounding="True"
                                  VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
            </Border>
        </Grid>
    </ControlTemplate>

    <!--ButtonBorderBrush-->

    <LinearGradientBrush x:Key="{themes:SearchPanelThemeKey ResourceKey=ButtonBorderBrush}"
                         cs:Name="LinearGradientBrush_0042"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFA3AEB9"
                      cs:Name="GradientStop_0003"
                      Offset="0"
                      cs:Office2010Blue.Color="#FFA3AEB9"
                      cs:Office2010Silver.Color="#FFA3AEB9"
                     />
        <GradientStop Color="#FF8399A9"
                      cs:Name="GradientStop_0004"
                      Offset="0.375"
                      cs:Office2010Blue.Color="#FF8399A9"
                      cs:Office2010Silver.Color="#FF8399A9"
                     />
        <GradientStop Color="#FF718597"
                      cs:Name="GradientStop_0005"
                      Offset="0.375"
                      cs:Office2010Blue.Color="#FF718597"
                      cs:Office2010Silver.Color="#FF718597"
                     />
        <GradientStop Color="#FF617584"
                      cs:Name="GradientStop_0006"
                      Offset="1"
                      cs:Office2010Blue.Color="#FF617584"
                      cs:Office2010Silver.Color="#FF617584"
                     />
    </LinearGradientBrush>

    <!--OptionsButtonStyle-->

    <!--BEGIN WPF-->

    <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=OptionsButtonStyle}"
           BasedOn="{StaticResource {themes:DefaultStyleThemeKey Type=Button}}"
           TargetType="{x:Type Button}">
        <!--END WPF-->
        <!--ONLY SL
        <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=OptionsButtonStyle}" TargetType="{x:Type Button}" BasedOn="{StaticResource ButtonThemeKey_ButtonControlStyle}">
        ONLY SL-->
        <Setter Property="Width" Value="21"/>
        <Setter Property="Height" Value="21"/>
        <Setter Property="Cursor" Value="Hand"/>
    </Style>

    <!--ReplaceButtonStyle-->

    <!--BEGIN WPF-->

    <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=ReplaceButtonStyle}"
           BasedOn="{StaticResource {themes:DefaultStyleThemeKey Type=Button}}"
           TargetType="{x:Type Button}">
        <!--END WPF-->
        <!--ONLY SL
        <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=ReplaceButtonStyle}" TargetType="{x:Type Button}" BasedOn="{StaticResource ButtonThemeKey_ButtonControlStyle}">
        ONLY SL-->
        <Setter Property="Margin" Value="0,0,4,0"/>
        <Setter Property="Height" Value="21"/>
    </Style>

    <!--ReplaceAllButtonStyle-->

    <!--BEGIN WPF-->

    <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=ReplaceAllButtonStyle}"
           BasedOn="{StaticResource {themes:DefaultStyleThemeKey Type=Button}}"
           TargetType="{x:Type Button}">
        <!--END WPF-->
        <!--ONLY SL
        <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=ReplaceAllButtonStyle}" TargetType="{x:Type Button}" BasedOn="{StaticResource ButtonThemeKey_ButtonControlStyle}">
        ONLY SL-->
        <Setter Property="Height" Value="21"/>
        <!--Changed-->
        <Setter Property="Margin" Value="0,0,8,0"/>
    </Style>

    <!--FindPrevButtonStyle-->

    <!--BEGIN WPF-->

    <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=FindPrevButtonStyle}"
           BasedOn="{StaticResource {themes:DefaultStyleThemeKey Type=Button}}"
           TargetType="{x:Type Button}">
        <!--END WPF-->
        <!--ONLY SL
        <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=FindPrevButtonStyle}" TargetType="{x:Type Button}" BasedOn="{StaticResource ButtonThemeKey_ButtonControlStyle}">
        ONLY SL-->
        <Setter Property="Margin" Value="0,0,4,0"/>
        <Setter Property="Height" Value="21"/>
        <!--Changed-->
        <Setter Property="Width" Value="21"/>
    </Style>

    <!--FindNextButtonStyle-->

    <!--BEGIN WPF-->

    <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=FindNextButtonStyle}"
           BasedOn="{StaticResource {themes:DefaultStyleThemeKey Type=Button}}"
           TargetType="{x:Type Button}">
        <!--END WPF-->
        <!--ONLY SL
        <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=FindNextButtonStyle}" TargetType="{x:Type Button}" BasedOn="{StaticResource ButtonThemeKey_ButtonControlStyle}">
        ONLY SL-->
        <Setter Property="Height" Value="21"/>
        <!--Changed-->
        <Setter Property="Margin" Value="0"/>
        <Setter Property="Width" Value="21"/>
    </Style>

    <!--CloseButtonStyle-->

    <!--<Style x:Key="{themes:SearchPanelThemeKey ResourceKey=CloseButtonStyle}" TargetType="{x:Type Button}" BasedOn="{StaticResource {themes:DefaultStyleThemeKey Type=Button}}">-->

    <!--BEGIN WPF-->

    <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=CloseButtonStyle}"
           BasedOn="{StaticResource {themes:DefaultStyleThemeKey Type=Button}}"
           TargetType="{x:Type Button}">
        <!--END WPF-->
        <!--ONLY SL
        <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=CloseButtonStyle}" TargetType="{x:Type Button}" BasedOn="{StaticResource ButtonThemeKey_ButtonControlStyle}">
        ONLY SL-->
        <Setter Property="Height" Value="21"/>
        <!--Changed-->
        <Setter Property="Width" Value="21"/>
    </Style>

    <!--ButtonTextBlockStyle-->

    <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=ButtonTextBlockStyle}" TargetType="{x:Type TextBlock}">
        <Setter Property="Width" Value="Auto"/>
        <Setter Property="Height" Value="Auto"/>
        <Setter Property="Margin" Value="10,0,10,1"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

    <!--PanelTextBlockStyle-->

    <Style x:Key="{themes:SearchPanelThemeKey ResourceKey=PanelTextBlockStyle}" TargetType="{x:Type TextBlock}">
        <Setter Property="Width" Value="Auto"/>
        <Setter Property="Height" Value="Auto"/>
        <Setter Property="Foreground" Value="{DynamicResource {themes:SearchPanelThemeKey ResourceKey=SearchPanelTextBrush}}"/>
        <Setter Property="Margin" Value="0,0,4,0"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
    </Style>

    <!--FindPrevButtonShape-->

    <DataTemplate x:Key="{themes:SearchPanelThemeKey ResourceKey=FindPrevButtonShape}">
        <Path Data="F1 M 290.391,235.281L 283.391,231.281L 283.391,230.281L 290.391,226.281L 290.391,235.281 Z"
              Fill="{DynamicResource {themes:SearchPanelThemeKey ResourceKey=SearchPanelGlyphBrush}}"
              Height="9"
              Stretch="Fill"
              Width="7"/>
    </DataTemplate>

    <!--FindNextButtonShape-->

    <DataTemplate x:Key="{themes:SearchPanelThemeKey ResourceKey=FindNextButtonShape}">
        <Path Data="F1 M 294.594,226.313L 301.594,230.313L 301.594,231.313L 294.594,235.313L 294.594,226.313 Z"
              Fill="{DynamicResource {themes:SearchPanelThemeKey ResourceKey=SearchPanelGlyphBrush}}"
              Height="9"
              Margin="2,0,0,0"
              Stretch="Fill"
              Width="7"/>
    </DataTemplate>

    <!--CloseButtonShape-->

    <DataTemplate x:Key="{themes:SearchPanelThemeKey ResourceKey=CloseButtonShape}">
        <Path Data="F1 M 285,223L 288,226L 288.997,226.003L 292,223L 294,225L 290.997,228.003L 291,229L 294,232L 292,234L 289,231L 288,231L 285,234L 283,232L 286,229L 286,228L 283,225L 285,223 Z"
              Fill="{DynamicResource {themes:SearchPanelThemeKey ResourceKey=SearchPanelGlyphBrush}}"
              Height="11"
              Stretch="Fill"
              Width="11"/>
    </DataTemplate>

    <!--OptionsButtonShape-->

    <DataTemplate x:Key="{themes:SearchPanelThemeKey ResourceKey=OptionsButtonShape}">
        <Path Data="F1 M 304,230L 301,235L 300,235L 297,230L 304,230 Z"
              Fill="{DynamicResource {themes:SearchPanelThemeKey ResourceKey=SearchPanelGlyphBrush}}"
              Height="5"
              Stretch="Fill"
              Width="7"/>
    </DataTemplate>

    <!--SeparatorControlTemplate-->

    <ControlTemplate x:Key="{themes:SearchPanelThemeKey ResourceKey=SeparatorControlTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FF484848"
                BorderBrush="#3FFFFFFF"
                BorderThickness="1"
                cs:Name="Border_0002"
                Margin="2"
                Width="3"
                cs:Office2010Blue.Background="#FF94ADCC"
                cs:Office2010Blue.BorderBrush="#3FFFFFFF"
                cs:Office2010Silver.Background="#FFA5ACB5"
                cs:Office2010Silver.BorderBrush="#3FFFFFFF"
               >
            <Border.OpacityMask>
                <LinearGradientBrush cs:Name="LinearGradientBrush_0043"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0"
                                    >
                    <GradientStop Color="#00000000"
                                  cs:Name="GradientStop_0007"
                                  Offset="0"
                                  cs:Office2010Blue.Color="#00000000"
                                  cs:Office2010Silver.Color="#00000000"
                                 />
                    <GradientStop Color="#00000000"
                                  cs:Name="GradientStop_0008"
                                  Offset="1"
                                  cs:Office2010Blue.Color="#00000000"
                                  cs:Office2010Silver.Color="#00000000"
                                 />
                    <GradientStop Color="#FF000000"
                                  cs:Name="GradientStop_0009"
                                  Offset="0.5"
                                  cs:Office2010Blue.Color="#FF000000"
                                  cs:Office2010Silver.Color="#FF000000"
                                 />
                </LinearGradientBrush>
            </Border.OpacityMask>
        </Border>
    </ControlTemplate>

    <!--BorderControlTemplate-->

    <ControlTemplate x:Key="{themes:SearchPanelThemeKey ResourceKey=BorderControlTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FF838383"
                BorderBrush="#FF484848"
                BorderThickness="0,0,0,1"
                cs:Name="Border_0004"
                Padding="8,3,8,3"
                cs:Office2010Blue.Background="#FFCFDDEE"
                cs:Office2010Blue.BorderBrush="#FF94ADCC"
                cs:Office2010Silver.Background="#FFE9EDF1"
                cs:Office2010Silver.BorderBrush="#FFA5ACB5"
               >
            <ContentPresenter Content="{TemplateBinding Content}"
                              ContentStringFormat="{TemplateBinding ContentStringFormat}"
                              ContentTemplate="{TemplateBinding ContentTemplate}"/>
        </Border>
    </ControlTemplate>

    <!--#include ..\Generic\SearchPanel.xaml-->

</ResourceDictionary>