    <ControlTemplate x:Key="{dxbt:BarControlThemeKey ResourceKey=QuickCustomizationButtonTemplate}" TargetType="{x:Type dxb:BarQuickCustomizationButton}">
        <Grid>
            <Border x:Name="DefaultState"
                    BorderThickness="1,0,0,0"
                    Opacity="1">
                <Border.BorderBrush>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0057"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="#FF838383"
                                      cs:Name="GradientStop_0017"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#FFD8E4F2"
                                      cs:Office2010Silver.Color="#FFE9EDF1"
                                     />
                        <GradientStop Color="#FF737373"
                                      cs:Name="GradientStop_0018"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#FF94ADCC"
                                      cs:Office2010Silver.Color="#FFC0C4C9"
                                     />
                    </LinearGradientBrush>
                </Border.BorderBrush>
                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0058"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="#FFB9B9B9"
                                      cs:Name="GradientStop_0019"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#FFEDF4FD"
                                      cs:Office2010Silver.Color="#FFFFFFFF"
                                     />
                        <GradientStop Color="#FF9D9D9D"
                                      cs:Name="GradientStop_0020"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#FFD8E4F2"
                                      cs:Office2010Silver.Color="#FFE9EDF1"
                                     />
                    </LinearGradientBrush>
                </Border.Background>
                <Border BorderBrush="#26FFFFFF"
                        BorderThickness="1,0,1,0"
                        cs:Name="Border_0002"
                        cs:Office2010Blue.BorderBrush="#CFFFFFFF"
                        cs:Office2010Silver.BorderBrush="#BFFFFFFF"
                       />
            </Border>
            <Border x:Name="HoverState"
                    Margin="1,0,0,0"
                    Opacity="0">
                <Border.BorderBrush>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0059"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="#BFD7A820"
                                      cs:Name="GradientStop_0021"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#FFECC757"
                                      cs:Office2010Silver.Color="#FFECC757"
                                     />
                        <GradientStop Color="#BFB89407"
                                      cs:Name="GradientStop_0022"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#FFF1D449"
                                      cs:Office2010Silver.Color="#FFF1D449"
                                     />
                    </LinearGradientBrush>
                </Border.BorderBrush>
                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0060"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="#FFFFF6A9"
                                      cs:Name="GradientStop_0023"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#FFF8EBB2"
                                      cs:Office2010Silver.Color="#FFF8EBB2"
                                     />
                        <GradientStop Color="#FFFFF27F"
                                      cs:Name="GradientStop_0024"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#FFF8EBB2"
                                      cs:Office2010Silver.Color="#FFF8EBB2"
                                     />
                        <GradientStop Color="#FFFFDA6F"
                                      cs:Name="GradientStop_0025"
                                      Offset="0.5"
                                      cs:Office2010Blue.Color="#FFFFE575"
                                      cs:Office2010Silver.Color="#FFFFE575"
                                     />
                    </LinearGradientBrush>
                </Border.Background>
                <Border BorderBrush="#3FFFFFFF"
                        BorderThickness="1,0,1,0"
                        cs:Name="Border_0003"
                        cs:Office2010Blue.BorderBrush="#3FFFFFFF"
                        cs:Office2010Silver.BorderBrush="#3FFFFFFF"
                       />
            </Border>
            <Grid x:Name="Glyph"
                  Height="5"
                  Margin="3,1,3,0"
                  SnapsToDevicePixels="True"
                  Width="8">
                <Path x:Name="Path1"
                      cs:Name="Path_0003"
                      Data="F1 M 54,23L 54,24L 58,28L 62,24L 62,23L 54,23 Z"
                      Fill="#7FFFFFFF"
                      Stretch="Fill"
                      cs:Office2010Blue.Fill="#7FFFFFFF"
                      cs:Office2010Silver.Fill="#7FFFFFFF"
                     />
                <Path x:Name="Path2"
                      cs:Name="Path_0004"
                      Data="F1 M 54,23L 62,23L 58,27L 54,23 Z"
                      Fill="#FF6A6A6A"
                      Margin="0,0,0,1"
                      Stretch="Fill"
                      cs:Office2010Blue.Fill="#FF515D7A"
                      cs:Office2010Silver.Fill="#FF80838F"
                     />
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="Path2"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF82630C"
                                            cs:Office2010Blue.To="#FF82630C"
                                            cs:Office2010Silver.To="#FF82630C"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                            Duration="0"
                                            Storyboard.TargetName="Path2"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF82630C"
                                            cs:Office2010Blue.To="#FF82630C"
                                            cs:Office2010Silver.To="#FF82630C"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0003"
                                            Duration="0"
                                            Storyboard.TargetName="Path2"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF82630C"
                                            cs:Office2010Blue.To="#FF82630C"
                                            cs:Office2010Silver.To="#FF82630C"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DefaultState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.35"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Glyph"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.35"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Path1"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="OrientationStates">
                    <VisualState x:Name="Horizontal"/>
                    <VisualState x:Name="Vertical">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="2,3,2,0"
                                                           Storyboard.TargetName="Glyph"
                                                           Storyboard.TargetProperty="Margin"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.VerticalAlignment="Top"
                                                           Storyboard.TargetName="Glyph"
                                                           Storyboard.TargetProperty="VerticalAlignment"/>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Glyph" Storyboard.TargetProperty="RenderTransform">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <RotateTransform Angle="180"
                                                         CenterX="3.5"
                                                         CenterY="3.5"/>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=ShowBackground, RelativeSource={RelativeSource TemplatedParent}}" Value="False">
                <Setter Property="Visibility"
                        TargetName="DefaultState"
                        Value="Collapsed"/>
                <Setter Property="Visibility"
                        TargetName="HoverState"
                        Value="Collapsed"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxbt:BarControlThemeKey ResourceKey=StatusBarContentTemplate}" TargetType="{x:Type ContentControl}">
        <ContentPresenter Content="{TemplateBinding Content}"
