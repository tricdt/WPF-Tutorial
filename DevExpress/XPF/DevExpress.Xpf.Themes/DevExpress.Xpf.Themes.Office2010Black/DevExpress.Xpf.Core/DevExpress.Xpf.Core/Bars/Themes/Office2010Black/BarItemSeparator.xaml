    <ControlTemplate x:Key="{dxbt:BarItemSeparatorThemeKey ResourceKey=InMenuTemplate}" TargetType="{x:Type dxb:BarItemLinkSeparatorControl}">
        <Grid SnapsToDevicePixels="True">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Border Width="{TemplateBinding InMenuContentOffset}"/>
            <Rectangle cs:Name="Rectangle_0001"
                       Grid.Column="1"
                       Height="1"
                       Margin="7,0,2,1"
                       SnapsToDevicePixels="True"
                       Stroke="#FFC7C8C9"
                       StrokeDashArray="1 3"
                       VerticalAlignment="Center"
                       cs:Office2010Blue.Stroke="#FF94ADCC"
                       cs:Office2010Silver.Stroke="#FFA5ACB5"
                      />
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxbt:BarItemSeparatorThemeKey ResourceKey=Template}" TargetType="{x:Type dxb:BarItemLinkSeparatorControl}">
        <dxc:PixelSnapper>
            <Border Background="#FF7F7F7F"
                    BorderBrush="#26FFFFFF"
                    BorderThickness="1"
                    cs:Name="Border_0002"
                    Margin="0,1,1,1"
                    Width="3"
                    cs:Office2010Blue.Background="#FF94ADCC"
                    cs:Office2010Blue.BorderBrush="#CFFFFFFF"
                    cs:Office2010Silver.Background="#FFA5ACB5"
                    cs:Office2010Silver.BorderBrush="#BFFFFFFF"
                   >
                <Border.OpacityMask>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0089"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="#00000000"
                                      cs:Name="GradientStop_0001"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#00000000"
                                      cs:Office2010Silver.Color="#00000000"
                                     />
                        <GradientStop Color="#00000000"
                                      cs:Name="GradientStop_0002"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#00000000"
                                      cs:Office2010Silver.Color="#00000000"
                                     />
                        <GradientStop Color="#FF000000"
                                      cs:Name="GradientStop_0003"
                                      Offset="0.5"
                                      cs:Office2010Blue.Color="#FF1E395B"
                                      cs:Office2010Silver.Color="#FF3C3C3C"
                                     />
                    </LinearGradientBrush>
                </Border.OpacityMask>
            </Border>
        </dxc:PixelSnapper>
    </ControlTemplate>

