<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Thickness x:Key="{dxbt:BarCheckItemThemeKey ResourceKey=BorderMarginInMenu}">-1,1,-1,1</Thickness>

    <ControlTemplate x:Key="{dxbt:BarCheckItemThemeKey ResourceKey=CheckInMenu}" TargetType="{x:Type ContentControl}">
        <Border x:Name="Root"
                Background="#FFF29536"
                CornerRadius="3"
                cs:Name="Border_0001"
                MinHeight="18"
                MinWidth="18"
                cs:Office2010Blue.Background="#FFEB8900"
                cs:Office2010Silver.Background="#FFEB8900"
               >
            <Border Background="#FFFCF1C2"
                    CornerRadius="2"
                    cs:Name="Border_0002"
                    Margin="1"
                    cs:Office2010Blue.Background="#FFFCF1C2"
                    cs:Office2010Silver.Background="#FFFCF1C2"
                   >
                <Path cs:Name="Path_0001"
                      Data="F1 M 399,86L 403,93L 409,81L 403,90L 399,86 Z"
                      Fill="#FF454545"
                      Height="12"
                      Stretch="Fill"
                      UseLayoutRounding="True"
                      Width="10"
                      cs:Office2010Blue.Fill="#FF515D7A"
                      cs:Office2010Silver.Fill="#FF80838F"
                     />
            </Border>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="FlowDirection" Value="RightToLeft">
                <Setter Property="RenderTransform" TargetName="Root">
                    <Setter.Value>
                        <ScaleTransform ScaleX="-1" ScaleY="1"/>
                    </Setter.Value>
                </Setter>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxbt:BarCheckItemThemeKey ResourceKey=GlyphCheckBorderInMenu}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFF29536"
                CornerRadius="3"
                cs:Name="Border_0003"
                MinHeight="18"
                MinWidth="18"
                cs:Office2010Blue.Background="#FFEB8900"
                cs:Office2010Silver.Background="#FFEB8900"
               >
            <Border Background="#FFFCF1C2"
                    CornerRadius="2"
                    cs:Name="Border_0004"
                    Margin="1"
                    cs:Office2010Blue.Background="#FFFCF1C2"
                    cs:Office2010Silver.Background="#FFFCF1C2"
                   >
                <ContentPresenter/>
            </Border>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxbt:BarCheckItemThemeKey ResourceKey=GlyphTriStateBorderInMenu}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFF29536"
                CornerRadius="3"
                cs:Name="Border_0005"
                MinHeight="18"
                MinWidth="18"
                cs:Office2010Blue.Background="#FFEB8900"
                cs:Office2010Silver.Background="#FFEB8900"
               >
            <Border Background="#FFFCF1C2"
                    CornerRadius="2"
                    cs:Name="Border_0006"
                    Margin="1"
                    cs:Office2010Blue.Background="#FFFCF1C2"
                    cs:Office2010Silver.Background="#FFFCF1C2"
                   >
                <ContentPresenter/>
            </Border>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxbt:BarCheckItemThemeKey ResourceKey=TriStateBorderInMenu}" TargetType="{x:Type ContentControl}">
        <Border x:Name="Root"
                Background="#FFF29536"
                CornerRadius="3"
                cs:Name="Border_0007"
                MinHeight="18"
                MinWidth="18"
                cs:Office2010Blue.Background="#FFEB8900"
                cs:Office2010Silver.Background="#FFEB8900"
               >
            <Border Background="#FFD6E4F8"
                    CornerRadius="2"
                    cs:Name="Border_0008"
                    Margin="1"
                    cs:Office2010Blue.Background="#FFD6E4F8"
                    cs:Office2010Silver.Background="#FFD6E4F8"
                   >
                <Path cs:Name="Path_0002"
                      Data="F1 M 399,86L 403,93L 409,81L 403,90L 399,86 Z"
                      Fill="#FF454545"
                      Height="12"
                      Stretch="Fill"
                      UseLayoutRounding="True"
                      Width="10"
                      cs:Office2010Blue.Fill="#FF515D7A"
                      cs:Office2010Silver.Fill="#FF80838F"
                     />
            </Border>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="FlowDirection" Value="RightToLeft">
                <Setter Property="RenderTransform" TargetName="Root">
                    <Setter.Value>
                        <ScaleTransform ScaleX="-1" ScaleY="1"/>
                    </Setter.Value>
                </Setter>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="CheckTriStateTemplate">
        <Grid x:Name="Root" RenderTransformOrigin="0.5,0.5">
            <Border Background="#FFF29536"
                    CornerRadius="3"
                    cs:Name="Border_0009"
                    MinHeight="18"
                    MinWidth="18"
                    Visibility="{Binding Path=(dxb:BarCheckItemLinkControlStatesProvider.CheckVisibility), RelativeSource={RelativeSource TemplatedParent}}"
                    cs:Office2010Blue.Background="#FFF29536"
                    cs:Office2010Silver.Background="#FFF29536"
                   >
                <Border Background="#FFFCF1C2"
                        CornerRadius="2"
                        cs:Name="Border_0010"
                        Margin="1"
                        cs:Office2010Blue.Background="#FFFCF1C2"
                        cs:Office2010Silver.Background="#FFFCF1C2"
                       >
                    <Path cs:Name="Path_0003"
                          Data="F1 M 399,86L 403,93L 409,81L 403,90L 399,86 Z"
                          Fill="#FF454545"
                          Height="12"
                          Stretch="Fill"
                          UseLayoutRounding="True"
                          Width="10"
                          cs:Office2010Blue.Fill="#FF515D7A"
                          cs:Office2010Silver.Fill="#FF454545"
                         />
                </Border>
            </Border>
            <Border Background="#FFF29536"
                    CornerRadius="3"
                    cs:Name="Border_0011"
                    MinHeight="18"
                    MinWidth="18"
                    Visibility="{Binding Path=(dxb:BarCheckItemLinkControlStatesProvider.GlyphCheckVisibility), RelativeSource={RelativeSource TemplatedParent}}"
                    cs:Office2010Blue.Background="#FFF29536"
                    cs:Office2010Silver.Background="#FFF29536"
                   >
                <Border Background="#FFFCF1C2"
                        CornerRadius="2"
                        cs:Name="Border_0012"
                        Margin="1"
                        cs:Office2010Blue.Background="#FFFCF1C2"
                        cs:Office2010Silver.Background="#FFFCF1C2"
                       >
                    <ContentPresenter/>
                </Border>
            </Border>
            <Border Background="#FFF29536"
                    CornerRadius="3"
                    cs:Name="Border_0013"
                    MinHeight="18"
                    MinWidth="18"
                    Opacity="0.5"
                    Visibility="{Binding Path=(dxb:BarCheckItemLinkControlStatesProvider.GlyphTriStateVisibility), RelativeSource={RelativeSource TemplatedParent}}"
                    cs:Office2010Blue.Background="#FFF29536"
                    cs:Office2010Silver.Background="#FFF29536"
                   >
                <Border Background="#FFFCF1C2"
                        CornerRadius="2"
                        cs:Name="Border_0014"
                        Margin="1"
                        cs:Office2010Blue.Background="#FFFCF1C2"
                        cs:Office2010Silver.Background="#FFFCF1C2"
                       >
                    <ContentPresenter/>
                </Border>
            </Border>
            <Border Background="#FFF29536"
                    CornerRadius="3"
                    cs:Name="Border_0015"
                    MinHeight="18"
                    MinWidth="18"
                    Opacity="0.5"
                    Visibility="{Binding Path=(dxb:BarCheckItemLinkControlStatesProvider.TriStateVisibility), RelativeSource={RelativeSource TemplatedParent}}"
                    cs:Office2010Blue.Background="#FFF29536"
                    cs:Office2010Silver.Background="#FFF29536"
                   >
                <Border Background="#FFFCF1C2"
                        CornerRadius="2"
                        cs:Name="Border_0016"
                        Margin="1"
                        cs:Office2010Blue.Background="#FFFCF1C2"
                        cs:Office2010Silver.Background="#FFFCF1C2"
                       >
                    <Path cs:Name="Path_0004"
                          Data="F1 M 399,86L 403,93L 409,81L 403,90L 399,86 Z"
                          Fill="#FF454545"
                          Height="12"
                          Stretch="Fill"
                          UseLayoutRounding="True"
                          Width="10"
                          cs:Office2010Blue.Fill="#FF515D7A"
                          cs:Office2010Silver.Fill="#FF454545"
                         />
                </Border>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="FlowDirection" Value="RightToLeft">
                <Setter Property="RenderTransform" TargetName="Root">
                    <Setter.Value>
                        <ScaleTransform ScaleX="-1" ScaleY="1"/>
                    </Setter.Value>
                </Setter>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="{dxbt:BarCheckItemThemeKey ResourceKey=TextSplitterStyleInRibbonPageGroup}" TargetType="{x:Type dxb:TextSplitterControl}">
        <Setter Property="FirstStringHorizontalAlignment" Value="Center"/>
        <Setter Property="SecondStringHorizontalAlignment" Value="Center"/>
        <Setter Property="SecondStringMargin" Value="0,-4,0,0"/>
        <Setter Property="Margin" Value="0,-4,0,0"/>
    </Style>

    <Style x:Key="{dxbt:BarCheckItemThemeKey ResourceKey=SelectedTextStyleInTextSplitter}" TargetType="{x:Type ContentControl}">
        <Setter cs:Name="Setter_0001"
                Property="Foreground"
                Value="#FF3C3C3C"
                cs:Office2010Blue.Value="#FF1E395B"
                cs:Office2010Silver.Value="#FF3C3C3C"
               />
    </Style>

    <Style x:Key="{dxbt:BarCheckItemThemeKey ResourceKey=NormalTextStyleInTextSplitter}" TargetType="{x:Type ContentControl}">
        <Setter cs:Name="Setter_0002"
                Property="Foreground"
                Value="#FF3C3C3C"
                cs:Office2010Blue.Value="#FF1E395B"
                cs:Office2010Silver.Value="#FF3C3C3C"
               />
    </Style>

</ResourceDictionary>