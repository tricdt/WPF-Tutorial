<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2010Black.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/TextEditBase.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!--END WPF-->

    <SolidColorBrush x:Key="{dxet:TextEditThemeKey ResourceKey=CaretBrush}"
                     Color="#FF000000"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2010Blue.Color="#FF000000"
                     cs:Office2010Silver.Color="#FF000000"/>

    <SolidColorBrush x:Key="TextEditSelectionBackground"
                     Color="#FFF8E695"
                     cs:Name="SolidColorBrush_0002"
                     cs:Office2010Blue.Color="#FFF8E695"
                     cs:Office2010Silver.Color="#FFF8E695"/>

    <SolidColorBrush x:Key="TextEditSelectionForeground"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0003"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

</ResourceDictionary>