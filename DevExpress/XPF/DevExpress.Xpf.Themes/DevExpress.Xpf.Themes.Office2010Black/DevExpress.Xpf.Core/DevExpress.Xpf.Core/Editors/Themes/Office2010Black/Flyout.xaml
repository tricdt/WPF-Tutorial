<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2010Black.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/Flyout.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!--END WPF-->

    <Thickness x:Key="{dxet:FlyoutControlThemeKey ResourceKey=BorderThickness}">1</Thickness>

    <Thickness x:Key="{dxet:FlyoutControlThemeKey ResourceKey=Padding}">12</Thickness>

    <SolidColorBrush x:Key="{dxet:FlyoutControlThemeKey ResourceKey=Background}"
                     Color="#FFB3B3B3"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2010Blue.Color="#FFD1E0EF"
                     cs:Office2010Silver.Color="#FFF3F4F5">
    </SolidColorBrush>

    <SolidColorBrush x:Key="{dxet:FlyoutControlThemeKey ResourceKey=BorderBrush}"
                     Color="#FF646464"
                     cs:Name="SolidColorBrush_0002"
                     cs:Office2010Blue.Color="#FF94ADCC"
                     cs:Office2010Silver.Color="#FFA5ACB5">
    </SolidColorBrush>

    <SolidColorBrush x:Key="{dxet:FlyoutControlThemeKey ResourceKey=Foreground}"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0003"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C">
    </SolidColorBrush>

</ResourceDictionary>