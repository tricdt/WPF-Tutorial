    </Style>

    <ControlTemplate x:Key="{dxet:ProgressBarEditStyleThemeKey ResourceKey=ProgressBarMarqueeHorizontalAnimationElementTemplate}">
        <Grid>
            <Grid.OpacityMask>
                <LinearGradientBrush cs:Name="LinearGradientBrush_1610"
                                     EndPoint="0,0.5"
                                     StartPoint="1,0.5"
                                    >
                    <GradientStop Color="#00FFFFFF"
                                  cs:Name="GradientStop_0015"
                                  Offset="0"
                                  cs:Office2010Blue.Color="#00FFFFFF"
                                  cs:Office2010Silver.Color="#00FFFFFF"
                                 />
                    <GradientStop Color="#00FFFFFF"
                                  cs:Name="GradientStop_0016"
                                  Offset="1"
                                  cs:Office2010Blue.Color="#00FFFFFF"
                                  cs:Office2010Silver.Color="#00FFFFFF"
                                 />
                    <GradientStop Color="#7FFFFFFF"
                                  cs:Name="GradientStop_0017"
                                  Offset="0.15"
                                  cs:Office2010Blue.Color="#7FFFFFFF"
                                  cs:Office2010Silver.Color="#7FFFFFFF"
                                 />
                    <GradientStop Color="#FFFFFFFF"
                                  cs:Name="GradientStop_0018"
                                  Offset="0.5"
                                  cs:Office2010Blue.Color="#FFFFFFFF"
                                  cs:Office2010Silver.Color="#FFFFFFFF"
                                 />
                    <GradientStop Color="#7FFFFFFF"
                                  cs:Name="GradientStop_0019"
                                  Offset="0.85"
                                  cs:Office2010Blue.Color="#7FFFFFFF"
                                  cs:Office2010Silver.Color="#7FFFFFFF"
                                 />
                </LinearGradientBrush>
            </Grid.OpacityMask>
            <Border Margin="1">
                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_1611"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="#FFFAE35C"
                                      cs:Name="GradientStop_0020"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#FFC9E3FA"
                                      cs:Office2010Silver.Color="#FFFAE35C"
                                     />
                        <GradientStop Color="#FFFDF270"
                                      cs:Name="GradientStop_0021"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#FFC9E3FA"
                                      cs:Office2010Silver.Color="#FFFDF270"
                                     />
                        <GradientStop Color="#FFECB300"
                                      cs:Name="GradientStop_0022"
                                      Offset="0.35"
                                      cs:Office2010Blue.Color="#FFA2CBED"
                                      cs:Office2010Silver.Color="#FFECB300"
                                     />
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
        </Grid>
    </ControlTemplate>

