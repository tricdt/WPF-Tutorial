<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2010Black.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/ProgressBarEdit.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!--END WPF-->

    <Thickness x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=DisplayTemplateMargin}">1,0,1,0</Thickness>

    <Thickness x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=DisplayInplaceTemplateMargin}">0</Thickness>

    <SolidColorBrush x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=AdditionalForeground}"
                     Color="#BF000000"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2010Blue.Color="#BF000000"
                     cs:Office2010Silver.Color="#BF000000"/>

    <LinearGradientBrush x:Key="fillBrush"
                         cs:Name="LinearGradientBrush_0219"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#95FFFFFF"
                      cs:Name="GradientStop_0001"
                      Offset="0.5"
                      cs:Office2010Blue.Color="#7FFFFFFF"
                      cs:Office2010Silver.Color="#95FFFFFF"
                     />
        <GradientStop Color="#00FFFFFF"
                      cs:Name="GradientStop_0002"
                      Offset="1"
                      cs:Office2010Blue.Color="#00FFFFFF"
                      cs:Office2010Silver.Color="#00FFFFFF"
                     />
        <GradientStop/>
    </LinearGradientBrush>

    <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideHorizontalTemplate}" TargetType="{x:Type ContentControl}">
        <Border Margin="1" UseLayoutRounding="True">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Border Grid.RowSpan="2">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0220"
                                             EndPoint="0,1"
                                             StartPoint="0,0"
                                            >
                            <GradientStop Color="#FFFAE35C"
                                          cs:Name="GradientStop_0003"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFC9E3FA"
                                          cs:Office2010Silver.Color="#FFFCDF85"
                                         />
                            <GradientStop Color="#FFFDF270"
                                          cs:Name="GradientStop_0004"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFC9E3FA"
                                          cs:Office2010Silver.Color="#FFF8EBB2"
                                         />
                            <GradientStop Color="#FFF4BF23"
                                          cs:Name="GradientStop_0005"
                                          Offset="0.35"
                                          cs:Office2010Blue.Color="#FFA2CBED"
                                          cs:Office2010Silver.Color="#FFF4BF23"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
                <Rectangle x:Name="Background"
                           Grid.ColumnSpan="2"
                           Grid.RowSpan="2"
                           Opacity="1">
                    <!--BEGIN WPF-->
                    <Rectangle.Fill>
                        <MultiBinding>
                            <MultiBinding.Converter>
                                <dxe:ProgressBarHighlightConverter/>
                            </MultiBinding.Converter>
                            <Binding Source="{StaticResource fillBrush}"/>
                            <Binding ElementName="Background" Path="ActualWidth"/>
                            <Binding ElementName="Background" Path="ActualHeight"/>
                            <Binding Path="(dxe:BaseEdit.OwnerEdit).StyleSettings.AccelerateRatio"/>
                        </MultiBinding>
                    </Rectangle.Fill>
                    <!--END WPF-->
                </Rectangle>
            </Grid>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideHorizontalTemplate}" TargetType="{x:Type ContentControl}">
        <Rectangle cs:Name="Rectangle_0001"
                   Fill="Transparent"
                   cs:Office2010Blue.Fill="Transparent"
                   cs:Office2010Silver.Fill="Transparent"
                  />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideVerticalTemplate}" TargetType="{x:Type ContentControl}">
        <Border Margin="1" UseLayoutRounding="True">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Border Grid.ColumnSpan="2">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0221"
                                             EndPoint="1,0"
                                             StartPoint="0,0"
                                            >
                            <GradientStop Color="#FFFAE35C"
                                          cs:Name="GradientStop_0006"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFC9E3FA"
                                          cs:Office2010Silver.Color="#FFFCDF85"
                                         />
                            <GradientStop Color="#FFFDF270"
                                          cs:Name="GradientStop_0007"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFC9E3FA"
                                          cs:Office2010Silver.Color="#FFF8EBB2"
                                         />
                            <GradientStop Color="#FFF4BF23"
                                          cs:Name="GradientStop_0008"
                                          Offset="0.35"
                                          cs:Office2010Blue.Color="#FFA2CBED"
                                          cs:Office2010Silver.Color="#FFF4BF23"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
                <Rectangle x:Name="Background"
                           Grid.ColumnSpan="2"
                           Grid.RowSpan="2"
                           Opacity="1"
                           RadiusX="1"
                           RadiusY="1">
                    <!--BEGIN WPF-->
                    <Rectangle.Fill>
                        <MultiBinding>
                            <MultiBinding.Converter>
                                <dxe:ProgressBarHighlightConverter/>
                            </MultiBinding.Converter>
                            <Binding Source="{StaticResource fillBrush}"/>
                            <Binding ElementName="Background" Path="ActualWidth"/>
                            <Binding ElementName="Background" Path="ActualHeight"/>
                            <Binding Path="(dxe:BaseEdit.OwnerEdit).StyleSettings.AccelerateRatio"/>
                        </MultiBinding>
                    </Rectangle.Fill>
                    <Rectangle.LayoutTransform>
                        <RotateTransform Angle="-90"/>
                    </Rectangle.LayoutTransform>
                    <!--END WPF-->
                </Rectangle>
            </Grid>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideVerticalTemplate}" TargetType="{x:Type ContentControl}">
        <Rectangle cs:Name="Rectangle_0002"
                   Fill="Transparent"
                   cs:Office2010Blue.Fill="Transparent"
                   cs:Office2010Silver.Fill="Transparent"
                  />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelForegroundTemplate}" TargetType="{x:Type ContentControl}">
        <Rectangle cs:Name="Rectangle_0003"
                   Fill="Transparent"
                   cs:Office2010Blue.Fill="Transparent"
                   cs:Office2010Silver.Fill="Transparent"
                  />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelBackgroundHorizontalTemplate}" TargetType="{x:Type ContentControl}">
        <Grid>
            <Border>
                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0222"
                                         EndPoint="0,1"
                                         StartPoint="0,0"
                                        >
                        <GradientStop Color="#FFEDEDED"
                                      cs:Name="GradientStop_0009"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#FFEDEDED"
                                      cs:Office2010Silver.Color="#FFEDEDED"
                                     />
                        <GradientStop Color="#FFFCFCFC"
                                      cs:Name="GradientStop_0010"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#FFEFF5FB"
                                      cs:Office2010Silver.Color="#FFFDFEFF"
                                     />
                        <GradientStop Color="#FFE5E5E5"
                                      cs:Name="GradientStop_0011"
                                      Offset="0.35"
                                      cs:Office2010Blue.Color="#FFE4E5EC"
                                      cs:Office2010Silver.Color="#FFE4E5EC"
                                     />
                    </LinearGradientBrush>
                </Border.Background>
                <Border BorderBrush="#59FFFFFF"
                        BorderThickness="1"
                        cs:Name="Border_0002"
                        cs:Office2010Blue.BorderBrush="#59FFFFFF"
                        cs:Office2010Silver.BorderBrush="#59FFFFFF"
                       />
            </Border>
            <Border x:Name="Border"
                    Background="#FFFFFFFF"
                    cs:Name="Border_0006"
                    Opacity="0"
                    cs:Office2010Blue.Background="#FFFFFFFF"
                    cs:Office2010Silver.Background="#FFFFFFFF"
                   />
            <ContentPresenter/>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).EditMode}" Value="InplaceInactive">
                <Setter Property="Opacity"
                        TargetName="Border"
                        Value="1"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).EditMode}" Value="InplaceActive">
                <Setter Property="Opacity"
                        TargetName="Border"
                        Value="1"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelBackgroundVerticalTemplate}" TargetType="{x:Type ContentControl}">
        <Border>
            <Border.Background>
                <LinearGradientBrush cs:Name="LinearGradientBrush_0223"
                                     EndPoint="1,0"
                                     StartPoint="0,0"
                                    >
                    <GradientStop Color="#FFEDEDED"
                                  cs:Name="GradientStop_0012"
                                  Offset="0"
                                  cs:Office2010Blue.Color="#FFEDEDED"
                                  cs:Office2010Silver.Color="#FFEDEDED"
                                 />
                    <GradientStop Color="#FFFCFCFC"
                                  cs:Name="GradientStop_0013"
                                  Offset="1"
                                  cs:Office2010Blue.Color="#FFEFF5FB"
                                  cs:Office2010Silver.Color="#FFFDFEFF"
                                 />
                    <GradientStop Color="#FFE5E5E5"
                                  cs:Name="GradientStop_0014"
                                  Offset="0.35"
                                  cs:Office2010Blue.Color="#FFE4E5EC"
                                  cs:Office2010Silver.Color="#FFE4E5EC"
                                 />
                </LinearGradientBrush>
            </Border.Background>
            <Grid>
                <Border BorderBrush="#59FFFFFF"
                        BorderThickness="1"
                        cs:Name="Border_0004"
                        cs:Office2010Blue.BorderBrush="#59FFFFFF"
                        cs:Office2010Silver.BorderBrush="#59FFFFFF"
                       />
                <ContentPresenter/>
            </Grid>
        </Border>
    </ControlTemplate>

    <Style x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideHorizontalStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Focusable" Value="false"/>
        <Setter Property="Template" Value="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideHorizontalTemplate}}"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).EditMode}" Value="InplaceInactive">
                <Setter Property="Margin" Value="0"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).EditMode}" Value="InplaceActive">
                <Setter Property="Margin" Value="0"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideHorizontalStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Focusable" Value="false"/>
        <Setter Property="Template" Value="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideHorizontalTemplate}}"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).EditMode}" Value="InplaceInactive">
                <Setter Property="Margin" Value="0"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).EditMode}" Value="InplaceActive">
                <Setter Property="Margin" Value="0"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideVerticalStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Focusable" Value="false"/>
        <Setter Property="Template" Value="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideVerticalTemplate}}"/>
    </Style>

    <Style x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideVerticalStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Focusable" Value="false"/>
