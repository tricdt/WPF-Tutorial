                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0815"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="#FFC28F36"
                                      cs:Name="GradientStop_0012"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#FFC28F36"
                                      cs:Office2010Silver.Color="#FFC28F36"
                                     />
                        <GradientStop Color="#FFC2A34C"
                                      cs:Name="GradientStop_0013"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#FFC2A34C"
                                      cs:Office2010Silver.Color="#FFC2A34C"
                                     />
                    </LinearGradientBrush>
                </Border.Background>
                <Border CornerRadius="64" Margin="1">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0816"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FFFFE575"
                                          cs:Name="GradientStop_0014"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFFFE575"
                                          cs:Office2010Silver.Color="#FFFFE575"
                                         />
                            <GradientStop Color="#FFFFF37F"
                                          cs:Name="GradientStop_0015"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFFFF37F"
                                          cs:Office2010Silver.Color="#FFFFF37F"
                                         />
                            <GradientStop Color="#FFFFD86C"
                                          cs:Name="GradientStop_0016"
                                          Offset="0.5"
                                          cs:Office2010Blue.Color="#FFFFD86C"
                                          cs:Office2010Silver.Color="#FFFFD86C"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
            </Border>
            <Path x:Name="GlyphBackward"
                  cs:Name="Path_0001"
                  Data="F1 M 204,6.99994L 204,14.9999L 200,10.9999L 204,6.99994 Z"
                  Fill="#FF000000"
                  Height="8"
                  HorizontalAlignment="Center"
                  Margin="0,0,2,0"
                  Stretch="Fill"
                  VerticalAlignment="Center"
                  Width="4"
                  cs:Office2010Blue.Fill="#FF515D7A"
                  cs:Office2010Silver.Fill="#FF333333"
                 />
        </Grid>
    </ControlTemplate>

