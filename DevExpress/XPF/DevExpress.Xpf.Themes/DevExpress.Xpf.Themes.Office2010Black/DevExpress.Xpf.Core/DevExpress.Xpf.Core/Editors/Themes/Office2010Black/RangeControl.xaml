                            <GradientStop Color="#FFFFFFED"
                                          cs:Name="GradientStop_0008"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFFFFFED"
                                          cs:Office2010Silver.Color="#FFFFFFED"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="64" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0814"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFEEA8"
                                              cs:Name="GradientStop_0009"
                                              Offset="0"
