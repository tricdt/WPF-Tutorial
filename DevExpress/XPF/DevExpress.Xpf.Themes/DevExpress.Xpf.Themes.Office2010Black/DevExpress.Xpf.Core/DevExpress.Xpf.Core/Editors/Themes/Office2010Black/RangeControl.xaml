<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxer="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2010Black.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/RangeControl.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarClientForegroundBrush}"
                     Color="#FFFFFFFF"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2010Blue.Color="#FFFFFFFF"
                     cs:Office2010Silver.Color="#FFFFFFFF"/>

    <Thickness x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftLabelMargin}"
               Bottom="1"
               Left="1"
               Right="5"
               Top="1"/>

    <Thickness x:Key="{dxt:RangeControlThemeKey ResourceKey=RightLabelMargin}"
               Bottom="1"
               Left="5"
               Right="1"
               Top="1"/>

    <!-- CalendarSelectionMarker -->

    <DataTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarSelectionMarkerTemplate}">
        <Ellipse Fill="{DynamicResource {dxt:CalendarClientThemeKey ResourceKey=CalendarClientForegroundBrush}}"
                 Height="20"
                 HorizontalAlignment="Center"
                 VerticalAlignment="Center"
                 Width="20"/>
    </DataTemplate>

    <!-- ToolTips -->

    <DataTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LabelTemplate}">
        <Border Background="#FF6A6A6A"
                cs:Name="Border_0001"
                MinHeight="22"
                MinWidth="80"
                VerticalAlignment="Top"
                cs:Office2010Blue.Background="#FF94ADCC"
                cs:Office2010Silver.Background="#FFA5ACB5"
               >
            <TextBlock cs:Name="TextBlock_0001"
                       Foreground="#FFD8D8D8"
                       HorizontalAlignment="Center"
                       Margin="2"
                       Text="{Binding}"
                       VerticalAlignment="Center"
                       cs:Office2010Blue.Foreground="#FFFFFFFF"
                       cs:Office2010Silver.Foreground="#FFF0F0F0"
                      />
        </Border>
    </DataTemplate>

    <!-- Left & Right Navigation Buttons -->

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftNavigationButtonTemplate}" TargetType="{x:Type Button}">
        <Grid x:Name="Root"
              Height="24"
              Margin="12,0,0,0"
              Width="24">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Root"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Root"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.75"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="NormalState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="GlyphBackward"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF333333"
                                            cs:Office2010Blue.To="#FF515D7A"
                                            cs:Office2010Silver.To="#FF333333"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Shadow"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="Shadow">
                <Grid.OpacityMask>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0810"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="#59000000"
                                      cs:Name="GradientStop_0001"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#59000000"
                                      cs:Office2010Silver.Color="#3F000000"
                                     />
                        <GradientStop Color="#FF000000"
                                      cs:Name="GradientStop_0002"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#FF000000"
                                      cs:Office2010Silver.Color="#FF000000"
                                     />
                    </LinearGradientBrush>
                </Grid.OpacityMask>
                <Border Background="#07000000"
                        CornerRadius="48"
                        cs:Name="Border_0002"
                        Margin="-2"
                        cs:Office2010Blue.Background="#0C000000"
                        cs:Office2010Silver.Background="#07000000"
                       />
                <Border Background="#0C000000"
                        CornerRadius="48"
                        cs:Name="Border_0003"
                        Margin="-1"
                        cs:Office2010Blue.Background="#07000000"
                        cs:Office2010Silver.Background="#0C000000"
                       />
            </Grid>
            <Border x:Name="NormalState"
                    Background="#FF6A6A6A"
                    CornerRadius="64"
                    cs:Name="Border_0004"
                    cs:Office2010Blue.Background="#FF94ADCC"
                    cs:Office2010Silver.Background="#FFA5ACB5"
                   >
                <Border CornerRadius="64" Margin="1">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0811"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FFB3B3B3"
                                          cs:Name="GradientStop_0003"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFE1EDFA"
                                          cs:Office2010Silver.Color="#FFF3F4F5"
                                         />
