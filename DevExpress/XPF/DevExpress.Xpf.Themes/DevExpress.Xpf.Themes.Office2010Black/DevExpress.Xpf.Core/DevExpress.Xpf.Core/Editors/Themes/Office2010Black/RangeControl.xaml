    <!-- Calendar -->

    <ControlTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarGroupItemTemplate}" TargetType="{x:Type dxer:CalendarGroupItem}">
        <Border Background="#FF6A6A6A"
                cs:Name="Border_0008"
                cs:Office2010Blue.Background="#FF94ADCC"
                cs:Office2010Silver.Background="#FFA5ACB5"
               >
            <TextBlock Margin="20,0"
                       Text="{TemplateBinding Text}"
                       TextAlignment="Center"
                       VerticalAlignment="Center"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarItemTemplate}" TargetType="{x:Type dxer:CalendarItem}">
        <Border x:Name="Border"
                Background="#FF6A6A6A"
                BorderThickness="0"
                cs:Name="Border_0009"
                cs:Office2010Blue.Background="#FF94ADCC"
                cs:Office2010Silver.Background="#FFA5ACB5"
               >
            <TextBlock x:Name="text"
                       HorizontalAlignment="Center"
                       Margin="0,10"
                       Text="{TemplateBinding Text}"
                       TextAlignment="Center"/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter cs:Name="Setter_0001"
                        Property="Background"
                        TargetName="Border"
                        Value="#FFF4BF23"
                        cs:Office2010Blue.Value="#FFFFA500"
                        cs:Office2010Silver.Value="#FFF78A09"
                       />
                <Setter cs:Name="Setter_0018"
                        Property="Foreground"
                        TargetName="text"
                        Value="#EF000000"
                        cs:Office2010Blue.Value="#EF000000"
                        cs:Office2010Silver.Value="#EF000000"
                       />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!-- Range Bar -->

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FF6A6A6A"
                BorderThickness="{Binding Path=(dxer:RangeControl.OwnerRangeControl).BorderThickness, RelativeSource={RelativeSource Self}}"
                cs:Name="Border_0011"
                cs:Office2010Blue.Background="#FFFFFFFF"
                cs:Office2010Blue.BorderBrush="#FF94ADCC"
                cs:Office2010Silver.Background="#FFFFFFFF"
                cs:Office2010Silver.BorderBrush="#FFA5ACB5"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

