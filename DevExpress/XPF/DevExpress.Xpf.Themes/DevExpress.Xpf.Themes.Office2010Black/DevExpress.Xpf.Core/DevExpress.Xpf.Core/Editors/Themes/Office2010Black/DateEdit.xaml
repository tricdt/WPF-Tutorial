    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=MonthInfoDelimeter}">
        <Border Background="#FFA2A2A2"
                cs:Name="Border_0003"
                Height="1"
                Margin="0,4,0,4"
                SnapsToDevicePixels="True"
                cs:Office2010Blue.Background="#FFD5DEE8"
                cs:Office2010Silver.Background="#FFCED4DB"
               />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=HeaderButtonTemplate}" TargetType="{x:Type Button}">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="Content"
                                                           Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FFFFCC00"
                                                         cs:Name="SolidColorBrush_0003"
                                                         cs:Office2010Blue.Color="#FFEB8900"
                                                         cs:Office2010Silver.Color="#FFEB8900"
                                                        />
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <dx:ContentContainer x:Name="Content"
                                 cs:Name="ContentContainer_0002"
                                 Foreground="#FFFFFFFF"
                                 cs:Office2010Blue.Foreground="#FF1E395B"
                                 cs:Office2010Silver.Foreground="#FF3C3C3C"
                                >
                <ContentPresenter Content="{TemplateBinding Content}"
                                  ContentTemplate="{TemplateBinding ContentTemplate}"
                                  HorizontalAlignment="Center"
                                  VerticalAlignment="Center"/>
            </dx:ContentContainer>
        </Grid>
    </ControlTemplate>

    <Style x:Key="{dxet:DateEditThemeKey ResourceKey=HeaderButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource {dxet:DateEditThemeKey ResourceKey=HeaderButtonTemplate}}"/>
        <Setter Property="dx:FocusHelper2.Focusable" Value="False"/>
        <Setter Property="dx:FocusHelper2.Focusable" Value="False"/>
    </Style>

    <Style x:Key="{dxet:DateEditThemeKey ResourceKey=LeftArrowStyle}" TargetType="{x:Type RepeatButton}">
        <Setter Property="ClickMode" Value="Press"/>
        <Setter Property="Delay" Value="400"/>
        <Setter Property="Interval" Value="400"/>
        <Setter Property="Template" Value="{DynamicResource {dxet:DateEditThemeKey ResourceKey=LeftArrowTemplate}}"/>
        <Setter Property="dx:FocusHelper2.Focusable" Value="False"/>
    </Style>

    <Style x:Key="{dxet:DateEditThemeKey ResourceKey=RightArrowStyle}" TargetType="{x:Type RepeatButton}">
        <Setter Property="ClickMode" Value="Press"/>
        <Setter Property="Delay" Value="400"/>
        <Setter Property="Interval" Value="400"/>
        <Setter Property="Template" Value="{DynamicResource {dxet:DateEditThemeKey ResourceKey=RightArrowTemplate}}"/>
        <Setter Property="dx:FocusHelper2.Focusable" Value="False"/>
    </Style>

    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=CurrentDateButtonTemplate}" TargetType="{x:Type Button}">
        <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center"/>
    </ControlTemplate>

    <Style x:Key="{dxet:DateEditThemeKey ResourceKey=CurrentDateButtonStyle}"
           BasedOn="{StaticResource {dxet:DateEditThemeKey ResourceKey=HeaderButtonStyle}}"
           TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource {dxet:DateEditThemeKey ResourceKey=CurrentDateButtonTemplate}}"/>
        <Setter cs:Name="Setter_0001"
                Property="Foreground"
                Value="#FFFFFFFF"
                cs:Office2010Blue.Value="#FF1E395B"
                cs:Office2010Silver.Value="#FF3C3C3C"
               />
    </Style>

    <Style x:Key="{dxet:DateEditThemeKey ResourceKey=ClearButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource {dxet:DateEditThemeKey ResourceKey=ClearButtonTemplate}}"/>
        <Setter Property="Margin" Value="0,10,0,10"/>
        <Setter Property="dx:FocusHelper2.Focusable" Value="false"/>
    </Style>

    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=CalendarTemplate}" TargetType="{x:Type dxepc:DateEditCalendar}">
        <Border Background="#FF838383"
                cs:Name="Border_0004"
                SnapsToDevicePixels="True"
                cs:Office2010Blue.Background="#FFCFDDEE"
                cs:Office2010Silver.Background="#FFE9EDF1"
               >
            <StackPanel>
                <Border BorderThickness="0,0,0,1" Visibility="Visible">
                    <Button x:Name="PART_Today"
                            Content="{TemplateBinding DateTimeText}"
                            cs:Name="Button_0001"
                            Foreground="#FF3C3C3C"
                            Margin="4,4,4,0"
                            Style="{DynamicResource {dxet:DateEditThemeKey IsVisibleInBlend=True, ResourceKey=HeaderButtonStyle}}"
                            cs:Office2010Blue.Foreground="#FF1E395B"
                            cs:Office2010Silver.Foreground="#FF3C3C3C"
                           />
                </Border>
                <Border Visibility="Visible">
                    <Grid Margin="4,4,4,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <RepeatButton x:Name="PART_LeftArrow"
                                      Grid.Column="0"
                                      Margin="2,0,0,0"
                                      Style="{DynamicResource {dxet:DateEditThemeKey IsVisibleInBlend=True, ResourceKey=LeftArrowStyle}}"/>
                        <Button x:Name="PART_CurrentDate"
                                Content="{TemplateBinding CurrentDateText}"
                                Grid.Column="1"
                                Margin="0,2,0,2"
                                Style="{DynamicResource {dxet:DateEditThemeKey IsVisibleInBlend=True, ResourceKey=CurrentDateButtonStyle}}"/>
                        <RepeatButton x:Name="PART_RightArrow"
                                      Grid.Column="2"
                                      Margin="0,0,2,0"
                                      Style="{DynamicResource {dxet:DateEditThemeKey IsVisibleInBlend=True, ResourceKey=RightArrowStyle}}"/>
                    </Grid>
                </Border>
                <Border>
                    <Grid Margin="12,4,12,8">
                        <dxepc:DateEditCalendarContent IsEnabled="False"
                                                       Opacity="0"
                                                       State="Month"
                                                       Template="{TemplateBinding MonthInfoTemplate}"/>
                        <dxepc:DateEditCalendarContent IsEnabled="False"
                                                       Opacity="0"
                                                       State="Year"
                                                       Template="{TemplateBinding YearInfoTemplate}"/>
                        <dxepc:DateEditCalendarContent IsEnabled="False"
                                                       Opacity="0"
                                                       State="Years"
                                                       Template="{TemplateBinding YearsInfoTemplate}"/>
                        <dxepc:DateEditCalendarContent IsEnabled="False"
                                                       Opacity="0"
                                                       State="YearsGroup"
                                                       Template="{TemplateBinding YearsGroupInfoTemplate}"/>
                        <dxepc:DateEditCalendarTransferControl x:Name="PART_CalendarTransferContent"
                                                               Style="{TemplateBinding CalendarTransferStyle}"
                                                               Visibility="Visible"/>
                    </Grid>
                </Border>
                <Border Visibility="Visible">
                    <Button x:Name="PART_Clear"
                            Content="{Binding Path=IsTabStop, RelativeSource={RelativeSource Self}, ConverterParameter=Clear, Converter={StaticResource EditorStringIdConverter}}"
                            HorizontalAlignment="Center"
                            Margin="0,0,0,8"
                            Style="{DynamicResource {dxet:DateEditThemeKey IsVisibleInBlend=True, ResourceKey=ClearButtonStyle}}"
                            VerticalAlignment="Center"/>
                </Border>
            </StackPanel>
        </Border>
    </ControlTemplate>

