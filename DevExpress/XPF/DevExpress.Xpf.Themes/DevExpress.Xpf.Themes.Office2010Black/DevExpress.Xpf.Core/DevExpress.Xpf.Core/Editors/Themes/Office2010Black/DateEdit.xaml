    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=ClearButtonTemplate}" TargetType="{x:Type Button}">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DefaultState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DefaultState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="DefaultState">
                <Border Background="#FF6A6A6A"
                        CornerRadius="3"
                        cs:Name="Border_0001"
                        Padding="1"
                        cs:Office2010Blue.Background="#FF94ADCC"
                        cs:Office2010Silver.Background="#FFA5ACB5"
                       >
                    <Border CornerRadius="2" Padding="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0202"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFB3B3B3"
                                              cs:Name="GradientStop_0001"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFE1EDFA"
                                              cs:Office2010Silver.Color="#FFF3F4F5"
                                             />
                                <GradientStop Color="#FF9F9F9F"
                                              cs:Name="GradientStop_0002"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFD1E0EF"
                                              cs:Office2010Silver.Color="#FFE3E6E8"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Border Background="#E5CF913A"
                        CornerRadius="3"
                        cs:Name="Border_0002"
                        Padding="1"
                        cs:Office2010Blue.Background="#FFECC757"
                        cs:Office2010Silver.Background="#FFECC757"
                       >
                    <Grid>
                        <Border CornerRadius="2" Padding="1">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_0203"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFF8F4E3"
                                                  cs:Name="GradientStop_0003"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFFCF7E4"
                                                  cs:Office2010Silver.Color="#FFFCF7E4"
                                                 />
                                    <GradientStop Color="#FFFBFBEB"
                                                  cs:Name="GradientStop_0004"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFCF8E7"
                                                  cs:Office2010Silver.Color="#FFFCF8E7"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                            <Border CornerRadius="1">
                                <Border.Background>
                                    <LinearGradientBrush cs:Name="LinearGradientBrush_0204"
                                                         EndPoint="0.5,1"
                                                         StartPoint="0.5,0"
                                                        >
                                        <GradientStop Color="#FFECDDA2"
                                                      cs:Name="GradientStop_0005"
                                                      Offset="0"
                                                      cs:Office2010Blue.Color="#FFF8EBB2"
                                                      cs:Office2010Silver.Color="#FFF8EBB2"
                                                     />
                                        <GradientStop Color="#FFFBF3CB"
                                                      cs:Name="GradientStop_0006"
                                                      Offset="1"
                                                      cs:Office2010Blue.Color="#FFFCF7E4"
                                                      cs:Office2010Silver.Color="#FFFCF7E4"
                                                     />
                                        <GradientStop Color="#FFECD377"
                                                      cs:Name="GradientStop_0007"
                                                      Offset="0.5"
                                                      cs:Office2010Blue.Color="#FFF8E088"
                                                      cs:Office2010Silver.Color="#FFF8E088"
                                                     />
                                    </LinearGradientBrush>
                                </Border.Background>
                            </Border>
                        </Border>
                        <Border>
                            <Border.Background>
                                <RadialGradientBrush cs:Name="RadialGradientBrush_0004">
                                    <RadialGradientBrush.RelativeTransform>
                                        <TranslateTransform X="0" Y="0.5"/>
                                    </RadialGradientBrush.RelativeTransform>
                                    <GradientStop Color="#00FFFFFF"
                                                  cs:Name="GradientStop_0008"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#00FFFFFF"
                                                  cs:Office2010Silver.Color="#00FFFFFF"
                                                 />
                                    <GradientStop Color="#BFFFFFFF"
                                                  cs:Name="GradientStop_0009"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#CFFFFFFF"
                                                  cs:Office2010Silver.Color="#BFFFFFFF"
                                                 />
                                </RadialGradientBrush>
                            </Border.Background>
                        </Border>
                    </Grid>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border CornerRadius="3" Padding="1">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0205"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFAB4600"
                                          cs:Name="GradientStop_0010"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFC28E35"
                                          cs:Office2010Silver.Color="#FFC28E35"
                                         />
                            <GradientStop Color="#BFA97B10"
                                          cs:Name="GradientStop_0011"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFC2A44D"
                                          cs:Office2010Silver.Color="#FFC2A44D"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="2" Padding="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0206"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFF6C867"
                                              cs:Name="GradientStop_0012"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFEEC670"
                                              cs:Office2010Silver.Color="#FFEEC670"
                                             />
                                <GradientStop Color="#FFFAD77A"
                                              cs:Name="GradientStop_0013"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFDDF86"
                                              cs:Office2010Silver.Color="#FFFDDF86"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="1">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_0207"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFF6C867"
                                                  cs:Name="GradientStop_0014"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFEEC670"
                                                  cs:Office2010Silver.Color="#FFEEC670"
                                                 />
                                    <GradientStop Color="#FFFFE48A"
                                                  cs:Name="GradientStop_0015"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFFE48A"
                                                  cs:Office2010Silver.Color="#FFFFE48A"
                                                 />
                                    <GradientStop Color="#FFFFE48A"
                                                  cs:Name="GradientStop_0016"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFFFE48A"
                                                  cs:Office2010Silver.Color="#FFFFE48A"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <dx:ContentContainer x:Name="Content"
                                 cs:Name="ContentContainer_0001"
                                 Foreground="#FFFFFFFF"
                                 cs:Office2010Blue.Foreground="#FF1E395B"
                                 cs:Office2010Silver.Foreground="#FF3C3C3C"
                                >
                <ContentPresenter Content="{TemplateBinding Content}"
                                  ContentTemplate="{TemplateBinding ContentTemplate}"
                                  HorizontalAlignment="Center"
                                  Margin="16,2,16,3"
                                  VerticalAlignment="Center"/>
            </dx:ContentContainer>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=LeftArrowTemplate}" TargetType="{x:Type RepeatButton}">
        <Grid SnapsToDevicePixels="True">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="Path_Color"
                                            Storyboard.TargetProperty="Color"
                                            To="#FFFFCC00"
                                            cs:Office2010Blue.To="#FFEB8900"
                                            cs:Office2010Silver.To="#FFEB8900"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                            Duration="0"
                                            Storyboard.TargetName="Path_Color"
                                            Storyboard.TargetProperty="Color"
                                            To="#FFEB8900"
                                            cs:Office2010Blue.To="#FFEB8900"
                                            cs:Office2010Silver.To="#FFEB8900"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid Background="Transparent"
                  cs:Name="Grid_0001"
                  Height="16"
                  Width="16"
                  cs:Office2010Blue.Background="Transparent"
                  cs:Office2010Silver.Background="Transparent"
                 >
                <Path x:Name="Path"
                      Data="F1M1334,811L1330,807 1334,803 1334,811z"
                      Height="8"
                      Stretch="Fill"
                      Width="4">
                    <Path.Fill>
                        <SolidColorBrush x:Name="Path_Color"
                                         Color="#FFFFFFFF"
                                         cs:Name="SolidColorBrush_0001"
                                         cs:Office2010Blue.Color="#FF515D7A"
                                         cs:Office2010Silver.Color="#FF80838F"
                                        />
                    </Path.Fill>
                </Path>
            </Grid>
        </Grid>
    </ControlTemplate>

