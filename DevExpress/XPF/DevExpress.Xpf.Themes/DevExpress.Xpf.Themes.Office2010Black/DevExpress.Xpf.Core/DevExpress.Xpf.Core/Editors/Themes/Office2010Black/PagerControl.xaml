              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Path Data="F1 M 156,47L 156,55L 152,51L 156,47 Z M 151,47L 152,47L 152,55L 151,55L 151,47 Z "
                  Fill="#FF3C3C3C"
                  Height="8"
                  Stretch="Fill"
                  Width="5"
                  cs:Office2010Blue.Fill="#FF1E395B"
                  cs:Office2010Silver.Fill="#FF80838F"
                 />
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="{dxet:PagerControlThemeKey ResourceKey=NavigationButtonPreviousPageGlyph}">
        <Grid Background="Transparent"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Path Data="F1 M 128,35L 128,43L 124,39L 128,35 Z "
                  Fill="#FF3C3C3C"
                  Height="8"
                  Margin="0,0,1,0"
                  Stretch="Fill"
                  Width="4"
                  cs:Office2010Blue.Fill="#FF1E395B"
                  cs:Office2010Silver.Fill="#FF80838F"
                 />
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="{dxet:PagerControlThemeKey ResourceKey=NavigationButtonNextPageGlyph}">
        <Grid Background="Transparent"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Path Data="F1 M 149,25L 149,33L 153,29L 149,25 Z "
                  Fill="#FF3C3C3C"
                  Height="8"
                  Margin="1,0,0,0"
                  Stretch="Fill"
                  Width="4"
                  cs:Office2010Blue.Fill="#FF1E395B"
                  cs:Office2010Silver.Fill="#FF80838F"
                 />
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="{dxet:PagerControlThemeKey ResourceKey=NavigationButtonLastPageGlyph}">
        <Grid Background="Transparent"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Path Data="F1 M 151,47L 151,55L 155,51L 151,47 Z M 156,47.0001L 155,47.0001L 155,55.0001L 156,55.0001L 156,47.0001 Z "
                  Fill="#FF3C3C3C"
                  Height="8"
                  Margin="1,0,0,0"
                  Stretch="Fill"
                  Width="5"
                  cs:Office2010Blue.Fill="#FF1E395B"
                  cs:Office2010Silver.Fill="#FF80838F"
                 />
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="{dxet:PagerControlThemeKey ResourceKey=RefreshPageGlyph}">
        <Grid>
            <Path Data="M1.0000008,5.9999999 C1.0000007,8.2090558 2.791002,9.9999207 5.0000038,9.9999207 5.8149419,9.9999207 6.5731067,9.7561959 7.2053051,9.3376683 L7.2834491,9.2834481 5,6.9999989 10,6.9999989 10,11.999999 8.0001802,10.000179 7.918436,10.060469 C7.0969662,10.651709 6.0887546,11.000001 5.0000038,11.000001 2.2390017,11.000001 -2.9806088E-08,8.7614449 0,6.0001 z M0,0 L1.9997931,1.9997934 2.0814781,1.939532 C2.902729,1.3482915 3.9104328,1 4.9999962,1 7.7609982,1 10,3.2385551 10,5.9998997 L8.999999,5.9999999 C8.999999,3.7909442 7.2089977,2.00008 4.9999962,2.00008 4.1537142,2.00008 3.3686571,2.2629133 2.7223119,2.7113947 L2.715928,2.7159282 5,5.0000004 0,5.0000004 z"
                  Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}"
                  Height="12"
                  HorizontalAlignment="Center"
                  Stretch="Fill"
                  Width="10"/>
        </Grid>
    </DataTemplate>

    <ControlTemplate x:Key="{dxet:PagerControlThemeKey ResourceKey=BorderTemplate}" TargetType="ContentControl">
        <Border>
            <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:PagerControlThemeKey ResourceKey=NavigationButtonTemplate}" TargetType="dxe:PagerControlNavigationButton">
        <Grid x:Name="Root"
              Background="Transparent"
              Height="22"
              Margin="0,0,1,0"
              MinWidth="22"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Grid x:Name="DefaultState">
                <Border Background="#FF6A6A6A"
                        CornerRadius="64"
                        Padding="1"
                        cs:Office2010Blue.Background="#FF94ADCC"
                        cs:Office2010Silver.Background="#FFA5ACB5"
                       >
                    <Border CornerRadius="64" Padding="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFB3B3B3"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFE0ECFA"
                                              cs:Office2010Silver.Color="#FFF2F3F4"
                                             />
                                <GradientStop Color="#FF9F9F9F"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFD2E1F0"
                                              cs:Office2010Silver.Color="#FFE4E7E9"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Border Background="#BFB89407"
                        CornerRadius="64"
                        Padding="1"
                        cs:Office2010Blue.Background="#BFB89407"
                        cs:Office2010Silver.Background="#BFB89407"
                       >
                    <Border CornerRadius="64" Padding="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFF8F4E3"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFF8F4E3"
                                              cs:Office2010Silver.Color="#FFF8F4E3"
                                             />
                                <GradientStop Color="#FFFBFBEB"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFBFBEB"
                                              cs:Office2010Silver.Color="#FFFBFBEB"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="64">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFECDDA2"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFECDDA2"
                                                  cs:Office2010Silver.Color="#FFECDDA2"
                                                 />
                                    <GradientStop Color="#FFFBF3CB"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFBF3CB"
                                                  cs:Office2010Silver.Color="#FFFBF3CB"
                                                 />
                                    <GradientStop Color="#FFECD377"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFECD377"
                                                  cs:Office2010Silver.Color="#FFECD377"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border CornerRadius="64" Padding="1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFAB4600"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#BFAB4600"
                                          cs:Office2010Silver.Color="#BFAB4600"
                                         />
                            <GradientStop Color="#BFA97B10"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#BFA97B10"
                                          cs:Office2010Silver.Color="#BFA97B10"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="64" Padding="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFF6C867"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFF6C867"
                                              cs:Office2010Silver.Color="#FFF6C867"
                                             />
                                <GradientStop Color="#FFFAD77A"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFAD77A"
                                              cs:Office2010Silver.Color="#FFFAD77A"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="64">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFF6C867"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFF6C867"
                                                  cs:Office2010Silver.Color="#FFF6C867"
                                                 />
                                    <GradientStop Color="#FFFFE48A"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFFE48A"
                                                  cs:Office2010Silver.Color="#FFFFE48A"
                                                 />
                                    <GradientStop Color="#FFFFE48A"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFFFE48A"
                                                  cs:Office2010Silver.Color="#FFFFE48A"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                <ContentPresenter Margin="{TemplateBinding Padding}"/>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="Opacity"
                        TargetName="DefaultState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="true">
                <Setter Property="Opacity"
                        TargetName="DefaultState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="1"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="false">
                <Setter Property="Opacity"
                        TargetName="Root"
                        Value="0.35"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:PagerControlThemeKey ResourceKey=NumericButtonTemplate}" TargetType="dxe:PagerControlNumericButton">
        <Grid x:Name="Root"
              Background="Transparent"
              Height="22"
              Margin="0,0,1,0"
              MinWidth="18"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Grid x:Name="HoverState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFC79810"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#BFC79810"
                                          cs:Office2010Silver.Color="#BFC79810"
                                         />
                            <GradientStop Color="#BFA88407"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#BFA88407"
                                          cs:Office2010Silver.Color="#BFA88407"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFFAE6"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFFAE6"
                                              cs:Office2010Silver.Color="#FFFFFAE6"
                                             />
                                <GradientStop Color="#FFFFFFED"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFFFFED"
                                              cs:Office2010Silver.Color="#FFFFFFED"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="1" Margin="1">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFFFEEA8"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFFFEEA8"
                                                  cs:Office2010Silver.Color="#FFFFEEA8"
                                                 />
                                    <GradientStop Color="#FFFFFCE0"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFFFCE0"
                                                  cs:Office2010Silver.Color="#FFFFFCE0"
                                                 />
                                    <GradientStop Color="#FFFFE277"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFFFE277"
                                                  cs:Office2010Silver.Color="#FFFFE277"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFA26F16"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#BFA26F16"
                                          cs:Office2010Silver.Color="#BFA26F16"
                                         />
                            <GradientStop Color="#BFA2832C"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#BFA2832C"
                                          cs:Office2010Silver.Color="#BFA2832C"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFE575"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFE575"
                                              cs:Office2010Silver.Color="#FFFFE575"
                                             />
                                <GradientStop Color="#FFFFF37F"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFFF37F"
                                              cs:Office2010Silver.Color="#FFFFF37F"
                                             />
                                <GradientStop Color="#FFFFD86C"
                                              Offset="0.5"
                                              cs:Office2010Blue.Color="#FFFFD86C"
                                              cs:Office2010Silver.Color="#FFFFD86C"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Grid HorizontalAlignment="Center"
                  Margin="{TemplateBinding Padding}"
                  VerticalAlignment="Center">
                <ContentPresenter x:Name="Content1"
                                  Content="{Binding Number, Converter={StaticResource pageIndexConverter}, RelativeSource={RelativeSource TemplatedParent}}"
                                  ContentTemplate="{TemplateBinding ContentTemplate}"
                                  HorizontalAlignment="Center"
                                  Margin="0,0,1,0"
                                  TextElement.Foreground="#FFFFFFFF"
                                  Visibility="{Binding Path=ShowEllipsis, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource boolToVisiblityConverterInverse}}"
                                  cs:Office2010Blue.TextElement.Foreground="#FF1E395B"
                                  cs:Office2010Silver.TextElement.Foreground="#FF3C3C3C"
                                 />
                <ContentPresenter x:Name="Content2"
                                  Content="..."
                                  ContentTemplate="{TemplateBinding ContentTemplate}"
                                  HorizontalAlignment="Center"
                                  Margin="0,0,1,0"
                                  TextElement.Foreground="#FFFFFFFF"
                                  Visibility="{Binding Path=ShowEllipsis, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource boolToVisibilityConverter}}"
                                  cs:Office2010Blue.TextElement.Foreground="#FF1E395B"
                                  cs:Office2010Silver.TextElement.Foreground="#FF3C3C3C"
                                 />
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding IsSelected}" Value="true">
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="1"/>
                <Setter Property="TextElement.Foreground"
                        TargetName="Content1"
                        Value="#FF3C3C3C"
                        cs:Office2010Blue.Value="#FF1E395B"
                        cs:Office2010Silver.Value="#FF3C3C3C"
                       />
                <Setter Property="TextElement.Foreground"
                        TargetName="Content2"
                        Value="#FF3C3C3C"
                        cs:Office2010Blue.Value="#FF1E395B"
                        cs:Office2010Silver.Value="#FF3C3C3C"
                       />
            </DataTrigger>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
                <Setter Property="TextElement.Foreground"
                        TargetName="Content1"
                        Value="#FF3C3C3C"
                        cs:Office2010Blue.Value="#FF1E395B"
                        cs:Office2010Silver.Value="#FF3C3C3C"
                       />
                <Setter Property="TextElement.Foreground"
                        TargetName="Content2"
                        Value="#FF3C3C3C"
                        cs:Office2010Blue.Value="#FF1E395B"
                        cs:Office2010Silver.Value="#FF3C3C3C"
                       />
            </Trigger>
            <Trigger Property="IsPressed" Value="true">
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="1"/>
                <Setter Property="TextElement.Foreground"
                        TargetName="Content1"
                        Value="#FF3C3C3C"
                        cs:Office2010Blue.Value="#FF1E395B"
                        cs:Office2010Silver.Value="#FF3C3C3C"
                       />
                <Setter Property="TextElement.Foreground"
                        TargetName="Content2"
                        Value="#FF3C3C3C"
                        cs:Office2010Blue.Value="#FF1E395B"
                        cs:Office2010Silver.Value="#FF3C3C3C"
                       />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

</ResourceDictionary>