<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxei="http://schemas.devexpress.com/winfx/2008/xaml/editors/internal"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2010Black.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/GradientMultiSlider.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=NextThumbButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=NextThumbButtonTemplate}}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>

    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=PreviousThumbButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=PreviousThumbButtonTemplate}}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>

    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=FlipThumbsButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=FlipThumbsButtonTemplate}}"/>
        <Setter Property="Width" Value="22"/>
        <Setter Property="Height" Value="22"/>
    </Style>

    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=SelectedThumbContentPresenterStyle}" TargetType="{x:Type ContentPresenter}">
        <Setter Property="Width" Value="14"/>
        <Setter Property="Height" Value="14"/>
        <Setter Property="ContentTemplate" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=SelectedThumbContentPresenterContentTemplate}}"/>
    </Style>

    <DataTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=SelectedThumbContentPresenterContentTemplate}">
        <Border BorderBrush="#FF000000"
                BorderThickness="1"
                cs:Name="Border_0001"
                cs:Office2010Blue.BorderBrush="#FF808080"
                cs:Office2010Silver.BorderBrush="#FF808080"
               >
            <Border.Background>
                <SolidColorBrush Color="{Binding Path=Color}"/>
            </Border.Background>
        </Border>
    </DataTemplate>

    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=PreviousThumbButtonTemplate}">
        <Grid x:Name="Root"
              Background="#00000000"
              cs:Name="Grid_0001"
              Height="22"
              Width="22"
              cs:Office2010Blue.Background="#00000000"
              cs:Office2010Silver.Background="#00000000"
             >
            <Path x:Name="Path"
                  cs:Name="Path_0001"
                  Data="M5,0L0,6L5,12Z"
                  Fill="#FF3C3C3C"
                  Height="10"
                  Stretch="Fill"
                  Width="5"
                  cs:Office2010Blue.Fill="#FF515D7A"
                  cs:Office2010Silver.Fill="#FF595959"
                 />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter cs:Name="Setter_0001"
                        Property="Fill"
                        TargetName="Path"
                        Value="#FF3C3C3C"
                        cs:Office2010Blue.Value="#FF515D7A"
                        cs:Office2010Silver.Value="#FF3C3C3C"
                       />
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter cs:Name="Setter_0002"
                        Property="Fill"
                        TargetName="Path"
                        Value="#FF3C3C3C"
                        cs:Office2010Blue.Value="#FF515D7A"
                        cs:Office2010Silver.Value="#FF3C3C3C"
                       />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity"
                        TargetName="Root"
                        Value="0.35"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=NextThumbButtonTemplate}">
        <Grid x:Name="Root"
              Background="#00000000"
              cs:Name="Grid_0002"
              Height="22"
              Width="22"
              cs:Office2010Blue.Background="#00000000"
              cs:Office2010Silver.Background="#00000000"
             >
            <Path x:Name="Path"
                  cs:Name="Path_0002"
                  Data="M0,0L5,6L0,12Z"
                  Fill="#FF3C3C3C"
                  Height="10"
                  Stretch="Fill"
                  Width="5"
                  cs:Office2010Blue.Fill="#FF515D7A"
                  cs:Office2010Silver.Fill="#FF595959"
                 />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter cs:Name="Setter_0003"
                        Property="Fill"
                        TargetName="Path"
                        Value="#FF3C3C3C"
                        cs:Office2010Blue.Value="#FF515D7A"
                        cs:Office2010Silver.Value="#FF3C3C3C"
                       />
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter cs:Name="Setter_0004"
                        Property="Fill"
                        TargetName="Path"
                        Value="#FF3C3C3C"
                        cs:Office2010Blue.Value="#FF515D7A"
                        cs:Office2010Silver.Value="#FF3C3C3C"
                       />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity"
                        TargetName="Root"
                        Value="0.35"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=FlipThumbsButtonTemplate}">
        <Grid x:Name="Root"
              Background="Transparent"
              cs:Name="Grid_0003"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Grid x:Name="HoverState" Opacity="0">
                <Border x:Name="hover1" CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_1539"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFD7A820"
                                          cs:Name="GradientStop_0001"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#BFD7A820"
                                          cs:Office2010Silver.Color="#BFD7A820"
                                         />
                            <GradientStop Color="#BFB89407"
                                          cs:Name="GradientStop_0002"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#BFB89407"
                                          cs:Office2010Silver.Color="#BFB89407"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border x:Name="hover2"
                            CornerRadius="1"
                            Margin="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_1540"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFFAE6"
                                              cs:Name="GradientStop_0003"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFFAE6"
                                              cs:Office2010Silver.Color="#FFFFFAE6"
                                             />
                                <GradientStop Color="#FFFFFFED"
                                              cs:Name="GradientStop_0004"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFFFFED"
                                              cs:Office2010Silver.Color="#FFFFFFED"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border x:Name="hover3"
                                CornerRadius="1"
                                Margin="1">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_1541"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFFFEEA8"
                                                  cs:Name="GradientStop_0005"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFFFEEA8"
                                                  cs:Office2010Silver.Color="#FFFFEEA8"
                                                 />
                                    <GradientStop Color="#FFFFFCE0"
                                                  cs:Name="GradientStop_0006"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFFFCE0"
                                                  cs:Office2010Silver.Color="#FFFFFCE0"
                                                 />
                                    <GradientStop Color="#FFFFE277"
                                                  cs:Name="GradientStop_0007"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFFFE277"
                                                  cs:Office2010Silver.Color="#FFFFE277"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border x:Name="pressed1" CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_1542"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFA26F16"
                                          cs:Name="GradientStop_0008"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#BFA26F16"
                                          cs:Office2010Silver.Color="#BFA26F16"
                                         />
                            <GradientStop Color="#BFA2832C"
                                          cs:Name="GradientStop_0009"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#BFA2832C"
                                          cs:Office2010Silver.Color="#BFA2832C"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border x:Name="pressed2"
                            CornerRadius="1"
                            Margin="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_1543"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFE575"
                                              cs:Name="GradientStop_0010"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFE575"
                                              cs:Office2010Silver.Color="#FFFFE575"
                                             />
                                <GradientStop Color="#FFFFF37F"
                                              cs:Name="GradientStop_0011"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFFF37F"
                                              cs:Office2010Silver.Color="#FFFFF37F"
                                             />
                                <GradientStop Color="#FFFFD86C"
                                              cs:Name="GradientStop_0012"
                                              Offset="0.5"
                                              cs:Office2010Blue.Color="#FFFFD86C"
                                              cs:Office2010Silver.Color="#FFFFD86C"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Path x:Name="Path"
                  cs:Name="Path_0003"
                  Data="F1 M 1,4L 2,4L 2,3L 3,3L 3,2L 4,2L 4,1L 5,1L 5,2L 5,4L 13,4L 13,4.99991L 5,4.99999L 5,7L 5,8L 4,8L 4,7L 3,7L 3,6L 2,6L 2,5L 1,5L 1,4 Z M 13,10L 12,10L 12,11L 11,11L 11,12L 10,12L 10,13L 9,13L 9,12L 9,10L 1,10L 1,9.00009L 9,9.00001L 9,7L 9,6L 10,6L 10,7.00001L 11,7.00001L 11,8.00001L 12,8.00001L 12,9L 13,9L 13,10 Z "
                  Fill="#FF000000"
                  Height="12"
                  Margin="5"
                  Stretch="Fill"
                  Width="12"
                  cs:Office2010Blue.Fill="#FF515D7A"
                  cs:Office2010Silver.Fill="#FF595959"
                 />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="0"/>
            </Trigger>
            <!-- <Trigger Property="IsSelected" Value="True">
                <Setter TargetName="HoverState"   Property="Opacity" Value="0" />
                <Setter TargetName="PressedState" Property="Opacity" Value="1" />
            </Trigger> -->
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="0"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity"
                        TargetName="Root"
                        Value="0.35"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=ThumbTemplate}" TargetType="{x:Type dxei:GradientMultiSliderThumb}">
        <Grid>
            <Path x:Name="PART_DarkPath"
                  cs:Name="Path_0004"
                  Data="F1 M 15,14L 21,7L 27,14L 27,25L 15,25L 15,14 Z "
                  Fill="#FF808080"
                  Height="17"
                  Stretch="Fill"
                  Width="12"
                  cs:Office2010Blue.Fill="#FFFFFFFF"
                  cs:Office2010Silver.Fill="#FFFFFFFF"
                 />
            <Path x:Name="PART_WhitePath"
                  cs:Name="Path_0005"
                  Data="F1 M 16,14L 21,9L 26,14L 26,24L 16,24L 16,14 Z "
                  Fill="#FFE0E0E0"
                  Height="15"
                  Stretch="Fill"
                  Width="10"
                  cs:Office2010Blue.Fill="#FFE0E0E0"
                  cs:Office2010Silver.Fill="#FFE0E0E0"
                 />
            <Border x:Name="PART_ColorPath"
                    Height="8"
                    Margin="0,5,0,0"
                    Width="8">
                <Border.Background>
                    <SolidColorBrush Color="{TemplateBinding Color}"/>
                </Border.Background>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True"></Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!--END WPF-->

</ResourceDictionary>