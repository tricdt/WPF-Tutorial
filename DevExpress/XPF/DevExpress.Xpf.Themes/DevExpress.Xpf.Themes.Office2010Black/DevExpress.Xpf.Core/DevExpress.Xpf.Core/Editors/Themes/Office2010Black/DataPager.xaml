<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2010Black.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/DataPager.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!--END WPF-->

    <SolidColorBrush x:Key="GlyphColor"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2010Blue.Color="#FF515D7A"
                     cs:Office2010Silver.Color="#FF80838F"/>

    <DataTemplate x:Key="{dxet:DataPagerButtonThemeKey ResourceKey=FirstPageGlyph}">
        <Grid Height="8" Width="8">
            <Path Data="F1 M 156,47L 156,55L 152,51L 156,47 Z M 151,47L 152,47L 152,55L 151,55L 151,47 Z "
                  Fill="{StaticResource GlyphColor}"
                  Height="8"
                  Margin="1,0,2,0"
                  Stretch="Fill"
                  Width="5"/>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="{dxet:DataPagerButtonThemeKey ResourceKey=PrevPageGlyph}">
        <Grid Height="8" Width="8">
            <Path Data="F1 M 128,35L 128,43L 124,39L 128,35 Z "
                  Fill="{StaticResource GlyphColor}"
                  Height="8"
                  Margin="1,0,3,0"
                  Stretch="Fill"
                  Width="4"/>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="{dxet:DataPagerButtonThemeKey ResourceKey=NextPageGlyph}">
        <Grid Height="8" Width="8">
            <Path Data="F1 M 149,25L 149,33L 153,29L 149,25 Z "
                  Fill="{StaticResource GlyphColor}"
                  Height="8"
                  Margin="3,0,1,0"
                  Stretch="Fill"
                  Width="4"/>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="{dxet:DataPagerButtonThemeKey ResourceKey=LastPageGlyph}">
        <Grid Height="8" Width="8">
            <Path Data="F1 M 151,47L 151,55L 155,51L 151,47 Z M 156,47.0001L 155,47.0001L 155,55.0001L 156,55.0001L 156,47.0001 Z "
                  Fill="{StaticResource GlyphColor}"
                  Height="8"
                  Margin="2,0,1,0"
                  Stretch="Fill"
                  Width="5"/>
        </Grid>
    </DataTemplate>

    <ControlTemplate x:Key="{dxet:DataPagerButtonThemeKey ResourceKey=Template}" TargetType="{x:Type dxe:DataPagerButton}">
        <Grid x:Name="Root" Margin="1">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DefaultState"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DefaultState"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Root"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0.35"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="DefaultState" Opacity="0">
                <Border Background="#FF6A6A6A"
                        CornerRadius="64"
                        cs:Name="Border_0001"
                        Padding="1"
                        cs:Office2010Blue.Background="#FF94ADCC"
                        cs:Office2010Silver.Background="#FFA5ACB5"
                       >
                    <Border CornerRadius="64" Padding="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0017"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFB3B3B3"
                                              cs:Name="GradientStop_0001"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFE1EDFA"
                                              cs:Office2010Silver.Color="#FFF3F4F5"
                                             />
                                <GradientStop Color="#FF9F9F9F"
                                              cs:Name="GradientStop_0002"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFD1E0EF"
                                              cs:Office2010Silver.Color="#FFE3E6E8"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Border Background="#BFB89407"
                        CornerRadius="64"
                        cs:Name="Border_0002"
                        Padding="1"
                        cs:Office2010Blue.Background="#FFF1D449"
                        cs:Office2010Silver.Background="#FFF1D449"
                       >
                    <Border CornerRadius="64" Padding="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0018"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFF8F4E3"
                                              cs:Name="GradientStop_0003"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFCF7E4"
                                              cs:Office2010Silver.Color="#FFFCF7E4"
                                             />
                                <GradientStop Color="#FFFBFBEB"
                                              cs:Name="GradientStop_0004"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFCF8E7"
                                              cs:Office2010Silver.Color="#FFFCF8E7"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="64">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_0019"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFECDDA2"
                                                  cs:Name="GradientStop_0005"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFF8EBB2"
                                                  cs:Office2010Silver.Color="#FFF8EBB2"
                                                 />
                                    <GradientStop Color="#FFFBF3CB"
                                                  cs:Name="GradientStop_0006"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFCF7E4"
                                                  cs:Office2010Silver.Color="#FFFCF7E4"
                                                 />
                                    <GradientStop Color="#FFECD377"
                                                  cs:Name="GradientStop_0007"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFF8E088"
                                                  cs:Office2010Silver.Color="#FFF8E088"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border CornerRadius="64" Padding="1">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0020"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFAB4600"
                                          cs:Name="GradientStop_0008"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFC28E35"
                                          cs:Office2010Silver.Color="#FFC28E35"
                                         />
                            <GradientStop Color="#BFA97B10"
                                          cs:Name="GradientStop_0009"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFC2A44D"
                                          cs:Office2010Silver.Color="#FFC2A44D"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="64" Padding="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0021"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFF6C867"
                                              cs:Name="GradientStop_0010"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFF4BF23"
                                              cs:Office2010Silver.Color="#FFF4BF23"
                                             />
                                <GradientStop Color="#FFFAD77A"
                                              cs:Name="GradientStop_0011"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFF4BF23"
                                              cs:Office2010Silver.Color="#FFF4BF23"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="64">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_0022"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFF6C867"
                                                  cs:Name="GradientStop_0012"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFF4BF23"
                                                  cs:Office2010Silver.Color="#FFF4BF23"
                                                 />
                                    <GradientStop Color="#FFFFE48A"
                                                  cs:Name="GradientStop_0013"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFFE575"
                                                  cs:Office2010Silver.Color="#FFFFE575"
                                                 />
                                    <GradientStop Color="#FFFFE48A"
                                                  cs:Name="GradientStop_0014"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFFFE575"
                                                  cs:Office2010Silver.Color="#FFFFE575"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <ContentPresenter Content="{TemplateBinding PageNumber}"
                              ContentTemplate="{TemplateBinding ContentTemplate}"
                              Margin="5"
                              Visibility="{Binding ShowEllipsis, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource DataPagerBoolToVisibilityInvertConverter}}"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DataPagerNumericButtonThemeKey ResourceKey=Template}" TargetType="{x:Type dxe:DataPagerNumericButton}">
        <Grid Margin="0,0,1,0">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="content"
                                            Storyboard.TargetProperty="ForegroundSolidColor4"
                                            To="#FF000000"
                                            cs:Office2010Blue.To="#FF1E395B"
                                            cs:Office2010Silver.To="#FF3C3C3C"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                            Duration="0"
                                            Storyboard.TargetName="content"
                                            Storyboard.TargetProperty="ForegroundSolidColor4"
                                            To="#FF000000"
                                            cs:Office2010Blue.To="#FF1E395B"
                                            cs:Office2010Silver.To="#FF3C3C3C"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="SelectedStates">
                    <VisualState x:Name="NotSelected"/>
                    <VisualState x:Name="Selected">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0003"
                                            Duration="0"
                                            Storyboard.TargetName="content"
                                            Storyboard.TargetProperty="ForegroundSolidColor5"
                                            To="#FF000000"
                                            cs:Office2010Blue.To="#FF1E395B"
                                            cs:Office2010Silver.To="#FF3C3C3C"
                                           />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="HoverState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0023"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFC79810"
                                          cs:Name="GradientStop_0015"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFECC757"
                                          cs:Office2010Silver.Color="#FFECC757"
                                         />
                            <GradientStop Color="#BFA88407"
                                          cs:Name="GradientStop_0016"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFF1D449"
                                          cs:Office2010Silver.Color="#FFF1D449"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0024"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFFAE6"
                                              cs:Name="GradientStop_0017"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFCF7E4"
                                              cs:Office2010Silver.Color="#FFFCF7E4"
                                             />
                                <GradientStop Color="#FFFFFFED"
                                              cs:Name="GradientStop_0018"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFCF8E7"
                                              cs:Office2010Silver.Color="#FFFCF8E7"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="1" Margin="1">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_0025"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFFFEEA8"
                                                  cs:Name="GradientStop_0019"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFF8EBB2"
                                                  cs:Office2010Silver.Color="#FFF8EBB2"
                                                 />
                                    <GradientStop Color="#FFFFFCE0"
                                                  cs:Name="GradientStop_0020"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFCF7E4"
                                                  cs:Office2010Silver.Color="#FFFCF7E4"
                                                 />
                                    <GradientStop Color="#FFFFE277"
                                                  cs:Name="GradientStop_0021"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFFFE575"
                                                  cs:Office2010Silver.Color="#FFFFE575"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0026"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFA26F16"
                                          cs:Name="GradientStop_0022"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFC28E35"
                                          cs:Office2010Silver.Color="#FFC28E35"
                                         />
                            <GradientStop Color="#BFA2832C"
                                          cs:Name="GradientStop_0023"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFC2A44D"
                                          cs:Office2010Silver.Color="#FFC2A44D"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0027"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFE575"
                                              cs:Name="GradientStop_0024"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFE575"
                                              cs:Office2010Silver.Color="#FFFFE575"
                                             />
                                <GradientStop Color="#FFFFF37F"
                                              cs:Name="GradientStop_0025"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFF8EBB2"
                                              cs:Office2010Silver.Color="#FFF8EBB2"
                                             />
                                <GradientStop Color="#FFFFD86C"
                                              cs:Name="GradientStop_0026"
                                              Offset="0.5"
                                              cs:Office2010Blue.Color="#FFFFD86C"
                                              cs:Office2010Silver.Color="#FFFFD86C"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <dx:ContentContainer x:Name="content"
                                 cs:Name="ContentContainer_0001"
                                 ForegroundSolidColor5="#FFFFFFFF"
                                 Margin="5,3,5,3"
                                 cs:Office2010Blue.ForegroundSolidColor5="#FF1E395B"
                                 cs:Office2010Silver.ForegroundSolidColor5="#FF3C3C3C"
                                >
                <Grid>
                    <ContentPresenter Content="99"
                                      ContentTemplate="{TemplateBinding ContentTemplate}"
                                      Opacity="0"/>
                    <ContentPresenter Content="{TemplateBinding PageNumber}"
                                      ContentTemplate="{TemplateBinding ContentTemplate}"
                                      HorizontalAlignment="Center"
                                      Visibility="{Binding ShowEllipsis, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource DataPagerBoolToVisibilityInvertConverter}}"/>
                    <ContentPresenter Content="..."
                                      ContentTemplate="{TemplateBinding ContentTemplate}"
                                      HorizontalAlignment="Center"
                                      Visibility="{Binding ShowEllipsis, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BoolToVisibilityConverter}}"/>
                </Grid>
            </dx:ContentContainer>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DataPagerThemeKey ResourceKey=ButtonAreaBorderTemplate}" TargetType="ContentControl">
        <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DataPagerThemeKey ResourceKey=NumericButtonAreaBorderTemplate}" TargetType="ContentControl">
        <ContentPresenter Content="{TemplateBinding Content}"
                          ContentTemplate="{TemplateBinding ContentTemplate}"
                          Margin="1,0,0,0"/>
    </ControlTemplate>

</ResourceDictionary>