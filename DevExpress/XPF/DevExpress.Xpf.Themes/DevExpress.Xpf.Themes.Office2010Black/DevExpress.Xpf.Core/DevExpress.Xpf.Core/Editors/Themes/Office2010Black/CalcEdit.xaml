                                                          cs:Office2010Blue.Color="#FFF4BF23"
                                                          cs:Office2010Silver.Color="#FFF4BF23"
                                                         />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                    <Border CornerRadius="1">
                                        <Border.Background>
                                            <LinearGradientBrush cs:Name="LinearGradientBrush_0176"
                                                                 EndPoint="0.5,1"
                                                                 StartPoint="0.5,0"
                                                                >
