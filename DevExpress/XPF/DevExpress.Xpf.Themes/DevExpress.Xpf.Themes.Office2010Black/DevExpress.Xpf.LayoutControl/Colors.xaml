    <Color x:Key="LayerSeparator.MouseOverColor"
           cs:Name="Color_0002"
           cs:Office2010Blue.Color="#FF94ADCC"
           cs:Office2010Silver.Color="#FFA5ACB5">
        #FF484848
    </Color>

    <Color x:Key="ElementSizer.GlyphNormalColor"
           cs:Name="Color_0003"
           cs:Office2010Blue.Color="#FF585B5E"
           cs:Office2010Silver.Color="#FF585B5E">
