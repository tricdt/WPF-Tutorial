    <SolidColorBrush x:Key="Text.Foreground"
                     Color="#FFFFFFFF"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

    <Color x:Key="LayerSeparator.NormalColor"
           cs:Name="Color_0001"
           cs:Office2010Blue.Color="#FF94ADCC"
           cs:Office2010Silver.Color="#FFA5ACB5">
