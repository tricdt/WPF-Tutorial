    <Color x:Key="GroupBoxButton.MouseOverColor"
           cs:Name="Color_0007"
           cs:Office2010Blue.Color="#FFEB8900"
           cs:Office2010Silver.Color="#FFEB8900">
        #FFFFD200
    </Color>

    <Color x:Key="GroupBoxButton.PressedColor"
           cs:Name="Color_0008"
           cs:Office2010Blue.Color="#FF515D7A"
           cs:Office2010Silver.Color="#FF80838F">
        #FF3C3C3C
    </Color>

    <SolidColorBrush x:Key="GroupBox.Background"
                     Color="#FF838383"
                     cs:Name="SolidColorBrush_0002"
                     cs:Office2010Blue.Color="#FFCFDDEE"
                     cs:Office2010Silver.Color="#FFE9EDF1"/>

    <SolidColorBrush x:Key="GroupBox.BorderBrush"
