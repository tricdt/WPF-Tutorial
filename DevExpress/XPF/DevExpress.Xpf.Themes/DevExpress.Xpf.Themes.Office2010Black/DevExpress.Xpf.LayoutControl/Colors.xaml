    </Color>

    <Color x:Key="ElementSizer.GlyphMouseOverColor"
           cs:Name="Color_0004"
           cs:Office2010Blue.Color="#FF94ADCC"
           cs:Office2010Silver.Color="#FFA5ACB5">
