                     cs:Name="SolidColorBrush_0004"
                     cs:Office2010Blue.Color="#FF94ADCC"
                     cs:Office2010Silver.Color="#FFA5ACB5"/>

    <SolidColorBrush x:Key="GroupBox.TitleBackground"
                     Color="#FF7E7E7E"
                     cs:Name="SolidColorBrush_0005"
                     cs:Office2010Blue.Color="#FFCFDDEE"
                     cs:Office2010Silver.Color="#FFE9EDF1"/>

    <SolidColorBrush x:Key="GroupSeparator.Brush1"
                     Color="#FF606060"
                     cs:Name="SolidColorBrush_0006"
                     cs:Office2010Blue.Color="#FF94ADCC"
                     cs:Office2010Silver.Color="#FFA5ACB5"/>

    <SolidColorBrush x:Key="GroupSeparator.Brush2"
                     Color="Transparent"
                     cs:Name="SolidColorBrush_0007"
                     cs:Office2010Blue.Color="Transparent"
                     cs:Office2010Silver.Color="Transparent"/>

    <SolidColorBrush x:Key="GroupBox.TitleForeground"
                     Color="#FFFFFFFF"
                     cs:Name="SolidColorBrush_0008"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

