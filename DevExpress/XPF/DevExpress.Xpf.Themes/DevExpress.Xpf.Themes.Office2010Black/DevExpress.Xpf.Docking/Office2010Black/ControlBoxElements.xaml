    <Style x:Key="GroupPane_ExpandButton" TargetType="ve:ControlBoxGlyphPresenter">
        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ve:ControlBoxGlyphPresenter">
                        <Grid x:Name="Button"
                              Background="Transparent"
                              cs:Name="Grid_0001"
                              Height="14"
                              ve:DockPane.HitTestType="ExpandButton"
                              Width="14"
                              cs:Office2010Blue.Background="Transparent"
                              cs:Office2010Silver.Background="Transparent"
                             >
                            <VisualStateManager.VisualStateGroups>
                                <VisualStateGroup x:Name="CommonStates">
                                    <VisualState x:Name="Normal"/>
                                    <VisualState x:Name="MouseOver">
                                        <Storyboard>
                                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                                            Duration="0"
                                                            Storyboard.TargetName="GlyphExpanded"
                                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                            To="#FF3C3C3C"
                                                            cs:Office2010Blue.To="#FF515D7A"
                                                            cs:Office2010Silver.To="#FF80838F"
                                                           />
                                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                                            Duration="0"
                                                            Storyboard.TargetName="GlyphCollapsed"
                                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                            To="#FF3C3C3C"
                                                            cs:Office2010Blue.To="#FF515D7A"
                                                            cs:Office2010Silver.To="#FF80838F"
                                                           />
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="Pressed">
                                        <Storyboard>
                                            <ColorAnimation cs:Name="ColorAnimation_0003"
                                                            Duration="0"
                                                            Storyboard.TargetName="GlyphExpanded"
                                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                            To="#FF3C3C3C"
                                                            cs:Office2010Blue.To="#FF515D7A"
                                                            cs:Office2010Silver.To="#FF80838F"
                                                           />
                                            <ColorAnimation cs:Name="ColorAnimation_0004"
                                                            Duration="0"
                                                            Storyboard.TargetName="GlyphCollapsed"
                                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                            To="#FF3C3C3C"
                                                            cs:Office2010Blue.To="#FF515D7A"
                                                            cs:Office2010Silver.To="#FF80838F"
                                                           />
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                                <VisualStateGroup x:Name="ExpandedStates">
                                    <VisualState x:Name="Expanded"/>
                                    <VisualState x:Name="Collapsed">
                                        <Storyboard>
                                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Collapsed"
                                                                           Storyboard.TargetName="GlyphExpanded"
                                                                           Storyboard.TargetProperty="(Control.Visibility)"/>
                                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible"
                                                                           Storyboard.TargetName="GlyphCollapsed"
                                                                           Storyboard.TargetProperty="(Control.Visibility)"/>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
                            <Path x:Name="GlyphExpanded"
                                  cs:Name="Path_0008"
                                  Data="F1 M 259,112L 259,113L 257,113L 255,111L 253,113L 251,113L 251,112L 255,108L 259,112 Z "
                                  Fill="#FFE4E4E4"
                                  Height="5"
                                  Margin="0,0,0,1"
                                  Stretch="Fill"
                                  Width="8"
                                  cs:Office2010Blue.Fill="#FF515D7A"
                                  cs:Office2010Silver.Fill="#FF80838F"
                                 />
                            <Path x:Name="GlyphCollapsed"
                                  cs:Name="Path_0009"
                                  Data="F1 M 259,116L 259,115L 257,115L 255,117L 253,115L 251,115L 251,116L 255,120L 259,116 Z "
                                  Fill="#FFE4E4E4"
                                  Height="5"
                                  Margin="0,1,0,0"
                                  Stretch="Fill"
                                  Visibility="Collapsed"
                                  Width="8"
                                  cs:Office2010Blue.Fill="#FF515D7A"
                                  cs:Office2010Silver.Fill="#FF80838F"
                                 />
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

