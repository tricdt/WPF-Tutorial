                                                  cs:Office2010Silver.Color="#FFFCF7E4"
                                                 />
                                    <GradientStop Color="#FFFFE277"
                                                  cs:Name="GradientStop_0009"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFFFE575"
                                                  cs:Office2010Silver.Color="#FFFFE575"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="SelectedState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0437"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFA26F16"
                                          cs:Name="GradientStop_0010"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFC28E35"
                                          cs:Office2010Silver.Color="#FFC28E35"
                                         />
                            <GradientStop Color="#BFA2832C"
                                          cs:Name="GradientStop_0011"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFC2A44D"
                                          cs:Office2010Silver.Color="#FFC2A44D"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0438"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFE575"
                                              cs:Name="GradientStop_0012"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFE575"
                                              cs:Office2010Silver.Color="#FFFFE575"
                                             />
                                <GradientStop Color="#FFFFF37F"
                                              cs:Name="GradientStop_0013"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFF8EBB2"
                                              cs:Office2010Silver.Color="#FFF8EBB2"
                                             />
                                <GradientStop Color="#FFFFD86C"
                                              cs:Name="GradientStop_0014"
                                              Offset="0.5"
                                              cs:Office2010Blue.Color="#FFFFD86C"
                                              cs:Office2010Silver.Color="#FFFFD86C"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Border Padding="{TemplateBinding Padding}">
                <ve:TemplatedCaptionControl x:Name="caption"
                                            LayoutItem="{Binding}"
                                            Margin="3,2"/>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Opacity"
                        TargetName="SelectedState"
                        Value="1"/>
                <Setter cs:Name="Setter_0002"
                        Property="Foreground"
                        TargetName="caption"
                        Value="#FF000000"
                        cs:Office2010Blue.Value="#FF515D7A"
                        cs:Office2010Silver.Value="#FF585B5E"
                       />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
                <Setter cs:Name="Setter_0003"
                        Property="Foreground"
                        TargetName="caption"
                        Value="#FF000000"
                        cs:Office2010Blue.Value="#FF515D7A"
                        cs:Office2010Silver.Value="#FF585B5E"
                       />
            </Trigger>
        </ControlTemplate.Triggers>
