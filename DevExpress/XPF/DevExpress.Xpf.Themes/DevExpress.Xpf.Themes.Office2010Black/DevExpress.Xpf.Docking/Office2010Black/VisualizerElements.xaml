        <ControlTemplate.Triggers>
            <Trigger Property="ShowShadow" Value="False">
                <Setter Property="Effect"
                        TargetName="Root"
                        Value="{x:Null}"/>
            </Trigger>
        </ControlTemplate.Triggers>
