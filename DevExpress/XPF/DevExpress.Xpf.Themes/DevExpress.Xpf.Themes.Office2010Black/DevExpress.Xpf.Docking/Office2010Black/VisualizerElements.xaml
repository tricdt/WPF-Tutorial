            </Border>
            <Border x:Name="gridRight"
                    Background="#FF2C2C2C"
                    CornerRadius="2"
                    cs:Name="Border_0074"
                    Grid.Column="2"
                    Grid.Row="1"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    cs:Office2010Blue.Background="#FF515D7A"
                    cs:Office2010Silver.Background="#FF585B5E"
                   >
                <Grid Margin="0,3,3,3">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <ve:DockHintButton x:Name="PART_TabRight"
                                       dxp:DockHintElement.DockType="Right"
                                       Grid.Column="0"
                                       Height="28"
                                       Style="{StaticResource button-right-2}"
                                       Width="28"/>
                    <ve:DockHintButton x:Name="PART_Right"
                                       dxp:DockHintElement.DockType="Right"
                                       Grid.Column="1"
                                       Height="28"
                                       Margin="3,0,0,0"
                                       Style="{StaticResource button-right-3}"
