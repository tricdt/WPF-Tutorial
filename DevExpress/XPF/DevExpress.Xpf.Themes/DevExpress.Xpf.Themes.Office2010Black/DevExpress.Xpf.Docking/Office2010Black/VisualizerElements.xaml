    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Docking.VisualElements.DocumentSelectorContainer}" TargetType="{x:Type ve:DocumentSelectorContainer}">
        <Style.Setters>
            <Setter cs:Name="Setter_0001"
                    Property="Foreground"
                    Value="#FFE0E0E0"
                    cs:Office2010Blue.Value="#FF515D7A"
                    cs:Office2010Silver.Value="#FF585B5E"
                   />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ve:DocumentSelectorContainer}">
                        <Grid>
                            <Grid x:Name="Shadow" Margin="2,2,-3,-3">
                                <Border Background="#FF000000"
                                        CornerRadius="8"
                                        cs:Name="Border_0045"
                                        Opacity="0.01"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="7"
                                        cs:Name="Border_0046"
                                        Margin="1"
                                        Opacity="0.02"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="6"
                                        cs:Name="Border_0047"
                                        Margin="2"
                                        Opacity="0.03"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                            </Grid>
                            <Border x:Name="Window"
                                    Background="#FF484848"
                                    CornerRadius="5"
                                    cs:Name="Border_0048"
                                    Margin="1"
                                    cs:Office2010Blue.Background="#FF94ADCC"
                                    cs:Office2010Silver.Background="#FFA5ACB5"
                                   >
                                <Border Background="#FF939393"
                                        CornerRadius="4"
                                        cs:Name="Border_0049"
                                        Margin="1"
                                        cs:Office2010Blue.Background="#FFFFFFFF"
                                        cs:Office2010Silver.Background="#FFFFFFFF"
                                       >
                                    <Grid Margin="1">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <Border x:Name="Header" CornerRadius="3,3,0,0">
                                            <Border.Background>
                                                <LinearGradientBrush cs:Name="LinearGradientBrush_0433"
                                                                     EndPoint="0.5,1"
                                                                     StartPoint="0.5,0"
                                                                    >
                                                    <GradientStop Color="#FF797979"
                                                                  cs:Name="GradientStop_0001"
                                                                  Offset="0"
                                                                  cs:Office2010Blue.Color="#FFD8E4F2"
                                                                  cs:Office2010Silver.Color="#FFE9EDF1"
                                                                 />
                                                    <GradientStop Color="#FF717171"
                                                                  cs:Name="GradientStop_0002"
                                                                  Offset="1"
                                                                  cs:Office2010Blue.Color="#FFD1E0EF"
                                                                  cs:Office2010Silver.Color="#FFE3E6E8"
                                                                 />
                                                </LinearGradientBrush>
                                            </Border.Background>
                                            <StackPanel HorizontalAlignment="Left"
                                                        Margin="12,8"
                                                        Orientation="Horizontal"
                                                        VerticalAlignment="Center">
                                                <Image HorizontalAlignment="Left"
                                                       Margin="0,0,6,0"
                                                       Source="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionImage}"
                                                       Stretch="None"
                                                       VerticalAlignment="Top"/>
                                                <StackPanel>
                                                    <ContentControl Content="{Binding Path=(local:DockLayoutManager.LayoutItem).Caption}"
                                                                    ContentTemplate="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionTemplate}"
                                                                    ContentTemplateSelector="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionTemplateSelector}"
                                                                    Focusable="False"
                                                                    FontWeight="Bold"
                                                                    HorizontalAlignment="Left"
                                                                    VerticalAlignment="Bottom"/>
                                                    <TextBlock HorizontalAlignment="Left"
                                                               Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Description}"
                                                               TextWrapping="Wrap"
                                                               VerticalAlignment="Top"/>
                                                </StackPanel>
                                            </StackPanel>
                                        </Border>
                                        <Border x:Name="Content"
                                                Background="#FF939393"
                                                cs:Name="Border_0050"
                                                Grid.Row="1"
                                                cs:Office2010Blue.Background="#FFECF3FC"
                                                cs:Office2010Silver.Background="#FFE9EDF1"
                                               >
                                            <ContentPresenter x:Name="PART_ContainerContent"
                                                              Content="{Binding}"
                                                              Margin="12"/>
                                        </Border>
                                        <Border x:Name="Footer"
                                                Background="#FF717171"
                                                CornerRadius="0,0,3,3"
                                                cs:Name="Border_0051"
                                                Grid.Row="2"
                                                MinHeight="24"
                                                cs:Office2010Blue.Background="#FFCFDDEE"
                                                cs:Office2010Silver.Background="#FFE9EDF1"
                                               >
                                            <ContentPresenter Content="{Binding Path=(local:DockLayoutManager.LayoutItem).FooterDescription}"
                                                              HorizontalAlignment="Left"
                                                              Margin="12,0,0,0"
                                                              VerticalAlignment="Center"/>
                                        </Border>
                                    </Grid>
                                </Border>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <ControlTemplate x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=ListBoxItem}" TargetType="{x:Type ListBoxItem}">
        <Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0434"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFD7A820"
                                          cs:Name="GradientStop_0003"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFECC757"
                                          cs:Office2010Silver.Color="#FFECC757"
                                         />
                            <GradientStop Color="#BFB89407"
                                          cs:Name="GradientStop_0004"
                                          Offset="1"
