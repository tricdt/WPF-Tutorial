<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxprg="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid"
                    xmlns:dxprgt="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <sys:Double x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=RowMinHeight}">21</sys:Double>

    <Thickness x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=CategoryHeaderMargin}">2,0,0,0</Thickness>

    <Thickness x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=CategoryHeaderPadding}">2</Thickness>

    <ControlTemplate x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=CategoryHeaderNormalTemplate}">
        <Border Background="Transparent"
                cs:Name="Border_0001"
                MinHeight="22"
                MinWidth="22"
                cs:Office2010Blue.Background="Transparent"
                cs:Office2010Silver.Background="Transparent"
               />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=CategoryHeaderMouseOverTemplate}">
        <Border x:Name="hover1"
                CornerRadius="2"
                MinHeight="22"
                MinWidth="22">
            <Border.Background>
                <LinearGradientBrush cs:Name="LinearGradientBrush_6452"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0"
                                    >
                    <GradientStop Color="#BFD7A820"
                                  cs:Name="GradientStop_0001"
                                  Offset="0"
                                  cs:Office2010Blue.Color="#BFD7A820"
                                  cs:Office2010Silver.Color="#BFD7A820"
                                 />
                    <GradientStop Color="#BFB89407"
                                  cs:Name="GradientStop_0002"
                                  Offset="1"
                                  cs:Office2010Blue.Color="#BFB89407"
                                  cs:Office2010Silver.Color="#BFB89407"
                                 />
                </LinearGradientBrush>
            </Border.Background>
            <Border x:Name="hover2"
                    CornerRadius="1"
                    Margin="1">
                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_6453"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="#FFFFFAE6"
                                      cs:Name="GradientStop_0003"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#FFFFFAE6"
                                      cs:Office2010Silver.Color="#FFFFFAE6"
                                     />
                        <GradientStop Color="#FFFFFFED"
                                      cs:Name="GradientStop_0004"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#FFFFFFED"
                                      cs:Office2010Silver.Color="#FFFFFFED"
                                     />
                    </LinearGradientBrush>
                </Border.Background>
                <Border x:Name="hover3"
                        CornerRadius="1"
                        Margin="1">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_6454"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FFFFEEA8"
                                          cs:Name="GradientStop_0005"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFFFEEA8"
                                          cs:Office2010Silver.Color="#FFFFEEA8"
                                         />
                            <GradientStop Color="#FFFFFCE0"
                                          cs:Name="GradientStop_0006"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFFFFCE0"
                                          cs:Office2010Silver.Color="#FFFFFCE0"
                                         />
                            <GradientStop Color="#FFFFE277"
                                          cs:Name="GradientStop_0007"
                                          Offset="0.5"
                                          cs:Office2010Blue.Color="#FFFFE277"
                                          cs:Office2010Silver.Color="#FFFFE277"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
            </Border>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=CategoryHeaderSelectedTemplate}">
        <Grid x:Name="PressedState"
              MinHeight="22"
              MinWidth="22">
            <Border x:Name="pressed1" CornerRadius="2">
                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_6455"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="#BFA26F16"
                                      cs:Name="GradientStop_0008"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#BFA26F16"
                                      cs:Office2010Silver.Color="#BFA26F16"
                                     />
                        <GradientStop Color="#BFA2832C"
                                      cs:Name="GradientStop_0009"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#BFA2832C"
                                      cs:Office2010Silver.Color="#BFA2832C"
                                     />
                    </LinearGradientBrush>
                </Border.Background>
                <Border x:Name="pressed2"
                        CornerRadius="1"
                        Margin="1">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_6456"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FFFFE575"
                                          cs:Name="GradientStop_0010"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFFFE575"
                                          cs:Office2010Silver.Color="#FFFFE575"
                                         />
                            <GradientStop Color="#FFFFF37F"
                                          cs:Name="GradientStop_0011"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFFFF37F"
                                          cs:Office2010Silver.Color="#FFFFF37F"
                                         />
                            <GradientStop Color="#FFFFD86C"
                                          cs:Name="GradientStop_0012"
                                          Offset="0.5"
                                          cs:Office2010Blue.Color="#FFFFD86C"
                                          cs:Office2010Silver.Color="#FFFFD86C"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
            </Border>
        </Grid>
    </ControlTemplate>

</ResourceDictionary>