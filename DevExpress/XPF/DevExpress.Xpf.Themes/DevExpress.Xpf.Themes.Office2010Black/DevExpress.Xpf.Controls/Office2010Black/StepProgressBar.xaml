<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxcot="http://schemas.devexpress.com/winfx/2008/xaml/controls/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Duration x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressAnimationDuration}">0:0:0.5</Duration>

    <sys:Boolean x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ShowOuterBorder}">False</sys:Boolean>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderBrush}" Color="Transparent"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterIndentBrush}" Color="Transparent"/>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderIndent}">0</sys:Double>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderSize}">0</sys:Double>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBarThickness}">6</sys:Double>

    <Thickness x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBorderThickness}">0</Thickness>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBackground}"
                         EndPoint="0,1"
                         StartPoint="0,0">
        <GradientStop Color="#FF262626"
                      Offset="0"
                      cs:Office2010Blue.Color="#FF94ADCC"
                      cs:Office2010Silver.Color="#FFA5ACB5"
                     />
        <GradientStop Color="#FFB9B9B9"
                      Offset="0.5"
                      cs:Office2010Blue.Color="#FFEFF5FB"
                      cs:Office2010Silver.Color="#FFFDFEFF"
                     />
        <GradientStop Color="#FFB9B9B9"
                      Offset="1"
                      cs:Office2010Blue.Color="#FFEFF5FB"
                      cs:Office2010Silver.Color="#FFFDFEFF"
                     />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressForeground}"
                         EndPoint="0,1"
                         StartPoint="0,0">
        <GradientStop Color="#BFA26F16"
                      Offset="0"
                      cs:Office2010Blue.Color="#FFC28E35"
                      cs:Office2010Silver.Color="#FFC28E35"
                     />
        <GradientStop Color="#FFECD377"
                      Offset="0.5"
                      cs:Office2010Blue.Color="#FFF8E088"
                      cs:Office2010Silver.Color="#FFF8E088"
                     />
        <GradientStop Color="#FFECD377"
                      Offset="1"
                      cs:Office2010Blue.Color="#FFF8E088"
                      cs:Office2010Silver.Color="#FFF8E088"
                     />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressAccentBorderBrush}" Color="Transparent"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressNeutralBorderBrush}" Color="Transparent"/>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorBorderSize}">1</sys:Double>

    <Thickness x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorPanelMargin}">3</Thickness>

    <EllipseGeometry x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorGeometry}"
                     Center="20,20"
                     RadiusX="20"
                     RadiusY="20"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedItemForeground}" Color="#363636"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedIndicatorFill}"
                         EndPoint="0,1"
                         StartPoint="0,0"
                        >
        <GradientStop Color="#B2B2B2"
                      Offset="0"
                      cs:Office2010Blue.Color="#CFDDEE"
                      cs:Office2010Silver.Color="#F3F4F5"
                     />
        <GradientStop Color="#9F9F9F"
                      Offset="1"
                      cs:Office2010Blue.Color="#CFDDEE"
                      cs:Office2010Silver.Color="#E3E6E8"
                     />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedIndicatorStroke}"
                     Color="#5C5C5C"
                     cs:Office2010Blue.Color="#94ADCC"
                     cs:Office2010Silver.Color="#A5ACB5"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedMouseOverFill}"
                         EndPoint="0,1"
                         StartPoint="0,0"
                        >
        <GradientStop Color="#C2C2C2"
                      Offset="0"
                      cs:Office2010Blue.Color="#E7EDF4"
                      cs:Office2010Silver.Color="#E3E4E5"
                     />
        <GradientStop Color="#AFAFAF"
                      Offset="1"
                      cs:Office2010Blue.Color="#E7EDF4"
                      cs:Office2010Silver.Color="#D3d6d8"
                     />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedMouseOverStroke}"
                     Color="#5C5C5C"
                     cs:Office2010Blue.Color="#94ADCC"
                     cs:Office2010Silver.Color="#A5ACB5"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateItemForeground}" Color="#363636"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateIndicatorFill}"
                         EndPoint="0,1"
                         StartPoint="0,0"
                        >
        <GradientStop Color="#FFFAE35C"
                      Offset="0"
                      cs:Office2010Blue.Color="#FFF8EBB2"
                      cs:Office2010Silver.Color="#FFFCDF85"
                     />
        <GradientStop Color="#FFFDF270"
                      Offset="1"
                      cs:Office2010Blue.Color="#FFFCF7E4"
                      cs:Office2010Silver.Color="#FFF8EBB2"
                     />
        <GradientStop Color="#FFF4BF23"
                      Offset="0.35"
                      cs:Office2010Blue.Color="#FFF8E088"
                     />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateIndicatorStroke}"
                     Color="#696969"
                     cs:Office2010Blue.Color="#FFEBC448"
                     cs:Office2010Silver.Color="#EBC448"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateMouseOverFill}"
                         EndPoint="0,1"
                         StartPoint="0,0"
                        >
        <GradientStop Color="#FFFAE35C"
                      Offset="0"
                      cs:Office2010Blue.Color="#FFF8EBB2"
                      cs:Office2010Silver.Color="#FFFCDF85"
                     />
        <GradientStop Color="#FFFDF270"
                      Offset="1"
                      cs:Office2010Blue.Color="#FFFCF7E4"
                      cs:Office2010Silver.Color="#FFF8EBB2"
                     />
        <GradientStop Color="#FFF4BF23"
                      Offset="0.35"
                      cs:Office2010Blue.Color="#FFF8E088"
                     />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateMouseOverStroke}"
                     Color="#FF696969"
                     cs:Office2010Blue.Color="#FFEBC448"
                     cs:Office2010Silver.Color="#EBC448"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedItemForeground}" Color="#FF000000"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedIndicatorFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0"
                        >
        <GradientStop Color="#FFECDDA2"
                      Offset="0"
                      cs:Office2010Blue.Color="#FFF8EBB2"
                      cs:Office2010Silver.Color="#FFF8EBB2"
                     />
        <GradientStop Color="#FFFBF3CB"
                      Offset="1"
                      cs:Office2010Blue.Color="#FFF1D449"
                      cs:Office2010Silver.Color="#FFFCF7E4"
                     />
        <GradientStop Color="#FFECD377"
                      Offset="0.5"
                      cs:Office2010Blue.Color="#FFF1D449"
                      cs:Office2010Silver.Color="#FFF8E088"
                     />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedIndicatorStroke}"
                     Color="#696969"
                     cs:Office2010Blue.Color="#FFEBC448"
                     cs:Office2010Silver.Color="#ECC856"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedMouseOverFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0"
                        >
        <GradientStop Color="#FFFFEEA8"
                      Offset="0"
                      cs:Office2010Blue.Color="#FFF8EBB2"
                      cs:Office2010Silver.Color="#FFF8EBB2"
                     />
        <GradientStop Color="#FFFFFCE0"
                      Offset="1"
                      cs:Office2010Blue.Color="#FFFCF7E4"
                      cs:Office2010Silver.Color="#FFFCF7E4"
                     />
        <GradientStop Color="#FFFFE277"
                      Offset="0.5"
                      cs:Office2010Blue.Color="#FFFFE575"
                      cs:Office2010Silver.Color="#FFFFE575"
                     />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedMouseOverStroke}"
                     Color="#BFB89407"
                     cs:Office2010Blue.Color="#FFEBC448"
                     cs:Office2010Silver.Color="#FFEBC448"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledNotStartedIndicatorFill}"
                     Color="#919191"
                     cs:Office2010Blue.Color="#CFDDEE"
                     cs:Office2010Silver.Color="#E9ECEF"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledNotStartedIndicatorStroke}"
                     Color="#797979"
                     cs:Office2010Blue.Color="#B7CAE0"
                     cs:Office2010Silver.Color="#CED3D9"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledIndeterminateIndicatorFill}"
                     Color="#B19F63"
                     cs:Office2010Blue.Color="#DFE0CB"
                     cs:Office2010Silver.Color="#EEDEA8"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledIndeterminateIndicatorStroke}"
                     Color="#797979"
                     cs:Office2010Blue.Color="#DAD3AC"
                     cs:Office2010Silver.Color="#D7CB9F"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledCompletedIndicatorFill}"
                     Color="#ADA588"
                     cs:Office2010Blue.Color="#DEDDBC"
                     cs:Office2010Silver.Color="#EFEACF"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledCompletedIndicatorStroke}"
                     Color="#797979"
                     cs:Office2010Blue.Color="#DAD3AC"
                     cs:Office2010Silver.Color="#EADEB3"/>

</ResourceDictionary>