<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:collections="clr-namespace:System.Collections;assembly=mscorlib"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxnav="http://schemas.devexpress.com/winfx/2008/xaml/navigation"
                    xmlns:dxnavi="clr-namespace:DevExpress.Xpf.Navigation.Internal;assembly=DevExpress.Xpf.Controls.v24.1"
                    xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
                    xmlns:dxwuii="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/internal"
                    xmlns:dxwuin="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/navigation"
                    xmlns:dxwuit="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=Background}"
                     Color="#FF838383"
                     cs:Name="SolidColorBrush_0004"
                     cs:Office2010Blue.Color="#00FFFFFF"
                     cs:Office2010Silver.Color="#00FFFFFF"/>

    <dxnavi:SizeEx x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=CustomizationFormFloatSize}"
                   Height="275"
                   Width="325"/>

    <system:Double x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=CustomizationFormMinWidth}">325</system:Double>

    <system:Double x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=CustomizationFormMinHeight}">275</system:Double>

    <dxwuii:OrientationInvertConverter x:Key="orientationInvertConverter"/>

    <dx:BoolToVisibilityConverter x:Key="boolToVisibilityConverter"/>

    <Color x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=ItemHoverForegroundColor}"
           cs:Name="Color_0001"
           cs:Office2010Blue.Color="#FF1E395B"
           cs:Office2010Silver.Color="#FF3C3C3C">
        #FFFFFFFF
    </Color>

    <Color x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=ItemSelectedForegroundColor}"
           cs:Name="Color_0002"
           cs:Office2010Blue.Color="#FF1E395B"
           cs:Office2010Silver.Color="#FF3C3C3C">
        #FFFFFFFF
    </Color>

    <ControlTemplate x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=ItemTemplate}" TargetType="{x:Type dxnav:NavigationBarItem}">
        <Border x:Name="Root"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                Padding="{TemplateBinding Padding}">
            <Border.RenderTransform>
                <ScaleTransform/>
            </Border.RenderTransform>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="content"
                                            Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)"
                                            To="{DynamicResource {dxwuit:NavigationBarElementsThemeKey ResourceKey=ItemHoverForegroundColor}}"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Root"
                                             Storyboard.TargetProperty="(RenderTransform).ScaleX"
                                             To="0.97"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Root"
                                             Storyboard.TargetProperty="(RenderTransform).ScaleY"
                                             To="0.97"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Root"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.35"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Normal"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="SelectionStates">
                    <VisualState x:Name="EmptySelectedState"/>
                    <VisualState x:Name="Unselected"/>
                    <VisualState x:Name="Selected">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="content"
                                            Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)"
                                            To="{DynamicResource {dxwuit:NavigationBarElementsThemeKey ResourceKey=ItemSelectedForegroundColor}}"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                <dx:LayoutTransformPanel Orientation="{Binding Orientation, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource orientationInvertConverter}}">
                    <dxb:SplitLayoutPanel Content1Location="{TemplateBinding GlyphAlignment}">
                        <dxb:SplitLayoutPanel.Content1>
                            <dx:PixelSnapper HorizontalAlignment="Center"
                                             VerticalAlignment="Center"
                                             Visibility="{Binding IsGlyphActuallyVisible, Converter={StaticResource ResourceKey=boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}">
                                <Image Source="{TemplateBinding Glyph}" Stretch="None"/>
                            </dx:PixelSnapper>
                        </dxb:SplitLayoutPanel.Content1>
                        <dxb:SplitLayoutPanel.Content2>
                            <ContentPresenter x:Name="content"
                                              cs:Name="ContentPresenter_0001"
                                              HorizontalAlignment="Center"
                                              TextElement.Foreground="#5FFFFFFF"
                                              VerticalAlignment="Center"
                                              Visibility="{Binding IsContentActuallyVisible, Converter={StaticResource ResourceKey=boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}"
                                              cs:Office2010Blue.TextElement.Foreground="#9F1E395B"
                                              cs:Office2010Silver.TextElement.Foreground="#9F3C3C3C"
                                             />
                        </dxb:SplitLayoutPanel.Content2>
                    </dxb:SplitLayoutPanel>
                </dx:LayoutTransformPanel>
            </Grid>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=ButtonTemplate}" TargetType="dxnav:NavigationBarButton">
        <Grid x:Name="Root"
              Background="Transparent"
              cs:Name="Grid_0001"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="content"
                                            Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)"
                                            To="{DynamicResource {dxwuit:NavigationBarElementsThemeKey ResourceKey=ItemHoverForegroundColor}}"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed"/>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Root"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.35"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Normal"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
                <dxb:SplitLayoutPanel Content1Location="{TemplateBinding GlyphAlignment}">
                    <dxb:SplitLayoutPanel.Content1>
                        <dx:PixelSnapper HorizontalAlignment="Center"
                                         VerticalAlignment="Center"
                                         Visibility="{Binding IsGlyphActuallyVisible, Converter={StaticResource ResourceKey=boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}">
                            <Image Source="{TemplateBinding Glyph}" Stretch="None"/>
                        </dx:PixelSnapper>
                    </dxb:SplitLayoutPanel.Content1>
                    <dxb:SplitLayoutPanel.Content2>
                        <dx:PixelSnapper HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <ContentPresenter x:Name="content" Visibility="{Binding IsContentActuallyVisible, Converter={StaticResource ResourceKey=boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}"/>
                        </dx:PixelSnapper>
                    </dxb:SplitLayoutPanel.Content2>
                </dxb:SplitLayoutPanel>
            </Grid>
        </Grid>
    </ControlTemplate>

    <DataTemplate x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=CustomizationButtonContentTemplate}">
        <Path Fill="{Binding Path=(Control.Foreground), RelativeSource={RelativeSource TemplatedParent}}">
            <Path.Data>
                <GeometryGroup>
                    <EllipseGeometry Center="2,2"
                                     RadiusX="2"
                                     RadiusY="2"/>
                    <EllipseGeometry Center="10,2"
                                     RadiusX="2"
                                     RadiusY="2"/>
                    <EllipseGeometry Center="18,2"
                                     RadiusX="2"
                                     RadiusY="2"/>
                </GeometryGroup>
            </Path.Data>
        </Path>
    </DataTemplate>

    <!-- Customization -->

    <ControlTemplate x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=CustomizationArrowControlTemplate}" TargetType="{x:Type dxnavi:CustomizationArrowControl}">
        <Grid>
            <Grid.Resources>
                <SolidColorBrush x:Key="GlyphBrush"
                                 Color="#FF3C3C3C"
                                 cs:Name="SolidColorBrush_0001"
                                 cs:Office2010Blue.Color="#FF3C3C3C"
                                 cs:Office2010Silver.Color="#FF3C3C3C"
                                />
            </Grid.Resources>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="DirectionStates">
                    <VisualState x:Name="Up">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible"
                                                           Storyboard.TargetName="PART_UpGlyph"
                                                           Storyboard.TargetProperty="Visibility"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Down">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible"
                                                           Storyboard.TargetName="PART_DownGlyph"
                                                           Storyboard.TargetProperty="Visibility"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Left">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible"
                                                           Storyboard.TargetName="PART_LeftGlyph"
                                                           Storyboard.TargetProperty="Visibility"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Right">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible"
                                                           Storyboard.TargetName="PART_RightGlyph"
                                                           Storyboard.TargetProperty="Visibility"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Path x:Name="PART_UpGlyph"
                  Data="F1 M 208,44.9996L 214,51L 211,51L 211,56.9977L 205,57.0023L 205,51L 202,51L 208,44.9996 Z "
                  Fill="{StaticResource GlyphBrush}"
                  Height="12"
                  Stretch="Fill"
                  Visibility="Collapsed"
                  Width="12"/>
            <Path x:Name="PART_RightGlyph"
                  Data="F1 M 224,61L 218,67L 218,64L 212.002,64L 211.997,58L 218,58L 218,55L 224,61 Z "
                  Fill="{StaticResource GlyphBrush}"
                  Height="12"
                  Stretch="Fill"
                  Visibility="Collapsed"
                  Width="12"/>
            <Path x:Name="PART_DownGlyph"
                  Data="F1 M 208,77L 202,70.9996L 205,70.9996L 205,65.002L 211,64.9974L 211,70.9996L 214,70.9996L 208,77 Z "
                  Fill="{StaticResource GlyphBrush}"
                  Height="12"
                  Stretch="Fill"
                  Visibility="Collapsed"
                  Width="12"/>
            <Path x:Name="PART_LeftGlyph"
                  Data="F1 M 191.997,61L 197.998,55L 197.998,58L 203.995,58.0001L 204,64L 197.998,64L 197.998,67L 191.997,61 Z "
                  Fill="{StaticResource GlyphBrush}"
                  Height="12"
                  Stretch="Fill"
                  Visibility="Collapsed"
                  Width="12"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxwuit:NavigationBarElementsThemeKey ResourceKey=CustomizationSeparatorControlTemplate}" TargetType="{x:Type dxnavi:CustomizationSeparatorControl}">
        <Grid SnapsToDevicePixels="True">
            <Grid.Resources>
                <SolidColorBrush x:Key="brush1"
                                 Color="#FFCCCCCC"
                                 cs:Name="SolidColorBrush_0002"
                                 cs:Office2010Blue.Color="#FFCCCCCC"
                                 cs:Office2010Silver.Color="#FFCCCCCC"
                                />
                <SolidColorBrush x:Key="brush2"
                                 Color="#FFFFFFFF"
                                 cs:Name="SolidColorBrush_0003"
                                 cs:Office2010Blue.Color="#FFFFFFFF"
                                 cs:Office2010Silver.Color="#FFFFFFFF"
                                />
            </Grid.Resources>
            <Grid x:Name="PART_VerticalRootElement" Width="2">
                <Rectangle Fill="{StaticResource brush1}"
                           HorizontalAlignment="Left"
                           Margin="0,0,0,1"
                           Stretch="Fill"
                           Width="1"/>
                <Rectangle Fill="{StaticResource brush2}"
                           HorizontalAlignment="Right"
                           Margin="0,1,0,0"
                           Stretch="Fill"
                           Width="1"/>
            </Grid>
            <Grid x:Name="PART_HorizontalRootElement" Height="2">
                <Rectangle Fill="{StaticResource brush1}"
                           Height="1"
                           Margin="0,0,1,0"
                           Stretch="Fill"
                           VerticalAlignment="Top"/>
                <Rectangle Fill="{StaticResource brush2}"
                           Height="1"
                           Margin="1,0,0,0"
                           Stretch="Fill"
                           VerticalAlignment="Bottom"/>
            </Grid>
        </Grid>
    </ControlTemplate>

</ResourceDictionary>