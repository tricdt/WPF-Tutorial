<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:collections="clr-namespace:System.Collections;assembly=mscorlib"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
                    xmlns:dxwuii="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/internal"
                    xmlns:dxwuin="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/navigation"
                    xmlns:dxwuit="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <system:Double x:Key="{dxwuit:MenuFlyoutElementsThemeKey ResourceKey=FontSize}">12</system:Double>

    <Thickness x:Key="{dxwuit:MenuFlyoutElementsThemeKey ResourceKey=Padding}">12,8,12,8</Thickness>

    <Thickness x:Key="{dxwuit:MenuFlyoutElementsThemeKey ResourceKey=SelectionPadding}">1</Thickness>

    <SolidColorBrush x:Key="{dxwuit:MenuFlyoutElementsThemeKey ResourceKey=Background}"
                     Color="#FFFFFFFF"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2010Blue.Color="#FFFFFFFF"
                     cs:Office2010Silver.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxwuit:MenuFlyoutElementsThemeKey ResourceKey=Foreground}"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0002"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="{dxwuit:MenuFlyoutElementsThemeKey ResourceKey=SelectedBackground}"
                     Color="#FFFFE277"
                     cs:Name="SolidColorBrush_0003"
                     cs:Office2010Blue.Color="#FFFFE277"
                     cs:Office2010Silver.Color="#FFFFE277"/>

    <SolidColorBrush x:Key="{dxwuit:MenuFlyoutElementsThemeKey ResourceKey=SelectedForeground}"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0004"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="{dxwuit:MenuFlyoutElementsThemeKey ResourceKey=PressedBackground}"
                     Color="#FFFFD86C"
                     cs:Name="SolidColorBrush_0005"
                     cs:Office2010Blue.Color="#FFFFD86C"
                     cs:Office2010Silver.Color="#FFFFD86C"/>

    <SolidColorBrush x:Key="{dxwuit:MenuFlyoutElementsThemeKey ResourceKey=PressedForeground}"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0006"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="{dxwuit:MenuFlyoutElementsThemeKey ResourceKey=MenuFlyoutSeparatorForeground}"
                     Color="#1F000000"
                     cs:Name="SolidColorBrush_0007"
                     cs:Office2010Blue.Color="#1F000000"
                     cs:Office2010Silver.Color="#1F000000"/>

    <ControlTemplate x:Key="{dxwuit:WindowsUIElementsThemeKey ResourceKey=FlyoutCommandButtonTemplate}" TargetType="dxwuii:FlyoutCommandButton">
        <Grid Height="15"
              Margin="0,0,-4,-2"
              SnapsToDevicePixels="True"
              Width="15">
            <Grid x:Name="HoverState" Opacity="0">
                <Border x:Name="hover1" CornerRadius="2,0,0,0">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_1475"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFD7A820"
                                          cs:Name="GradientStop_0001"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#BFD7A820"
                                          cs:Office2010Silver.Color="#BFD7A820"
                                         />
                            <GradientStop Color="#BFB89407"
                                          cs:Name="GradientStop_0002"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#BFB89407"
                                          cs:Office2010Silver.Color="#BFB89407"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border x:Name="hover2"
                            CornerRadius="1,0,0,0"
                            Margin="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_1476"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFFAE6"
                                              cs:Name="GradientStop_0003"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFFAE6"
                                              cs:Office2010Silver.Color="#FFFFFAE6"
                                             />
                                <GradientStop Color="#FFFFFFED"
                                              cs:Name="GradientStop_0004"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFFFFED"
                                              cs:Office2010Silver.Color="#FFFFFFED"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border x:Name="hover3" Margin="1">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_1477"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFFFEEA8"
                                                  cs:Name="GradientStop_0005"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFFFEEA8"
                                                  cs:Office2010Silver.Color="#FFFFEEA8"
                                                 />
                                    <GradientStop Color="#FFFFFCE0"
                                                  cs:Name="GradientStop_0006"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFFFCE0"
                                                  cs:Office2010Silver.Color="#FFFFFCE0"
                                                 />
                                    <GradientStop Color="#FFFFE277"
                                                  cs:Name="GradientStop_0007"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFFFE277"
                                                  cs:Office2010Silver.Color="#FFFFE277"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border x:Name="pressed1" CornerRadius="2,0,0,0">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_1478"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FFC28F36"
                                          cs:Name="GradientStop_0008"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFC28F36"
                                          cs:Office2010Silver.Color="#FFC28F36"
                                         />
                            <GradientStop Color="#FFC2A34C"
                                          cs:Name="GradientStop_0009"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFC2A34C"
                                          cs:Office2010Silver.Color="#FFC2A34C"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border x:Name="pressed2"
                            CornerRadius="1,0,0,0"
                            Margin="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_1479"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFE575"
                                              cs:Name="GradientStop_0010"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFE575"
                                              cs:Office2010Silver.Color="#FFFFE575"
                                             />
                                <GradientStop Color="#FFFFF37F"
                                              cs:Name="GradientStop_0011"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFFF37F"
                                              cs:Office2010Silver.Color="#FFFFF37F"
                                             />
                                <GradientStop Color="#FFFFD86C"
                                              cs:Name="GradientStop_0012"
                                              Offset="0.5"
                                              cs:Office2010Blue.Color="#FFFFD86C"
                                              cs:Office2010Silver.Color="#FFFFD86C"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Grid Height="7" Width="7">
                <Path x:Name="Path1"
                      cs:Name="Path_0001"
                      Data="F1 M 6.10352e-005,6L 1.00006,6L 1,1L 6,1L 6,0L 0,0L 6.10352e-005,6 Z"
                      Fill="#FF3C3C3C"
                      Margin="0,0,1,1"
                      Stretch="Fill"
                      cs:Office2010Blue.Fill="#FF515D7A"
                      cs:Office2010Silver.Fill="#FF3C3C3C"
                     />
                <Path x:Name="Path2"
                      cs:Name="Path_0002"
                      Data="F1 M 3.00006,3L 3.00006,4L 4,4.00003L 4,3.00003L 3.00006,3 Z"
                      Fill="#FF3C3C3C"
                      Height="1"
                      HorizontalAlignment="Left"
                      Margin="3,3,0,0"
                      Stretch="Fill"
                      VerticalAlignment="Top"
                      Width="1"
                      cs:Office2010Blue.Fill="#FF515D7A"
                      cs:Office2010Silver.Fill="#FF3C3C3C"
                     />
                <Path x:Name="Path3"
                      cs:Name="Path_0003"
                      Data="F1 M 4.00006,7L 3.00006,7L 3,6L 4,6L 4,4L 6,4L 6,3L 7,3L 7,7L 4.00006,7 Z"
                      Fill="#FF3C3C3C"
                      Margin="3,3,0,0"
                      Stretch="Fill"
                      cs:Office2010Blue.Fill="#FF515D7A"
                      cs:Office2010Silver.Fill="#FF3C3C3C"
                     />
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

</ResourceDictionary>