<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:interop="clr-namespace:System.Windows.Interop;assembly=PresentationFramework"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                cs:Name="Border_0001"
                cs:Office2010Blue.Background="#FFFFFFFF"
                cs:Office2010Silver.Background="#FFFFFFFF"
               />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBarControlBorderTemplate2007}" TargetType="{x:Type ContentControl}">
        <Grid SnapsToDevicePixels="True">
            <Border x:Name="Shadow"
                    Background="#FF000000"
                    CornerRadius="3"
                    cs:Name="Border_0002"
                    Margin="3,3,0,0"
                    Opacity="0.1"
                    cs:Office2010Blue.Background="#FF000000"
                    cs:Office2010Silver.Background="#FF000000"
                   />
            <Border x:Name="Border"
                    Background="#FF484848"
                    cs:Name="Border_0003"
                    Margin="0,0,3,3"
                    cs:Office2010Blue.Background="#FF8492A6"
                    cs:Office2010Silver.Background="#FF80838F"
                   >
                <Grid Margin="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" MinHeight="22"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Border x:Name="Header" BorderThickness="0,1">
                        <Border.BorderBrush>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0527"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFCBCBCB"
                                              cs:Name="GradientStop_0001"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFE1EDFA"
                                              cs:Office2010Silver.Color="#FFF3F4F5"
                                             />
                                <GradientStop Color="#FF898989"
                                              cs:Name="GradientStop_0002"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FF94ADCC"
                                              cs:Office2010Silver.Color="#FFC0C4C9"
                                             />
                            </LinearGradientBrush>
                        </Border.BorderBrush>
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0528"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFD3D3D3"
                                              cs:Name="GradientStop_0003"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFF6FBFF"
                                              cs:Office2010Silver.Color="#FFFDFEFF"
                                             />
                                <GradientStop Color="#FFBEBEBE"
                                              cs:Name="GradientStop_0004"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFDCE6F4"
                                              cs:Office2010Silver.Color="#FFE3E8EE"
                                             />
                                <GradientStop Color="#FFC6C6C6"
                                              cs:Name="GradientStop_0005"
                                              Offset="0.5"
                                              cs:Office2010Blue.Color="#FFE7EEF8"
                                              cs:Office2010Silver.Color="#FFECEFF3"
                                             />
                                <GradientStop Color="#FFBEBEBE"
                                              cs:Name="GradientStop_0006"
                                              Offset="0.5"
                                              cs:Office2010Blue.Color="#FFDCE6F4"
                                              cs:Office2010Silver.Color="#FFE3E8EE"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                    <Border Background="#FFFFFFFF"
                            cs:Name="Border_0004"
                            Grid.Row="1"
                            cs:Office2010Blue.Background="#FFFFFFFF"
                            cs:Office2010Silver.Background="#FFFFFFFF"
                           >
                        <ContentPresenter/>
                    </Border>
                </Grid>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(interop:BrowserInteropHelper.IsBrowserHosted)}" Value="True">
                <Setter Property="Visibility"
                        TargetName="Shadow"
                        Value="Collapsed"/>
                <Setter Property="Margin"
                        TargetName="Border"
                        Value="0"/>
                <Setter Property="CornerRadius"
                        TargetName="Border"
                        Value="0"/>
                <Setter Property="CornerRadius"
                        TargetName="Header"
                        Value="0"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBottomPaneTemplate}" TargetType="{x:Type ContentControl}">
        <Border x:Name="Footer"
                BorderBrush="#FFABABAB"
                BorderThickness="0,1,0,0"
                cs:Name="Border_0005"
                cs:Office2010Blue.BorderBrush="#FFD5DEE8"
                cs:Office2010Silver.BorderBrush="#FFCED4DB"
               >
            <Border.Background>
                <LinearGradientBrush cs:Name="LinearGradientBrush_0529"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0"
                                    >
                    <GradientStop Color="#FFD5D5D5"
                                  cs:Name="GradientStop_0007"
                                  Offset="0"
                                  cs:Office2010Blue.Color="#FFF6FBFF"
                                  cs:Office2010Silver.Color="#FFFDFEFF"
                                 />
                    <GradientStop Color="#FFC5C5C5"
                                  cs:Name="GradientStop_0008"
                                  Offset="1"
                                  cs:Office2010Blue.Color="#FFE3EBFA"
                                  cs:Office2010Silver.Color="#FFEAEEF2"
                                 />
                </LinearGradientBrush>
            </Border.Background>
            <ContentPresenter HorizontalAlignment="Right"
                              Margin="2"
                              SnapsToDevicePixels="True"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuRightPaneTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FFC3C3C3"
                BorderThickness="1,0,0,0"
                cs:Name="Border_0007"
                HorizontalAlignment="Stretch"
                cs:Office2010Blue.Background="#FFFFFFFF"
                cs:Office2010Blue.BorderBrush="#FFC0C3C6"
                cs:Office2010Silver.Background="#FFFFFFFF"
                cs:Office2010Silver.BorderBrush="#FFC0C3C6"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <Thickness x:Key="{dxrt:BackstageThemeKey ResourceKey=BorderThickness}">1,2,1,1</Thickness>

    <Thickness x:Key="{dxrt:BackstageThemeKey ResourceKey=BorderThicknessNoFullscreen}">0,2,0,0</Thickness>

    <ControlTemplate x:Key="{dxrt:BackstageThemeKey ResourceKey=BorderTemplate}">
        <Border Background="{DynamicResource {dxt:ThemedWindowThemeKey ResourceKey=WindowActiveBorderBrush}}">
            <Border BorderThickness="0,2,0,0">
                <Border.BorderBrush>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_6488"
                                         EndPoint="1,0.5"
                                         StartPoint="0,0.5"
                                        >
                        <GradientStop Color="#FFFFD51C"
                                      cs:Name="GradientStop_0009"
                                      Offset="0"
                                      cs:Office2010Blue.Color="#FFFFCC00"
                                      cs:Office2010Silver.Color="#FFFFCC00"
                                     />
                        <GradientStop Color="#00000000"
                                      cs:Name="GradientStop_0010"
                                      Offset="1"
                                      cs:Office2010Blue.Color="#00000000"
                                      cs:Office2010Silver.Color="#00000000"
                                     />
                    </LinearGradientBrush>
                </Border.BorderBrush>
            </Border>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuContentControlBorderTemplate}" TargetType="{x:Type ContentControl}">
        <ContentPresenter Content="{TemplateBinding Content}"/>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBarControlBorderTemplate2010}" TargetType="{x:Type ContentControl}">
        <ContentControl Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBarControlBorderTemplate2007}}">
            <ContentPresenter/>
        </ContentControl>
    </ControlTemplate>

    <SolidColorBrush x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuRightPaneForeground}"
                     Color="#FF333333"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2010Blue.Color="#FF333333"
                     cs:Office2010Silver.Color="#FF333333"/>

</ResourceDictionary>