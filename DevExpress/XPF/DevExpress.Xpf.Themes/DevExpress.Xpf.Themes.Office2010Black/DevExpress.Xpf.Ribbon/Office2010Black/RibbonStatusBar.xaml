<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=LeftPartTemplate}" TargetType="{x:Type ContentControl}">
        <Grid Background="#FF323232"
              cs:Name="Border_0001"
              cs:Office2010Blue.Background="#FF849DBD"
              cs:Office2010Silver.Background="#FF80838F"
             >
            <Grid Margin="0,1,0,0">
                <Border>
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0693"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FF4B4B4B"
                                          cs:Name="GradientStop_0001"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFDBE7F6"
                                          cs:Office2010Silver.Color="#FFE6EAEE"
                                         />
                            <GradientStop Color="#FF323232"
                                          cs:Name="GradientStop_0002"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFB4C5D9"
                                          cs:Office2010Silver.Color="#FFC0C4C9"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border BorderBrush="#20FFFFFF"
                            BorderThickness="0,1,0,1"
                            cs:Name="Border_0004"
                            cs:Office2010Blue.BorderBrush="#7FFFFFFF"
                            cs:Office2010Silver.BorderBrush="#7FFFFFFF"
                           />
                </Border>
            </Grid>
            <ContentPresenter Margin="2,2,1,1"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=RightPartTemplate}" TargetType="{x:Type ContentControl}">
        <Grid Background="#FF323232"
              cs:Name="Border_0003"
              cs:Office2010Blue.Background="#FF849DBD"
              cs:Office2010Silver.Background="#FF80838F"
             >
            <Grid Margin="0,1,0,0">
                <Border>
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0694"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FF4B4B4B"
                                          cs:Name="GradientStop_0003"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFDBE7F6"
                                          cs:Office2010Silver.Color="#FFE6EAEE"
                                         />
                            <GradientStop Color="#FF323232"
                                          cs:Name="GradientStop_0004"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFB4C5D9"
                                          cs:Office2010Silver.Color="#FFC0C4C9"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border BorderBrush="#20FFFFFF"
                            BorderThickness="0,1,0,1"
                            cs:Name="Border_0005"
                            cs:Office2010Blue.BorderBrush="#7FFFFFFF"
                            cs:Office2010Silver.BorderBrush="#7FFFFFFF"
                           />
                </Border>
            </Grid>
            <ContentPresenter Margin="2,2,1,1"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=SizeGripTemplate}" TargetType="{x:Type Thumb}">
        <Grid Background="Transparent"
              cs:Name="Grid_0001"
              Grid.Row="0"
              Grid.RowSpan="2"
              Height="10"
              HorizontalAlignment="Right"
              Margin="2"
              VerticalAlignment="Bottom"
              Width="10"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Path cs:Name="Path_0001"
                  Data="F1 M 0,8.99998L 2,8.99998L 2,11L 0,11L 0,8.99998 Z M 3.99997,8.99999L 5.99997,8.99999L 5.99997,11L 3.99997,11L 3.99997,8.99999 Z M 7.99998,8.99999L 9.99998,8.99999L 9.99998,11L 7.99998,11L 7.99998,8.99999 Z M 7.99998,4.99999L 9.99998,4.99999L 9.99998,6.99999L 7.99998,6.99999L 7.99998,4.99999 Z M 7.99999,0.999989L 9.99999,0.999989L 9.99999,2.99998L 7.99999,2.99998L 7.99999,0.999989 Z M 4,4.99998L 6,4.99998L 6,6.99998L 4,6.99998L 4,4.99998 Z "
                  Fill="#3FFFFFFF"
                  Margin="1,1,-1,-1"
                  Stretch="Fill"
                  cs:Office2010Blue.Fill="#3FFFFFFF"
                  cs:Office2010Silver.Fill="#3FFFFFFF"
                 />
            <Path cs:Name="Path_0002"
                  Data="F1 M 0,7.99999L 2,7.99999L 2,9.99999L 0,9.99999L 0,7.99999 Z M 3.99997,8L 5.99997,8L 5.99997,10L 3.99997,10L 3.99997,8 Z M 7.99998,8L 9.99998,8L 9.99998,10L 7.99998,10L 7.99998,8 Z M 7.99998,4L 9.99998,4L 9.99998,6L 7.99998,6L 7.99998,4 Z M 7.99999,0L 9.99999,0L 9.99999,2L 7.99999,2L 7.99999,0 Z M 4,4L 6,4L 6,6L 4,6L 4,4 Z "
                  Fill="#FF000000"
                  Stretch="Fill"
                  cs:Office2010Blue.Fill="#FF515D7A"
                  cs:Office2010Silver.Fill="#FF80838F"
                 />
        </Grid>
    </ControlTemplate>

    <sys:Double x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=MinHeight}">26</sys:Double>

</ResourceDictionary>