    <ControlTemplate x:Key="{dxrt:InRibbonGalleryThemeKey ResourceKey=GalleryItemGlyphBorderTemplate}" TargetType="{x:Type dxb:ItemBorderControl}">
        <Border Name="Root"
                BorderBrush="Transparent"
                cs:Name="Border_0001"
                cs:Office2010Blue.BorderBrush="Transparent"
                cs:Office2010Silver.BorderBrush="Transparent"
               >
            <ContentPresenter/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="State" Value="Checked">
                <Setter cs:Name="Setter_0001"
                        Property="BorderBrush"
                        TargetName="Root"
                        Value="#FFDFFFFC"
                        cs:Office2010Blue.Value="#FFDFFFFC"
                        cs:Office2010Silver.Value="#FFDFFFFC"
                       />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

