              SnapsToDevicePixels="True"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Grid x:Name="PART_Shadow">
                <Border Background="#3F000000"
                        CornerRadius="1,1,0,0"
                        cs:Name="Border_0001"
                        Margin="-1,2,-1,1"
                        Opacity="0.10"
                        cs:Office2010Blue.Background="#3F000000"
                        cs:Office2010Silver.Background="#3F000000"
                       />
            </Grid>
            <Grid x:Name="PART_NormalChecked" Opacity="0">
                <Border Background="#FF5E5E5E"
                        CornerRadius="2,2,0,0"
                        cs:Name="Border_0002"
                        cs:Office2010Blue.Background="#FF94ADCC"
                        cs:Office2010Silver.Background="#FFA5ACB5"
                       >
                    <Border CornerRadius="1,1,0,0" Margin="1,1,1,0">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0638"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFCFCFCF"
                                              cs:Name="GradientStop_0001"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFEFF5FB"
                                              cs:Office2010Silver.Color="#FFFDFEFF"
                                             />
