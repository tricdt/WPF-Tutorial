                            cs:Name="Border_0004"
                            Margin="1,1,1,0"
                            cs:Office2010Blue.Background="#FFFFFFFF"
                            cs:Office2010Silver.Background="#FFFAFBFC"
                           >
                        <Border Margin="1,1,1,0">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_0639"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FF949494"
                                                  cs:Name="GradientStop_0003"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFE1EDFA"
                                                  cs:Office2010Silver.Color="#FFF3F4F5"
                                                 />
