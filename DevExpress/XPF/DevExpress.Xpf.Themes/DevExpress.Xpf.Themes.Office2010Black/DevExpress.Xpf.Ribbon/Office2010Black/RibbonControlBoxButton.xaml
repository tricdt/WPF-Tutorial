    <!-- RIBBON HEADER BUTTONS -->

    <!--CloseButtonGlyphTemplate-->

    <DataTemplate x:Key="{dxt:RibbonHeaderItemsThemeKey ResourceKey=RibbonCloseButtonGlyph}">
        <Grid x:Name="Glyph"
              Height="9"
              HorizontalAlignment="Center"
              VerticalAlignment="Center"
              Width="11">
            <Path x:Name="Path1"
                  cs:Name="Path_0015"
                  Data="F1 M 137,170L 140,166.5L 137,163L 137,162L 141,162L 142.5,163.583L 144,162L 148,162L 148,163L 145,166.5L 148,170L 148,171L 144,171L 142.5,169.417L 141,171L 137,171L 137,170 Z"
                  Fill="#FF4C4C4C"
                  Stretch="Fill"
                  cs:Office2010Blue.Fill="#FF4C4C4C"
                  cs:Office2010Silver.Fill="#FF4C4C4C"
                 />
            <Path x:Name="Path2"
                  cs:Name="Path_0016"
                  Data="F1 M 137,170L 140,166.5L 137,163L 137,162L 141,162L 142.5,163.583L 144,162L 148,162L 148,163L 145,166.5L 148,170L 148,171L 144,171L 142.5,169.417L 141,171L 137,171L 137,170 Z"
                  Fill="#FF4C4C4C"
                  Stretch="Fill"
                  cs:Office2010Blue.Fill="#FF4C4C4C"
                  cs:Office2010Silver.Fill="#FF4C4C4C"
                 />
            <Path x:Name="Path3"
                  Data="F1 M 112,158L 115,154.5L 112,151L 115,151L 116.5,152.75L 118,151L 121,151L 118,154.5L 121,158L 118,158L 116.5,156.25L 115,158L 112,158 Z"
                  Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}"
                  Margin="1"
                  Stretch="Fill"/>
        </Grid>
    </DataTemplate>

