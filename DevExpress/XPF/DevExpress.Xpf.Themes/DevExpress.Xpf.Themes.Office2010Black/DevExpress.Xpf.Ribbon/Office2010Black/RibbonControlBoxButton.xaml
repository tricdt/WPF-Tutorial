                  Margin="0,3,4,0"
                  Stretch="Fill"
                  cs:Office2010Blue.Fill="#FF5C5C5C"
                  cs:Office2010Silver.Fill="#FF5C5C5C"
                 />
            <Path cs:Name="Path_0019"
                  Data="F1 M 64,45L 70,45L 70,51L 64,51L 64,45 Z M 69,50L 69,47L 65,47L 65,50L 69,50 Z"
                  Fill="#FFF0F0F0"
                  Margin="1,4,5,1"
                  Stretch="Fill"
                  cs:Office2010Blue.Fill="#FFF0F0F0"
                  cs:Office2010Silver.Fill="#FFF0F0F0"
                 />
        </Grid>
    </DataTemplate>

    <!--MinimizeButtonGlyphTemplate-->

    <DataTemplate x:Key="{dxt:RibbonHeaderItemsThemeKey ResourceKey=RibbonMinimizeButtonGlyph}">
        <Grid x:Name="Glyph"
              Height="9"
              HorizontalAlignment="Center"
              VerticalAlignment="Center"
              Width="11">
            <Rectangle cs:Name="Rectangle_0001"
                       Fill="#FF5C5C5C"
                       Height="5"
                       RadiusX="1"
                       RadiusY="1"
                       StrokeThickness="0"
                       VerticalAlignment="Bottom"
                       Width="11"
                       cs:Office2010Blue.Fill="#FF5C5C5C"
                       cs:Office2010Silver.Fill="#FF5C5C5C"
                      />
            <Rectangle Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}"
                       Height="3"
                       Margin="0,0,0,1"
                       StrokeThickness="0"
                       VerticalAlignment="Bottom"
                       Width="9"/>
        </Grid>
    </DataTemplate>

    <!--MaximizeButtonGlyphTemplate-->

    <DataTemplate x:Key="{dxt:RibbonHeaderItemsThemeKey ResourceKey=RibbonMaximizeButtonGlyph}">
        <Grid x:Name="Glyph"
              Height="9"
              HorizontalAlignment="Center"
              VerticalAlignment="Center"
              Width="11">
            <Rectangle cs:Name="Rectangle_0002"
                       Fill="#FF5C5C5C"
                       Height="9"
                       RadiusX="1"
                       RadiusY="1"
                       StrokeThickness="0"
                       VerticalAlignment="Bottom"
                       Width="11"
                       cs:Office2010Blue.Fill="#FF5C5C5C"
                       cs:Office2010Silver.Fill="#FF5C5C5C"
                      />
            <Rectangle Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}"
                       Height="7"
                       Margin="0,0,0,1"
                       StrokeThickness="0"
                       VerticalAlignment="Bottom"
                       Width="9"/>
            <Rectangle cs:Name="Rectangle_0003"
                       Fill="#FF5C5C5C"
                       Height="3"
                       Margin="3"
                       StrokeThickness="0"
                       VerticalAlignment="Bottom"
                       Width="5"
                       cs:Office2010Blue.Fill="#FF5C5C5C"
                       cs:Office2010Silver.Fill="#FF5C5C5C"
                      />
            <Rectangle cs:Name="Rectangle_0004"
                       Fill="#FF838383"
                       Height="1"
                       Margin="4"
                       StrokeThickness="0"
                       Width="3"
                       cs:Office2010Blue.Fill="#FFCFDDEE"
                       cs:Office2010Silver.Fill="#FFE9EDF1"
                      />
        </Grid>
    </DataTemplate>

