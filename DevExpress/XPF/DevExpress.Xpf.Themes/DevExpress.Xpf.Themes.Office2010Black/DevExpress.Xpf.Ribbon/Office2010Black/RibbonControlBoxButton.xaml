    <!--Ribbon HeaderDefaultButton MouseOver Background-->

    <ControlTemplate x:Key="{dxt:RibbonHeaderItemsThemeKey ResourceKey=RibbonDefaultButtonBackgroundMouseOverState}">
        <Grid x:Name="HoverState">
            <Border Background="#FF515151"
                    CornerRadius="2"
                    cs:Name="Border_0021"
                    cs:Office2010Blue.Background="#FF94ADCC"
                    cs:Office2010Silver.Background="#FFA5ACB5"
                   >
                <Border CornerRadius="1" Margin="1">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_6460"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FF949494"
                                          cs:Name="GradientStop_0107"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFD4E8FF"
                                          cs:Office2010Silver.Color="#FFFFFFFF"
                                         />
                            <GradientStop Color="#FF777777"
                                          cs:Name="GradientStop_0108"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFBDD0E8"
                                          cs:Office2010Silver.Color="#FFE3E6E8"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
            </Border>
        </Grid>
    </ControlTemplate>

