                  Margin="4,0,0,3"
                  Stretch="Fill"
                  cs:Office2010Blue.Fill="#FF5C5C5C"
                  cs:Office2010Silver.Fill="#FF5C5C5C"
                 />
            <Path Data="F1 M 68,42L 74,42L 74,48L 68,48L 68,42 Z M 73,47L 73,44L 69,44L 69,47L 73,47 Z"
                  Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}"
                  Margin="5,1,1,4"
                  Stretch="Fill"/>
            <Path cs:Name="Path_0018"
                  Data="F1 M 63,45L 64,44L 70,44L 71,45L 71,51L 70,52L 64,52L 63,51L 63,45 Z M 66,48L 66,49L 68,49L 68,48L 66,48 Z"
