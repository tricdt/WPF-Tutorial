<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
                    xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <dx:ColorOverlayConverter x:Key="colorOverlayConverter"/>

    <Style x:Key="{dxrt:RibbonThemeKey ResourceKey=NormalPageCategoryTextStyle}" TargetType="{x:Type ContentControl}">
        <Setter cs:Name="Setter_0001"
                Property="Foreground"
                Value="#FFFFFFFF"
                cs:Office2010Blue.Value="#FF1E395B"
                cs:Office2010Silver.Value="#FF3C3C3C"
               />
    </Style>

    <Style x:Key="{dxrt:RibbonThemeKey ResourceKey=SelectedPageCategoryTextStyle}" TargetType="{x:Type ContentControl}">
        <Setter cs:Name="Setter_0002"
                Property="Foreground"
                Value="#FFFFFFFF"
                cs:Office2010Blue.Value="#FF1E395B"
                cs:Office2010Silver.Value="#FF3C3C3C"
               />
    </Style>

    <ControlTemplate x:Key="{dxrt:RibbonPageCategoryThemeKey ResourceKey=HeaderBackgroundTemplate}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
        <Grid Margin="1,0,1,0" SnapsToDevicePixels="True">
            <Grid x:Name="Grid" Opacity="0.9">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="2"/>
                </Grid.ColumnDefinitions>
                <Border Grid.Column="1" Opacity="0.35">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0670"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#FF707070}" Offset="0"/>
                            <GradientStop Color="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#FF707070}" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
                <Border Background="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#FF707070}"
                        Grid.Column="1"
                        Height="4"
                        VerticalAlignment="Top"/>
                <Border Background="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#FF707070}"
                        BorderBrush="#7FA0A0A0"
                        BorderThickness="1,0,0,0"
                        cs:Name="Border_0001"
                        Grid.Column="0"
                        cs:Office2010Blue.BorderBrush="#FFFFFFFF"
                        cs:Office2010Silver.BorderBrush="#FFFFFFFF"
                       />
                <Border Background="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#FF707070}"
                        BorderBrush="#7FA0A0A0"
                        BorderThickness="0,0,1,0"
                        cs:Name="Border_0002"
                        Grid.Column="2"
                        cs:Office2010Blue.BorderBrush="#FFFFFFFF"
                        cs:Office2010Silver.BorderBrush="#FFFFFFFF"
                       />
                <ContentPresenter x:Name="Content"
                                  Grid.Column="1"
                                  Margin="4,0,4,0"/>
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="Placement">
                    <VisualState x:Name="RibbonWindow">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Grid" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0,-1,0,0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Content" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>4,0,4,0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Standalone">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Grid" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0,0,0,0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Content" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>4,0,4,1</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonPageCategoryThemeKey ResourceKey=HeadersBackgroundTemplate}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}"/>

    <ControlTemplate x:Key="{dxrt:RibbonPageCategoryThemeKey ResourceKey=HighlightedHeadersBackgroundTemplate}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
        <Grid Margin="1,0,1,0" SnapsToDevicePixels="True">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="2"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="2"/>
            </Grid.ColumnDefinitions>
            <Grid.OpacityMask>
                <LinearGradientBrush cs:Name="LinearGradientBrush_0671"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0"
                                    >
                    <GradientStop Color="#FF000000"
                                  cs:Name="GradientStop_0001"
                                  Offset="0"
                                  cs:Office2010Blue.Color="#FF000000"
                                  cs:Office2010Silver.Color="#FF000000"
                                 />
                    <GradientStop Color="#00000000"
                                  cs:Name="GradientStop_0002"
                                  Offset="1"
                                  cs:Office2010Blue.Color="#00000000"
                                  cs:Office2010Silver.Color="#00000000"
                                 />
                </LinearGradientBrush>
            </Grid.OpacityMask>
            <Border Background="{Binding Converter={StaticResource colorOverlayConverter}, ConverterParameter=#FF707070}"
                    Grid.Column="1"
                    Opacity="0.35"/>
            <Border Background="{Binding Converter={StaticResource colorOverlayConverter}, ConverterParameter=#FF707070}"
                    BorderBrush="#7FA0A0A0"
                    BorderThickness="1,0,0,0"
                    cs:Name="Border_0003"
                    Grid.Column="0"
                    cs:Office2010Blue.BorderBrush="#FFFFFFFF"
                    cs:Office2010Silver.BorderBrush="#FFFFFFFF"
                   />
            <Border Background="{Binding Converter={StaticResource colorOverlayConverter}, ConverterParameter=#FF707070}"
                    BorderBrush="#7FA0A0A0"
                    BorderThickness="0,0,1,0"
                    cs:Name="Border_0004"
                    Grid.Column="2"
                    cs:Office2010Blue.BorderBrush="#FFFFFFFF"
                    cs:Office2010Silver.BorderBrush="#FFFFFFFF"
                   />
        </Grid>
    </ControlTemplate>

    <Style x:Key="{dxrt:RibbonPageCategoryThemeKey ResourceKey=HeadersPresenterStyle}" TargetType="{x:Type ItemsPresenter}"/>

    <Style x:Key="{dxrt:RibbonThemeKey ResourceKey=NormalPageCategoryTextStyleInAeroWindow}" TargetType="{x:Type ContentControl}">
        <Setter cs:Name="Setter_0003"
                Property="Foreground"
                Value="#FFFFFFFF"
                cs:Office2010Blue.Value="#FF1E395B"
                cs:Office2010Silver.Value="#FF3C3C3C"
               />
        <Setter Property="Margin" Value="5,3,5,0"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
    </Style>

    <Style x:Key="{dxrt:RibbonThemeKey ResourceKey=SelectedPageCategoryTextStyleInAeroWindow}" TargetType="{x:Type ContentControl}">
        <Setter cs:Name="Setter_0004"
                Property="Foreground"
                Value="#FFFFFFFF"
                cs:Office2010Blue.Value="#FF1E395B"
                cs:Office2010Silver.Value="#FF3C3C3C"
               />
        <Setter Property="Margin" Value="5,3,5,0"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
    </Style>

    <ControlTemplate x:Key="{dxrt:RibbonPageCategoryThemeKey ResourceKey=HeaderBackgroundTemplateInAeroWindow}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
        <Grid SnapsToDevicePixels="True">
            <Grid x:Name="Grid">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="1"/>
                </Grid.ColumnDefinitions>
                <Border Grid.Column="0" SnapsToDevicePixels="True">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0672"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#DD707070}"/>
                            <GradientStop Color="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#DD707070}" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
                <Border Grid.Column="2" SnapsToDevicePixels="True">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0673"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#DD707070}"/>
                            <GradientStop Color="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#DD707070}" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
                <Grid Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Border Height="4">
                        <Border.Background>
                            <SolidColorBrush Color="{TemplateBinding Color}"/>
                        </Border.Background>
                    </Border>
                    <Border Grid.Row="1" SnapsToDevicePixels="True">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0674"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#807C7C7C}"/>
                                <GradientStop Color="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#997C7C7C}" Offset="1"/>
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Grid>
                <ContentPresenter x:Name="Content"
                                  Grid.Column="1"
                                  Margin="0,0,0,1"
                                  SnapsToDevicePixels="True"/>
            </Grid>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonPageCategoryThemeKey ResourceKey=HeadersBackgroundTemplateInAeroWindow}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}"></ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonPageCategoryThemeKey ResourceKey=HighlightedHeadersBackgroundTemplateInAeroWindow}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="1"/>
            </Grid.ColumnDefinitions>
            <Border Grid.Column="0" SnapsToDevicePixels="True">
                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0675"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="{Binding Converter={StaticResource colorOverlayConverter}, ConverterParameter=#007C7C7C}" Offset="1"/>
                        <GradientStop Color="{Binding Converter={StaticResource colorOverlayConverter}, ConverterParameter=#CC707070}" Offset="0"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border Grid.Column="2" SnapsToDevicePixels="True">
                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0676"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="{Binding Converter={StaticResource colorOverlayConverter}, ConverterParameter=#007C7C7C}" Offset="1"/>
                        <GradientStop Color="{Binding Converter={StaticResource colorOverlayConverter}, ConverterParameter=#CC707070}" Offset="0"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border Grid.Column="1" SnapsToDevicePixels="True">
                <Border.Background>
                    <LinearGradientBrush cs:Name="LinearGradientBrush_0677"
                                         EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                        >
                        <GradientStop Color="{Binding Converter={StaticResource colorOverlayConverter}, ConverterParameter=#007C7C7C}" Offset="1"/>
                        <GradientStop Color="{Binding Converter={StaticResource colorOverlayConverter}, ConverterParameter=#997C7C7C}" Offset="0"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="CollapseButtonTemplate" TargetType="{x:Type ToggleButton}">
        <Grid Background="Transparent"
              cs:Name="Grid_0001"
              Margin="0,0,0,-1"
              SnapsToDevicePixels="True"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Grid x:Name="PART_Hover" Opacity="0">
                <Border Background="#FF5E5E5E"
                        CornerRadius="2,2,0,0"
                        cs:Name="Border_0005"
                        Margin="0,0,0,1"
                        cs:Office2010Blue.Background="#FF7FA0CF"
                        cs:Office2010Silver.Background="#FF7FA0CF"
                       >
                    <Border Background="#FFB8B8B8"
                            CornerRadius="1,1,0,0"
                            cs:Name="Border_0006"
                            Margin="1,1,1,0"
                            cs:Office2010Blue.Background="#FFB8B8B8"
                            cs:Office2010Silver.Background="#FFB8B8B8"
                           >
                        <Border Margin="1,1,1,0">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_1608"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FF949494"
                                                  cs:Name="GradientStop_0003"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FF949494"
                                                  cs:Office2010Silver.Color="#FF949494"
                                                 />
                                    <GradientStop Color="#FF7F7F7F"
                                                  cs:Name="GradientStop_0004"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FF7F7F7F"
                                                  cs:Office2010Silver.Color="#FF7F7F7F"
                                                 />
                                    <GradientStop Color="#FF7F7F7F"
                                                  cs:Name="GradientStop_0005"
                                                  Offset="0.25"
                                                  cs:Office2010Blue.Color="#FF7F7F7F"
                                                  cs:Office2010Silver.Color="#FF7F7F7F"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PART_Checked" Opacity="0">
                <Border Background="#FF5E5E5E"
                        CornerRadius="2,2,0,0"
                        cs:Name="Border_3002"
                        cs:Office2010Blue.Background="#FF5E5E5E"
                        cs:Office2010Silver.Background="#FF5E5E5E"
                       >
                    <Border CornerRadius="1,1,0,0" Margin="1,1,1,0">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_1609"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFFFFF"
                                              cs:Name="GradientStop_0006"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFFFFF"
                                              cs:Office2010Silver.Color="#FFFFFFFF"
                                             />
                                <GradientStop Color="#FFFFFFFF"
                                              cs:Name="GradientStop_0007"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFFFFFF"
                                              cs:Office2010Silver.Color="#FFFFFFFF"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Grid Margin="3,0,3,-3">
                <Path cs:Name="Path_0001"
                      Data="F1 M 6,12L 10,8L 9,7L 6,10L 3,7L 2,8L 6,12 Z "
                      Fill="#FF3C3C3C"
                      Height="5"
                      Margin="6"
                      Stretch="Fill"
                      Width="8"
                      cs:Office2010Blue.Fill="#FF3C3C3C"
                      cs:Office2010Silver.Fill="#FF3C3C3C"
                     />
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity"
                        TargetName="PART_Hover"
                        Value="1"/>
            </Trigger>
            <Trigger Property="ToggleButton.IsChecked" Value="True">
                <Setter Property="Opacity"
                        TargetName="PART_Checked"
                        Value="1"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="{dxrt:RibbonPageCategoryThemeKey ResourceKey=CollapseButtonStyle}" TargetType="{x:Type ToggleButton}">
        <Setter Property="Content" Value="V"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template" Value="{StaticResource CollapseButtonTemplate}"/>
        <Setter Property="Margin" Value="0,2,0,0"/>
    </Style>

    <Style x:Key="{dxrt:RibbonPageCategoryThemeKey ResourceKey=AeroCollapseButtonStyle}" TargetType="{x:Type ToggleButton}">
        <Setter Property="Content" Value="V"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template" Value="{StaticResource CollapseButtonTemplate}"/>
        <Setter Property="Margin" Value="0,2,0,0"/>
    </Style>

</ResourceDictionary>