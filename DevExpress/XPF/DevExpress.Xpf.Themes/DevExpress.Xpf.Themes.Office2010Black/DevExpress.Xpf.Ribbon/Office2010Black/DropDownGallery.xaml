<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
                    xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=SizeGripContainerTemplate}" TargetType="{x:Type ContentControl}">
        <Border BorderBrush="#FFD5D5D5"
                BorderThickness="0,1,0,0"
                cs:Name="Border_0001"
                Height="16"
                SnapsToDevicePixels="True"
                cs:Office2010Blue.BorderBrush="#FFB1C0D6"
                cs:Office2010Silver.BorderBrush="#FFF3F4F5"
               >
            <Border.Background>
                <LinearGradientBrush cs:Name="LinearGradientBrush_0535"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0"
                                    >
                    <GradientStop Color="#FFE1E1E1"
                                  cs:Name="GradientStop_0001"
                                  Offset="0"
                                  cs:Office2010Blue.Color="#FFECF5FD"
                                  cs:Office2010Silver.Color="#FFF3F4F5"
                                 />
                    <GradientStop Color="#FFC4C4C4"
                                  cs:Name="GradientStop_0002"
                                  Offset="1"
                                  cs:Office2010Blue.Color="#FFD5E5F6"
                                  cs:Office2010Silver.Color="#FFE3E5E8"
                                 />
                </LinearGradientBrush>
            </Border.Background>
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <Style x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryScrollBarContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Margin" Value="1,0,0,0"/>
    </Style>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=MenuBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border BorderBrush="#FFC3C3C3"
                BorderThickness="0,1,0,0"
                cs:Name="Border_0002"
                cs:Office2010Blue.BorderBrush="#FFCFDDEE"
                cs:Office2010Silver.BorderBrush="#FFE9EDF1"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=MenuGlyphSideBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                cs:Name="Border_0003"
                cs:Office2010Blue.Background="#FFFFFFFF"
                cs:Office2010Silver.Background="#FFFFFFFF"
               />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=MenuContentSideBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                cs:Name="Border_0004"
                cs:Office2010Blue.Background="#FFFFFFFF"
                cs:Office2010Silver.Background="#FFFFFFFF"
               />
    </ControlTemplate>

