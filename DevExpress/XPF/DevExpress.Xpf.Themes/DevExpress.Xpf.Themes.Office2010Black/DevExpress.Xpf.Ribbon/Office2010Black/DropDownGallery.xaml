    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                cs:Name="Border_0006"
                cs:Office2010Blue.Background="#FFFFFFFF"
                cs:Office2010Silver.Background="#FFFFFFFF"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryGroupsBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <ContentPresenter/>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryCaptionTemplate}" TargetType="{x:Type ToggleButton}">
        <Border BorderBrush="#FFFFFFFF"
                BorderThickness="1,1,1,0"
                cs:Name="Border_0007"
                cs:Office2010Blue.BorderBrush="#FFFFFFFF"
                cs:Office2010Silver.BorderBrush="#FFFFFFFF"
               >
            <Border.Background>
                <LinearGradientBrush cs:Name="LinearGradientBrush_0536"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0"
                                    >
                    <GradientStop Color="#FFD6D6D6"
                                  cs:Name="GradientStop_0003"
                                  Offset="0"
                                  cs:Office2010Blue.Color="#FFE1EDFA"
                                  cs:Office2010Silver.Color="#FFF3F4F5"
                                 />
                    <GradientStop Color="#FFC0C0C0"
                                  cs:Name="GradientStop_0004"
                                  Offset="1"
                                  cs:Office2010Blue.Color="#FFD3E5F7"
                                  cs:Office2010Silver.Color="#FFDFE5EC"
                                 />
                </LinearGradientBrush>
            </Border.Background>
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryItemBorderTemplate}" TargetType="{x:Type dxb:ItemBorderControl}">
        <Grid Background="Transparent"
              cs:Name="Grid_0001"
              SnapsToDevicePixels="True"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <Grid x:Name="HoverState" Opacity="0">
                <Border CornerRadius="2" Padding="1">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0537"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FFF7C840"
                                          cs:Name="GradientStop_0005"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFECC757"
                                          cs:Office2010Silver.Color="#FFECC757"
                                         />
                            <GradientStop Color="#FFF8D427"
                                          cs:Name="GradientStop_0006"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFF1D449"
                                          cs:Office2010Silver.Color="#FFF1D449"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="1" Padding="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0538"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFFAE6"
                                              cs:Name="GradientStop_0007"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFCF7E4"
                                              cs:Office2010Silver.Color="#FFFCF7E4"
                                             />
                                <GradientStop Color="#FFFFFFED"
                                              cs:Name="GradientStop_0008"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFCF8E7"
                                              cs:Office2010Silver.Color="#FFFCF8E7"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="1">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_0539"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFFFEEA8"
                                                  cs:Name="GradientStop_0009"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFF8EBB2"
                                                  cs:Office2010Silver.Color="#FFF8EBB2"
                                                 />
                                    <GradientStop Color="#FFFFFCE0"
                                                  cs:Name="GradientStop_0010"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFCF7E4"
                                                  cs:Office2010Silver.Color="#FFFCF7E4"
                                                 />
                                    <GradientStop Color="#FFFFE277"
                                                  cs:Name="GradientStop_0011"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFF8E088"
                                                  cs:Office2010Silver.Color="#FFF8E088"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border CornerRadius="2" Padding="1">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0540"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FFC28F36"
                                          cs:Name="GradientStop_0012"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFC28E35"
                                          cs:Office2010Silver.Color="#FFC28E35"
                                         />
                            <GradientStop Color="#FFC2A34C"
                                          cs:Name="GradientStop_0013"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFBF7E3B"
                                          cs:Office2010Silver.Color="#FFBF7E3B"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0541"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFE575"
                                              cs:Name="GradientStop_0014"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFE575"
                                              cs:Office2010Silver.Color="#FFFFE575"
                                             />
                                <GradientStop Color="#FFFFF37F"
                                              cs:Name="GradientStop_0015"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFF8EBB2"
                                              cs:Office2010Silver.Color="#FFF8EBB2"
                                             />
                                <GradientStop Color="#FFFFD86C"
                                              cs:Name="GradientStop_0016"
                                              Offset="0.5"
                                              cs:Office2010Blue.Color="#FFFFD86C"
                                              cs:Office2010Silver.Color="#FFFFD86C"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <ContentPresenter/>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="State">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed"/>
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="HoverChecked">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryItemGlyphBorderTemplate}" TargetType="{x:Type dxb:ItemBorderControl}">
        <Border Name="Root"
                BorderBrush="Transparent"
                cs:Name="Border_0008"
                cs:Office2010Blue.BorderBrush="Transparent"
                cs:Office2010Silver.BorderBrush="Transparent"
               >
            <ContentPresenter/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="State" Value="Checked">
                <Setter cs:Name="Setter_0001"
                        Property="BorderBrush"
                        TargetName="Root"
                        Value="#FFFFFCDF"
                        cs:Office2010Blue.Value="#FFFCF1C2"
                        cs:Office2010Silver.Value="#FFFCF1C2"
                       />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=PopupBorderTemplate}" TargetType="{x:Type dxr:GalleryDropDownPopupBorderControl}">
        <Grid x:Name="Root" SnapsToDevicePixels="True">
            <Border x:Name="Shadow"
                    Background="#FF000000"
                    cs:Name="Border_0009"
                    Margin="2,2,0,0"
                    Opacity="0.12"
                    Visibility="Collapsed"
                    cs:Office2010Blue.Background="#FF000000"
                    cs:Office2010Silver.Background="#FF000000"
                   />
            <Border x:Name="Border"
                    Background="#FF484848"
                    cs:Name="Border_0010"
                    Padding="1"
                    cs:Office2010Blue.Background="#FF94ADCC"
                    cs:Office2010Silver.Background="#FFA5ACB5"
                   >
                <ContentPresenter Content="{TemplateBinding Content}"/>
            </Border>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="AppType">
                    <VisualState x:Name="Standalone">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Shadow" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0,0,3,3</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border" Storyboard.TargetProperty="CornerRadius">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <CornerRadius>0</CornerRadius>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Root" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0,0,3,3</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="BrowserHosted"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

</ResourceDictionary>