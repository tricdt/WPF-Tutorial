<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys"
                    xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxpdf="http://schemas.devexpress.com/winfx/2008/xaml/pdf"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:themes="http://schemas.devexpress.com/winfx/2008/xaml/pdf/themekeys"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/DevExpress.Xpf.Themes.Office2010Black.v24.1;component/DevExpress.Xpf.PdfViewer/Generic/Templates.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfCommentViewerSelectionBrush}"
                     Color="#FFFDF6DB"
                     cs:Office2010Blue.Color="#FFFCF1C2"
                     cs:Office2010Silver.Color="#FFFCF1C2"/>

    <SolidColorBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfCommentViewerCommentDateForegroundBrush}" Color="#8A000000"/>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=HighlightSearchTextColor}"
           cs:Name="Color_0001"
           cs:Office2010Blue.Color="#596098C0"
           cs:Office2010Silver.Color="#596098C0">
        #3F0133AF
    </Color>

    <SolidColorBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=HighlightSelectionColor}"
                     Color="#0133AF"
                     Opacity="0.25"
                     cs:Office2010Blue.Color="#0133AF"
                     cs:Office2010Silver.Color="#0133AF"/>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=CaretColor}"
           cs:Name="Color_0007"
           cs:Office2010Blue.Color="#FF000000"
           cs:Office2010Silver.Color="#FF000000">
        #FF000000
    </Color>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenCaptionForegroundColor}"
           cs:Name="Color_0002"
           cs:Office2010Blue.Color="#BFFFFFFF"
           cs:Office2010Silver.Color="#BFFFFFFF">
        #BFFFFFFF
    </Color>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenDocumentLinkForegroundColor}"
           cs:Name="Color_0003"
           cs:Office2010Blue.Color="#BFFFE277"
           cs:Office2010Silver.Color="#BFFFE277">
        #BFFFE277
    </Color>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenOpenDocumentForegroundColor}"
           cs:Name="Color_0004"
           cs:Office2010Blue.Color="#BFFFFFFF"
           cs:Office2010Silver.Color="#BFFFFFFF">
        #BFFFFFFF
    </Color>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=LoadingDocumentLinkForegroundColor}"
           cs:Name="Color_0005"
           cs:Office2010Blue.Color="#FF0072C6"
           cs:Office2010Silver.Color="#FF0072C6">
        #FF0072C6
    </Color>

    <sys:Double x:Key="{themes:PdfViewerThemeKey ResourceKey=PrintDialogRightColumnWidth}">120</sys:Double>

    <GridLength x:Key="{themes:PdfViewerThemeKey ResourceKey=PrintDialogRightColumnGridLength}">120</GridLength>

    <LinearGradientBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPresenterControlBackgroundBrush}"
                         cs:Name="LinearGradientBrush_1491"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF6F6F6F"
                      cs:Name="GradientStop_0001"
                      Offset="0"
                      cs:Office2010Blue.Color="#FFD1E0EF"
                      cs:Office2010Silver.Color="#FFE3E6E8"
                     />
        <GradientStop Color="#FF3E3E3E"
                      cs:Name="GradientStop_0002"
                      Offset="1"
                      cs:Office2010Blue.Color="#FF7994B4"
                      cs:Office2010Silver.Color="#FFC0C4C9"
                     />
    </LinearGradientBrush>

    <ControlTemplate x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPageControlItemTemplate}" TargetType="{x:Type dxpdf:PdfPageControlItem}">
        <Grid>
            <Grid x:Name="Shadow">
                <Border BorderBrush="#FF000000"
                        BorderThickness="1"
                        cs:Name="Border_0001"
                        Margin="-1"
                        Opacity="0.05"
                        cs:Office2010Blue.BorderBrush="#FF000000"
                        cs:Office2010Silver.BorderBrush="#FF000000"
                       />
                <Border BorderBrush="#FF000000"
                        BorderThickness="1"
                        CornerRadius="1"
                        cs:Name="Border_0002"
                        Margin="-2"
                        Opacity="0.03"
                        cs:Office2010Blue.BorderBrush="#FF000000"
                        cs:Office2010Silver.BorderBrush="#FF000000"
                       />
                <Border BorderBrush="#FF000000"
                        BorderThickness="2"
                        CornerRadius="2"
                        cs:Name="Border_0003"
                        Margin="-3"
                        Opacity="0.03"
                        cs:Office2010Blue.BorderBrush="#FF000000"
                        cs:Office2010Silver.BorderBrush="#FF000000"
                       />
                <Border BorderBrush="#FF000000"
                        BorderThickness="3"
                        CornerRadius="3"
                        cs:Name="Border_0004"
                        Margin="-4"
                        Opacity="0.02"
                        cs:Office2010Blue.BorderBrush="#FF000000"
                        cs:Office2010Silver.BorderBrush="#FF000000"
                       />
                <Border BorderBrush="#FF000000"
                        BorderThickness="4"
                        CornerRadius="4"
                        cs:Name="Border_0005"
                        Margin="-5"
                        Opacity="0.02"
                        cs:Office2010Blue.BorderBrush="#FF000000"
                        cs:Office2010Silver.BorderBrush="#FF000000"
                       />
                <Border BorderBrush="#FF000000"
                        BorderThickness="5"
                        CornerRadius="5"
                        cs:Name="Border_0006"
                        Margin="-6"
                        Opacity="0.02"
                        cs:Office2010Blue.BorderBrush="#FF000000"
                        cs:Office2010Silver.BorderBrush="#FF000000"
                       />
                <Border BorderBrush="#FF000000"
                        BorderThickness="6"
                        CornerRadius="6"
                        cs:Name="Border_0007"
                        Margin="-7"
                        Opacity="0.01"
                        cs:Office2010Blue.BorderBrush="#FF000000"
                        cs:Office2010Silver.BorderBrush="#FF000000"
                       />
                <Border BorderBrush="#FF000000"
                        BorderThickness="7"
                        CornerRadius="7"
                        cs:Name="Border_0008"
                        Margin="-8"
                        Opacity="0.01"
                        cs:Office2010Blue.BorderBrush="#FF000000"
                        cs:Office2010Silver.BorderBrush="#FF000000"
                       />
                <Border BorderBrush="#FF000000"
                        BorderThickness="8"
                        CornerRadius="8"
                        cs:Name="Border_0009"
                        Margin="-9"
                        Opacity="0.01"
                        cs:Office2010Blue.BorderBrush="#FF000000"
                        cs:Office2010Silver.BorderBrush="#FF000000"
                       />
                <Border BorderBrush="#3F000000"
                        BorderThickness="1"
                        cs:Name="Border_0010"
                        Margin="-1"
                        cs:Office2010Blue.BorderBrush="#BF515D7A"
                        cs:Office2010Silver.BorderBrush="#26000000"
                       />
            </Grid>
            <Border x:Name="Selection"
                    BorderBrush="#FFFFE277"
                    BorderThickness="3"
                    cs:Name="Border_0011"
                    Margin="-1"
                    Opacity="0"
                    cs:Office2010Blue.BorderBrush="#FFFFE277"
                    cs:Office2010Silver.BorderBrush="#FFFFE277"
                   />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="true">
                <Setter Property="Opacity"
                        TargetName="Selection"
                        Value="1"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfThumbnailControlItemTemplate}" TargetType="{x:Type dxpdf:PdfThumbnailPageControlItem}">
        <Grid>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid x:Name="Shadow" Grid.Row="0">
                    <Border BorderBrush="#FF000000"
                            BorderThickness="5"
                            CornerRadius="5"
                            cs:Name="Border_0014"
                            Margin="-6"
                            Opacity="0.01"
                            cs:Office2010Blue.BorderBrush="#FF000000"
                            cs:Office2010Silver.BorderBrush="#FF000000"
                           />
                    <Border BorderBrush="#FF000000"
                            BorderThickness="4"
                            CornerRadius="4"
                            cs:Name="Border_0015"
                            Margin="-5"
                            Opacity="0.01"
                            cs:Office2010Blue.BorderBrush="#FF000000"
                            cs:Office2010Silver.BorderBrush="#FF000000"
                           />
                    <Border BorderBrush="#FF000000"
                            BorderThickness="3"
                            CornerRadius="3"
                            cs:Name="Border_0016"
                            Margin="-4"
                            Opacity="0.02"
                            cs:Office2010Blue.BorderBrush="#FF000000"
                            cs:Office2010Silver.BorderBrush="#FF000000"
                           />
                    <Border BorderBrush="#FF000000"
                            BorderThickness="2"
                            CornerRadius="2"
                            cs:Name="Border_0017"
                            Margin="-3"
                            Opacity="0.03"
                            cs:Office2010Blue.BorderBrush="#FF000000"
                            cs:Office2010Silver.BorderBrush="#FF000000"
                           />
                    <Border BorderBrush="#FF000000"
                            BorderThickness="1"
                            CornerRadius="1"
                            cs:Name="Border_0018"
                            Margin="-2"
                            Opacity="0.05"
                            cs:Office2010Blue.BorderBrush="#FF000000"
                            cs:Office2010Silver.BorderBrush="#FF000000"
                           />
                    <Border BorderBrush="#FFFFFFFF"
                            BorderThickness="1"
                            cs:Name="Border_0019"
                            Margin="-1"
                            cs:Office2010Blue.BorderBrush="#FFFFFFFF"
                            cs:Office2010Silver.BorderBrush="#FFFFFFFF"
                           />
                </Grid>
                <Border x:Name="Highlight"
                        BorderBrush="#3F0133AF"
                        BorderThickness="6"
                        cs:Name="Border_0021"
                        Grid.Row="0"
                        Margin="-7"
                        Opacity="0"
                        cs:Office2010Blue.BorderBrush="#3F0133AF"
                        cs:Office2010Silver.BorderBrush="#3F0133AF"
                       />
                <Border x:Name="Selection"
                        BorderBrush="#FFFFE277"
                        BorderThickness="2"
                        cs:Name="Border_0020"
                        Grid.Row="0"
                        Margin="-9"
                        Opacity="0"
                        cs:Office2010Blue.BorderBrush="#FFFFDC58"
                        cs:Office2010Silver.BorderBrush="#FFFFDC58"
                       />
                <TextBlock Grid.Row="1"
                           Margin="0,12,0,10"
                           Text="{Binding Path=PageNumber}"
                           TextAlignment="Center"/>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="true">
                <Setter Property="Opacity"
                        TargetName="Selection"
                        Value="1"/>
            </Trigger>
            <Trigger Property="IsHighlighted" Value="true">
                <Setter Property="Opacity"
                        TargetName="Highlight"
                        Value="1"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <DataTemplate x:Key="{themes:PdfViewerThemeKey ResourceKey=SelectionCanvasContentTemplate}">
        <Border BorderBrush="#FF007FFF"
                BorderThickness="1"
                cs:Name="Border_0013"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                cs:Office2010Blue.BorderBrush="#FF007FFF"
                cs:Office2010Silver.BorderBrush="#FF007FFF"
               >
            <Border.Background>
                <SolidColorBrush Color="#007FFF"
                                 Opacity="0.3"
                                 cs:Office2010Blue.Color="#007FFF"
                                 cs:Office2010Silver.Color="#007FFF"
                                />
            </Border.Background>
        </Border>
    </DataTemplate>

</ResourceDictionary>