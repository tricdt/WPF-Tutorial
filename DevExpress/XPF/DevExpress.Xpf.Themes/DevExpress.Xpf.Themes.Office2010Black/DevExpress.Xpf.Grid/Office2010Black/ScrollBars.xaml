<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                    xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2010Black.v24.1;component/DevExpress.Xpf.Grid/Office2010Black/Constants.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!--END WPF-->

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingCornerStyle}" TargetType="Border">
        <Setter Property="Background">
            <Setter.Value>
                <RadialGradientBrush Center="0,0"
                                     cs:Name="RadialGradientBrush_0006"
                                     GradientOrigin="0,0"
                                     RadiusX="1"
                                     RadiusY="1"
                                    >
                    <GradientStop Color="#FFBCBCBC"
                                  cs:Name="GradientStop_0001"
                                  cs:Office2010Blue.Color="#FFE1EDFA"
                                  cs:Office2010Silver.Color="#FFF3F4F5"
                                 />
                    <GradientStop Color="#FFCECECE"
                                  cs:Name="GradientStop_0002"
                                  Offset="1"
                                  cs:Office2010Blue.Color="#FFE1EDFA"
                                  cs:Office2010Silver.Color="#FFF3F4F5"
                                 />
                    <GradientStop Color="#FFC0C0C0"
                                  cs:Name="GradientStop_0003"
                                  Offset="0.35"
                                  cs:Office2010Blue.Color="#FFE1EDFA"
                                  cs:Office2010Silver.Color="#FFF3F4F5"
                                 />
                </RadialGradientBrush>
            </Setter.Value>
        </Setter>
        <!--BEGIN WPF-->
        <Style.Triggers>
            <Trigger Property="dxg:GridBorderHelper.ShowBorder" Value="True">
                <Setter cs:Name="Setter_0010"
                        Property="Background"
                        Value="#FF838383"
                        cs:Office2010Blue.Value="Transparent"
                        cs:Office2010Silver.Value="Transparent"
                       />
                <Setter Property="BorderBrush" Value="{StaticResource RowIndicatorBorderBrush}"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="dxg:GridBorderHelper.ShowBorder" Value="False"/>
                    <Condition Property="dxg:GridScrollBarHelper.HasFixedRightColumns" Value="True"/>
                    <Condition Property="dxg:GridScrollBarHelper.ExtendScrollBarToFixedColumns" Value="False"/>
                </MultiTrigger.Conditions>
                <MultiTrigger.Setters>
                    <Setter cs:Name="Setter_0016"
                            Property="Background"
                            Value="#FF838383"
                            cs:Office2010Blue.Value="Transparent"
                            cs:Office2010Silver.Value="Transparent"
                           />
                    <Setter Property="BorderBrush" Value="{StaticResource RowIndicatorBorderBrush}"/>
                    <Setter Property="BorderThickness" Value="0,1,0,0"/>
                </MultiTrigger.Setters>
            </MultiTrigger>
        </Style.Triggers>
        <!--END WPF-->
    </Style>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingCornerLeftStyle}" TargetType="Border">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter cs:Name="Setter_0011"
                Property="Background"
                Value="#FF838383"
                cs:Office2010Blue.Value="Transparent"
                cs:Office2010Silver.Value="Transparent"
               />
        <Setter Property="BorderBrush" Value="{StaticResource RowIndicatorBorderBrush}"/>
        <Setter Property="BorderThickness" Value="0,1,1,0"/>
        <!--BEGIN WPF-->
        <Style.Triggers>
            <Trigger Property="dx:ScrollBarExtensions.ScrollBarMode" Value="TouchOverlap">
                <Setter cs:Name="Setter_0001"
                        Property="Background"
                        Value="Transparent"
                        cs:Office2010Blue.Value="Transparent"
                        cs:Office2010Silver.Value="Transparent"
                       />
                <Setter cs:Name="Setter_0002"
                        Property="BorderBrush"
                        Value="Transparent"
                        cs:Office2010Blue.Value="Transparent"
                        cs:Office2010Silver.Value="Transparent"
                       />
            </Trigger>
        </Style.Triggers>
        <!--END WPF-->
    </Style>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedLeftThumbStyle}" TargetType="Border">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter cs:Name="Setter_0012"
                Property="Background"
                Value="#FF838383"
                cs:Office2010Blue.Value="Transparent"
                cs:Office2010Silver.Value="Transparent"
               />
        <Setter Property="BorderBrush" Value="{StaticResource RowIndicatorBorderBrush}"/>
        <Setter Property="BorderThickness" Value="0,1,0,0"/>
        <!--BEGIN WPF-->
        <Style.Triggers>
            <Trigger Property="dx:ScrollBarExtensions.ScrollBarMode" Value="TouchOverlap">
                <Setter cs:Name="Setter_0003"
                        Property="Background"
                        Value="Transparent"
                        cs:Office2010Blue.Value="Transparent"
                        cs:Office2010Silver.Value="Transparent"
                       />
                <Setter cs:Name="Setter_0004"
                        Property="BorderBrush"
                        Value="Transparent"
                        cs:Office2010Blue.Value="Transparent"
                        cs:Office2010Silver.Value="Transparent"
                       />
            </Trigger>
            <Trigger Property="dxg:GridBorderHelper.ShowBorder" Value="True">
                <Setter Property="BorderThickness" Value="0"/>
            </Trigger>
        </Style.Triggers>
        <!--END WPF-->
    </Style>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedRightThumbStyle}" TargetType="Border">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter cs:Name="Setter_0017"
                Property="Background"
                Value="#FF838383"
                cs:Office2010Blue.Value="Transparent"
                cs:Office2010Silver.Value="Transparent"
               />
        <Setter Property="BorderBrush" Value="{StaticResource RowIndicatorBorderBrush}"/>
        <Setter Property="BorderThickness" Value="0,1,0,0"/>
        <!--BEGIN WPF-->
        <Style.Triggers>
            <Trigger Property="dx:ScrollBarExtensions.ScrollBarMode" Value="TouchOverlap">
                <Setter cs:Name="Setter_0005"
                        Property="Background"
                        Value="Transparent"
                        cs:Office2010Blue.Value="Transparent"
                        cs:Office2010Silver.Value="Transparent"
                       />
                <Setter cs:Name="Setter_0006"
                        Property="BorderBrush"
                        Value="Transparent"
                        cs:Office2010Blue.Value="Transparent"
                        cs:Office2010Silver.Value="Transparent"
                       />
            </Trigger>
            <Trigger Property="dxg:GridBorderHelper.ShowBorder" Value="True">
                <Setter Property="BorderThickness" Value="0"/>
            </Trigger>
        </Style.Triggers>
        <!--END WPF-->
    </Style>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedLineStyle}" TargetType="Border">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter cs:Name="Setter_0014"
                Property="Background"
                Value="#FF838383"
                cs:Office2010Blue.Value="Transparent"
                cs:Office2010Silver.Value="Transparent"
               />
        <Setter Property="BorderBrush" Value="{StaticResource RowIndicatorBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1,1,1,0"/>
        <!--BEGIN WPF-->
        <Style.Triggers>
            <Trigger Property="dx:ScrollBarExtensions.ScrollBarMode" Value="TouchOverlap">
                <Setter cs:Name="Setter_0008"
                        Property="Background"
                        Value="Transparent"
                        cs:Office2010Blue.Value="Transparent"
                        cs:Office2010Silver.Value="Transparent"
                       />
                <Setter cs:Name="Setter_0009"
                        Property="BorderBrush"
                        Value="Transparent"
                        cs:Office2010Blue.Value="Transparent"
                        cs:Office2010Silver.Value="Transparent"
                       />
            </Trigger>
            <Trigger Property="dxg:GridBorderHelper.ShowBorder" Value="True">
                <Setter Property="BorderThickness" Value="1,0"/>
            </Trigger>
        </Style.Triggers>
        <!--END WPF-->
    </Style>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedLineContentTotalSummaryStyle}" TargetType="Border">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter cs:Name="Setter_0015"
                Property="Background"
                Value="#FF838383"
                cs:Office2010Blue.Value="Transparent"
                cs:Office2010Silver.Value="Transparent"
               />
        <Setter Property="BorderBrush" Value="{StaticResource RowIndicatorBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1,1,1,0"/>
    </Style>

    <DataTemplate x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedLineTotalSummaryTemplate}">
        <Border Style="{DynamicResource ResourceKey={dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedLineContentTotalSummaryStyle}}"/>
    </DataTemplate>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=FixedLeftLinePlaceHolderTotalSummaryStyle}" TargetType="ContentPresenter"></Style>

</ResourceDictionary>