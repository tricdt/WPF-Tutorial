<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:core="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:native="clr-namespace:DevExpress.Xpf.Printing.Native;assembly=DevExpress.Xpf.Printing.v24.1"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Printing.Native.PageBorderControl}" TargetType="{x:Type native:PageBorderControl}">
        <Style.Setters>
            <!--BEGIN WPF-->
            <Setter Property="SnapsToDevicePixels" Value="True"/>
            <!--END WPF-->
            <!--ONLY SL
            <Setter Property="UseLayoutRounding" Value="True"/>
             ONLY SL-->
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type native:PageBorderControl}">
                        <Grid>
                            <Grid.Resources>
                                <core:BoolToVisibilityConverter x:Key="boolToVisibilityConverter"/>
                            </Grid.Resources>
                            <Grid x:Name="pageShadow" Visibility="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=IsPageShadowVisible, Converter={StaticResource boolToVisibilityConverter}}">
                                <Border Background="#FF000000"
                                        cs:Name="Border_0001"
                                        Opacity="0.05"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="12"
                                        cs:Name="Border_0002"
                                        Margin="-1"
                                        Opacity="0.03"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="13"
                                        cs:Name="Border_0003"
                                        Margin="-2"
                                        Opacity="0.03"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="14"
                                        cs:Name="Border_0004"
                                        Margin="-3"
                                        Opacity="0.02"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="15"
                                        cs:Name="Border_0005"
                                        Margin="-4"
                                        Opacity="0.02"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="16"
                                        cs:Name="Border_0006"
                                        Margin="-5"
                                        Opacity="0.02"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="17"
                                        cs:Name="Border_0007"
                                        Margin="-6"
                                        Opacity="0.01"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="18"
                                        cs:Name="Border_0008"
                                        Margin="-7"
                                        Opacity="0.01"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="19"
                                        cs:Name="Border_0009"
                                        Margin="-8"
                                        Opacity="0.01"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="20"
                                        cs:Name="Border_0010"
                                        Margin="-9"
                                        Opacity="0.01"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                                <Border Background="#FF000000"
                                        CornerRadius="21"
                                        cs:Name="Border_0011"
                                        Margin="-10"
                                        Opacity="0.01"
                                        cs:Office2010Blue.Background="#FF000000"
                                        cs:Office2010Silver.Background="#FF000000"
                                       />
                            </Grid>
                            <Border Background="#FFFFFFFF"
                                    BorderBrush="#3F484848"
                                    BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness}"
                                    cs:Name="Border_0013"
                                    cs:Office2010Blue.Background="#FFFFFFFF"
                                    cs:Office2010Blue.BorderBrush="#3FA5ACB5"
                                    cs:Office2010Silver.Background="#FFFFFFFF"
                                    cs:Office2010Silver.BorderBrush="#3FA5ACB5"
                                   >
                                <ContentPresenter Content="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Content}"/>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

</ResourceDictionary>