<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxpt="http://schemas.devexpress.com/winfx/2008/xaml/printing/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:native="clr-namespace:DevExpress.Xpf.Printing.Native;assembly=DevExpress.Xpf.Printing.v24.1"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxpt:PrintingThemeKey ResourceKey=PreviewSurfaceTemplate}">
        <Border>
            <Border.Background>
                <LinearGradientBrush cs:Name="LinearGradientBrush_0511"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0"
                                    >
                    <GradientStop Color="#FF6F6F6F"
                                  cs:Name="GradientStop_0001"
                                  Offset="0"
                                  cs:Office2010Blue.Color="#FFE1EDFA"
                                  cs:Office2010Silver.Color="#FFF3F4F5"
                                 />
                    <GradientStop Color="#FF3E3E3E"
                                  cs:Name="GradientStop_0002"
                                  Offset="1"
                                  cs:Office2010Blue.Color="#FF94ADCC"
                                  cs:Office2010Silver.Color="#FFC0C4C9"
                                 />
                </LinearGradientBrush>
            </Border.Background>
        </Border>
    </ControlTemplate>

</ResourceDictionary>