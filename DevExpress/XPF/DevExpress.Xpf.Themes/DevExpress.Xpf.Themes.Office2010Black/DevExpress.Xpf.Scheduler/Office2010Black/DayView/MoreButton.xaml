                                                             StartPoint="0.5,0"
                                                            >
                                            <GradientStop Color="#FFFFE575"
                                                          cs:Name="GradientStop_0012"
                                                          Offset="0"
                                                          cs:Office2010Blue.Color="#FFFFE575"
                                                          cs:Office2010Silver.Color="#FFFFE575"
                                                         />
                                            <GradientStop Color="#FFFFF37F"
                                                          cs:Name="GradientStop_0013"
                                                          Offset="1"
                                                          cs:Office2010Blue.Color="#FFFFF37F"
                                                          cs:Office2010Silver.Color="#FFFFF37F"
                                                         />
                                            <GradientStop Color="#FFFFD86C"
                                                          cs:Name="GradientStop_0014"
                                                          Offset="0.5"
                                                          cs:Office2010Blue.Color="#FFFFD86C"
                                                          cs:Office2010Silver.Color="#FFFFD86C"
                                                         />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                            <Path x:Name="GlyphUp"
                                  cs:Name="Path_0001"
                                  Data="F1 M 209,6.00006L 201,6.00006L 205,2L 209,6.00006 Z"
                                  Fill="#FF3C3C3C"
                                  Height="4"
                                  HorizontalAlignment="Center"
                                  Margin="0,0,0,2"
                                  Stretch="Fill"
                                  VerticalAlignment="Center"
                                  Width="8"
                                  cs:Office2010Blue.Fill="#FF515D7A"
                                  cs:Office2010Silver.Fill="#FF80838F"
                                 />
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

