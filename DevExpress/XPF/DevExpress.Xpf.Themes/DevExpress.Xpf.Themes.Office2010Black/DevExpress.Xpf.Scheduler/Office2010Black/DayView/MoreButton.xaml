            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Grid Cursor="Hand" dxsch:SchedulerControl.HitTestType="MoreButton">
                        <!--BEGIN WPF-->
                        <!--<ToolTipService.ShowOnDisabled>True</ToolTipService.ShowOnDisabled>-->
                        <!--END WPF-->
