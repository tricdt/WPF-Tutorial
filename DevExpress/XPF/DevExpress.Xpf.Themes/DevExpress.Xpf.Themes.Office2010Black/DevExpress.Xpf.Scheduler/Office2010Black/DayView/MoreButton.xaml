                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid x:Name="Root"
                              Height="16"
                              Width="16">
                            <Border x:Name="NormalState"
                                    Background="#FF6A6A6A"
                                    CornerRadius="64"
