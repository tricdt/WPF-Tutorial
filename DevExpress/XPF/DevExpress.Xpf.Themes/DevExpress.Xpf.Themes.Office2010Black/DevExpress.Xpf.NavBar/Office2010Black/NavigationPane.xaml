    <Style x:Key="{dxnt:NavigationPanePopupThemeKey ResourceKey=SizeGripStyle}" TargetType="dxn:NavPanePopupSizeGrip">
        <Setter cs:Name="Setter_0001"
                Property="Background"
                Value="Transparent"
                cs:Office2010Blue.Value="Transparent"
                cs:Office2010Silver.Value="Transparent"
               />
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="VerticalAlignment" Value="Bottom"/>
        <Setter Property="Cursor" Value="SizeWE"/>
        <Setter Property="Height" Value="Auto"/>
        <Setter Property="Width" Value="Auto"/>
