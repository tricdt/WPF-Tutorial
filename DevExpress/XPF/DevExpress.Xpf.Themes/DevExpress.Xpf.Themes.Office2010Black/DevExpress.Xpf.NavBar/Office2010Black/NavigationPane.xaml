    <ControlTemplate x:Key="{dxnt:NavigationPaneOverflowPanelThemeKey ResourceKey=DropDownButtonTemplate}" TargetType="{x:Type dxn:NavPaneDropDownMenuControl}">
        <Grid Background="Transparent"
              cs:Name="Grid_0003"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup Name="CommonStates">
                    <VisualState Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Shadow"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="Normal"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
                <Grid x:Name="Shadow"
                      Margin="-2,-2,-2,-3"
                      Opacity="0">
                    <Border Background="#FF000000"
                            CornerRadius="1"
                            cs:Name="Border_0005"
                            Margin="1"
                            Opacity="0.05"
                            cs:Office2010Blue.Background="#FF000000"
                            cs:Office2010Silver.Background="#FF000000"
                           />
                    <Border Background="#FF000000"
                            CornerRadius="1"
                            cs:Name="Border_0006"
                            Margin="2"
                            Opacity="0.05"
                            cs:Office2010Blue.Background="#FF000000"
                            cs:Office2010Silver.Background="#FF000000"
                           />
                </Grid>
                <Grid x:Name="HoverState" Opacity="0">
                    <Border BorderBrush="#FF484848"
                            BorderThickness="1"
                            cs:Name="Border_0007"
                            cs:Office2010Blue.BorderBrush="#FF859EBF"
                            cs:Office2010Silver.BorderBrush="#FFA5ACB5"
                           >
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0496"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FF7D7D7D"
                                              cs:Name="GradientStop_0015"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFE4F1FC"
                                              cs:Office2010Silver.Color="#FFE9EDF1"
                                             />
                                <GradientStop Color="#FF7D7D7D"
                                              cs:Name="GradientStop_0016"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFE4F1FC"
                                              cs:Office2010Silver.Color="#FFE9EDF1"
                                             />
                                <GradientStop Color="#FF505050"
                                              cs:Name="GradientStop_0017"
                                              Offset="0.5"
                                              cs:Office2010Blue.Color="#FFC1D8EF"
                                              cs:Office2010Silver.Color="#FFDCE1E8"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Grid>
                            <Border Opacity="0.25">
