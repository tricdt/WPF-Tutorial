<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/navbar"
                    xmlns:dxnt="http://schemas.devexpress.com/winfx/2008/xaml/navbar/themekeys"
                    xmlns:interop="clr-namespace:System.Windows.Interop;assembly=PresentationFramework"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--END WPF-->

    <dx:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>

    <dx:BoolToVisibilityInverseConverter x:Key="BoolToVisibilityInverseConverter"/>

    <dxn:BooleanToOpacityConverter x:Key="BooleanToOpacityConverter"/>

    <Style x:Key="{dxnt:NavigationPaneHeaderThemeKey ResourceKey=OverflowButtonHeaderStyle}" TargetType="dx:XPFContentPresenter">
        <Setter Property="dxn:NavBarViewBase.ImageSettings" Value="{Binding ActualImageSettings}"/>
        <Setter Property="dxn:NavBarViewBase.LayoutSettings" Value="{Binding ActualLayoutSettings}"/>
        <Setter Property="dxn:NavBarViewBase.DisplayMode" Value="{Binding ActualDisplayMode}"/>
    </Style>

    <Style x:Key="{dxnt:NavigationPaneHeaderThemeKey ResourceKey=GroupButtonHeaderStyle}" TargetType="dx:XPFContentPresenter">
        <Setter Property="dxn:NavBarViewBase.ImageSettings" Value="{Binding ActualImageSettings}"/>
        <Setter Property="dxn:NavBarViewBase.LayoutSettings" Value="{Binding ActualLayoutSettings}"/>
        <Setter Property="dxn:NavBarViewBase.DisplayMode" Value="{Binding ActualDisplayMode}"/>
    </Style>

    <ControlTemplate x:Key="{dxnt:NavigationPanePopupThemeKey ResourceKey=ScrollUpButtonTemplate}" TargetType="dxn:NavBarRepeatButton">
        <dx:LayoutTransformPanel Orientation="{TemplateBinding ItemsPanelOrientation}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup Name="CommonStates">
                    <VisualState Name="Normal"/>
                    <VisualState Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF484848"
                                            cs:Office2010Blue.To="#FF515D7A"
                                            cs:Office2010Silver.To="#FF80838F"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="Pressed">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF2C2C2C"
                                            cs:Office2010Blue.To="#FF515D7A"
                                            cs:Office2010Silver.To="#FF80838F"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Glyph"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.35"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border Background="Transparent"
                    cs:Name="Border_0001"
                    cs:Office2010Blue.Background="Transparent"
                    cs:Office2010Silver.Background="Transparent"
                   >
                <Path x:Name="Glyph"
                      cs:Name="Path_0001"
                      Data="F1M1323,792L1327,788 1331,792 1323,792z"
                      Fill="#FF3C3C3C"
                      Height="4"
                      HorizontalAlignment="Center"
                      Margin="5"
                      RenderTransformOrigin="0.5,0.5"
                      Stretch="Fill"
                      VerticalAlignment="Center"
                      Width="8"
                      cs:Office2010Blue.Fill="#FF515D7A"
                      cs:Office2010Silver.Fill="#FF80838F"
                     />
            </Border>
        </dx:LayoutTransformPanel>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxnt:NavigationPanePopupThemeKey ResourceKey=ScrollDownButtonTemplate}" TargetType="dxn:NavBarRepeatButton">
        <dx:LayoutTransformPanel Orientation="{TemplateBinding ItemsPanelOrientation}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup Name="CommonStates">
                    <VisualState Name="Normal"/>
                    <VisualState Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0003"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF484848"
                                            cs:Office2010Blue.To="#FF515D7A"
                                            cs:Office2010Silver.To="#FF80838F"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="Pressed">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0004"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF2C2C2C"
                                            cs:Office2010Blue.To="#FF515D7A"
                                            cs:Office2010Silver.To="#FF80838F"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Glyph"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.35"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border Background="Transparent"
                    cs:Name="Border_0002"
                    cs:Office2010Blue.Background="Transparent"
                    cs:Office2010Silver.Background="Transparent"
                   >
                <Path x:Name="Glyph"
                      cs:Name="Path_0002"
                      Data="F1M1316,795L1312,799 1308,795 1316,795z"
                      Fill="#FF3C3C3C"
                      Height="4"
                      HorizontalAlignment="Center"
                      Margin="5"
                      Stretch="Fill"
                      VerticalAlignment="Center"
                      Width="8"
                      cs:Office2010Blue.Fill="#FF515D7A"
                      cs:Office2010Silver.Fill="#FF80838F"
                     />
            </Border>
        </dx:LayoutTransformPanel>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxnt:NavigationPaneSplitterThemeKey ResourceKey=SplitterTemplate}" TargetType="dxn:NavPaneSplitter">
        <Border>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="OrientationStates">
                    <VisualState x:Name="Vertical"/>
                    <VisualState x:Name="Horizontal"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="DraggingStates">
                    <VisualState x:Name="NotDragging"/>
                    <VisualState x:Name="Dragging"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <dx:LayoutTransformPanel Orientation="{TemplateBinding Orientation}">
                <Border x:Name="Border"
                        BorderBrush="#FF484848"
                        BorderThickness="1"
                        cs:Name="Border_0003"
                        Height="5"
                        cs:Office2010Blue.BorderBrush="#FF859EBF"
                        cs:Office2010Silver.BorderBrush="#FFA5ACB5"
                       >
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0490"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FF666666"
                                          cs:Name="GradientStop_0001"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFE4F1FC"
                                          cs:Office2010Silver.Color="#FFEAEDF1"
                                         />
                            <GradientStop Color="#FF505050"
                                          cs:Name="GradientStop_0002"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFC7DBEF"
                                          cs:Office2010Silver.Color="#FFDCE1E8"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
            </dx:LayoutTransformPanel>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxnt:NavigationPaneHeaderThemeKey ResourceKey=ExpandButtonTemplate}" TargetType="dxn:NavPaneExpandButton">
        <Grid Background="Transparent"
              cs:Name="Grid_0001"
              Height="18"
              HorizontalAlignment="Center"
              Margin="5,0,5,0"
              VerticalAlignment="Center"
              Width="18"
              cs:Office2010Blue.Background="Transparent"
              cs:Office2010Silver.Background="Transparent"
             >
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0005"
                                            Duration="0"
                                            Storyboard.TargetName="GlyphExpanded"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF3C3C3C"
                                            cs:Office2010Blue.To="#FF515D7A"
                                            cs:Office2010Silver.To="#FF80838F"
                                           />
                            <ColorAnimation cs:Name="ColorAnimation_0006"
                                            Duration="0"
                                            Storyboard.TargetName="GlyphCollapsed"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF3C3C3C"
                                            cs:Office2010Blue.To="#FF515D7A"
                                            cs:Office2010Silver.To="#FF80838F"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0007"
                                            Duration="0"
                                            Storyboard.TargetName="GlyphExpanded"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF3C3C3C"
                                            cs:Office2010Blue.To="#FF515D7A"
                                            cs:Office2010Silver.To="#FF80838F"
                                           />
                            <ColorAnimation cs:Name="ColorAnimation_0008"
                                            Duration="0"
                                            Storyboard.TargetName="GlyphCollapsed"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF3C3C3C"
                                            cs:Office2010Blue.To="#FF515D7A"
                                            cs:Office2010Silver.To="#FF80838F"
                                           />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="ExpandModeStates">
                    <VisualState x:Name="NormalMode"/>
                    <VisualState x:Name="Invert">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="glyphBorder" Storyboard.TargetProperty="RenderTransform">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <ScaleTransform ScaleX="-1"/>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="ExpandStates">
                    <VisualState x:Name="Expanded"/>
                    <VisualState x:Name="Collapsed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Collapsed"
                                                           Storyboard.TargetName="GlyphExpanded"
                                                           Storyboard.TargetProperty="(Control.Visibility)"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible"
                                                           Storyboard.TargetName="GlyphCollapsed"
                                                           Storyboard.TargetProperty="(Control.Visibility)"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="HoverState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0491"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFD7A820"
                                          cs:Name="GradientStop_0003"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFECC757"
                                          cs:Office2010Silver.Color="#FFECC757"
                                         />
                            <GradientStop Color="#BFB89407"
                                          cs:Name="GradientStop_0004"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFF1D449"
                                          cs:Office2010Silver.Color="#FFF1D449"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0492"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFFAE6"
                                              cs:Name="GradientStop_0005"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFCF7E4"
                                              cs:Office2010Silver.Color="#FFFCF7E4"
                                             />
                                <GradientStop Color="#FFFFFFED"
                                              cs:Name="GradientStop_0006"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFFCF8E7"
                                              cs:Office2010Silver.Color="#FFFCF8E7"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border Margin="1">
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_0493"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFFFEEA8"
                                                  cs:Name="GradientStop_0007"
                                                  Offset="0"
                                                  cs:Office2010Blue.Color="#FFF8EBB2"
                                                  cs:Office2010Silver.Color="#FFF8EBB2"
                                                 />
                                    <GradientStop Color="#FFFFFCE0"
                                                  cs:Name="GradientStop_0008"
                                                  Offset="1"
                                                  cs:Office2010Blue.Color="#FFFCF7E4"
                                                  cs:Office2010Silver.Color="#FFFCF7E4"
                                                 />
                                    <GradientStop Color="#FFFFE277"
                                                  cs:Name="GradientStop_0009"
                                                  Offset="0.5"
                                                  cs:Office2010Blue.Color="#FFFFE575"
                                                  cs:Office2010Silver.Color="#FFFFE575"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush cs:Name="LinearGradientBrush_0494"
                                             EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#BFA26F16"
                                          cs:Name="GradientStop_0010"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFC28E35"
                                          cs:Office2010Silver.Color="#FFC28E35"
                                         />
                            <GradientStop Color="#BFA2832C"
                                          cs:Name="GradientStop_0011"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFC2A44D"
                                          cs:Office2010Silver.Color="#FFC2A44D"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush cs:Name="LinearGradientBrush_0495"
                                                 EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                >
                                <GradientStop Color="#FFFFE575"
                                              cs:Name="GradientStop_0012"
                                              Offset="0"
                                              cs:Office2010Blue.Color="#FFFFE575"
                                              cs:Office2010Silver.Color="#FFFFE575"
                                             />
                                <GradientStop Color="#FFFFF37F"
                                              cs:Name="GradientStop_0013"
                                              Offset="1"
                                              cs:Office2010Blue.Color="#FFF8EBB2"
                                              cs:Office2010Silver.Color="#FFF8EBB2"
                                             />
                                <GradientStop Color="#FFFFD86C"
                                              cs:Name="GradientStop_0014"
                                              Offset="0.5"
                                              cs:Office2010Blue.Color="#FFFFD86C"
                                              cs:Office2010Silver.Color="#FFFFD86C"
                                             />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="glyphBorder"
                  HorizontalAlignment="Stretch"
                  RenderTransformOrigin="0.5,0.5"
                  VerticalAlignment="Stretch">
                <Path x:Name="GlyphExpanded"
                      cs:Name="Path_0003"
                      Data="F1 M 249,118L 250,118L 250,116L 248,114L 250,112L 250,110L 249,110L 245,114L 249,118 Z "
                      Fill="#FFE4E4E4"
                      Height="8"
                      Margin="0,0,1,0"
                      Stretch="Fill"
                      Width="5"
                      cs:Office2010Blue.Fill="#FF515D7A"
                      cs:Office2010Silver.Fill="#FF80838F"
                     />
                <Path x:Name="GlyphCollapsed"
                      cs:Name="Path_0004"
                      Data="F1 M 261,118L 260,118L 260,116L 262,114L 260,112L 260,110L 261,110L 265,114L 261,118 Z "
                      Fill="#FFE4E4E4"
                      Height="8"
                      Margin="1,0,0,0"
                      Stretch="Fill"
                      Visibility="Collapsed"
                      Width="5"
                      cs:Office2010Blue.Fill="#FF515D7A"
                      cs:Office2010Silver.Fill="#FF80838F"
                     />
            </Grid>
        </Grid>
    </ControlTemplate>

