                                   DisplayModeTextMargin="3"
                                   DockingBottomContentMargin="4,3,4,3"
                                   DockingBottomImageMargin="4,-3,4,3"
                                   DockingLeftContentMargin="4,3,4,3"
                                   DockingLeftImageMargin="4,3,0,3"
                                   DockingRightContentMargin="4,3,4,3"
                                   DockingRightImageMargin="0,3,4,3"
                                   DockingTopContentMargin="4,3,4,3"
                                   DockingTopImageMargin="4,3,4,-3"
