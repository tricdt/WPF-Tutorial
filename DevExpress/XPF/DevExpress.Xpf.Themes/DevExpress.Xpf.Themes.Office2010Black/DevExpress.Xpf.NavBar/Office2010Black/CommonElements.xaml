                        ImageHorizontalAlignment="Left"
                        ImageVerticalAlignment="Center"
                        TextHorizontalAlignment="Stretch"
                        TextVerticalAlignment="Center"/>

    <dxn:LayoutSettings x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewGroupLayoutSettings}"
                        ImageDocking="Left"
                        ImageHorizontalAlignment="Left"
                        ImageVerticalAlignment="Center"
                        TextHorizontalAlignment="Stretch"
                        TextVerticalAlignment="Center"/>

    <dxn:DisplayMode x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewItemDisplayMode}">ImageAndText</dxn:DisplayMode>

    <dxn:DisplayMode x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewGroupDisplayMode}">ImageAndText</dxn:DisplayMode>

    <SolidColorBrush x:Key="{dxnt:CommonElementsThemeKey ResourceKey=GroupForegroundBrush}"
                     Color="#FFFFFFFF"
