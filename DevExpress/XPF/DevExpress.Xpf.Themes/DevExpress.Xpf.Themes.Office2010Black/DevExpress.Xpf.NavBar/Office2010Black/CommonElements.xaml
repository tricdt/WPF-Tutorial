    <ControlTemplate x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ScrollUpButtonTemplate}" TargetType="dxn:NavBarRepeatButton">
        <dx:LayoutTransformPanel x:Name="Root" Orientation="{TemplateBinding Orientation}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0"
                                                            >
                                            <GradientStop Color="#FFFFEEA8"
                                                          Offset="0"
                                                          cs:Office2010Blue.Color="#FFF8EBB2"
                                                          cs:Office2010Silver.Color="#FFF8EBB2"
                                                         />
                                            <GradientStop Color="#FFFFFCE0"
                                                          Offset="1"
                                                          cs:Office2010Blue.Color="#FFFCF7E4"
                                                          cs:Office2010Silver.Color="#FFFCF7E4"
                                                         />
                                            <GradientStop Color="#FFFFE277"
                                                          Offset="0.5"
                                                          cs:Office2010Blue.Color="#FFF8E088"
                                                          cs:Office2010Silver.Color="#FFF8E088"
                                                         />
                                        </LinearGradientBrush>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0"
                                                            >
                                            <GradientStop Color="#FFFFE575"
                                                          Offset="0"
                                                         />
                                            <GradientStop Color="#FFFFF37F"
                                                          Offset="1"
                                                          cs:Office2010Blue.Color="#FFF8EBB2"
                                                          cs:Office2010Silver.Color="#FFF8EBB2"
                                                         />
                                            <GradientStop Color="#FFFFD86C"
                                                          Offset="0.5"
                                                         />
                                        </LinearGradientBrush>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Root"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.35"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
                <Border Background="#14000000"
                        Margin="0,0,0,-1"
                       />
                <Border x:Name="Border" BorderThickness="1">
                    <Border.BorderBrush>
                        <LinearGradientBrush EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FF484848"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFC1D8EF"
                                          cs:Office2010Silver.Color="#FFCED6DD"
                                         />
                            <GradientStop Color="#FF3C3C3C"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FF1E395B"
                                         />
                        </LinearGradientBrush>
                    </Border.BorderBrush>
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                            >
                            <GradientStop Color="#FFB9B9B9"
                                          Offset="0"
                                          cs:Office2010Blue.Color="#FFE4F1FC"
                                          cs:Office2010Silver.Color="#FFF3F4F5"
                                         />
                            <GradientStop Color="#FF9D9D9D"
                                          Offset="1"
                                          cs:Office2010Blue.Color="#FFE4F1FC"
                                          cs:Office2010Silver.Color="#FFE5E9EE"
                                         />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Grid>
                        <Border BorderBrush="#3FFFFFFF"
                                BorderThickness="1,1,1,0"
                               />
                        <Path x:Name="Glyph"
                              Data="F1M1323,792L1327,788 1331,792 1323,792z"
                              Fill="#FF000000"
                              Height="4"
                              HorizontalAlignment="Center"
                              Margin="2"
                              Stretch="Fill"
                              VerticalAlignment="Center"
                              Width="8"
                              cs:Office2010Blue.Fill="#FF515D7A"
                              cs:Office2010Silver.Fill="#FF80838F"
                             />
                    </Grid>
                </Border>
                <Border Background="#14FFFFFF"
                        Height="1"
                        VerticalAlignment="Top"
                       />
            </Grid>
        </dx:LayoutTransformPanel>
    </ControlTemplate>

