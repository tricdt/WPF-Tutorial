<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxh="http://schemas.devexpress.com/winfx/2008/xaml/heatmap/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Thickness x:Key="{dxh:HeatmapThemeKey ResourceKey=PaddingValue}">16</Thickness>

    <Thickness x:Key="{dxh:HeatmapThemeKey ResourceKey=BorderThickness}">1</Thickness>

    <SolidColorBrush x:Key="{dxh:HeatmapBrushesThemeKey ResourceKey=BackgroundBrush}"
                     Color="#FF838383"
                     cs:Office2010Blue.Color="#FFFFFFFF"
                     cs:Office2010Silver.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxh:HeatmapBrushesThemeKey ResourceKey=BorderBrush}"
                     Color="#FF484848"
                     cs:Office2010Blue.Color="#FF94ADCC"
                     cs:Office2010Silver.Color="#FFA5ACB5"/>

    <SolidColorBrush x:Key="{dxh:HeatmapBrushesThemeKey ResourceKey=ToolTipForeground}"
                     Color="#FF000000"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="{dxh:HeatmapBrushesThemeKey ResourceKey=ToolTipBackground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxh:HeatmapBrushesThemeKey ResourceKey=ToolTipBorder}"
                     Color="#BF646464"
                     cs:Office2010Blue.Background="#FF000000"
                     cs:Office2010Silver.Background="#FF000000"/>

    <SolidColorBrush x:Key="{dxh:HeatmapBrushesThemeKey ResourceKey=LabelForeground}"
                     Color="#FFFFFFFF"
                     cs:Office2010Blue.Color="#FF1E395B"
                     cs:Office2010Silver.Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="{dxh:HeatmapBrushesThemeKey ResourceKey=HighlightBrush}"
                     Color="#FF838383"
                     cs:Office2010Blue.Color="#FFFFFFFF"
                     cs:Office2010Silver.Color="#FFFFFFFF"/>

</ResourceDictionary>