<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/charts/themekeys"
                    xmlns:local="http://schemas.devexpress.com/winfx/2008/xaml/charts"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=Domain2DBrush}"
                     Color="#FF707070"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2010Blue.Color="#FFFFFFFF"
                     cs:Office2010Silver.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=DomainBorderBrush}"
                     Color="#FF484848"
                     cs:Name="SolidColorBrush_0002"
                     cs:Office2010Blue.Color="#FF94ADCC"
                     cs:Office2010Silver.Color="#FFA5ACB5"/>

    <ControlTemplate x:Key="{dxt:ChartControlThemeKey ResourceKey=XYDiagram2DWrapperTemplate}" TargetType="ContentControl">
        <Border Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                Padding="{TemplateBinding Padding}"
                Visibility="{TemplateBinding Visibility}">
            <local:ChartContentPresenter Content="{TemplateBinding Content}"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:ChartControlThemeKey ResourceKey=SimpleDiagram2DWrapperTemplate}" TargetType="ContentControl">
        <Border Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                Padding="{TemplateBinding Padding}"
                Visibility="{TemplateBinding Visibility}">
            <local:ChartContentPresenter Content="{TemplateBinding Content}"/>
        </Border>
    </ControlTemplate>

</ResourceDictionary>