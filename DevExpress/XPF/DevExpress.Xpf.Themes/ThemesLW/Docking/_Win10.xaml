        <dxi:ThemeResourceDictionary Path="Docking/_{ThemeName}.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <sys:Boolean x:Key="FloatingPaneWindow.UseNullRegion">False</sys:Boolean>
    <dx:WpfSvgPalette x:Key="{dxdot:LWKey TabbedPaneElements.SvgPalette}">
        <SolidColorBrush x:Key="BlackColor" Color="{dx:LWColor 'Win10Dark=#FFFFFF, Win10Light=#FF646464'}" />
        <SolidColorBrush x:Key="WhiteColor" Color="{dx:LWColor 'Win10Dark=#FF191919, Win10Light=#FFFFFFFF'}" />
        <SolidColorBrush x:Key="RedColor" Color="{dx:LWColor 'Win10Dark=#FFE81123, Win10Light=#FFeb3d00'}" />
        <SolidColorBrush x:Key="YellowColor" Color="{dx:LWColor 'Win10Dark=#FFf5c82a, Win10Light=#FFF5C82A'}" />
        <SolidColorBrush x:Key="GreenColor" Color="{dx:LWColor 'Win10Dark=#FF27CC2C, Win10Light=#FF0f7d0d'}" />
        <SolidColorBrush x:Key="BlueColor" Color="{dx:LWColor 'Win10Dark=#FF006DC9, Win10Light=#FF0078d7'}" />
        <dx:WpfSvgPalette.States>
            <dx:WpfSvgPalette x:Key="Hover">
                <SolidColorBrush x:Key="BlackColor" Color="{dx:LWColor 'Win10Dark=#FFF6F6F6, Win10Light=#FF646464'}" />
                <SolidColorBrush x:Key="WhiteColor" Color="{dx:LWColor 'Win10Dark=#FF444444, Win10Light=#FFE5F1FB'}" />
                <SolidColorBrush x:Key="RedColor" Color="{dx:LWColor 'Win10Dark=#FFE81123, Win10Light=#FFeb3d00'}" />
                <SolidColorBrush x:Key="YellowColor" Color="{dx:LWColor 'Win10Dark=#FFf5c82a, Win10Light=#FFF5C82A'}" />
                <SolidColorBrush x:Key="GreenColor" Color="{dx:LWColor 'Win10Dark=#FF27CC2C, Win10Light=#FF0f7d0d'}" />
                <SolidColorBrush x:Key="BlueColor" Color="{dx:LWColor 'Win10Dark=#FF006DC9, Win10Light=#FF0078d7'}" />
            </dx:WpfSvgPalette>
            <dx:WpfSvgPalette x:Key="Selected">
                <SolidColorBrush x:Key="BlackColor" Color="{dx:LWColor 'Win10Dark=#FFFFFFFF, Win10Light=#FF646464'}" />
                <SolidColorBrush x:Key="WhiteColor" Color="{dx:LWColor 'Win10Dark=#FF353535, Win10Light=#FFF5F6F7'}" />
                <SolidColorBrush x:Key="RedColor" Color="{dx:LWColor 'Win10Dark=#FFE81123, Win10Light=#FFeb3d00'}" />
                <SolidColorBrush x:Key="YellowColor" Color="#FFf5c82a" />
                <SolidColorBrush x:Key="GreenColor" Color="{dx:LWColor 'Win10Dark=#FF27CC2C, Win10Light=#FF0f7d0d'}" />
                <SolidColorBrush x:Key="BlueColor" Color="{dx:LWColor 'Win10Dark=#FF006DC9, Win10Light=#FF0078d7'}" />
            </dx:WpfSvgPalette>
            <dx:WpfSvgPalette x:Key="Inactive">
                <SolidColorBrush x:Key="BlackColor" Color="{dx:LWColor 'Win10Dark=#FFFFFFFF, Win10Light=#FF646464'}" />
                <SolidColorBrush x:Key="WhiteColor" Color="{dx:LWColor 'Win10Dark=#FF202020, Win10Light=#FFF5F6F7'}" />
                <SolidColorBrush x:Key="RedColor" Color="{dx:LWColor 'Win10Dark=#FFE81123, Win10Light=#FFeb3d00'}" />
                <SolidColorBrush x:Key="YellowColor" Color="#FFf5c82a" />
                <SolidColorBrush x:Key="GreenColor" Color="{dx:LWColor 'Win10Dark=#FF27CC2C, Win10Light=#FF0f7d0d'}" />
                <SolidColorBrush x:Key="BlueColor" Color="{dx:LWColor 'Win10Dark=#FF006DC9, Win10Light=#FF0078d7'}" />
            </dx:WpfSvgPalette>
        </dx:WpfSvgPalette.States>
    </dx:WpfSvgPalette>
