    <CornerRadius x:Key="{dxdot:LWKey DockPaneElements.FloatingCaptionCornerRadius}">0</CornerRadius>
    <Thickness x:Key="{dxdot:LWKey DockPaneElements.HeaderMargin}">0</Thickness>
