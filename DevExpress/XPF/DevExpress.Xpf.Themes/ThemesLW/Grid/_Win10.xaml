    <ControlTemplate x:Key="{dxgt:LWKey GroupRow.GroupRowLightweightTemplate}" TargetType="dxg:GroupRowControl">
        <Grid x:Name="PART_RootPanel" Background="{StaticResource {dxgt:LWKey GroupRow.GroupRowBackground}}" MinHeight="{DynamicResource {dxgt:LWKey GroupRow.GroupRowMinHeight}}">
