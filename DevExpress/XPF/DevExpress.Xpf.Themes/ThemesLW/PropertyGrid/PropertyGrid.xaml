    <Thickness x:Key="{dxprgt:LWKey PropertyGridControl.BorderThickness}">1</Thickness>
    <sys:Double x:Key="{dxprgt:LWKey PropertyGridControl.ValueColumnMinWidth}">21</sys:Double>
    <sys:Double x:Key="{dxprgt:LWKey PropertyGridControl.HeaderColumnMinWidth}">2</sys:Double>
    <sys:Double x:Key="{dxprgt:LWKey PropertyGridControl.SelectorAndSearchSeparatorWidth}">4</sys:Double>
    <sys:Double x:Key="{dxprgt:LWKey PropertyGridControl.ViewKindButtonsIndent}">1</sys:Double>
    <sys:Double x:Key="{dxprgt:LWKey DataRow.DataRowHeaderThumbWidth}">5</sys:Double>
    <sys:Double x:Key="{dxprgt:LWKey DataRow.ExpanderWidth}">22</sys:Double>
    <sys:Double x:Key="{dxprgt:LWKey DataRow.TouchExpanderWidth}">44</sys:Double>
    <SolidColorBrush x:Key="{dxprgt:LWKey DataRow.AlternateRowBackgroundBrush}" Color="{dx:LWColor 'Color.Delimiter, VS2019Dark=Color.Border'}" Opacity="0.16"/>
    <sys:Double x:Key="{dxprgt:LWKey PropertyGridView.RowMinHeight}">21</sys:Double>
    <Thickness x:Key="{dxprgt:LWKey PropertyGridView.CategoryHeaderMargin}">2,0,0,0</Thickness>
    <dx:LWThickness x:Key="{dxprgt:LWKey PropertyGridView.CategoryHeaderPadding}" Value="2, Win11=(4,2,4,4)"/>


