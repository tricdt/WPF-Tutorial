    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\PropertyGrid\Base\BrushEdit.xaml-->
    <!-- General -->

    <ControlTemplate x:Key="{dxprgt:LWKey PopupBrushEdit.BrushTypeButtonTemplate}" TargetType="{x:Type ToggleButton}">
        <Grid x:Name="Root" Margin="0,0,1,10">
            <dx:DXBorder x:Name="IsDefault" Background="Transparent" />
            <ContentPresenter HorizontalAlignment="Center" Margin="4" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" TargetName="IsDefault" Value="{dx:LWBrush 'Brush.HoverBackground, Win11=Brush.ListItem.HoverBackground'}" />
                <Setter Property="ToggleButton.Foreground" Value="{dx:LWBrush 'Brush.Foreground, Office2019HighContrast=Brush.HoverForeground, Win11=Brush.Foreground.Primary'}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" TargetName="IsDefault" Value="{dx:LWBrush 'Brush.SelectionBackground, Win11=Brush.ListItem.SelectionBackground'}" />
                <Setter Property="ToggleButton.Foreground" Value="{dx:LWBrush 'Brush.Foreground, Office2019HighContrast=Brush.SelectionForeground, VS2019Light=Brush.SelectionForeground, Win11=Brush.Foreground.Primary'}" />
            </Trigger>
            <Trigger Property="ToggleButton.IsChecked" Value="True">
                <Setter Property="Background" TargetName="IsDefault" Value="{dx:LWBrush 'Brush.SelectionBackground, Win11=Brush.ListItem.SelectionBackground'}" />
                <Setter Property="ToggleButton.Foreground" Value="{dx:LWBrush 'Brush.Foreground, Office2019HighContrast=Brush.SelectionForeground, VS2019Light=Brush.SelectionForeground, Win11=Brush.Foreground.Primary'}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" TargetName="Root" Value="0.38" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="ToggleButton.IsChecked" Value="False" />
                    <Condition Property="ToggleButton.IsMouseOver" Value="False" />
                </MultiTrigger.Conditions>
                <MultiTrigger.Setters>
                    <Setter Property="ToggleButton.Foreground" Value="{dx:LWBrush 'Brush.Foreground, Win11=Brush.Foreground.Primary'}" />
                </MultiTrigger.Setters>
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\PropertyGrid\Base\BrushEdit.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\PropertyGrid\Base\PropertyGridView.xaml-->
    <!-- General -->
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\PropertyGrid\Base\PropertyGridView.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\PropertyGrid\Base\PropertyGridControl.xaml-->
    <!-- General -->

    <ControlTemplate x:Key="{dxprgt:LWKey PropertyGridControl.DescriptionPresenterTemplate}" TargetType="{x:Type dxprg:PropertyDescriptionPresenterControl}">
        <dx:DXBorder
            x:Name="PART_Border"
            Background="{dx:LWBrush 'Brush.Window.Background, Win11=Brush.WindowBackground'}"
            BorderBrush="{StaticResource Brush.Border}"
            BorderThickness="0,1,0,0">
            <Grid x:Name="Root" Margin="12">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <ContentPresenter
                    x:Name="PART_HeaderPresenter"
                    Content="{Binding SelectedRow.Header, RelativeSource={RelativeSource TemplatedParent}}"
                    HorizontalAlignment="Left"
                    Margin="0,-3,0,0"
                    TextElement.FontSize="16"
                    TextElement.Foreground="{dx:LWBrush 'Brush.Foreground, Win11=Brush.Foreground.Primary'}" />
                <ContentPresenter x:Name="ContentPresenter1" Grid.Row="1" Margin="0,4,0,0" />
            </Grid>
        </dx:DXBorder>
        <ControlTemplate.Triggers>
            <Trigger Property="ShowSelectedRowHeader" Value="False">
                <Setter Property="Visibility" TargetName="PART_HeaderPresenter" Value="Collapsed" />
            </Trigger>
            <Trigger Property="IsInTooltip" Value="True">
                <Setter Property="Background" TargetName="PART_Border" Value="Transparent" />
                <Setter Property="BorderBrush" TargetName="PART_Border" Value="Transparent" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\PropertyGrid\Base\PropertyGridControl.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\PropertyGrid\Base.xaml-->
