    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\PropertyGrid\Base\RenderTemplates.xaml-->
    <!-- General -->

    <dxi:RenderTemplate x:Key="{dxprgt:LWKey VSViewRenderTemplate.RowCommandButton}">
        <dxi:RenderPanel
            Name="Root"
            Height="8"
            Margin="6,0"
            Width="8">
            <dxi:RenderBorder
                Name="Content"
                Background="{dx:LWBrush 'Brush.Button.Background, Win11=Brush.Button.SimpleButtonBackground'}"
                BorderBrush="{StaticResource Brush.Border}"
                BorderThickness="1" />
        </dxi:RenderPanel>
        <dxi:RenderTemplate.Triggers>
            <dxi:RenderStateGroup Name="CommonStates">
                <dxi:RenderStateTrigger Name="Normal" />
                <dxi:RenderStateTrigger Name="MouseOver">
                    <dxi:RenderSetter Property="Background" TargetName="Content" Value="{dx:LWBrush 'Brush.HoverBackground, Win11=Brush.ListItem.HoverBackground'}" />
                    <dxi:RenderSetter Property="BorderBrush" TargetName="Content" Value="{dx:LWBrush 'Brush.Focused, Office2019Colorful=Brush.Foreground, Office2019HighContrast=Brush.Foreground, Win11=Brush.Accent'}" />
                </dxi:RenderStateTrigger>
            </dxi:RenderStateGroup>
            <dxi:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.ShouldHighlightValueProperty}" Value="True">
                <dxi:RenderSetter Property="Background" TargetName="Content" Value="{StaticResource Brush.Border}" />
                <dxi:RenderSetter Property="BorderBrush" TargetName="Content" Value="{dx:LWBrush 'Brush.Foreground, VS2019Dark=Brush.Glyph, Win11=Brush.Foreground.Primary'}" />
            </dxi:RenderTrigger>
            <dxi:RenderTrigger DependencyProperty="{x:Static dx:ThemeManager.IsTouchEnabledProperty}" Value="True">
                <dxi:RenderSetter Property="Width" TargetName="Root" Value="28" />
                <dxi:RenderSetter Property="Height" TargetName="Root" Value="28" />
                <dxi:RenderSetter Property="Margin" TargetName="Root" Value="8" />
            </dxi:RenderTrigger>
        </dxi:RenderTemplate.Triggers>
    </dxi:RenderTemplate>

    <dxi:RenderTemplate x:Key="{dxprgt:LWKey VSViewRenderTemplate.RowCollectionButton}">
        <dxi:RenderPanel Name="Root">
            <dxi:RenderPanel Name="PART_Add" Opacity="0.62">
                <dxi:RenderPath
                    Name="Path1"
                    Data="F1 M 6,0C 9.31371,0 12,2.68629 12,6C 12,9.31371 9.31371,12 6,12C 2.68629,12 -6.35783e-007,9.31371 -6.35783e-007,6C -6.35783e-007,2.68629 2.68629,0 6,0 Z M 2,5L 2,7L 5,7L 5,10L 7,10L 7,7L 10,7L 10,5L 7,5L 7,2L 5,2L 5,5L 2,5 Z "
                    Fill="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.ControlLight.Foreground, Win11=Brush.Foreground.Primary'}"
                    Height="12"
                    Stretch="Fill"
                    Width="12" />
            </dxi:RenderPanel>
            <dxi:RenderPanel Name="PART_Remove" Opacity="0.62" Visibility="Collapsed">
                <dxi:RenderPath
                    Name="Path"
                    Data="F1 M 1.75736,1.75736C 4.1005,-0.585785 7.89949,-0.585785 10.2426,1.75736C 12.5858,4.10051 12.5858,7.8995 10.2426,10.2426C 7.89949,12.5858 4.1005,12.5858 1.75736,10.2426C -0.585787,7.8995 -0.585787,4.10051 1.75736,1.75736 Z M 2.46447,8.12132L 3.87868,9.53553L 6,7.41421L 8.12132,9.53553L 9.53553,8.12132L 7.41421,6L 9.53553,3.87868L 8.12132,2.46447L 6,4.58579L 3.87868,2.46447L 2.46447,3.87868L 4.58579,6L 2.46447,8.12132 Z "
                    Fill="{StaticResource Brush.Custom.Red}"
                    Height="12"
                    Stretch="Fill"
                    Width="12" />
            </dxi:RenderPanel>
        </dxi:RenderPanel>
        <dxi:RenderTemplate.Triggers>
            <dxi:RenderStateGroup Name="CommonStates">
                <dxi:RenderStateTrigger Name="Normal" />
                <dxi:RenderStateTrigger Name="MouseOver">
                    <dxi:RenderSetter Property="Opacity" TargetName="PART_Add" Value="1" />
                    <dxi:RenderSetter Property="Opacity" TargetName="PART_Remove" Value="1" />
                </dxi:RenderStateTrigger>
            </dxi:RenderStateGroup>
            <dxi:RenderStateGroup Name="ButtonKind">
                <dxi:RenderStateTrigger Name="Add" />
                <dxi:RenderStateTrigger Name="Remove">
                    <dxi:RenderSetter Property="Visibility" TargetName="PART_Remove" Value="Visible" />
                    <dxi:RenderSetter Property="Visibility" TargetName="PART_Add" Value="Collapsed" />
                </dxi:RenderStateTrigger>
            </dxi:RenderStateGroup>
            <dxi:RenderTrigger DependencyProperty="{x:Static dx:ThemeManager.IsTouchEnabledProperty}" Value="True">
                <dxi:RenderSetter Property="Width" TargetName="Root" Value="28" />
                <dxi:RenderSetter Property="Height" TargetName="Root" Value="28" />
                <dxi:RenderSetter Property="Margin" TargetName="Root" Value="8" />
            </dxi:RenderTrigger>
        </dxi:RenderTemplate.Triggers>
    </dxi:RenderTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\PropertyGrid\Base\RenderTemplates.xaml-->


</ResourceDictionary>