    <SolidColorBrush x:Key="{dxt:LWKey Format.FormatRedFillBrush}" Color="{dx:LWColor '#B2FFD9D9, Win10Dark=#3FDC5E55'}" />
    <SolidColorBrush x:Key="{dxt:LWKey Format.FormatYellowTextBrush}" Color="{dx:LWColor '#D99C6500, Win10Dark=#FFE5B264'}" />
    <SolidColorBrush x:Key="{dxt:LWKey Format.FormatYellowFillBrush}" Color="{dx:LWColor '#B2FFF5D9, Win10Dark=#3FDC9A2C'}" />
    <SolidColorBrush x:Key="{dxt:LWKey Format.FormatGreenTextBrush}" Color="{dx:LWColor '#D9158800, Win10Dark=#FF64E564'}" />
