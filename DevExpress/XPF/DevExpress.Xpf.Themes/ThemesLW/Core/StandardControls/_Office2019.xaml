                            <Setter Property="Background" Value="{dx:LWBrush 'Office2019Black=Brush.ControlLight.SelectionBackground, Office2019Colorful=Brush.SelectionBackground, Office2019HighContrast=Brush.SelectionBackground'}" />
                            <Setter Property="BorderBrush" Value="{dx:LWBrush 'Office2019Black=Brush.ControlLight.SelectionBackground, Office2019Colorful=Brush.SelectionBorder, Office2019HighContrast=Brush.SelectionBorder'}" />
