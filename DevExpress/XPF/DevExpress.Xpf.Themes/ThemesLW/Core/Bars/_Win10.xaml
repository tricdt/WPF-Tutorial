        </dx:DXBorder>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxb:BarContainerControl}}, Path=Orientation}" Value="Vertical">
                <Setter Property="Background" TargetName="BarContainerPanelBackground" Value="Transparent" />
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
