<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="clr-namespace:DevExpress.Xpf.Core;assembly=DevExpress.Xpf.Core.v24.1"
    xmlns:dxb="clr-namespace:DevExpress.Xpf.Bars;assembly=DevExpress.Xpf.Core.v24.1"
    xmlns:dxbi="clr-namespace:DevExpress.Xpf.Bars.Native;assembly=DevExpress.Xpf.Core.v24.1"
    xmlns:dxbt="clr-namespace:DevExpress.Xpf.Bars.Themes;assembly=DevExpress.Xpf.Core.v24.1"
    xmlns:dxi="clr-namespace:DevExpress.Xpf.Core.Native;assembly=DevExpress.Xpf.Core.v24.1"
    xmlns:dxt="clr-namespace:DevExpress.Xpf.Utils.Themes;assembly=DevExpress.Xpf.Core.v24.1">

    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="Core/Bars/Bars.RenderTemplates.xaml" />
    </ResourceDictionary.MergedDictionaries>


    <!--#region Bars\Themes\Generic\RenderTemplates\_Common.xaml-->
    <dxi:RenderTriggersCollection x:Key="{dxt:LWKey BarItemRender.AppearanceTriggers}">
        <!-- Background -->
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.BackgroundProperty}" TargetProperty="ExternalBackground" TargetName="PART_Background" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.BorderBrushProperty}" TargetProperty="ExternalBorderBrush" TargetName="PART_Background" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.BorderThicknessProperty}" TargetProperty="ExternalBorderThickness" TargetName="PART_Background" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.CornerRadiusProperty}" TargetProperty="ExternalCornerRadius" TargetName="PART_Background" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.HasBackgroundProperty}" TargetProperty="ApplyExternalBackground" TargetName="PART_Background" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.HasBorderBrushProperty}" TargetProperty="ApplyExternalBorderBrush" TargetName="PART_Background" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.HasBorderThicknessProperty}" TargetProperty="ApplyExternalBorderThickness" TargetName="PART_Background" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.HasCornerRadiusProperty}" TargetProperty="ApplyExternalCornerRadius" TargetName="PART_Background" />
        <!-- Foreground -->
        <dxi:ConditionalRenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.ForegroundProperty}" TargetProperty="Foreground" TargetName="PART_Container">
            <dxi:RenderCondition DependencyProperty="{x:Static dxi:AppearanceOptions.HasForegroundProperty}" Value="True" />
        </dxi:ConditionalRenderBinding>
        <!-- Padding -->
        <dxi:ConditionalRenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.PaddingProperty}" TargetProperty="Margin" TargetName="PART_Container">
            <dxi:RenderCondition DependencyProperty="{x:Static dxi:AppearanceOptions.HasPaddingProperty}" Value="True" />
        </dxi:ConditionalRenderBinding>
    </dxi:RenderTriggersCollection>

    <dxi:RenderTriggersCollection x:Key="{dxt:LWKey BarItemRender.SplitAppearanceTriggers}">
        <dxi:RenderTriggersCollection.MergedTriggers>
            <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.AppearanceTriggers}}" />
        </dxi:RenderTriggersCollection.MergedTriggers>
        <!-- ContentBackground -->
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.BackgroundProperty}" TargetProperty="ExternalBackground" TargetName="PART_ContentBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.BorderBrushProperty}" TargetProperty="ExternalBorderBrush" TargetName="PART_ContentBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.BorderThicknessProperty}" TargetProperty="ExternalBorderThickness" TargetName="PART_ContentBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.CornerRadiusProperty}" TargetProperty="ExternalCornerRadius" TargetName="PART_ContentBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.HasBackgroundProperty}" TargetProperty="ApplyExternalBackground" TargetName="PART_ContentBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.HasBorderBrushProperty}" TargetProperty="ApplyExternalBorderBrush" TargetName="PART_ContentBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.HasBorderThicknessProperty}" TargetProperty="ApplyExternalBorderThickness" TargetName="PART_ContentBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxi:AppearanceOptions.HasCornerRadiusProperty}" TargetProperty="ApplyExternalCornerRadius" TargetName="PART_ContentBackground" />
        <!-- ArrowBackground -->
        <dxi:RenderBinding DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.ArrowBackgroundProperty}" TargetProperty="ExternalBackground" TargetName="PART_ArrowBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.ArrowBorderBrushProperty}" TargetProperty="ExternalBorderBrush" TargetName="PART_ArrowBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.ArrowBorderThicknessProperty}" TargetProperty="ExternalBorderThickness" TargetName="PART_ArrowBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.ArrowCornerRadiusProperty}" TargetProperty="ExternalCornerRadius" TargetName="PART_ArrowBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.HasArrowBackgroundProperty}" TargetProperty="ApplyExternalBackground" TargetName="PART_ArrowBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.HasArrowBorderBrushProperty}" TargetProperty="ApplyExternalBorderBrush" TargetName="PART_ArrowBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.HasArrowBorderThicknessProperty}" TargetProperty="ApplyExternalBorderThickness" TargetName="PART_ArrowBackground" />
        <dxi:RenderBinding DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.HasArrowCornerRadiusProperty}" TargetProperty="ApplyExternalCornerRadius" TargetName="PART_ArrowBackground" />
        <!-- Foreground -->
        <dxi:ConditionalRenderBinding DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.ArrowForegroundProperty}" TargetProperty="Foreground" TargetName="PART_ArrowContainer">
            <dxi:RenderCondition DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.HasArrowForegroundProperty}" Value="True" />
        </dxi:ConditionalRenderBinding>
        <!-- Padding -->
        <dxi:ConditionalRenderBinding DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.ArrowPaddingProperty}" TargetProperty="Margin" TargetName="PART_PathArrowContainer">
            <dxi:RenderCondition DependencyProperty="{x:Static dxbi:BarItemAppearanceOptions.HasArrowPaddingProperty}" Value="True" />
        </dxi:ConditionalRenderBinding>
        <dxi:RenderStateGroup Name="ActAsDropDownStates">
            <dxi:RenderStateTrigger Name="ActAsDropDownTrue" />
            <dxi:RenderStateTrigger Name="ActAsDropDownFalse">
                <dxi:RenderSetter TargetName="PART_Background" Property="Opacity" Value="0" />
            </dxi:RenderStateTrigger>
        </dxi:RenderStateGroup>
    </dxi:RenderTriggersCollection>

    <dxi:RenderTriggersCollection x:Key="{dxt:LWKey BarItemRender.EnabledBorderStates}">
        <dxi:RenderStateGroup Name="ForegroundColor">
            <dxi:RenderStateTrigger Name="Disabled">
                <dxi:RenderSetter TargetName="PART_Root" Property="Opacity" Value="0.35" />
            </dxi:RenderStateTrigger>
            <dxi:RenderStateTrigger Name="Normal">
                <dxi:RenderSetter TargetName="PART_Root" Property="Opacity" Value="1" />
            </dxi:RenderStateTrigger>
        </dxi:RenderStateGroup>
    </dxi:RenderTriggersCollection>
    <dxi:RenderTriggersCollection x:Key="{dxt:LWKey BarItemRender.EnabledBorderStatesEditor}">
        <dxi:RenderStateGroup Name="ForegroundColor">
            <dxi:RenderStateTrigger Name="Disabled">
                <dxi:RenderSetter TargetName="PART_Editor" Property="Opacity" Value="1" />
                <dxi:RenderSetter TargetName="PART_Content" Property="Opacity" Value="0.35" />
                <dxi:RenderSetter TargetName="PART_Content2" Property="Opacity" Value="0.35" />
                <dxi:RenderSetter TargetName="PART_Glyph" Property="Opacity" Value="0.35" />
            </dxi:RenderStateTrigger>
            <dxi:RenderStateTrigger Name="Normal">
                <dxi:RenderSetter TargetName="PART_Editor" Property="Opacity" Value="1" />
                <dxi:RenderSetter TargetName="PART_Content" Property="Opacity" Value="1" />
                <dxi:RenderSetter TargetName="PART_Content2" Property="Opacity" Value="1" />
                <dxi:RenderSetter TargetName="PART_Glyph" Property="Opacity" Value="1" />
            </dxi:RenderStateTrigger>
        </dxi:RenderStateGroup>
    </dxi:RenderTriggersCollection>
    <dxi:RenderTriggersCollection x:Key="{dxt:LWKey BarItemRender.ArrowAlignmentStates}">
        <dxi:RenderStateGroup Name="Dock">
            <dxi:RenderStateTrigger Name="ArrowAlignmentLeft">
                <dxi:RenderSetter TargetName="PART_ContentContainer" Property="Dock" Value="Right" />
                <dxi:RenderSetter TargetName="PART_ArrowContainer" Property="Dock" Value="Left" />
                <dxi:RenderSetter TargetName="PART_ContentBackground" Property="HideBorderSide" Value="Left" />
                <dxi:RenderSetter TargetName="PART_ArrowBackground" Property="HideBorderSide" Value="Right" />
            </dxi:RenderStateTrigger>
            <dxi:RenderStateTrigger Name="ArrowAlignmentTop">
                <dxi:RenderSetter TargetName="PART_ContentContainer" Property="Dock" Value="Bottom" />
                <dxi:RenderSetter TargetName="PART_ArrowContainer" Property="Dock" Value="Top" />
                <dxi:RenderSetter TargetName="PART_ContentBackground" Property="HideBorderSide" Value="Top" />
                <dxi:RenderSetter TargetName="PART_ArrowBackground" Property="HideBorderSide" Value="Bottom" />
            </dxi:RenderStateTrigger>
            <dxi:RenderStateTrigger Name="ArrowAlignmentRight">
                <dxi:RenderSetter TargetName="PART_ContentContainer" Property="Dock" Value="Left" />
                <dxi:RenderSetter TargetName="PART_ArrowContainer" Property="Dock" Value="Right" />
                <dxi:RenderSetter TargetName="PART_ContentBackground" Property="HideBorderSide" Value="Right" />
                <dxi:RenderSetter TargetName="PART_ArrowBackground" Property="HideBorderSide" Value="Left" />
            </dxi:RenderStateTrigger>
            <dxi:RenderStateTrigger Name="ArrowAlignmentBottom">
                <dxi:RenderSetter TargetName="PART_ContentContainer" Property="Dock" Value="Top" />
                <dxi:RenderSetter TargetName="PART_ArrowContainer" Property="Dock" Value="Bottom" />
                <dxi:RenderSetter TargetName="PART_ContentBackground" Property="HideBorderSide" Value="Bottom" />
                <dxi:RenderSetter TargetName="PART_ArrowBackground" Property="HideBorderSide" Value="Top" />
            </dxi:RenderStateTrigger>
        </dxi:RenderStateGroup>
    </dxi:RenderTriggersCollection>

    <dxb:RenderArrowControlTemplateSelector
        x:Key="{dxt:LWKey BarItemRender.RenderArrowControlTemplateSelector}"
        LazyDefaultTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.DefaultArrowTemplate}}"
        LazyTemplateInMenu="{dx:LazyStaticResource {dxt:LWKey BarItemRender.ArrowTemplateInMenu}}"
        LazyTemplateInStatusBar="{dx:LazyStaticResource {dxt:LWKey BarItemRender.ArrowTemplateInPageHeader}}"
        LazyTemplatePageHeader="{dx:LazyStaticResource {dxt:LWKey BarItemRender.ArrowTemplateInPageHeader}}" />
    <!--#endregion Bars\Themes\Generic\RenderTemplates\_Common.xaml-->

    <!--#region Bars\Themes\Generic\RenderTemplates\ApplicationMenu.xaml-->
    <dxb:RenderBarItemBorderStateTemplateSelector
        x:Key="{dxt:LWKey BarItemRender.BorderStateTemplateSelectorInApplicationMenu}"
        LazyCheckedTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.CheckedBackgroundTemplateInMenu}}"
        LazyContentHighlight="{dx:LazyStaticResource {dxt:LWKey BarItemRender.ContentHighlightTemplateInMenu}}"
        LazyCustomizationTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.CustomizationBackgroundTemplateInMenu}}"
        LazyHoverTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.HoverBackgroundTemplateInMenu}}"
        LazyIndeterminateTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.IndeterminateBackgroundTemplateInMenu}}"
        LazyNormalTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.NormalBackgroundTemplateInMenu}}"
        LazyPressedTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.PressedBackgroundTemplateInMenu}}" />
    <dxb:RenderBarItemBorderStateTemplateSelector x:Key="{dxt:LWKey BarItemRender.OnlyCustomizationBorderStateTemplateSelectorInApplicationMenu}" LazyCustomizationTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.CustomizationBackgroundTemplateInBar}}" />

    <dxi:RenderTemplate x:Key="{dxt:LWKey BarItemRender.BaseEditTemplateInApplicationMenu}" x:Shared="False">
        <dxi:RenderBorder Name="PART_Root" Background="Transparent">
            <dxi:RenderPanel>
                <dxb:RenderBarItemBorderControl Name="PART_Background" RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.OnlyCustomizationBorderStateTemplateSelectorInApplicationMenu}}" />
                <dxi:RenderPanel Name="PART_Container" LayoutProvider="{x:Static dxi:LayoutProvider.DockInstance}">
                    <dxi:RenderContentControl Name="PART_Glyph" PreferRenderTemplate="False" RenderContentTemplate="{StaticResource {dxt:LWKey BarItemRender.GlyphTemplate}}" />
                    <dxi:RenderPanel Name="PART_ContentContainer" LayoutProvider="{x:Static dxi:LayoutProvider.DockInstance}">
                        <dxi:RenderContentControl Name="PART_Content" PreferRenderTemplate="False" />
                        <dxi:RenderContentControl Name="PART_Editor" />
                        <dxi:RenderContentControl Name="PART_Content2" />
                    </dxi:RenderPanel>
                </dxi:RenderPanel>
            </dxi:RenderPanel>
        </dxi:RenderBorder>
        <dxi:RenderTemplate.Triggers>
            <dxi:RenderTriggersCollection>
                <dxi:RenderTriggersCollection.MergedTriggers>
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.GlyphAlignmentStatesBarEditItemInBar}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.ForegroundColorStatesInApplicationMenu}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.EnabledBorderStates}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.EnabledBorderStatesEditor}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.AppearanceTriggers}}" />
                </dxi:RenderTriggersCollection.MergedTriggers>
            </dxi:RenderTriggersCollection>
        </dxi:RenderTemplate.Triggers>
    </dxi:RenderTemplate>
    <dxi:RenderTemplate x:Key="{dxt:LWKey BarItemRender.BaseSplitTemplateInApplicationMenu}" x:Shared="False">
        <dxi:RenderBorder Name="PART_Root" Background="Transparent">
            <dxi:RenderPanel>
                <dxb:RenderBarItemBorderControl Name="PART_Background" RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.BorderStateTemplateSelectorInApplicationMenu}}" />
                <dxi:RenderPanel LayoutProvider="{x:Static dxi:LayoutProvider.DockInstance}">
                    <dxi:RenderPanel Dock="Left">
                        <dxb:RenderBarItemBorderControl Name="PART_ContentBackground" RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.BorderStateTemplateSelectorInApplicationMenu}}" />
                        <dxi:RenderPanel Name="PART_Container" LayoutProvider="{x:Static dxi:LayoutProvider.DockInstance}">
                            <dxi:RenderContentControl
                                Name="PART_Glyph"
                                Dock="Left"
                                PreferRenderTemplate="False"
                                RenderContentTemplate="{StaticResource {dxt:LWKey BarItemRender.GlyphTemplate}}" />
                            <dxi:RenderPanel Name="PART_FirsContainer" Dock="Left" LayoutProvider="{x:Static dxi:LayoutProvider.DockInstance}">
                                <dxi:RenderContentControl Name="PART_Content" Dock="Top" PreferRenderTemplate="False" />
                                <dxi:RenderContentControl Name="PART_Description" Dock="Bottom" />
                            </dxi:RenderPanel>
                        </dxi:RenderPanel>
                    </dxi:RenderPanel>
                    <dxi:RenderBorder Name="PART_Separator" Width="1" />
                    <dxi:RenderPanel Name="PART_ArrowContainer" Dock="Right">
                        <dxi:RenderBorder Background="Transparent" />
                        <dxb:RenderBarItemBorderControl Name="PART_ArrowBackground" RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.BorderStateTemplateSelectorInApplicationMenu}}" />
                        <dxi:RenderPanel>
                            <dxb:RenderArrowControl RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.RenderArrowControlTemplateSelector}}" />
                        </dxi:RenderPanel>
                    </dxi:RenderPanel>
                </dxi:RenderPanel>
            </dxi:RenderPanel>
        </dxi:RenderBorder>
        <dxi:RenderTemplate.Triggers>
            <dxi:RenderTriggersCollection>
                <dxi:RenderTriggersCollection.MergedTriggers>
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.GlyphAlignmentStatesInBar}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.ForegroundColorStatesInApplicationMenu}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.EnabledBorderStates}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.SplitAppearanceTriggers}}" />
                </dxi:RenderTriggersCollection.MergedTriggers>
            </dxi:RenderTriggersCollection>
        </dxi:RenderTemplate.Triggers>
    </dxi:RenderTemplate>
    <dxi:RenderTemplate x:Key="{dxt:LWKey BarItemRender.BaseTemplateInApplicationMenu}" x:Shared="False">
        <dxi:RenderBorder Name="PART_Root" Background="Transparent">
            <dxi:RenderPanel>
                <dxb:RenderBarItemBorderControl Name="PART_Background" RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.BorderStateTemplateSelectorInApplicationMenu}}" />
                <dxi:RenderPanel Name="PART_Container" LayoutProvider="{x:Static dxi:LayoutProvider.DockInstance}">
                    <dxi:RenderContentControl
                        Name="PART_Glyph"
                        Dock="Left"
                        PreferRenderTemplate="False"
                        RenderContentTemplate="{StaticResource {dxt:LWKey BarItemRender.GlyphTemplate}}" />
                    <dxi:RenderPanel Dock="Right" LayoutProvider="{x:Static dxi:LayoutProvider.DockInstance}">
                        <dxi:RenderContentControl Name="PART_Content" Dock="Top" PreferRenderTemplate="False" />
                        <dxi:RenderContentControl Name="PART_Description" Dock="Bottom" />
                    </dxi:RenderPanel>
                </dxi:RenderPanel>
            </dxi:RenderPanel>
        </dxi:RenderBorder>
        <dxi:RenderTemplate.Triggers>
            <dxi:RenderTriggersCollection>
                <dxi:RenderTriggersCollection.MergedTriggers>
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.GlyphAlignmentStatesInBar}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.ForegroundColorStatesInApplicationMenu}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.EnabledBorderStates}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.AppearanceTriggers}}" />
                </dxi:RenderTriggersCollection.MergedTriggers>
            </dxi:RenderTriggersCollection>
        </dxi:RenderTemplate.Triggers>
    </dxi:RenderTemplate>

    <dxi:RenderTemplate x:Key="{dxt:LWKey BarItemRender.EditTemplateInApplicationMenu}" BasedOn="{StaticResource {dxt:LWKey BarItemRender.BaseEditTemplateInApplicationMenu}}" Styles="{StaticResource {dxt:LWKey BarItemRender.EditTemplateInApplicationMenuStyles}}" />
    <dxi:RenderTemplate x:Key="{dxt:LWKey BarItemRender.SplitTemplateInApplicationMenu}" BasedOn="{StaticResource {dxt:LWKey BarItemRender.BaseSplitTemplateInApplicationMenu}}" Styles="{StaticResource {dxt:LWKey BarItemRender.SplitTemplateInApplicationMenuStyles}}" />
    <dxi:RenderTemplate x:Key="{dxt:LWKey BarItemRender.TemplateInApplicationMenu}" BasedOn="{StaticResource {dxt:LWKey BarItemRender.BaseTemplateInApplicationMenu}}" Styles="{StaticResource {dxt:LWKey BarItemRender.TemplateInApplicationMenuStyles}}" />
    <!--#endregion Bars\Themes\Generic\RenderTemplates\ApplicationMenu.xaml-->

    <!--#region Bars\Themes\Generic\RenderTemplates\Bar.xaml-->
    <dxb:RenderBarItemBorderStateTemplateSelector
        x:Key="{dxt:LWKey BarItemRender.BorderStateTemplateSelectorInBar}"
        LazyCheckedTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.CheckedBackgroundTemplateInBar}}"
        LazyContentHighlight="{dx:LazyStaticResource {dxt:LWKey BarItemRender.ContentHighlightTemplateInBar}}"
        LazyCustomizationTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.CustomizationBackgroundTemplateInBar}}"
        LazyHoverTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.HoverBackgroundTemplateInBar}}"
        LazyIndeterminateTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.IndeterminateBackgroundTemplateInBar}}"
        LazyNormalTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.NormalBackgroundTemplateInBar}}"
        LazyPressedTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.PressedBackgroundTemplateInBar}}" />
    <dxb:RenderBarItemBorderStateTemplateSelector x:Key="{dxt:LWKey BarItemRender.EditBorderStateTemplateSelectorInBar}" LazyCustomizationTemplate="{dx:LazyStaticResource {dxt:LWKey BarItemRender.CustomizationBackgroundTemplateInBar}}" />

    <dxi:RenderTemplate x:Key="{dxt:LWKey BarItemRender.BaseStaticTemplateInBar}" x:Shared="False">
        <dxi:RenderBorder Name="PART_Root" Background="Transparent">
            <dxi:RenderPanel>
                <dxb:RenderBarItemBorderControl Name="PART_Background" />
                <dxi:RenderPanel Name="PART_Container" LayoutProvider="{dxi:DockPanelLayoutProvider LastChildFill=True}">
                    <dxi:RenderContentControl Name="PART_Glyph" PreferRenderTemplate="False" RenderContentTemplate="{StaticResource {dxt:LWKey BarItemRender.GlyphTemplate}}" />
                    <dxi:RenderContentControl Name="PART_Content" PreferRenderTemplate="False" />
                </dxi:RenderPanel>
            </dxi:RenderPanel>
        </dxi:RenderBorder>
        <dxi:RenderTemplate.Triggers>
            <dxi:RenderTriggersCollection>
                <dxi:RenderTriggersCollection.MergedTriggers>
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.GlyphAlignmentStatesInBar}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.ForegroundColorStatesInBar}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.EnabledBorderStates}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.AppearanceTriggers}}" />
                </dxi:RenderTriggersCollection.MergedTriggers>
            </dxi:RenderTriggersCollection>
        </dxi:RenderTemplate.Triggers>
    </dxi:RenderTemplate>
    <dxi:RenderTemplate x:Key="{dxt:LWKey BarItemRender.BaseSplitItemTemplateInBar}" x:Shared="False">
        <dxi:RenderBorder Name="PART_Root" Background="Transparent">
            <dxi:RenderPanel>
                <dxb:RenderBarItemBorderControl Name="PART_Background" RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.BorderStateTemplateSelectorInBar}}" />
                <dxi:RenderPanel LayoutProvider="{x:Static dxi:LayoutProvider.DockInstance}">
                    <dxi:RenderPanel Name="PART_ContentContainer">
                        <dxb:RenderBarItemBorderControl Name="PART_ContentBackground" HideBorderSide="Right" RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.BorderStateTemplateSelectorInBar}}" />
                        <dxi:RenderPanel Name="PART_Container" LayoutProvider="{x:Static dxi:LayoutProvider.DockInstance}">
                            <dxi:RenderContentControl Name="PART_Glyph" PreferRenderTemplate="False" RenderContentTemplate="{StaticResource {dxt:LWKey BarItemRender.GlyphTemplate}}" />
                            <dxi:RenderContentControl
                                Name="PART_Content"
                                HorizontalAlignment="Center"
                                HorizontalContentAlignment="Center"
                                PreferRenderTemplate="False" />
                        </dxi:RenderPanel>
                    </dxi:RenderPanel>
                    <dxi:RenderBorder Name="PART_Separator" Width="1" />
                    <dxi:RenderPanel Name="PART_ArrowContainer">
                        <dxi:RenderBorder Background="Transparent" />
                        <dxb:RenderBarItemBorderControl Name="PART_ArrowBackground" HideBorderSide="Left" RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.BorderStateTemplateSelectorInBar}}" />
                        <dxi:RenderPanel Name="PART_PathArrowContainer" HorizontalAlignment="Center" VerticalAlignment="Center">
                            <dxb:RenderArrowControl RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.RenderArrowControlTemplateSelector}}" />
                        </dxi:RenderPanel>
                    </dxi:RenderPanel>
                </dxi:RenderPanel>
            </dxi:RenderPanel>
        </dxi:RenderBorder>
        <dxi:RenderTemplate.Triggers>
            <dxi:RenderTriggersCollection>
                <dxi:RenderTriggersCollection.MergedTriggers>
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.GlyphAlignmentStatesInBar}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.ArrowAlignmentStates}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.ForegroundColorStatesInBar}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.EnabledBorderStates}}" />
                    <dxi:RenderTriggersCollection Source="{StaticResource {dxt:LWKey BarItemRender.SplitAppearanceTriggers}}" />
                </dxi:RenderTriggersCollection.MergedTriggers>
            </dxi:RenderTriggersCollection>
        </dxi:RenderTemplate.Triggers>
    </dxi:RenderTemplate>
    <dxi:RenderTemplate x:Key="{dxt:LWKey BarItemRender.BaseDefaultTemplateInBar}" x:Shared="False">
        <dxi:RenderBorder Name="PART_Root" Background="Transparent">
            <dxi:RenderPanel>
                <dxb:RenderBarItemBorderControl Name="PART_Background" RenderTemplateSelector="{StaticResource {dxt:LWKey BarItemRender.BorderStateTemplateSelectorInBar}}" />
                <dxi:RenderPanel Name="PART_Container" LayoutProvider="{x:Static dxi:LayoutProvider.DockInstance}">
                    <dxi:RenderContentControl
                        Name="PART_Glyph"
                        HorizontalAlignment="Center"
                        HorizontalContentAlignment="Center"
                        Dock="Top"
                        PreferRenderTemplate="False"
                        RenderContentTemplate="{StaticResource {dxt:LWKey BarItemRender.GlyphTemplate}}"
                        Visibility="Collapsed" />
                    <dxi:RenderContentControl
                        Name="PART_Content"
                        HorizontalAlignment="Center"
                        HorizontalContentAlignment="Center"
                        Dock="Bottom"
                        PreferRenderTemplate="False" />
                </dxi:RenderPanel>
            </dxi:RenderPanel>
        </dxi:RenderBorder>
        <dxi:RenderTemplate.Triggers>
            <dxi:RenderTriggersCollection>
