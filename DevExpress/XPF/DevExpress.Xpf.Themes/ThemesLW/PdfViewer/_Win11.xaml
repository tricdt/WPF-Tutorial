<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
    xmlns:dxpdft="http://schemas.devexpress.com/winfx/2008/xaml/pdf/themekeys"
    xmlns:dxpdf="http://schemas.devexpress.com/winfx/2008/xaml/pdf">
    <ResourceDictionary.MergedDictionaries>
        <dxi:PaletteResourceDictionary />
    </ResourceDictionary.MergedDictionaries>
    <SolidColorBrush x:Key="{dxpdft:LWKey PdfViewer.PdfPresenterControlBackgroundBrush}" Color="{StaticResource Color.WindowBackground}" />
    <dx:ColorStaticResource x:Key="{dxpdft:LWKey PdfViewer.HighlightSearchTextColor}" Color="{StaticResource Color.ListItem.SelectionBackground}" />
    <SolidColorBrush x:Key="{dxpdft:LWKey PdfViewer.HighlightSelectionColor}" Color="{StaticResource Color.ListItem.SelectionBackground}" Opacity="0.38" />
    <dx:ColorStaticResource x:Key="{dxpdft:LWKey PdfViewer.CaretColor}" Color="{StaticResource Color.Backstage.Background}" />
    <dx:ColorStaticResource x:Key="{dxpdft:LWKey PdfViewer.StartScreenCaptionForegroundColor}" Color="{StaticResource Color.Foreground.Primary}" />
    <dx:ColorStaticResource x:Key="{dxpdft:LWKey PdfViewer.StartScreenOpenDocumentForegroundColor}" Color="{StaticResource Color.Foreground.Primary}" />
    <dx:ColorStaticResource x:Key="{dxpdft:LWKey PdfViewer.StartScreenDocumentLinkForegroundColor}" Color="{StaticResource Color.Custom.Blue}" />
    <dx:ColorStaticResource x:Key="{dxpdft:LWKey PdfViewer.LoadingDocumentLinkForegroundColor}" Color="{StaticResource Color.Custom.Blue}" />
    <SolidColorBrush x:Key="{dxpdft:LWKey PdfViewer.PdfCommentViewerSelectionBrush}" Color="{StaticResource Color.ListItem.HoverBackground}" />
    <SolidColorBrush x:Key="{dxpdft:LWKey PdfViewer.PdfCommentViewerCommentDateForegroundBrush}" Color="{StaticResource Color.Foreground.Primary}" Opacity="0.56" />
</ResourceDictionary>