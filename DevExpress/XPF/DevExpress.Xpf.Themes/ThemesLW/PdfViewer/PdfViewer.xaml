    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\PdfViewer\Base\Templates.xaml-->
    <!-- General -->

    <ControlTemplate x:Key="{dxpdft:LWKey PdfViewer.PdfPageControlItemTemplate}" TargetType="{x:Type dxpdf:PdfPageControlItem}">
        <dx:DXBorder
            x:Name="PageBorder"
            BorderBrush="{dx:LWBrush 'Brush.Border, Win11=Brush.PanelBorder'}"
            BorderThickness="1"
            Margin="-1" />
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="BorderBrush" TargetName="PageBorder" Value="{dx:LWBrush 'Brush.Focused, Win11=Brush.Accent'}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxpdft:LWKey PdfViewer.PdfThumbnailControlItemTemplate}" TargetType="{x:Type dxpdf:PdfThumbnailPageControlItem}">
        <Grid Margin="{dx:LWThickness '0, Win11=(0,2,0,0)'}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid x:Name="Shadow" Grid.Row="0">
                    <dx:DXBorder
                        BorderBrush="{dx:LWBrush 'Brush.Backstage.Editor.Background, Win11=#FF262626'}"
                        BorderThickness="5"
                        CornerRadius="5"
                        Margin="-6"
                        Opacity="0.01" />
                    <dx:DXBorder
                        BorderBrush="{dx:LWBrush 'Brush.Backstage.Editor.Background, Win11=#FF262626'}"
                        BorderThickness="4"
                        CornerRadius="4"
                        Margin="-5"
                        Opacity="0.01" />
                    <dx:DXBorder
                        BorderBrush="{dx:LWBrush 'Brush.Backstage.Editor.Background, Win11=#FF262626'}"
                        BorderThickness="3"
                        CornerRadius="3"
                        Margin="-4"
                        Opacity="0.02" />
                    <dx:DXBorder
                        BorderBrush="{dx:LWBrush 'Brush.Backstage.Editor.Background, Win11=#FF262626'}"
                        BorderThickness="2"
                        CornerRadius="2"
                        Margin="-3"
                        Opacity="0.03" />
                    <dx:DXBorder
                        BorderBrush="{dx:LWBrush 'Brush.Backstage.Editor.Background, Win11=#FF262626'}"
                        BorderThickness="1"
                        CornerRadius="1"
                        Margin="-2"
                        Opacity="0.05" />
                    <dx:DXBorder
                        BorderBrush="{dx:LWBrush 'Brush.Editor.Background, Office2019Black=Brush.Focused, Win11=Brush.Editor.Background'}"
                        BorderThickness="1" Margin="-1" />
                </Grid>
                <dx:DXBorder
                    x:Name="Highlight"
                    Grid.Row="0"
                    BorderBrush="{dx:LWBrush 'Brush.SelectionBackground, Win11=Brush.ListItem.SelectionBackground'}"
                    BorderThickness="6"
                    CornerRadius="{dx:LWCornerRadius '0, Win11=3'}"
                    Margin="-7"
                    Opacity="0" />
                <dx:DXBorder
                    x:Name="Selection"
                    Grid.Row="0"
                    BorderBrush="{dx:LWBrush 'Brush.Border, Office2019Black=Brush.SelectionBackground'}"
                    BorderThickness="2"
                    CornerRadius="{dx:LWCornerRadius '0, Win11=8'}"
                    Margin="-9"
                    Opacity="0" />
                <TextBlock
                    Grid.Row="1"
                    Margin="0,12,0,10"
                    Text="{Binding Path=PageNumber}"
                    TextAlignment="Center" />
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="true">
                <Setter Property="Opacity" TargetName="Selection" Value="1" />
            </Trigger>
            <Trigger Property="IsHighlighted" Value="true">
                <Setter Property="Opacity" TargetName="Highlight" Value="1" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <DataTemplate x:Key="{dxpdft:LWKey PdfViewer.SelectionCanvasContentTemplate}">
        <dx:DXBorder HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <dx:DXBorder.Background>
                <SolidColorBrush Color="{dx:LWColor 'Color.SelectionBackground, Win11=Color.ListItem.SelectionBackground'}" Opacity="0.3" />
            </dx:DXBorder.Background>
        </dx:DXBorder>
    </DataTemplate>
