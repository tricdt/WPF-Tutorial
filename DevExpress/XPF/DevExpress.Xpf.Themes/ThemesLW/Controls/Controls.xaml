    <!--#region c:\Work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Controls\WindowsUI\Base\SlideView.xaml-->
    <!-- General -->

    <DataTemplate x:Key="{dxcot:LWKey SlideViewElements.InteractiveHeader}">
        <Path Fill="{dx:LWBrush 'Brush.Foreground, Win11=Brush.Foreground.Primary'}" Style="{StaticResource UIShape.Right}" />
    </DataTemplate>

    <!-- Item -->

    <ControlTemplate x:Key="{dxcot:LWKey SlideViewElements.ItemTemplate}" TargetType="dxwui:SlideViewItem">
        <dx:DXBorder Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
            <Grid Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <dx:DXBorder x:Name="PART_HeaderPlaceHolder" />
                <dxwuii:SlideViewItemHeader
                    x:Name="PART_Header"
                    Grid.Row="1"
                    Content="{TemplateBinding Header}"
                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                    InteractiveHeaderTemplate="{TemplateBinding InteractiveHeaderTemplate}"
                    IsHeaderInteractive="{TemplateBinding IsHeaderInteractive}" />
                <ContentPresenter Grid.Row="2" />
            </Grid>
        </dx:DXBorder>
    </ControlTemplate>

    <!-- Header -->

    <ControlTemplate x:Key="{dxcot:LWKey SlideViewElements.ItemHeaderTemplate}" TargetType="dxwuii:SlideViewItemHeader">
        <dxwuii:SlideViewHeaderPanel x:Name="PART_HeaderPanel" ClipToBounds="True">
            <Grid x:Name="Root" Margin="-3,0,0,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <dx:DXBorder
                    x:Name="IsDefault"
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    Background="Transparent" />
                <ContentControl
                    x:Name="content"
                    Grid.Column="0"
                    Grid.ColumnSpan="1"
                    Content="{TemplateBinding Content}"
                    ContentTemplate="{TemplateBinding ContentTemplate}"
                    Margin="4,0,0,2" />
                <ContentPresenter
                    x:Name="interactiveContent"
                    Grid.Column="1"
                    Grid.ColumnSpan="1"
                    ContentTemplate="{TemplateBinding InteractiveHeaderTemplate}"
                    Margin="4,0,4,0"
                    Opacity="0" />
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)" To="{dx:LWColor 'Color.HoverBackground, Win11=Color.ListItem.HoverBackground'}" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)" To="{dx:LWColor 'Color.SelectionBackground, Win11=Color.ListItem.SelectionBackground'}" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="PART_HeaderPanel" Storyboard.TargetProperty="Opacity" To="0.38" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="InteractiveGroup">
                    <VisualState x:Name="Interactive">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="interactiveContent" Storyboard.TargetProperty="Opacity" To="1" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Noninteractive" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </dxwuii:SlideViewHeaderPanel>
    </ControlTemplate>
