    <!--#region c:\Work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Controls\WindowsUI\Base\FlipView.xaml-->
    <!-- General -->

    <ControlTemplate x:Key="{dxcot:LWKey WindowsUIElements.FlipViewTemplate}" TargetType="dxwui:FlipView">
        <Grid x:Name="Root">
            <dx:DXBorder
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                Margin="{TemplateBinding Padding}">
                <Grid>
                    <ScrollViewer
                        x:Name="ScrollingHost"
                        dx:ScrollBarExtensions.AllowMouseScrolling="False"
                        Focusable="False"
                        HorizontalScrollBarVisibility="Disabled"
                        IsTabStop="False"
                        Padding="{TemplateBinding Padding}"
                        VerticalScrollBarVisibility="Disabled">
                        <ItemsPresenter />
                    </ScrollViewer>
                    <dxwuii:FadingButton
                        x:Name="PreviousButtonHorizontal"
                        Height="40"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Width="70" />
                    <dxwuii:FadingButton
                        x:Name="NextButtonHorizontal"
                        Height="40"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Width="70" />
                    <dxwuii:FadingButton
                        x:Name="PreviousButtonVertical"
                        Height="40"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Width="70" />
                    <dxwuii:FadingButton
                        x:Name="NextButtonVertical"
                        Height="40"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Bottom"
                        Width="70" />
                </Grid>
            </dx:DXBorder>
            <Rectangle
                x:Name="FocusVisualWhite"
                IsHitTestVisible="False"
                Opacity="0"
                Stroke="{dx:LWBrush 'Brush.Backstage.Foreground, Office2019Black=Brush.Foreground, Win11=Brush.Foreground.Alt'}"
                StrokeDashArray="1,1"
                StrokeDashOffset="1.5"
                StrokeEndLineCap="Square" />
            <Rectangle
                x:Name="FocusVisualBlack"
                IsHitTestVisible="False"
                Opacity="0"
                Stroke="{dx:LWBrush 'Brush.Foreground, Win11=Brush.Foreground.Primary'}"
                StrokeDashArray="1,1"
                StrokeDashOffset="0.5"
                StrokeEndLineCap="Square" />
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="FocusStates">
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="FocusVisualWhite" Storyboard.TargetProperty="Opacity" To="1" />
                            <DoubleAnimation Duration="0" Storyboard.TargetName="FocusVisualBlack" Storyboard.TargetProperty="Opacity" To="1" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Unfocused" />
                    <VisualState x:Name="PointerFocused" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

    <!-- Item -->

    <ControlTemplate x:Key="{dxcot:LWKey WindowsUIElements.FlipViewItemTemplate}" TargetType="dxwui:FlipViewItem">
        <Grid x:Name="Root" Background="{TemplateBinding Background}">
