    <DataTemplate x:Key="queryItemTemplate">
        <ContentControl>
            <dxb:BarButtonItem Command="{Binding Command}" CommandParameter="{Binding QueryName}" Content="{Binding QueryName}">
                <dxb:BarButtonItem.ContentTemplate>
                    <DataTemplate>
                        <TextBlock Padding="0" Text="{Binding}" />
                    </DataTemplate>
                </dxb:BarButtonItem.ContentTemplate>
            </dxb:BarButtonItem>
        </ContentControl>
    </DataTemplate>
    <Style x:Key="dropDownButtonStyle" TargetType="Button">
        <Setter Property="MinWidth" Value="65" />
        <Setter Property="MinHeight" Value="22" />
        <Setter Property="Margin" Value="2,0" />
    </Style>



    <DataTemplate x:Key="{x:Type dxdadw:MultiQueryConfigureParametersPage}">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition Height="auto" />
            </Grid.RowDefinitions>
            <dxdaie:ParameterEditor
                x:Name="queryParametersEditor"
                dxdaie:ParameterEditorExtensions.GroupByQuery="True"
                EditValue="{Binding SelectedParameter, Mode=TwoWay}"
                ItemsSource="{Binding Parameters}"
                ParameterService="{Binding ParameterService}">
                <dxdaie:ParameterEditor.ValidationRulesCollection>
                    <dxdaie:ValidationRulesCollection>
                        <dxdaie:QueryParameterNameValidationRule ColumnName="Name" />
                    </dxdaie:ValidationRulesCollection>
                </dxdaie:ParameterEditor.ValidationRulesCollection>
            </dxdaie:ParameterEditor>
            <dx:ThemedWindowDialogButtonsControl Grid.Row="1">
                <dxdadw:WizardButtonsContainer HorizontalAlignment="Left" DataContext="{Binding DataContext, ElementName=queryParametersEditor}">
                    <dx:DropDownButton HorizontalAlignment="Left" Content="{dxdal:DataAccessUIStringId StringId=Button_Preview}" Style="{StaticResource dropDownButtonStyle}">
                        <dxb:PopupMenu ItemTemplate="{StaticResource queryItemTemplate}">
                            <dxb:PopupMenu.ItemLinksSource>
                                <MultiBinding Converter="{dxdadwn:QueryNameToQueryCommandConverter}">
                                    <Binding Path="QueriesToPreview" />
                                    <Binding Path="PreviewCommand" />
                                </MultiBinding>
                            </dxb:PopupMenu.ItemLinksSource>
                        </dxb:PopupMenu>
                    </dx:DropDownButton>
                    <dx:DropDownButton Content="{dxdal:DataAccessUIStringId StringId=Button_Add}" IsEnabled="{Binding QueriesToAdd, Converter={dxdadwn:EnumerableCountToBooleanConverter}}" Style="{StaticResource dropDownButtonStyle}">
                        <dxb:PopupMenu ItemTemplate="{StaticResource queryItemTemplate}">
                            <dxb:PopupMenu.ItemLinksSource>
                                <MultiBinding Converter="{dxdadwn:QueryNameToQueryCommandConverter}">
                                    <Binding Path="QueriesToAdd" />
                                    <Binding Path="AddParameterCommand" />
                                </MultiBinding>
                            </dxb:PopupMenu.ItemLinksSource>
                        </dxb:PopupMenu>
                    </dx:DropDownButton>
                    <dx:ThemedWindowDialogButton Command="{Binding RemoveParameterCommand}" Content="{dxdal:DataAccessUIStringId StringId=Button_Remove}" Style="{DynamicResource {dxdat:LWKey DataAccess.LeftAlignedWizardFooterButtonStyle}}" />
                </dxdadw:WizardButtonsContainer>
                <dxdadw:DefaultWizardButtons />
            </dx:ThemedWindowDialogButtonsControl>
        </Grid>
    </DataTemplate>
