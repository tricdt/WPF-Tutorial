    <DataTemplate x:Key="{x:Type dxdadw:ConnectionPropertiesPage}">
        <Grid>
            <dxlc:LayoutControl Grid.Row="1">
                <dxlc:LayoutControl.Resources>
                    <DataTemplate x:Key="LabelTemplate">
                        <Grid>
                            <!-- TODO: Bad method for setting min width -->
                            <TextBlock IsHitTestVisible="False" Opacity="0" Text="Authentication type:" />
                            <TextBlock Text="{Binding}" />
                        </Grid>
                    </DataTemplate>
                </dxlc:LayoutControl.Resources>
                <dxlc:LayoutGroup Orientation="Vertical">
                    <dxlc:LayoutItem Label="{dxdal:DataAccessUIStringId StringId=ConnectionProperties_Provider}" LabelTemplate="{StaticResource LabelTemplate}" Visibility="{Binding ShowProvider, Converter={dxmvvm:BooleanToVisibilityConverter}}">
                        <dxe:ComboBoxEdit
                            AllowSpinOnMouseWheel="False"
                            AutoComplete="True"
                            DisplayMember="ProviderName"
                            EditValue="{Binding SelectedProvider, UpdateSourceTrigger=PropertyChanged}"
                            IncrementalSearch="True"
                            IsTextEditable="False"
                            ItemsSource="{Binding Providers}"
                            PopupMaxHeight="124" />
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem
                        IsEnabled="{Binding DisabledConnectionEdits, Converter={dxdadwn:ConnectionParameterEditsToBooleanConverter Editor=ServerType}}"
                        Label="{dxdal:DataAccessUIStringId StringId=ConnectionProperties_ServerType}"
                        LabelTemplate="{StaticResource LabelTemplate}"
                        Visibility="{Binding ConnectionEdits, Converter={dxdadwn:ConnectionParameterEditsToVisibilityConverter Editor=ServerType}}">
                        <dxe:ComboBoxEdit
                            ApplyItemTemplateToSelectedItem="True"
                            EditValue="{Binding ServerBased, UpdateSourceTrigger=PropertyChanged}"
                            IsTextEditable="False"
                            ItemsSource="{dxdai:BooleanItemsSource FirstValue=True}">
                            <dxe:ComboBoxEdit.Resources>
                                <dxmvvm:BooleanToObjectConverter x:Key="booleanToTextConverter" FalseValue="{dxdal:DataAccessUIStringId StringId=ConnectionProperties_ServerTypeEmbedded}" TrueValue="{dxdal:DataAccessUIStringId StringId=ConnectionProperties_ServerTypeServer}" />
                            </dxe:ComboBoxEdit.Resources>
                            <dxe:ComboBoxEdit.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Margin="4,2" Text="{Binding Converter={StaticResource booleanToTextConverter}}" />
                                </DataTemplate>
                            </dxe:ComboBoxEdit.ItemTemplate>
                        </dxe:ComboBoxEdit>
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem
                        IsEnabled="{Binding DisabledConnectionEdits, Converter={dxdadwn:ConnectionParameterEditsToBooleanConverter Editor=FileName}}"
                        Label="{dxdal:DataAccessUIStringId StringId=ConnectionProperties_Database}"
                        LabelTemplate="{StaticResource LabelTemplate}"
                        Visibility="{Binding ConnectionEdits, Converter={dxdadwn:ConnectionParameterEditsToVisibilityConverter Editor=FileName}}">
                        <dxe:ButtonEdit AllowDefaultButton="False" EditValue="{Binding FileName, UpdateSourceTrigger=PropertyChanged}">
                            <dxe:ButtonInfo Command="{Binding BrowseForDatabaseFileCommand}" GlyphKind="Regular" />
                        </dxe:ButtonEdit>
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem Label="Hostname" LabelTemplate="{StaticResource LabelTemplate}" Visibility="{Binding ConnectionEdits, Converter={dxdadwn:ConnectionParameterEditsToVisibilityConverter Editor=Hostname}}">
                        <dxe:TextEdit EditValue="{Binding Hostname, UpdateSourceTrigger=PropertyChanged}" />
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem
                        IsEnabled="{Binding DisabledConnectionEdits, Converter={dxdadwn:ConnectionParameterEditsToBooleanConverter Editor=ServerName}}"
                        Label="{dxdal:DataAccessUIStringId StringId=ConnectionProperties_ServerName}"
                        LabelTemplate="{StaticResource LabelTemplate}"
                        Visibility="{Binding ConnectionEdits, Converter={dxdadwn:ConnectionParameterEditsToVisibilityConverter Editor=ServerName}}">
                        <dxe:TextEdit EditValue="{Binding ServerName, UpdateSourceTrigger=PropertyChanged}" />
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem
                        IsEnabled="{Binding DisabledConnectionEdits, Converter={dxdadwn:ConnectionParameterEditsToBooleanConverter Editor=ServerNameLookup}}"
                        Label="{dxdal:DataAccessUIStringId StringId=ConnectionProperties_ServerName}"
                        LabelTemplate="{StaticResource LabelTemplate}"
                        Visibility="{Binding ConnectionEdits, Converter={dxdadwn:ConnectionParameterEditsToVisibilityConverter Editor=ServerNameLookup}}">
                        <dxe:ComboBoxEdit
                            AllowDefaultButton="False"
                            EditValue="{Binding ServerNameLookup, UpdateSourceTrigger=PropertyChanged}"
                            IsPopupOpen="{Binding CanSelectServer, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            ItemsSource="{Binding AvailableServers}">
                            <dxe:ComboBoxEdit.Buttons>
                                <dxe:ButtonInfo Command="{Binding GetAvailableServersCommand}" GlyphKind="DropDown" />
