<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:dacp="clr-namespace:DevExpress.DataAccess.ConnectionParameters;assembly=DevExpress.DataAccess.v24.1"
    xmlns:dae="clr-namespace:DevExpress.DataAccess.Excel;assembly=DevExpress.DataAccess.v24.1"
    xmlns:dawi="clr-namespace:DevExpress.DataAccess.Wizard.Native;assembly=DevExpress.Xpf.DataAccess.v24.1"
    xmlns:dawp="clr-namespace:DevExpress.DataAccess.Wizard.Presenters;assembly=DevExpress.DataAccess.v24.1"
    xmlns:dawv="clr-namespace:DevExpress.DataAccess.Wizard.Views;assembly=DevExpress.DataAccess.v24.1"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
    xmlns:dxdadw="clr-namespace:DevExpress.Xpf.DataAccess.DataSourceWizard;assembly=DevExpress.Xpf.DataAccess.v24.1"
    xmlns:dxdadwn="clr-namespace:DevExpress.Xpf.DataAccess.DataSourceWizard.Native;assembly=DevExpress.Xpf.DataAccess.v24.1"
    xmlns:dxdai="clr-namespace:DevExpress.Xpf.DataAccess.Native;assembly=DevExpress.Xpf.DataAccess.v24.1"
    xmlns:dxdaie="clr-namespace:DevExpress.Xpf.DataAccess.Native.Editors;assembly=DevExpress.Xpf.DataAccess.v24.1"
    xmlns:dxdal="clr-namespace:DevExpress.Xpf.DataAccess.Localization;assembly=DevExpress.Xpf.DataAccess.v24.1"
    xmlns:dxdat="clr-namespace:DevExpress.Xpf.DataAccess.Themes;assembly=DevExpress.Xpf.DataAccess.v24.1"
    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
    xmlns:utils="clr-namespace:DevExpress.Utils;assembly=DevExpress.Data.v24.1">

    <dxmvvm:ObjectToObjectConverter x:Key="queryTypeToCanRunQueryBuilderConverter">
        <dxmvvm:MapItem Source="TableOrCustomSql" Target="True" />
        <dxmvvm:MapItem Source="StoredProcedure" Target="False" />
    </dxmvvm:ObjectToObjectConverter>

    <dxmvvm:BooleanToObjectConverter x:Key="booleanToTextWizardPageConverter" 
        TrueValue="{dxdal:DataAccessUIStringId StringId=WizardPageChooseObjectBindingMode_RetrieveSchema}"
        FalseValue="{dxdal:DataAccessUIStringId StringId=WizardPageChooseObjectBindingMode_RetrieveData}" />
    <dxmvvm:BooleanToObjectConverter x:Key="BooleanToDescriptionWizardPageConverter" 
        TrueValue="{dxdal:DataAccessUIStringId StringId=WizardPageChooseObjectBindingMode_RetrieveSchemaDescription}"
        FalseValue="{dxdal:DataAccessUIStringId StringId=WizardPageChooseObjectBindingMode_RetrieveDataDescription}" />

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseDataProviderPage.xaml-->
    <DataTemplate x:Key="{x:Type dxdadw:ChooseDataProviderPage}">
        <Grid Margin="12,12,12,0">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <dxe:SearchControl x:Name="searchControl" Grid.Row="0"
                Margin="0,0,0,8"
                HorizontalAlignment="Stretch" VerticalAlignment="Center"
                FilterByColumnsMode="Custom" FilterCondition="Contains"
                NullText="{dxdal:DataAccessUIStringId StringId=WizardPageChooseDataProvider_SearchPrompt}"
                ShowMRUButton="False" SearchTextPostDelay="200">
                <dxe:SearchControl.ColumnProvider>
                    <dxe:SelectorEditColumnProvider>
                        <dxe:SelectorEditColumnProvider.CustomColumns>
                            <sys:String>DisplayName</sys:String>
                        </dxe:SelectorEditColumnProvider.CustomColumns>
                    </dxe:SelectorEditColumnProvider>
                </dxe:SearchControl.ColumnProvider>
            </dxe:SearchControl>
            <dxe:ListBoxEdit
                Grid.Row="1"
                Padding="12"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                HorizontalContentAlignment="Center"
                AllowItemHighlighting="True"
                EditValue="{Binding SelectedItem, UpdateSourceTrigger=PropertyChanged}"
                FilterCriteria="{Binding FilterCriteria, ElementName=searchControl}"
                Grid.IsSharedSizeScope="True"
                ItemsSource="{Binding Items}"
                ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                ScrollViewer.VerticalScrollBarVisibility="Auto"
                ShowBorder="True">
                <dxe:ListBoxEdit.ItemTemplate>
                    <DataTemplate>
                        <Grid HorizontalAlignment="Left">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition Width="Auto" SharedSizeGroup="Caption" />
                            </Grid.ColumnDefinitions>
                            <dx:DXImage
                                Grid.Column="0"
                                Width="32"
                                Margin="8,8,12,8"
                                Source="{Binding Path=Value, Converter={dxdadwn:DataSourceTypeOrProviderToImageSourceConverter}}" />
                            <TextBlock
                                Grid.Column="1"
                                VerticalAlignment="Center"
                                Text="{Binding DisplayName}"
                                TextWrapping="Wrap" />
                        </Grid>
                    </DataTemplate>
                </dxe:ListBoxEdit.ItemTemplate>
                <dxe:ListBoxEdit.ItemsPanel>
                    <ItemsPanelTemplate>
                        <UniformGrid VerticalAlignment="Top" Columns="3" />
                    </ItemsPanelTemplate>
                </dxe:ListBoxEdit.ItemsPanel>
                <dxe:ListBoxEdit.ItemContainerStyle>
                    <Style TargetType="dxe:ListBoxEditItem">
                        <Setter Property="Margin" Value="0,0" />
                        <Setter Property="HorizontalContentAlignment" Value="Left" />
                        <Setter Property="VerticalContentAlignment" Value="Center" />
                        <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                            <Setter.Value>
                                <DataTemplate>
                                    <ContentControl>
                                        <dxdai:DoubleClickCommandExecutor Command="{Binding Path=AssociatedObject.(dxdadw:DataSourceWizardControlBase.NavigationModel).NextCommand, RelativeSource={RelativeSource Self}}" />
                                    </ContentControl>
                                </DataTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </dxe:ListBoxEdit.ItemContainerStyle>
            </dxe:ListBoxEdit>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseDataProviderPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseExistingConnectionPage.xaml-->
    <DataTemplate x:Key="{x:Type dxdadw:ChooseExistingConnectionPage}">
        <Grid>
            <dxlc:LayoutControl Grid.Row="1">
                <dxlc:LayoutGroup Orientation="Vertical">
                    <dxlc:LayoutItem>
                        <dxe:ListBoxEdit
                                ShowBorder="False"
                                EditValue="{Binding ShouldCreateNewConnection, UpdateSourceTrigger=PropertyChanged}"
                                StyleSettings="{dxdaie:RadioListBoxEditStyleSettingsEx}" >
                            <dxe:ListBoxEditItem
                                IsEnabled="{Binding DataContext.AllowCreateNewConnection, RelativeSource={RelativeSource AncestorType={x:Type dxe:ListBoxEdit}}}" >
                                <sys:Boolean>True</sys:Boolean>
                            </dxe:ListBoxEditItem>
                            <dxe:ListBoxEditItem >
                                <sys:Boolean>False</sys:Boolean>
                            </dxe:ListBoxEditItem>
                            <dxe:ListBoxEdit.ItemContainerStyle>
                                <Style TargetType="dxe:ListBoxEditItem">
                                    <Setter Property="Margin" Value="3" />
                                    <Setter Property="ContentTemplate">
                                        <Setter.Value>
                                            <DataTemplate>
                                                <DataTemplate.Resources>
                                                    <dxmvvm:BooleanToObjectConverter x:Key="booleanToTextCovnerter" 
                                                                     TrueValue="{dxdal:DataAccessUIStringId StringId=WizardPageChooseConnection_SpecifyCustomConnection}"
                                                                     FalseValue="{dxdal:DataAccessUIStringId StringId=WizardPageChooseConnection_ChooseExistingConnection}"/>

                                                </DataTemplate.Resources>
                                                <TextBlock Text="{Binding Converter={StaticResource booleanToTextCovnerter}}" />
                                            </DataTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </dxe:ListBoxEdit.ItemContainerStyle>
                        </dxe:ListBoxEdit>
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem VerticalAlignment="Stretch">
                        <dxe:ListBoxEdit
                                IsEnabled="{Binding ShouldCreateNewConnection, Converter={dxmvvm:BooleanNegationConverter}}"
                                ItemsSource="{Binding AvailableConnections}"
                                EditValue="{Binding SelectedConnection, UpdateSourceTrigger=PropertyChanged}">
                            <dxe:ListBoxEdit.ItemContainerStyle>
                                <Style TargetType="dxe:ListBoxEditItem">
                                    <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                                        <Setter.Value>
                                            <DataTemplate>
                                                <ContentControl>
                                                    <dxdai:DoubleClickCommandExecutor Command="{Binding Path=AssociatedObject.(dxdadw:DataSourceWizardControlBase.NavigationModel).NextCommand, RelativeSource={RelativeSource Self}}"/>
                                                </ContentControl>
                                            </DataTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </dxe:ListBoxEdit.ItemContainerStyle>
                        </dxe:ListBoxEdit>
                    </dxlc:LayoutItem>
                </dxlc:LayoutGroup>
            </dxlc:LayoutControl>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseExistingConnectionPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseMongoDBSchemaPage.xaml-->
    <DataTemplate x:Key="{x:Type dxdadw:ChooseMongoDBSchemaPage}">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition Height="auto" />
            </Grid.RowDefinitions>
            <Grid Margin="12,12,12,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <TextBlock Margin="0,0,24,0" VerticalAlignment="Center" Text="{dxdal:DataAccessUIStringId StringId=WizardPageChooseMongoDBSchema_QueryName}" />
                <dxe:ComboBoxEdit
                    Grid.Column="1"
                    Margin="0,0,0,4"
                    IsTextEditable="False"
                    ItemsSource="{Binding Queries}"
                    SelectedItem="{Binding SelectedQuery, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                <dxg:GridControl
                    Grid.Row="1"
                    Grid.ColumnSpan="2"
                    ItemsSource="{Binding SelectedQuerySchema}"
                    ShowBorder="True">
                    <dxg:GridControl.Columns>
                        <dxg:GridColumn FieldName="Name" AllowEditing="False" Header="{dxdal:DataAccessUIStringId StringId=WizardPageChooseMongoDBSchema_FieldName}" />
                        <dxg:GridColumn FieldName="DisplayName" AllowEditing="True" Header="{dxdal:DataAccessUIStringId StringId=WizardPageChooseMongoDBSchema_FieldDisplayName}">
                            <dxg:GridColumn.CellTemplate>
                                <DataTemplate>
                                    <dxe:TextEdit Name="PART_Editor" NullText="{Binding RowData.Row.Name}" />
                                </DataTemplate>
                            </dxg:GridColumn.CellTemplate>
                            <dxmvvm:Interaction.Behaviors>
                                <dxdadwn:MongoDBNodeAliasValidationBehavior ItemsSource="{Binding Path=AssociatedObject.DataContext.SelectedQuerySchema, RelativeSource={RelativeSource Self}}" />
                            </dxmvvm:Interaction.Behaviors>
                        </dxg:GridColumn>
                        <dxg:GridColumn AllowEditing="False" Binding="{Binding Converter={dxdadwn:MongoDBNodeTypeConverter}}" Header="{dxdal:DataAccessUIStringId StringId=WizardPageChooseMongoDBSchema_Type}" />
                    </dxg:GridControl.Columns>
                    <dxg:GridControl.View>
                        <dxg:TreeListView
                            AllowEditing="True"
                            AllowRecursiveNodeChecking="True"
                            AutoWidth="True"
                            CheckBoxFieldName="Selected"
                            ChildNodesPath="Nodes"
                            NavigationStyle="Cell"
                            ShowCheckboxes="True"
                            ShowIndicator="False"
                            TreeDerivationMode="ChildNodesSelector"
                            VerticalScrollbarVisibility="Auto">
                            <dxmvvm:Interaction.Behaviors>
                                <dxdai:ExpandTreeListNodeBehavior Depth="-1" />
                            </dxmvvm:Interaction.Behaviors>
                        </dxg:TreeListView>
                    </dxg:GridControl.View>
                </dxg:GridControl>
            </Grid>
            <dx:ThemedWindowDialogButtonsControl Grid.Row="1">
                <dxdadw:WizardButtonsContainer HorizontalAlignment="Left" Visibility="{Binding ShowAnalyzeAllDocumentsButton, Converter={dxmvvm:BooleanToVisibilityConverter}}">
                    <dx:ThemedWindowDialogButton Command="{Binding AnalyzeAllDocumentsCommand}" Content="{dxdal:DataAccessUIStringId StringId=WizardPageChooseJsonSchema_AnalyzeAllRecordsButton}" Style="{DynamicResource {dxdat:LWKey DataAccess.LeftAlignedWizardFooterButtonStyle}}" />
                    <dx:DXImage
                        Width="16"
                        Height="16"
                        Margin="4,0,0,0"
                        Source="{dxdai:DataAccessSvgImage Uri=Images/Info.svg}"
                        ToolTipService.ShowDuration="{x:Static sys:Int32.MaxValue}">
                        <dx:DXImage.ToolTip>
                            <ToolTip MaxWidth="400">
                                <StackPanel>
                                    <TextBlock Text="{Binding AnalyzeDocumentsTooltip}" TextWrapping="Wrap" />
                                </StackPanel>
                            </ToolTip>
                        </dx:DXImage.ToolTip>
                    </dx:DXImage>
                </dxdadw:WizardButtonsContainer>

                <dxdadw:DefaultWizardButtons />
            </dx:ThemedWindowDialogButtonsControl>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseMongoDBSchemaPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseEFConnectionStringPage.xaml-->
    <DataTemplate x:Key="defaultConnectionTemplate" />
    <DataTemplate x:Key="chooseConnectionTemplate">
        <dxe:ListBoxEdit EditValue="{Binding SelectedConnectionName}" ItemsSource="{Binding AvailableConnections}" />
    </DataTemplate>
    <DataTemplate x:Key="newConnectionTemplate">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <TextBlock Text="{dxdal:DataAccessUIStringId StringId=ConnectionProperties_ConnectionString}" />
            <dxe:TextEdit
                Grid.Row="1"
                VerticalContentAlignment="Top"
                EditValue="{Binding NewConnectionString}"
                TextWrapping="Wrap" />
        </Grid>
    </DataTemplate>



    <dxmvvm:ObjectToObjectConverter x:Key="modeToTemplateConverter">
        <dxmvvm:ObjectToObjectConverter.Map>
            <dxmvvm:MapItem Source="{x:Static dawv:ConfigureConnectionMode.Default}" Target="{StaticResource defaultConnectionTemplate}" />
            <dxmvvm:MapItem Source="{x:Static dawv:ConfigureConnectionMode.New}" Target="{StaticResource newConnectionTemplate}" />
            <dxmvvm:MapItem Source="{x:Static dawv:ConfigureConnectionMode.Existing}" Target="{StaticResource chooseConnectionTemplate}" />
        </dxmvvm:ObjectToObjectConverter.Map>
    </dxmvvm:ObjectToObjectConverter>

    <dxmvvm:ObjectToObjectConverter x:Key="modeLocalizationConverter">
        <dxmvvm:ObjectToObjectConverter.Map>
            <dxmvvm:MapItem Source="{x:Static dawv:ConfigureConnectionMode.Default}"
                            Target="{dxdal:DataAccessUIStringId StringId=WizardPageConfigureEFConnection_DefaultConnection}" />
            <dxmvvm:MapItem Source="{x:Static dawv:ConfigureConnectionMode.New}"
                            Target="{dxdal:DataAccessUIStringId StringId=WizardPageConfigureEFConnection_CustomConnection}" />
            <dxmvvm:MapItem Source="{x:Static dawv:ConfigureConnectionMode.Existing}"
                            Target="{dxdal:DataAccessUIStringId StringId=WizardPageConfigureEFConnection_ChooseConnection}" />
        </dxmvvm:ObjectToObjectConverter.Map>
    </dxmvvm:ObjectToObjectConverter>
    <DataTemplate x:Key="{x:Type dxdadw:ChooseEFConnectionStringPage}">
        <Grid Margin="36,24,36,0">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" />
                <RowDefinition />
                <RowDefinition Height="auto" />
            </Grid.RowDefinitions>
            <dxe:ListBoxEdit ItemsSource="{Binding AvailableModes}" SelectedItem="{Binding Mode, Mode=TwoWay}" ShowBorder="False">
                <dxe:ListBoxEdit.StyleSettings>
                    <dxe:RadioListBoxEditStyleSettings />
                </dxe:ListBoxEdit.StyleSettings>
                <dxe:ListBoxEdit.ItemTemplate>
                    <DataTemplate>
                        <TextBlock Text="{Binding Converter={StaticResource modeLocalizationConverter}}" />
                    </DataTemplate>
                </dxe:ListBoxEdit.ItemTemplate>
            </dxe:ListBoxEdit>

            <ContentControl Grid.Row="1" Content="{Binding}" ContentTemplate="{Binding Mode, Converter={StaticResource modeToTemplateConverter}}" />

            <StackPanel Grid.Row="2" Visibility="{Binding CanSaveToStorage, Converter={dxmvvm:BooleanToVisibilityConverter}}">
                <dxe:CheckEdit Content="{dxdal:DataAccessUIStringId StringId=WizardPageConfigureEFConnection_SaveToConfig}" IsChecked="{Binding ShouldSaveConnectionString}" />
                <dxe:TextEdit EditValue="{Binding NewConnectionName}" IsEnabled="{Binding ShouldSaveConnectionString}" />
            </StackPanel>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseEFConnectionStringPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseJsonConnectionPage.xaml-->
    <DataTemplate x:Key="{x:Type dxdadw:ChooseJsonConnectionPage}">
        <Grid>
            <dxlc:LayoutControl Grid.Row="1">
                <dxlc:LayoutGroup Orientation="Vertical">
                    <dxlc:LayoutItem>
                        <dxe:ListBoxEdit EditValue="{Binding ShouldCreateNewConnection, UpdateSourceTrigger=PropertyChanged}" ShowBorder="False" StyleSettings="{dxdaie:RadioListBoxEditStyleSettingsEx}">
                            <dxe:ListBoxEditItem IsEnabled="{Binding DataContext.AllowCreateNewConnection, RelativeSource={RelativeSource AncestorType={x:Type dxe:ListBoxEdit}}}">
                                <sys:Boolean>True</sys:Boolean>
                            </dxe:ListBoxEditItem>
                            <dxe:ListBoxEditItem>
                                <sys:Boolean>False</sys:Boolean>
                            </dxe:ListBoxEditItem>
                            <dxe:ListBoxEdit.ItemContainerStyle>
                                <Style TargetType="dxe:ListBoxEditItem">
                                    <Setter Property="Margin" Value="3" />
                                    <Setter Property="ContentTemplate">
                                        <Setter.Value>
                                            <DataTemplate>
                                                <DataTemplate.Resources>
                                                    <dxmvvm:BooleanToObjectConverter x:Key="booleanToTextCovnerter" FalseValue="{dxdal:DataAccessUIStringId StringId=WizardPageChooseConnection_ChooseExistingConnection}" TrueValue="{dxdal:DataAccessUIStringId StringId=WizardPageChooseConnection_SpecifyCustomConnection}" />

                                                </DataTemplate.Resources>
                                                <TextBlock Text="{Binding Converter={StaticResource booleanToTextCovnerter}}" />
                                            </DataTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </dxe:ListBoxEdit.ItemContainerStyle>
                        </dxe:ListBoxEdit>
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem VerticalAlignment="Stretch">
                        <dxe:ListBoxEdit EditValue="{Binding SelectedConnection, UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding ShouldCreateNewConnection, Converter={dxmvvm:BooleanNegationConverter}}" ItemsSource="{Binding AvailableConnections}">
                            <dxe:ListBoxEdit.ItemContainerStyle>
                                <Style TargetType="dxe:ListBoxEditItem">
                                    <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                                        <Setter.Value>
                                            <DataTemplate>
                                                <ContentControl>
                                                    <dxdai:DoubleClickCommandExecutor Command="{Binding Path=AssociatedObject.(dxdadw:DataSourceWizardControlBase.NavigationModel).NextCommand, RelativeSource={RelativeSource Self}}" />
                                                </ContentControl>
                                            </DataTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </dxe:ListBoxEdit.ItemContainerStyle>
                        </dxe:ListBoxEdit>
                    </dxlc:LayoutItem>
                </dxlc:LayoutGroup>
            </dxlc:LayoutControl>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseJsonConnectionPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseJsonSchemaPage.xaml-->
    <DataTemplate x:Key="{x:Type dxdadw:ChooseJsonSchemaPage}">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition Height="auto" />
            </Grid.RowDefinitions>
            <Grid Margin="12,12,12,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <TextBlock Margin="0,0,24,0" VerticalAlignment="Center" Text="{dxdal:DataAccessUIStringId StringId=WizardPageChooseJsonSchema_RootElement}" />
                <dxe:ComboBoxEdit
                    Grid.Column="1"
                    Margin="0,0,0,4"
                    IsTextEditable="False"
                    ItemsSource="{Binding Roots}"
                    SelectedItem="{Binding RootElement, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                <dxg:GridControl
                    Grid.Row="1"
                    Grid.ColumnSpan="2"
                    ItemsSource="{Binding Schema}"
                    ShowBorder="True">
                    <dxg:GridControl.Columns>
                        <dxg:GridColumn FieldName="Name" AllowEditing="False" Header="{dxdal:DataAccessUIStringId StringId=WizardPageChooseJsonSchema_FieldName}" />
                        <dxg:GridColumn FieldName="DisplayName" AllowEditing="True" Header="{dxdal:DataAccessUIStringId StringId=WizardPageChooseJsonSchema_FieldDisplayName}">
                            <dxg:GridColumn.CellTemplate>
                                <DataTemplate>
                                    <dxe:TextEdit Name="PART_Editor" NullText="{Binding RowData.Row.Name}" />
                                </DataTemplate>
                            </dxg:GridColumn.CellTemplate>
                            <dxmvvm:Interaction.Behaviors>
                                <dxdadwn:JsonNodeAliasValidationBehavior ItemsSource="{Binding Path=AssociatedObject.DataContext.SelectedQuerySchema, RelativeSource={RelativeSource Self}}" />
                            </dxmvvm:Interaction.Behaviors>
                        </dxg:GridColumn>
                        <dxg:GridColumn AllowEditing="False" Binding="{Binding Converter={dxdadwn:JsonNodeTypeConverter}}" Header="{dxdal:DataAccessUIStringId StringId=WizardPageChooseJsonSchema_Type}" />
                    </dxg:GridControl.Columns>
                    <dxg:GridControl.View>
                        <dxg:TreeListView
                            AllowEditing="True"
                            AllowRecursiveNodeChecking="True"
                            AutoWidth="True"
                            CheckBoxFieldName="Selected"
                            KeyFieldName="Id"
                            NavigationStyle="Cell"
                            ParentFieldName="ParentId"
                            ShowCheckboxes="True"
                            ShowIndicator="False"
                            VerticalScrollbarVisibility="Auto">
                            <dxmvvm:Interaction.Behaviors>
                                <dxdai:ExpandTreeListNodeBehavior Depth="-1" />
                            </dxmvvm:Interaction.Behaviors>
                        </dxg:TreeListView>
                    </dxg:GridControl.View>
                </dxg:GridControl>
            </Grid>
            <dx:ThemedWindowDialogButtonsControl Grid.Row="1">
                <dxdadw:WizardButtonsContainer HorizontalAlignment="Left" Visibility="{Binding ShowAnalyzeRecordsButton, Converter={dxmvvm:BooleanToVisibilityConverter}}">
                    <dx:ThemedWindowDialogButton Command="{Binding AnalyzeAllRecordsCommand}" Content="{dxdal:DataAccessUIStringId StringId=WizardPageChooseJsonSchema_AnalyzeAllRecordsButton}" Style="{DynamicResource {dxdat:LWKey DataAccess.LeftAlignedWizardFooterButtonStyle}}" />
                    <dx:DXImage
                        Width="16"
                        Height="16"
                        Margin="4,0,0,0"
                        Source="{dxdai:DataAccessSvgImage Uri=Images/Info.svg}"
                        ToolTipService.ShowDuration="{x:Static sys:Int32.MaxValue}">
                        <dx:DXImage.ToolTip>
                            <ToolTip MaxWidth="400">
                                <StackPanel>
                                    <TextBlock Text="{Binding AnalyzeRecordsTooltip}" TextWrapping="Wrap" />
                                </StackPanel>
                            </ToolTip>
                        </dx:DXImage.ToolTip>
                    </dx:DXImage>
                </dxdadw:WizardButtonsContainer>
                <dxdadw:DefaultWizardButtons />
            </dx:ThemedWindowDialogButtonsControl>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseJsonSchemaPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.DataAccess\Themes\Generic\DataSourceWizard\Pages\ChooseJsonSourcePage.xaml-->
    <DataTemplate x:Key="{x:Type dxdadw:ChooseJsonSourcePage}">
        <DataTemplate.Resources>
            <DataTemplate x:Key="JsonCustomTemplate">
                <dxlc:LayoutItem VerticalContentAlignment="Stretch">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" SharedSizeGroup="A" />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <TextBlock Margin="0,0,4,0" Text="{dxdal:DataAccessUIStringId StringId=WizardPageChooseJsonSource_Custom}" />
                        <dxe:TextEdit
                            Grid.Column="1"
                            Height="300"
                            VerticalContentAlignment="Top"
                            AcceptsReturn="True"
                            EditValue="{Binding Json, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            TextWrapping="Wrap" />
                    </Grid>
                </dxlc:LayoutItem>
            </DataTemplate>
            <DataTemplate x:Key="JsonUriTemplate">
                <Grid>
                    <dxlc:LayoutItem VerticalAlignment="Stretch" VerticalContentAlignment="Stretch">
                        <dxlc:LayoutGroup Orientation="Vertical">
                            <dxlc:LayoutItem VerticalContentAlignment="Stretch">
