    <DataTemplate x:Key="{x:Type dxdadw:ConfigureQueryPage}">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition Height="auto" />
            </Grid.RowDefinitions>
            <dxlc:LayoutControl>
                <dxlc:LayoutGroup Orientation="Vertical">
                    <dxlc:LayoutItem>
                        <dxe:ListBoxEdit
                            EditValue="{Binding QueryType, UpdateSourceTrigger=PropertyChanged}"
                            ShowBorder="False"
                            StyleSettings="{dxdaie:RadioListBoxEditStyleSettingsEx}"
                            ValueMember="Value">
                            <dxe:ListBoxEditItem Content="{x:Static dawp:QueryType.TableOrCustomSql}" />
                            <dxe:ListBoxEditItem Content="{x:Static dawp:QueryType.StoredProcedure}" IsEnabled="{Binding DataContext.CanSelectStoredProcedures, RelativeSource={RelativeSource AncestorType={x:Type dxe:ListBoxEdit}}}" />
                            <dxe:ListBoxEdit.ItemContainerStyle>
                                <Style TargetType="dxe:ListBoxEditItem">
                                    <Setter Property="Margin" Value="3" />
                                    <Setter Property="ContentTemplate">
                                        <Setter.Value>
                                            <DataTemplate>
                                                <DataTemplate.Resources>
                                                    <dxmvvm:ObjectToObjectConverter x:Key="queryTypeToTextConverter">
                                                        <dxmvvm:MapItem Source="TableOrCustomSql" Target="{dxdal:DataAccessUIStringId StringId=WizardPageConfigureQuery_Query}" />
                                                        <dxmvvm:MapItem Source="StoredProcedure" Target="{dxdal:DataAccessUIStringId StringId=WizardPageConfigureQuery_StoredProcedure}" />
                                                    </dxmvvm:ObjectToObjectConverter>
                                                </DataTemplate.Resources>
                                                <TextBlock Text="{Binding Converter={StaticResource queryTypeToTextConverter}}" />
                                            </DataTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </dxe:ListBoxEdit.ItemContainerStyle>
                        </dxe:ListBoxEdit>
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem
                        VerticalAlignment="Stretch"
                        Label="{dxdal:DataAccessUIStringId StringId=QueryControl_SqlString}"
                        LabelPosition="Top"
                        Visibility="{Binding QueryType, Converter={dxdadwn:QueryTypeToVisibilityConverter Expected=TableOrCustomSql}}">
                        <dxe:TextEdit
                            VerticalContentAlignment="Top"
                            AcceptsReturn="True"
                            AcceptsTab="True"
                            EditValue="{Binding SqlString, UpdateSourceTrigger=PropertyChanged}"
                            FontFamily="Courier New, Lucida Console"
                            IsReadOnly="{Binding AllowCustomSql, Converter={dxmvvm:BooleanNegationConverter}}" />
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem
                        VerticalAlignment="Stretch"
                        Label="{dxdal:DataAccessUIStringId StringId=StoredProcControl_Caption}"
                        LabelPosition="Top"
                        Visibility="{Binding QueryType, Converter={dxdadwn:QueryTypeToVisibilityConverter Expected=StoredProcedure}}">
                        <dxe:ListBoxEdit ItemsSource="{Binding StoredProcedures}" SelectedIndex="{Binding SelectedStoredProcedureIndex}" />
                    </dxlc:LayoutItem>
                </dxlc:LayoutGroup>
            </dxlc:LayoutControl>
            <dx:ThemedWindowDialogButtonsControl Grid.Row="1">
                <dxdadw:WizardButtonsContainer HorizontalAlignment="Left">
                    <dx:ThemedWindowDialogButton
                        Command="{Binding RunQueryBuilderCommand}"
                        Content="{dxdal:DataAccessUIStringId StringId=Button_QueryBuilder}"
                        IsEnabled="{Binding QueryType, Converter={StaticResource queryTypeToCanRunQueryBuilderConverter}}"
                        Style="{DynamicResource {dxdat:LWKey DataAccess.LeftAlignedWizardFooterButtonStyle}}" />
                </dxdadw:WizardButtonsContainer>
                <dxdadw:DefaultWizardButtons />
            </dx:ThemedWindowDialogButtonsControl>
        </Grid>
    </DataTemplate>
