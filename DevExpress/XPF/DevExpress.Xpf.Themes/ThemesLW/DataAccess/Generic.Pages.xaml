                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem VerticalAlignment="Stretch">
                        <dxe:ListBoxEdit EditValue="{Binding SelectedConnection, UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding ShouldCreateNewConnection, Converter={dxmvvm:BooleanNegationConverter}}" ItemsSource="{Binding AvailableConnections}">
