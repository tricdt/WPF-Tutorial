<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
    xmlns:dxdai="http://schemas.devexpress.com/winfx/2008/xaml/dataaccess/internal"
    xmlns:dxdat="http://schemas.devexpress.com/winfx/2008/xaml/dataaccess/themekeys">

    <ResourceDictionary.MergedDictionaries>
        <dxi:PaletteResourceDictionary />
        <dxi:ThemeResourceDictionary Path="DataAccess/Generic.DataAccess.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="{dxdat:LWKey ConditionControl.ErrorColor}" Color="{StaticResource Color.Custom.Red}" />

</ResourceDictionary>