    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduling\Themes\Generic\Controls\SchedulerScrollViewer.xaml-->
    <ControlTemplate x:Key="SchedulerScrollViewer.DefaultTemplate" TargetType="dxschv:SchedulerScrollViewer">
        <Grid x:Name="Grid" Background="{TemplateBinding Background}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <ScrollContentPresenter
                x:Name="PART_ScrollContentPresenter"
                Grid.Row="0"
                Grid.Column="0"
                Margin="{TemplateBinding Padding}"
                CanContentScroll="{TemplateBinding CanContentScroll}"
                CanHorizontallyScroll="False"
                CanVerticallyScroll="False"
                Content="{TemplateBinding Content}"
                ContentTemplate="{TemplateBinding ContentTemplate}" />
            <DockPanel
                x:Name="PART_VerticalScrollBarArea"
                Grid.Row="0"
                Grid.Column="1"
                Visibility="{TemplateBinding ComputedVerticalScrollBarVisibility}">
                <ContentPresenter
                    Content="{TemplateBinding VerticalScrollBarControlBox}"
                    ContentTemplate="{TemplateBinding VerticalScrollBarControlBoxTemplate}"
                    DockPanel.Dock="Bottom"
                    Visibility="{TemplateBinding VerticalScrollBarControlBoxVisibility}" />
                <dxschv:SchedulerScrollBar
                    x:Name="PART_VerticalScrollBar"
                    AutomationProperties.AutomationId="VerticalScrollBar"
                    Cursor="Arrow"
                    Maximum="{TemplateBinding ScrollableHeight}"
                    Minimum="0"
                    Orientation="Vertical"
                    ToolTipService.IsEnabled="False"
                    ViewportSize="{TemplateBinding ViewportHeight}"
                    Value="{Binding VerticalOffset, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}">
                    <dxschv:SchedulerScrollBar.ToolTip>
                        <ToolTip
                            HorizontalOffset="-6"
                            Placement="Left"
                            StaysOpen="True"
                            VerticalOffset="24">
                            <dx:SuperTipControl Width="140" PatchDataContext="True">
                                <dx:SuperTipControl.SuperTip>
                                    <dx:SuperTip>
                                        <dx:SuperTipHeaderItem Content="{Binding VisualData.ScrollTipInfo.IntervalStart, Converter={dxschc:DateTimeToScrollTipStringConverter}, ConverterParameter='MMM, yyyy'}" />
                                        <dx:SuperTipItem Content="{Binding VisualData.ScrollTipInfo.Interval, Converter={dxschc:DateTimeRangeToScrollTipStringConverter}, ConverterParameter=d}" />
                                    </dx:SuperTip>
                                </dx:SuperTipControl.SuperTip>
                            </dx:SuperTipControl>
                        </ToolTip>
                    </dxschv:SchedulerScrollBar.ToolTip>
                </dxschv:SchedulerScrollBar>
            </DockPanel>
            <DockPanel
                x:Name="PART_HorizontalScrollBarArea"
                Grid.Row="1"
                Grid.Column="0"
                Panel.ZIndex="1"
                Visibility="{TemplateBinding ComputedHorizontalScrollBarVisibility}">
                <ContentPresenter
                    Content="{TemplateBinding HorizontalScrollBarControlBox}"
                    ContentTemplate="{TemplateBinding HorizontalScrollBarControlBoxTemplate}"
                    DockPanel.Dock="Right"
                    Visibility="{TemplateBinding HorizontalScrollBarControlBoxVisibility}" />
                <dxschv:SchedulerScrollBar
                    x:Name="PART_HorizontalScrollBar"
                    AutomationProperties.AutomationId="HorizontalScrollBar"
                    Cursor="Arrow"
                    Maximum="{TemplateBinding ScrollableWidth}"
                    Minimum="0"
                    Orientation="Horizontal"
                    ViewportSize="{TemplateBinding ViewportWidth}"
                    Value="{Binding HorizontalOffset, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}" />
            </DockPanel>
            <dx:DXBorder
                x:Name="corner"
                Grid.Row="1"
                Grid.Column="1"
                Style="{TemplateBinding CornerBoxStyle}" />
        </Grid>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="ComputedVerticalScrollBarVisibility" Value="Collapsed" />
                    <Condition Property="ComputedHorizontalScrollBarVisibility" Value="Collapsed" />
                </MultiTrigger.Conditions>
                <Setter TargetName="corner" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>

            <Trigger Property="HorizontalScrollBarControlBoxVisibility" Value="Visible">
                <Setter TargetName="corner" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="PART_HorizontalScrollBarArea" Property="Grid.ColumnSpan" Value="2" />
            </Trigger>
            <Trigger Property="VerticalScrollBarControlBoxVisibility" Value="Visible">
                <Setter TargetName="corner" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="PART_VerticalScrollBarArea" Property="Grid.RowSpan" Value="2" />
            </Trigger>

            <Trigger Property="dx:ScrollBarExtensions.ScrollBarMode" Value="TouchOverlap">
                <Setter TargetName="corner" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="PART_HorizontalScrollBar" Property="AutoHide" Value="True" />
                <Setter TargetName="PART_VerticalScrollBar" Property="AutoHide" Value="True" />
                <Setter TargetName="PART_ScrollContentPresenter" Property="Grid.ColumnSpan" Value="2" />
                <Setter TargetName="PART_ScrollContentPresenter" Property="Grid.RowSpan" Value="2" />
                <Setter TargetName="PART_VerticalScrollBarArea" Property="Grid.RowSpan" Value="2" />
                <Setter TargetName="PART_HorizontalScrollBarArea" Property="Grid.ColumnSpan" Value="2" />
            </Trigger>
            <Trigger Property="HorizontalScrollBarControlBoxVisibility" Value="Visible">
                <Setter TargetName="PART_VerticalScrollBarArea" Property="Grid.RowSpan" Value="1" />
            </Trigger>
            <Trigger Property="VerticalScrollBarControlBoxVisibility" Value="Visible">
                <Setter TargetName="PART_HorizontalScrollBarArea" Property="Grid.ColumnSpan" Value="1" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>


