<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
    xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduling"
    xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduling/themekeys">
    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="Common/{ThemeName}/Common.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Color x:Key="BrushSet.Resource1.Color">
        #0078d7
    </Color>
    <Color x:Key="BrushSet.Resource2.Color">
        #298835
    </Color>
    <Color x:Key="BrushSet.Resource3.Color">
        #d83b01
    </Color>
    <Color x:Key="BrushSet.Resource4.Color">
        #1e3698
    </Color>
    <Color x:Key="BrushSet.Resource5.Color">
        #ffb900
    </Color>
    <Color x:Key="BrushSet.Resource6.Color">
        #0b857d
    </Color>
    <Color x:Key="BrushSet.Resource7.Color">
        #e40089
    </Color>
    <Color x:Key="BrushSet.Resource8.Color">
        #b4009e
    </Color>
    <Color x:Key="BrushSet.Resource9.Color">
        #a80000
    </Color>
    <SolidColorBrush x:Key="ResourceForeground" Color="White" />
    <SolidColorBrush x:Key="Scheduler.Background" Color="{StaticResource Color.Control.Background}" />
    <SolidColorBrush x:Key="Scheduler.Foreground" Color="{StaticResource Color.Foreground}" />
</ResourceDictionary>