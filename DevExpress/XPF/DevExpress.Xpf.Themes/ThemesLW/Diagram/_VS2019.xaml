<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
    xmlns:dxdiagt="http://schemas.devexpress.com/winfx/2008/xaml/diagram/themekeys"
    xmlns:dxdiag="http://schemas.devexpress.com/winfx/2008/xaml/diagram"
    xmlns:dxdiagi="http://schemas.devexpress.com/winfx/2008/xaml/diagram/internal"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm">
    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="Diagram/_{ThemeName}.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramToolboxControl.Background}" Color="{dx:LWColor 'VS2019Blue=Color.Editor.Background, VS2019Dark=Color.Window.Background, VS2019Light=Color.Window.Background'}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.Shadow}" Color="{StaticResource Color.Foreground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.Foreground}" Color="{StaticResource Color.Foreground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.HorizontalBackground}" Color="{StaticResource Color.Window.Background}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.VerticalBackground}" Color="{StaticResource Color.Window.Background}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.BorderBrush}" Color="{StaticResource Color.Border}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.TickBrush}" Color="{StaticResource Color.Border}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Controls.SelectionPreviewBorderBrush}" Color="{StaticResource Color.SelectionBackground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Controls.SelectionPreviewBackground}" Color="{StaticResource Color.SelectionBackground}" Opacity="0.38" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Controls.GridLineBrush}" Color="{StaticResource Color.Delimiter}" Opacity="0.38" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Controls.DragPreviewBrush}" Color="{StaticResource Color.SelectionBackground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramControl.DiagramControlBackground}" Color="{StaticResource Color.Window.Background}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramToolboxControl.ToolsAreaBackground}" Color="{StaticResource Color.Editor.Background}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramToolboxControl.StencilsSelectorForeground}" Color="{StaticResource Color.Foreground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramPanAndZoomControl.ClientFrameBorderBrush}" Color="#FF4D6AB8" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramPanAndZoomControl.PreviewFrameBorderBrush}" Color="#FFA0A0A0" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramPanAndZoomControl.PresenterBackground}" Color="Transparent" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramPanAndZoomControl.ZoomTrackBarBackground}" Color="Transparent" />
    <sys:Double x:Key="{dxdiagt:LWKey DiagramDesignerControl.FontSizeEditorWidth}">50</sys:Double>
</ResourceDictionary>