    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Diagram\Base\Controls.xaml-->


    <!-- General -->

    <ControlTemplate x:Key="{dxdiagt:LWKey DiagramControl.PageBackgroundTemplate}" TargetType="ContentControl">
        <dx:DXBorder Background="{TemplateBinding Background}" BorderBrush="{StaticResource Brush.Border}" BorderThickness="{TemplateBinding BorderThickness}">
            <ContentPresenter ClipToBounds="True" />
        </dx:DXBorder>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxdiagt:LWKey DiagramControl.BackgroundTemplate}" TargetType="ContentControl">
        <dx:DXBorder Background="{TemplateBinding Background}">
            <ContentPresenter />
        </dx:DXBorder>
    </ControlTemplate>

    <Style x:Key="{dxdiagt:LWKey DiagramToolboxControl.StencilsSelectorItemStyle}" TargetType="ListBoxItem">
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="Margin" Value="0,0,1,1" />
    </Style>

    <Style x:Key="{dxdiagt:LWKey DiagramToolboxControl.ShapesSelectorItemStyle}" TargetType="ListBoxItem">
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="Margin" Value="0,0,1,1" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="Tag" Value="{Binding RelativeSource={RelativeSource AncestorType={x:Type dxdiag:DiagramToolboxControl}}}" />
        <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
            <Setter.Value>
                <DataTemplate>
                    <ItemsControl>
                        <dxdiag:DiagramToolBehavior Diagram="{Binding AssociatedObject.Tag.DiagramControl, RelativeSource={RelativeSource Self}}" DiagramTool="{Binding}" />
                    </ItemsControl>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ToolTip">
            <Setter.Value>
                <MultiBinding Converter="{StaticResource ToolTipConverter}">
                    <Binding Path="ToolName" />
                    <Binding Path="Tag.IsCompact" RelativeSource="{RelativeSource Self}" />
                    <Binding Path="Tag.ShapePreviewMode" RelativeSource="{RelativeSource Self}" />
                </MultiBinding>
            </Setter.Value>
        </Setter>
    </Style>
