    <sys:Double x:Key="{dxdiagt:LWKey DiagramDesignerControl.FontFamilyEditorWidth}">100</sys:Double>
    <Thickness x:Key="{dxdiagt:LWKey Controls.PageBorderThickness}">1</Thickness>
    <Thickness x:Key="{dxdiagt:LWKey DiagramDesignerControl.DockLayoutManagerBorderThickness}">0</Thickness>
    <Thickness x:Key="{dxdiagt:LWKey DiagramDesignerControl.DiagramControlMargin}">0</Thickness>


