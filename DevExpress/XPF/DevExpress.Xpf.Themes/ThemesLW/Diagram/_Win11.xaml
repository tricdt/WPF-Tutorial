<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
    xmlns:dxdiagt="http://schemas.devexpress.com/winfx/2008/xaml/diagram/themekeys"
    xmlns:dxdiag="http://schemas.devexpress.com/winfx/2008/xaml/diagram"
    xmlns:dxdiagi="http://schemas.devexpress.com/winfx/2008/xaml/diagram/internal"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm">
    <ResourceDictionary.MergedDictionaries>
        <dxi:PaletteResourceDictionary />
    </ResourceDictionary.MergedDictionaries>
    <dxmvvm:ObjectToObjectConverter x:Key="IdToMenuPointerVisibilityConverter" DefaultTarget="Collapsed">
        <dxmvvm:MapItem Source="MoreShapes" Target="Visible" />
    </dxmvvm:ObjectToObjectConverter>
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramToolboxControl.Background}" Color="{StaticResource Color.WindowBackground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.Shadow}" Color="{StaticResource Color.Foreground.Primary}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.Foreground}" Color="{StaticResource Color.Foreground.Primary}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.HorizontalBackground}" Color="{StaticResource Color.WindowBackground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.VerticalBackground}" Color="{StaticResource Color.WindowBackground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.BorderBrush}" Color="{StaticResource Color.Border}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Ruler.TickBrush}" Color="{StaticResource Color.Border}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Controls.SelectionPreviewBorderBrush}" Color="{StaticResource Color.ListItem.SelectionBackground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Controls.SelectionPreviewBackground}" Color="{StaticResource Color.ListItem.SelectionBackground}" Opacity="0.38" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Controls.GridLineBrush}" Color="{StaticResource Color.Delimiter}" Opacity="0.38" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey Controls.DragPreviewBrush}" Color="{StaticResource Color.ListItem.SelectionBackground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramControl.DiagramControlBackground}" Color="{StaticResource Color.WindowBackground}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramToolboxControl.ToolsAreaBackground}" Color="{StaticResource Color.Editor.Background}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramToolboxControl.StencilsSelectorForeground}" Color="{StaticResource Color.Foreground.Primary}" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramPanAndZoomControl.ClientFrameBorderBrush}" Color="#FF4D6AB8" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramPanAndZoomControl.PreviewFrameBorderBrush}" Color="#FFA0A0A0" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramPanAndZoomControl.PresenterBackground}" Color="Transparent" />
    <SolidColorBrush x:Key="{dxdiagt:LWKey DiagramPanAndZoomControl.ZoomTrackBarBackground}" Color="Transparent" />
    <DataTemplate x:Key="{dxdiagt:LWKey DiagramToolboxControl.StencilsSelectorItemTemplate}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <TextBlock
                x:Name="stencilName"
                Foreground="{DynamicResource {dxdiagt:LWKey DiagramToolboxControl.StencilsSelectorForeground}}"
                Margin="0,1,3,1"
                Text="{Binding Name}"
                Visibility="{Binding IsCompact, Converter={dxmvvm:BooleanToVisibilityConverter Inverse=True}, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramToolboxControl}}}" />
            <Image
                Grid.ColumnSpan="2"
                Height="16"
                HorizontalAlignment="Center"
                Margin="-2,1,0,1"
                Source="{dxdiag:DiagramSvgImage Size='16,16', Path=Images/Svg/CompactView.svg}"
                Visibility="{Binding IsCompact, Converter={dxmvvm:BooleanToVisibilityConverter}, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramToolboxControl}}}"
                Width="16" />
            <Path
                Grid.Column="1"
                Data="F1 M5,10z M0,0z M0.4375,9.6875C0.335938,9.6875 0.248047,9.65039 0.173828,9.57617 0.0996097,9.50195 0.0625004,9.41406 0.0625004,9.3125 0.0625004,9.21094 0.0996097,9.12305 0.173828,9.04883L4.0293,5.1875 0.173828,1.32617C0.0996094,1.25195 0.0625,1.16406 0.0625,1.0625 0.0625,0.960938 0.0996094,0.873047 0.173828,0.798828 0.248047,0.724609 0.335937,0.6875 0.4375,0.6875 0.539062,0.6875 0.626953,0.724609 0.701172,0.798828L4.82617,4.92383C4.90039,4.99805 4.9375,5.08594 4.9375,5.1875 4.9375,5.28906 4.90039,5.37695 4.82617,5.45117L0.701172,9.57617C0.626953,9.65039,0.539063,9.6875,0.4375,9.6875z"
                Fill="{Binding ElementName=stencilName, Path=Foreground}"
                HorizontalAlignment="Right"
                Margin="0,0,6,0"
                VerticalAlignment="Center"
                Visibility="{Binding Id, Converter={StaticResource IdToMenuPointerVisibilityConverter}}" />
        </Grid>
    </DataTemplate>
    <sys:Double x:Key="{dxdiagt:LWKey DiagramDesignerControl.FontSizeEditorWidth}">70</sys:Double>
</ResourceDictionary>