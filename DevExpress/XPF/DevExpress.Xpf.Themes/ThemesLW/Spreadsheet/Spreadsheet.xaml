    <Style x:Key="{dxspst:LWKey Spreadsheet.ResizeThumbStyle}" TargetType="{x:Type Thumb}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Thumb}">
                    <dx:DXBorder
                        x:Name="Root"
                        Background="{dx:LWBrush 'Brush.Button.Background, Office2019Black=Brush.Window.Background, Office2019Colorful=Brush.Control.Background, Office2019HighContrast=Brush.Control.Background, Win11=Brush.Button.SimpleButtonBackground'}"
                        BorderBrush="{StaticResource Brush.Border}"
                        BorderThickness="0,1,1,0">
                        <Path x:Name="Content" Fill="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.ForegroundDark, Win11=Brush.Foreground.Primary'}" Style="{StaticResource UIShape.SizeGrip.Vertical}" />
                    </dx:DXBorder>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Fill" TargetName="Content" Value="{dx:LWBrush 'Brush.HoverForeground, Office2019Black=Brush.Backstage.Foreground, VS2019Dark=Brush.Focused, Win11=Brush.Foreground.Primary'}" />
                            <Setter Property="Background" TargetName="Root" Value="{dx:LWBrush 'Brush.HoverBackground, Win11=Brush.ListItem.HoverBackground'}" />
                            <Setter Property="Opacity" TargetName="Root" Value="1" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity" TargetName="Root" Value="0.26" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Scroll Corner -->

    <Style x:Key="{dxspst:LWKey Spreadsheet.BottomRightCornerStyle}" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="{dx:LWBrush 'Brush.Button.Background, Office2019Black=Brush.Control.Background, Office2019Colorful=Brush.Window.Background, Office2019HighContrast=Brush.Window.Background, Win11=Brush.Button.SimpleButtonBackground'}" />
    </Style>
