                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid x:Name="Root" MinHeight="22" MinWidth="22">
                        <dx:DXBorder
                            x:Name="IsDefault"
                            Background="Transparent"
                            CornerRadius="{dx:LWCornerRadius '0, Win11=4'}"
