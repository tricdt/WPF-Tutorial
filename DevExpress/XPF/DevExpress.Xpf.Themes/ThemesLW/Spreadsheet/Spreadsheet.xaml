    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Spreadsheet\Base\FormulaBar\FormulaBarExpandButton.xaml-->
    <!-- General -->

    <Style x:Key="{dxspst:LWKey Spreadsheet.FormulaBarExpandButtonStyle}" TargetType="{x:Type ToggleButton}">
        <Setter Property="Focusable" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToggleButton}">
                    <Grid x:Name="Root" Background="Transparent">
                        <dx:DXBorder
                            x:Name="IsDefault"
                            Background="Transparent"
                            CornerRadius="{dx:LWCornerRadius '0, Win11=4'}"
                            Margin="{dx:LWThickness '0, Win11=(1,1,1,2)'}">
                            <Grid Margin="{dx:LWThickness '(4,7,4,7), Win11=(6,6,6,7)'}">
                                <Path
                                    x:Name="Expanded"
                                    Fill="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.ControlLight.Foreground, Win11=Brush.Foreground.Secondary'}"
                                    Opacity="0"
                                    Style="{StaticResource UIShape.Up}" />
                                <Path x:Name="Collapsed" Fill="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.ControlLight.Foreground, Win11=Brush.Foreground.Secondary'}" Style="{StaticResource UIShape.Down}" />
                            </Grid>
                        </dx:DXBorder>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Fill" TargetName="Collapsed" Value="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.ControlLight.Foreground, Win11=Brush.Foreground.Primary'}" />
                            <Setter Property="Fill" TargetName="Expanded" Value="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.ControlLight.Foreground, Win11=Brush.Foreground.Primary'}" />
                            <Setter Property="Background" TargetName="IsDefault" Value="{dx:LWBrush 'Brush.HoverBackground, Office2019Black=Brush.ControlLight.HoverBackground, Win11=Brush.Button.HoverBackground'}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Fill" TargetName="Collapsed" Value="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.ControlLight.Foreground, Win11=Brush.Foreground.Muted'}" />
                            <Setter Property="Fill" TargetName="Expanded" Value="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.ControlLight.Foreground, Win11=Brush.Foreground.Muted'}" />
                            <Setter Property="Background" TargetName="IsDefault" Value="{dx:LWBrush 'Brush.SelectionBackground, Office2019Black=Brush.ControlLight.SelectionBackground, Win11=Brush.Button.PressedBackground'}" />
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Opacity" TargetName="Collapsed" Value="0" />
                            <Setter Property="Opacity" TargetName="Expanded" Value="1" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight" Value="44" />
                <Setter Property="MinWidth" Value="44" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Spreadsheet\Base\FormulaBar\FormulaBarExpandButton.xaml-->


    <Style x:Key="{dxspst:LWKey Spreadsheet.WorksheetSelectionControlStyle}" TargetType="{x:Type dxspsi:WorksheetSelectionControl}">
        <Setter Property="AutoFilterBackgroundBrush" Value="{DynamicResource {dxspst:LWKey Spreadsheet.AutoFilterBackgroundBrush}}" />
        <Setter Property="Focusable" Value="False" />
        <Setter Property="SelectionBorderBrush" Value="{dx:LWBrush 'Brush.Focused, VS2019Blue=Brush.Border, Win11=Brush.Accent'}" />
        <Setter Property="SelectionBrush">
            <Setter.Value>
                <SolidColorBrush Color="{StaticResource UIBrush.Color}" Opacity="0.38" />
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Reference Editor -->

    <DataTemplate x:Key="{dxspst:LWKey Spreadsheet.ReferenceEditCollapseButtonTemplate}">
        <Image
            x:Name="collapse"
            dxb:ImageColorizer.Color="{StaticResource UIColor.Spreadsheet.ReferenceEdit.Foreground}"
            dxb:ImageColorizer.IsEnabled="True"
            HorizontalAlignment="Center"
            Source="/DevExpress.Xpf.Spreadsheet.v24.1;component/Images/ReferenceEdit_Collapse_16x16.png"
            Stretch="None"
            VerticalAlignment="Center" />
        <DataTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="dxb:ImageColorizer.Color" TargetName="collapse" Value="{StaticResource UIColor.Spreadsheet.ReferenceEdit.HoverForeground}" />
            </Trigger>
        </DataTemplate.Triggers>
    </DataTemplate>

    <DataTemplate x:Key="{dxspst:LWKey Spreadsheet.ReferenceEditExpandButtonTemplate}">
        <Image
            x:Name="expand"
            dxb:ImageColorizer.Color="{StaticResource UIColor.Spreadsheet.ReferenceEdit.Foreground}"
            dxb:ImageColorizer.IsEnabled="True"
            HorizontalAlignment="Center"
            Source="/DevExpress.Xpf.Spreadsheet.v24.1;component/Images/ReferenceEdit_Expand_16x16.png"
            Stretch="None"
            VerticalAlignment="Center" />
        <DataTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="dxb:ImageColorizer.Color" TargetName="expand" Value="{StaticResource UIColor.Spreadsheet.ReferenceEdit.HoverForeground}" />
            </Trigger>
        </DataTemplate.Triggers>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Spreadsheet\Base\Spreadsheet\SpreadsheetControl.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Spreadsheet\Base\TabSelector\TabSelectorElements.xaml-->
