    <!-- General -->

    <ControlTemplate x:Key="{dxspst:LWKey Spreadsheet.GroupButtonTemplate}" TargetType="{x:Type dxspsi:GroupItem}">
        <dx:DXBorder
            x:Name="IsDefault"
            Background="{dx:LWBrush 'Brush.Window.Background, Win11=Brush.WindowBackground'}"
            BorderBrush="{StaticResource Brush.Border}"
            BorderThickness="1"
            Margin="0,0,1,1">
            <Grid>
                <Path x:Name="CollapseShape" Style="{StaticResource UIShape.SmallPlus}" Visibility="Hidden" />
                <Path x:Name="ExpandShape" Style="{StaticResource UIShape.SmallMinus}" Visibility="Hidden" />
            </Grid>
        </dx:DXBorder>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" TargetName="IsDefault" Value="{dx:LWBrush 'Brush.HoverBackground, Win11=Brush.ListItem.HoverBackground'}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" TargetName="IsDefault" Value="{dx:LWBrush 'Brush.SelectionBackground, Win11=Brush.ListItem.SelectionBackground'}" />
            </Trigger>
            <Trigger Property="IsExpanded" Value="True">
                <Setter Property="Visibility" TargetName="ExpandShape" Value="Visible" />
            </Trigger>
            <Trigger Property="IsCollapsed" Value="True">
                <Setter Property="Visibility" TargetName="CollapseShape" Value="Visible" />
            </Trigger>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight" Value="44" />
                <Setter Property="MinWidth" Value="44" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!-- Header -->

    <ControlTemplate x:Key="{dxspst:LWKey Spreadsheet.GroupHeaderTemplate}" TargetType="{x:Type dxspsi:GroupItem}">
        <dx:DXBorder
            x:Name="IsDefault"
            Background="{dx:LWBrush 'Brush.Window.Background, Win11=Brush.WindowBackground'}"
            BorderBrush="{StaticResource Brush.Border}"
            BorderThickness="1">
            <TextBlock
                HorizontalAlignment="Center"
                Text="{TemplateBinding Text}"
                FontSize="{TemplateBinding FontHeight}"
                TextAlignment="Center"
                VerticalAlignment="Center" />
        </dx:DXBorder>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" TargetName="IsDefault" Value="{dx:LWBrush 'Brush.HoverBackground, Win11=Brush.ListItem.HoverBackground'}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" TargetName="IsDefault" Value="{dx:LWBrush 'Brush.SelectionBackground, Win11=Brush.ListItem.SelectionBackground'}" />
            </Trigger>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight" Value="44" />
                <Setter Property="MinWidth" Value="44" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
