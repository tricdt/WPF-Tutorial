    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Printing\Base\PageBorderControl.xaml-->
    <ControlTemplate x:Key="PageBorderTemplate" TargetType="{x:Type dxpn:PageBorderControl}">
        <dx:DXBorder Background="{StaticResource Brush.Editor.Background}" BorderBrush="{StaticResource Brush.Border}" BorderThickness="1">
            <ContentPresenter Content="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Content}" />
        </dx:DXBorder>
    </ControlTemplate>


    <!-- General -->
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Printing\Base\PageBorderControl.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Printing\Base.xaml-->
    <!-- General -->
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Printing\Base.xaml-->


    <!--#region DEFAULT STYLES-->

    <Style x:Key="PageBorderControl.DefaultStyle" TargetType="{x:Type dxpn:PageBorderControl}">
        <Setter Property="Template" Value="{StaticResource PageBorderTemplate}" />
    </Style>
    <!--#endregion DEFAULT STYLES-->


</ResourceDictionary>