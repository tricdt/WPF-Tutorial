    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Printing\Base\PreviewSurface.xaml-->
    <!-- General -->

    <ControlTemplate x:Key="{dxpt:LWKey Printing.PreviewSurfaceTemplate}" TargetType="{x:Type dxpn:PreviewSurface}">
        <dx:BackgroundPanel />
    </ControlTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Printing\Base\PreviewSurface.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\Printing\Base\NavigationPane.xaml-->
