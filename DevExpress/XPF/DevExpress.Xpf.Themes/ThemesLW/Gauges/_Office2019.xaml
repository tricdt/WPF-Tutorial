<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxgat="http://schemas.devexpress.com/winfx/2008/xaml/gauges/themekeys"
    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="Gauges/_{ThemeName}.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <SolidColorBrush x:Key="{dxgat:LWKey GaugesBrushes.NeedleFill}" Color="{StaticResource Color.Foreground}" />
    <SolidColorBrush x:Key="{dxgat:LWKey GaugesBrushes.ForegroundBrush}" Color="{StaticResource Color.Foreground}" />
    <SolidColorBrush x:Key="{dxgat:LWKey GaugesBrushes.ScaleElementsBrush}" Color="{StaticResource Color.Border}" />
    <SolidColorBrush x:Key="{dxgat:LWKey GaugesBrushes.ScaleInnerBackgroundBrush}" Color="{StaticResource Color.Control.Background}" Opacity="0.62" />
    <SolidColorBrush x:Key="{dxgat:LWKey GaugesBrushes.SpindleCapBackgroundBrush}" Color="{StaticResource Color.Foreground}" />
    <SolidColorBrush x:Key="{dxgat:LWKey GaugesBrushes.SpindleCapForegroundBrush}" Color="{StaticResource Color.Window.Background}" />
    <SolidColorBrush x:Key="{dxgat:LWKey GaugesBrushes.MarkerForegroundBrush}" Color="{StaticResource Color.Foreground}" />
    <SolidColorBrush x:Key="{dxgat:LWKey GaugesBrushes.GaugeBackgroundBrush}" Color="Transparent" />
</ResourceDictionary>