    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/navbar">

    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="NavBar/Generic.xaml" />
        <dxi:ThemeResourceDictionary Path="NavBar/NavBar.xaml" />
    </ResourceDictionary.MergedDictionaries>


    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.NavBarGroupItemsControl'}" BasedOn="{StaticResource NavBarGroupItemsControlDefaultStyle}" TargetType="{x:Type dxn:NavBarGroupItemsControl}" />
