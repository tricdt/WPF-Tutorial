    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.GroupItemsContainer'}" BasedOn="{StaticResource GroupItemsContainerDefaultStyle}" TargetType="{x:Type dxn:GroupItemsContainer}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.NavPaneDropDownMenuControl'}" BasedOn="{StaticResource NavPaneDropDownMenuControlDefaultStyle}" TargetType="{x:Type dxn:NavPaneDropDownMenuControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.NavPanePopupWindowContent'}" BasedOn="{StaticResource NavPanePopupWindowContentDefaultStyle}" TargetType="{x:Type dxn:NavPanePopupWindowContent}" />
