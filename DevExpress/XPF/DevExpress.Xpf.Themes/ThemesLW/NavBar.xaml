    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.NavPaneExpandButton'}" BasedOn="{StaticResource NavPaneExpandButtonDefaultStyle}" TargetType="{x:Type dxn:NavPaneExpandButton}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.NavPanePopupWindowFrame'}" BasedOn="{StaticResource NavPanePopupWindowFrameDefaultStyle}" TargetType="{x:Type dxn:NavPanePopupWindowFrame}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.NavPaneOverflowPanel'}" BasedOn="{StaticResource NavPaneOverflowPanelDefaultStyle}" TargetType="{x:Type dxn:NavPaneOverflowPanel}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.NavPaneItemsControl'}" BasedOn="{StaticResource NavPaneItemsControlDefaultStyle}" TargetType="{x:Type dxn:NavPaneItemsControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.NavigationPaneView'}" BasedOn="{StaticResource NavigationPaneViewDefaultStyle}" TargetType="{x:Type dxn:NavigationPaneView}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.SideBarView'}" BasedOn="{StaticResource SideBarViewDefaultStyle}" TargetType="{x:Type dxn:SideBarView}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.NavBar.ExplorerBarExpandButton'}" BasedOn="{StaticResource ExplorerBarExpandButtonDefaultStyle}" TargetType="{x:Type dxn:ExplorerBarExpandButton}" />
