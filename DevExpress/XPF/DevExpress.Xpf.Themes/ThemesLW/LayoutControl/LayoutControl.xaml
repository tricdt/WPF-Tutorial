<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:cs="colorscheme_ignorable_uri"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    mc:Ignorable="cs">

    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="LayoutControl/_{ThemeCategory}.xaml" />
        <dxi:ThemeResourceDictionary Path="LayoutControl/Generic.xaml" />
    </ResourceDictionary.MergedDictionaries>


    <!--#region Base\LayoutControl\Base\ElementSizer.xaml-->
    <!-- General -->

    <Style x:Key="ElementSizerStyle" TargetType="dxlc:ElementSizer">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dxlc:ElementSizer">
                    <dx:DXBorder
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        Padding="{TemplateBinding Padding}">
                        <dx:Container>
                            <dx:Container.Resources>
                                <dx:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
                            </dx:Container.Resources>
                            <Grid x:Name="Root">
                                <dx:Container x:Name="HorizontalRootElement" Cursor="SizeNS">
                                    <Path
                                        x:Name="PathNS"
                                        Fill="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.Delimiter, Win11=Brush.Foreground.Primary'}"
                                        Stretch="Fill"
                                        Style="{StaticResource UIShape.SizeGrip.Horizontal}"
                                        Visibility="{Binding ShowResizingHandle, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BoolToVisibilityConverter}}" />
                                </dx:Container>
                                <dx:Container x:Name="VerticalRootElement" Cursor="SizeWE">
                                    <Path
                                        x:Name="PathWE"
                                        Fill="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.Delimiter, Win11=Brush.Foreground.Primary'}"
                                        Stretch="Fill"
                                        Style="{StaticResource UIShape.SizeGrip.Vertical}"
                                        Visibility="{Binding ShowResizingHandle, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BoolToVisibilityConverter}}" />
                                </dx:Container>
                            </Grid>
                        </dx:Container>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimation Duration="0" Storyboard.TargetName="PathNS" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="{dx:LWColor 'Color.HoverForeground, Office2019Black=Color.ForegroundDark, Office2019HighContrast=Color.HoverBackground, VS2019Dark=Color.Focused, VS2019Light=Color.Focused, Win11=Color.Foreground.Primary'}" />
                                        <ColorAnimation Duration="0" Storyboard.TargetName="PathWE" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="{dx:LWColor 'Color.HoverForeground, Office2019Black=Color.ForegroundDark, Office2019HighContrast=Color.HoverBackground, VS2019Dark=Color.Focused, VS2019Light=Color.Focused, Win11=Color.Foreground.Primary'}" />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </dx:DXBorder>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--#endregion Base\LayoutControl\Base\ElementSizer.xaml-->

    <!--#region Base\LayoutControl\Base\GroupBox.xaml-->
    <ControlTemplate x:Key="NormalGroupBoxTemplate" TargetType="dxlc:GroupBox">
        <dx:Container>
            <dxlc:GroupBoxShadow CornerRadius="{TemplateBinding CornerRadius}" Offset="{TemplateBinding ShadowOffset}" Visibility="{TemplateBinding ShadowVisibility}" />
            <dx:DXBorder
                x:Name="BorderElement"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{TemplateBinding CornerRadius}"
                dx:BorderExtensions.ClipChild="True">
                <dxlc:LayoutControl
                    dx:DXSerializer.Enabled="False"
                    ItemSpace="0"
                    Orientation="Vertical"
                    Padding="0"
                    ScrollBars="None">
                    <dxlc:LayoutGroup
                        x:Name="Header"
                        Background="{TemplateBinding TitleBackground}"
                        ItemSpace="0"
                        Padding="2"
                        Visibility="{TemplateBinding TitleVisibility}">
                        <Rectangle x:Name="TitleStretcher" Visibility="Collapsed" Width="0" />
                        <dx:DXContentPresenter
                            x:Name="Title"
                            Content="{TemplateBinding Header}"
                            ContentTemplate="{TemplateBinding HeaderTemplate}"
                            Foreground="{TemplateBinding TitleForeground}"
                            Margin="9,0"
                            VerticalAlignment="Center" />
                        <dxlc:GroupBoxButton
                            x:Name="MinimizeElement"
                            AutomationProperties.Name="Minimize"
                            HorizontalAlignment="Right"
                            Margin="2,0,0,0"
                            VerticalAlignment="Center"
                            Visibility="{TemplateBinding MinimizeElementVisibility}" />
                        <dxlc:GroupBoxButton
                            x:Name="MaximizeElement"
                            AutomationProperties.Name="Maximize"
                            HorizontalAlignment="Right"
                            Margin="2,0,0,0"
                            VerticalAlignment="Center"
                            Visibility="{TemplateBinding MaximizeElementVisibility}" />
                    </dxlc:LayoutGroup>
                    <Rectangle
                        x:Name="Separator"
                        Fill="{TemplateBinding SeparatorBrush}"
                        Height="1"
                        Visibility="{TemplateBinding TitleVisibility}" />
                    <dx:DXContentPresenter
                        x:Name="Content"
                        Content="{TemplateBinding Content}"
                        ContentTemplate="{TemplateBinding CurrentContentTemplate}"
                        Foreground="{TemplateBinding TitleForeground}"
                        Margin="{TemplateBinding Padding}" />
                </dxlc:LayoutControl>
            </dx:DXBorder>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="LayoutStates">
                    <VisualState x:Name="NormalLayout" />
                    <VisualState x:Name="MinimizedLayout">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxi:ValueSetter.Visibility="Visible" Storyboard.TargetName="TitleStretcher" Storyboard.TargetProperty="Visibility" />
                            <ObjectAnimationUsingKeyFrames dxi:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="Separator" Storyboard.TargetProperty="Visibility" />
                            <ObjectAnimationUsingKeyFrames dxi:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="Content" Storyboard.TargetProperty="Visibility" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MaximizedLayout" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </dx:Container>
        <ControlTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight" TargetName="Header" Value="44" />
                <Setter Property="Margin" TargetName="MaximizeElement" Value="4,0,0,0" />
                <Setter Property="Margin" TargetName="MinimizeElement" Value="4,0,0,0" />
                <Setter Property="MinHeight" TargetName="MinimizeElement" Value="44" />
                <Setter Property="MinHeight" TargetName="MinimizeElement" Value="44" />
                <Setter Property="MinWidth" TargetName="MinimizeElement" Value="44" />
                <Setter Property="MinWidth" TargetName="MinimizeElement" Value="44" />
                <Setter Property="Margin" TargetName="Title" Value="14" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="LightGroupBoxTemplate" TargetType="dxlc:GroupBox">
        <dx:Container>
            <dx:DXBorder
                x:Name="BorderElement"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                Background="{TemplateBinding Background}"
                CornerRadius="{TemplateBinding CornerRadius}"
                dx:BorderExtensions.ClipChild="True">
                <dxlc:LayoutControl
                    dx:DXSerializer.Enabled="False"
                    ItemSpace="0"
                    Orientation="Vertical"
                    Padding="{dx:LWThickness '0, Win11=2'}"
                    ScrollBars="None">
                    <dxlc:LayoutGroup
                        x:Name="Header"
                        ItemSpace="0"
                        Padding="2"
                        Visibility="{TemplateBinding TitleVisibility}">
                        <Rectangle x:Name="TitleStretcher" Visibility="Collapsed" Width="0" />
                        <dx:DXContentPresenter
                            x:Name="Title"
                            Content="{TemplateBinding Header}"
                            ContentTemplate="{TemplateBinding HeaderTemplate}"
                            Foreground="{TemplateBinding TitleForeground}"
                            Margin="9,0"
                            VerticalAlignment="Center" />
                        <dxlc:GroupBoxButton
                            x:Name="MinimizeElement"
                            AutomationProperties.Name="Minimize"
                            HorizontalAlignment="Right"
                            Margin="2,0,0,0"
                            VerticalAlignment="Center"
                            Visibility="{TemplateBinding MinimizeElementVisibility}" />
                        <dxlc:GroupBoxButton
                            x:Name="MaximizeElement"
                            AutomationProperties.Name="Maximize"
                            HorizontalAlignment="Right"
                            Margin="2,0,0,0"
                            VerticalAlignment="Center"
                            Visibility="{TemplateBinding MaximizeElementVisibility}" />
                    </dxlc:LayoutGroup>
                    <dx:DXContentPresenter
                        x:Name="Content"
                        Content="{TemplateBinding Content}"
                        ContentTemplate="{TemplateBinding CurrentContentTemplate}"
                        Foreground="{TemplateBinding TitleForeground}"
                        Margin="{TemplateBinding Padding}" />
                </dxlc:LayoutControl>
            </dx:DXBorder>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="LayoutStates">
                    <VisualState x:Name="NormalLayout" />
                    <VisualState x:Name="MinimizedLayout">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxi:ValueSetter.Visibility="Visible" Storyboard.TargetName="TitleStretcher" Storyboard.TargetProperty="Visibility" />
                            <ObjectAnimationUsingKeyFrames dxi:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="Content" Storyboard.TargetProperty="Visibility" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MaximizedLayout" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </dx:Container>
        <ControlTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight" TargetName="Header" Value="44" />
                <Setter Property="Margin" TargetName="MaximizeElement" Value="4,0,0,0" />
                <Setter Property="Margin" TargetName="MinimizeElement" Value="4,0,0,0" />
                <Setter Property="MinHeight" TargetName="MinimizeElement" Value="44" />
                <Setter Property="MinHeight" TargetName="MinimizeElement" Value="44" />
                <Setter Property="MinWidth" TargetName="MinimizeElement" Value="44" />
                <Setter Property="MinWidth" TargetName="MinimizeElement" Value="44" />
                <Setter Property="Margin" TargetName="Title" Value="14" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="GroupBoxSeparatorTemplate" TargetType="dxlc:GroupSeparator">
        <dx:Container>
            <Grid>
                <dx:DXBorder
                    x:Name="HorizontalRootElement"
                    Background="{dx:LWBrush 'Brush.Delimiter, Win10Dark=Brush.Border'}"
                    Height="1"
                    Margin="0,3,0,3" />
                <dx:DXBorder
                    x:Name="VerticalRootElement"
                    Background="{dx:LWBrush 'Brush.Delimiter, Win10Dark=Brush.Border'}"
                    Margin="3,0,3,0"
                    Width="1" />
            </Grid>
        </dx:Container>
    </ControlTemplate>


    <!-- General -->

    <!-- Style -->

    <Style x:Key="GroupBoxButtonStyle" TargetType="dxlc:GroupBoxButton">
        <Setter Property="Height" Value="16" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template" Value="{StaticResource GroupBoxButtonTemplate}" />
        <Setter Property="Width" Value="16" />
        <Style.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight" Value="44" />
                <Setter Property="MinWidth" Value="44" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="GroupSeparatorStyle" TargetType="dxlc:GroupSeparator">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template" Value="{StaticResource GroupBoxSeparatorTemplate}" />
    </Style>

    <Style x:Key="GroupBoxStyle" TargetType="dxlc:GroupBox">
        <Setter Property="Background" Value="{dx:LWBrush 'Brush.Window.Background, Office2019Colorful=#00000000, Office2019HighContrast=#00000000, Win11=Brush.WindowBackground'}" />
        <Setter Property="BorderBrush" Value="{dx:LWBrush 'Brush.Border, Win10Dark=Brush.AltBorder, Win11=Brush.PanelBorder'}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{dx:LWCornerRadius '0, Win11=7'}" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="LightTemplate" Value="{StaticResource LightGroupBoxTemplate}" />
        <Setter Property="MinimizationDirection" Value="Vertical" />
        <Setter Property="NormalTemplate" Value="{StaticResource NormalGroupBoxTemplate}" />
        <Setter Property="Padding" Value="11" />
        <Setter Property="SeparatorBrush" Value="{dx:LWBrush 'Brush.Border, Win11=Brush.Separator'}" />
        <Setter Property="ShadowOffset" Value="3" />
        <Setter Property="TitleBackground" Value="{dx:LWBrush 'Brush.Button.Background, Office2019Black=Brush.Border, Office2019Colorful=Brush.Control.Background, Office2019HighContrast=Brush.Control.Background, VS2019Blue=Brush.ControlDark.Background, Win11=Brush.Button.SimpleButtonBackground'}" />
        <Setter Property="TitleForeground" Value="{Binding Foreground, RelativeSource={RelativeSource Self}}" />
    </Style>
    <!--#endregion Base\LayoutControl\Base\GroupBox.xaml-->

    <!--#region Base\LayoutControl\Base\LayerSeparator.xaml-->
    <Style x:Key="LayerSeparatorStyle" TargetType="dxlc:LayerSeparator">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dxlc:LayerSeparator">
                    <dx:Container Background="Transparent">
                        <Grid x:Name="Root">
                            <dx:Container x:Name="HorizontalRootElement" Cursor="SizeNS">
                                <Grid>
                                    <Rectangle Fill="Transparent" Margin="0,-2" />
                                    <Rectangle Fill="{StaticResource Brush.Delimiter}" Height="{TemplateBinding Thickness}" />
                                </Grid>
                            </dx:Container>
                            <dx:Container x:Name="VerticalRootElement" Cursor="SizeWE">
                                <Grid>
                                    <Rectangle Fill="Transparent" Margin="-2,0" />
                                    <Rectangle Fill="{StaticResource Brush.Delimiter}" Width="{TemplateBinding Thickness}" />
                                </Grid>
                            </dx:Container>
                        </Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="1" />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </dx:Container>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Thickness" Value="2" />
    </Style>


    <!-- General -->
    <!--#endregion Base\LayoutControl\Base\LayerSeparator.xaml-->

    <!--#region Base\LayoutControl\Base\UICore.xaml-->
    <!--#endregion Base\LayoutControl\Base\UICore.xaml-->

    <!--#region Base\LayoutControl\Base\UICustom.xaml-->
    <!-- General -->
    <!--#endregion Base\LayoutControl\Base\UICustom.xaml-->

    <!--#region Base\LayoutControl\Base\UIPreset.xaml-->
    <!-- General -->
    <!--#endregion Base\LayoutControl\Base\UIPreset.xaml-->

    <!--#region Base\LayoutControl\Base.xaml-->
    <!-- General -->
    <!--#endregion Base\LayoutControl\Base.xaml-->


    <!--#region DEFAULT STYLES-->

    <Style x:Key="LayerSeparator.DefaultStyle" BasedOn="{StaticResource LayerSeparatorStyle}" TargetType="{x:Type dxlc:LayerSeparator}" />
    <Style x:Key="ElementSizer.DefaultStyle" BasedOn="{StaticResource ElementSizerStyle}" TargetType="{x:Type dxlc:ElementSizer}" />
    <Style x:Key="GroupBox.DefaultStyle1" BasedOn="{StaticResource GroupBoxStyle}" TargetType="{x:Type dxlc:GroupBox}" />
    <Style x:Key="GroupBoxButton.DefaultStyle" BasedOn="{StaticResource GroupBoxButtonStyle}" TargetType="{x:Type dxlc:GroupBoxButton}" />
    <Style x:Key="GroupSeparator.DefaultStyle" BasedOn="{StaticResource GroupSeparatorStyle}" TargetType="{x:Type dxlc:GroupSeparator}" />

    <!--#endregion DEFAULT STYLES-->


</ResourceDictionary>