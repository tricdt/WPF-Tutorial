    <Color x:Key="Color.Control.NeutralBackground">#FF333337</Color>
    <Color x:Key="Color.Custom.Blue">#FFADD8E6</Color>
    <Color x:Key="Color.Custom.Green">#FF90EE90</Color>
    <Color x:Key="Color.Custom.Red">#FFFFB6C1</Color>

