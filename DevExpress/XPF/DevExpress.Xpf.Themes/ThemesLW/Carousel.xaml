<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxca="http://schemas.devexpress.com/winfx/2008/xaml/carousel">

    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="Carousel/Generic.xaml" />
        <dxi:ThemeResourceDictionary Path="Carousel/Carousel.xaml" />
    </ResourceDictionary.MergedDictionaries>


    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.Carousel.CarouselItemsControl'}" BasedOn="{StaticResource CarouselItemsControl.DefaultStyleBase}" TargetType="{x:Type dxca:CarouselItemsControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.Carousel.CarouselPanel'}" BasedOn="{StaticResource CarouselPanel.DefaultStyleBase}" TargetType="{x:Type dxca:CarouselPanel}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.Carousel.CarouselNavigator'}" BasedOn="{StaticResource CarouselNavigator.DefaultStyleBase}" TargetType="{x:Type dxca:CarouselNavigator}" />


</ResourceDictionary>