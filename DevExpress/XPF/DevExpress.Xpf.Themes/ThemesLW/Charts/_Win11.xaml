    <ControlTemplate x:Key="VerticalThumbTemplate" TargetType="dxc:AxisScrollBarThumb">
        <Grid x:Name="Root">
            <Grid.RowDefinitions>
                <RowDefinition x:Name="Row1" Height="4" />
                <RowDefinition x:Name="Row2" Height="*" />
                <RowDefinition x:Name="Row3" Height="4" />
            </Grid.RowDefinitions>
            <Rectangle
                x:Name="CornerSide1"
                Grid.Row="0"
                Fill="{StaticResource Brush.Foreground.Muted}"
                Height="4"
                Margin="0,-1,0,0"
                RadiusX="3"
                RadiusY="3"
                Width="{TemplateBinding Width}" />
            <Rectangle
                x:Name="IsDefault"
                Grid.Row="1"
                Fill="{StaticResource Brush.Foreground.Muted}"
                Height="{TemplateBinding Height}"
                Margin="0,-3,0,-3"
                MinHeight="{TemplateBinding MinHeight}"
                MinWidth="{TemplateBinding MinWidth}"
                Width="{TemplateBinding Width}" />
            <Rectangle
                x:Name="CornerSide2"
                Grid.Row="2"
                Fill="{StaticResource Brush.Foreground.Muted}"
                Height="4"
                Margin="0,0,0,-2"
                RadiusX="3"
                RadiusY="3"
                Width="{TemplateBinding Width}" />
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide1" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="{StaticResource Color.Foreground.Secondary}" />
                            <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="{StaticResource Color.Foreground.Secondary}" />
                            <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide2" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="{StaticResource Color.Foreground.Secondary}" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide1" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="{StaticResource Color.Foreground.Muted}" />
                            <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="{StaticResource Color.Foreground.Muted}" />
                            <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide2" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="{StaticResource Color.Foreground.Muted}" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="0.38" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>
