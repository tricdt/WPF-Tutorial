<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol">

    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="LayoutControl/LayoutControl.xaml" />
    </ResourceDictionary.MergedDictionaries>


    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutItem'}" BasedOn="{StaticResource LayoutItem.DefaultStyleBase}" TargetType="dxlc:LayoutItem" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutItemLabel'}" BasedOn="{StaticResource LayoutItemLabel.DefaultStyleBase}" TargetType="dxlc:LayoutItemLabel" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.Tile'}" BasedOn="{StaticResource Tile.DefaultStyleBase}" TargetType="dxlc:Tile" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.TileGroupHeader'}" BasedOn="{StaticResource TileGroupHeader.DefaultStyleBase}" TargetType="dxlc:TileGroupHeader" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.MaximizedElementPositionIndicator'}" BasedOn="{StaticResource MaximizedElementPositionIndicator.DefaultStyleBase}" TargetType="dxlc:MaximizedElementPositionIndicator" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutItemSelectionIndicator'}" BasedOn="{StaticResource LayoutItemSelectionIndicator.DefaultStyleBase}" TargetType="dxlc:LayoutItemSelectionIndicator" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutItemParentIndicator'}" BasedOn="{StaticResource LayoutItemParentIndicator.DefaultStyleBase}" TargetType="dxlc:LayoutItemParentIndicator" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutItemDragAndDropInsertionPointIndicator'}" BasedOn="{StaticResource LayoutItemDragAndDropInsertionPointIndicator.DefaultStyleBase}" TargetType="dxlc:LayoutItemDragAndDropInsertionPointIndicator" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutItemCustomizationToolbar'}" BasedOn="{StaticResource LayoutItemCustomizationToolbar.DefaultStyleBase}" TargetType="dxlc:LayoutItemCustomizationToolbar" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutItemAlignmentButton'}" BasedOn="{StaticResource LayoutItemAlignmentButton.DefaultStyleBase}" TargetType="dxlc:LayoutItemAlignmentButton" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutItemAlignmentControl'}" BasedOn="{StaticResource LayoutItemAlignmentControl.DefaultStyleBase}" TargetType="dxlc:LayoutItemAlignmentControl" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutItemCustomizationToolbarButton'}" BasedOn="{StaticResource LayoutItemCustomizationToolbarButton.DefaultStyleBase}" TargetType="dxlc:LayoutItemCustomizationToolbarButton" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutItemCustomizationToolbarButtonArrow'}" BasedOn="{StaticResource LayoutItemCustomizationToolbarButtonArrow.DefaultStyleBase}" TargetType="dxlc:LayoutItemCustomizationToolbarButtonArrow" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutControlCustomizationControl'}" BasedOn="{StaticResource LayoutControlCustomizationControl.DefaultStyleBase}" TargetType="dxlc:LayoutControlCustomizationControl" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayoutControlAvailableItemsControl'}" BasedOn="{StaticResource LayoutControlAvailableItemsControl.DefaultStyleBase}" TargetType="dxlc:LayoutControlAvailableItemsControl" />

    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.LayerSeparator'}" BasedOn="{StaticResource LayerSeparator.DefaultStyle}" TargetType="{x:Type dxlc:LayerSeparator}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.ElementSizer'}" BasedOn="{StaticResource ElementSizer.DefaultStyle}" TargetType="{x:Type dxlc:ElementSizer}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.GroupBox'}" BasedOn="{StaticResource GroupBox.DefaultStyle1}" TargetType="{x:Type dxlc:GroupBox}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.GroupBoxButton'}" BasedOn="{StaticResource GroupBoxButton.DefaultStyle}" TargetType="{x:Type dxlc:GroupBoxButton}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.LayoutControl.GroupSeparator'}" BasedOn="{StaticResource GroupSeparator.DefaultStyle}" TargetType="{x:Type dxlc:GroupSeparator}" />


</ResourceDictionary>