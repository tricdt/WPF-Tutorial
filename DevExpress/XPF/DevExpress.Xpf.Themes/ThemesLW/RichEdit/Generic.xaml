    <Style x:Key="XpfRichEditCaret.DefaultStyleBase" TargetType="{x:Type dxrec:XpfRichEditCaret}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dxrec:XpfRichEditCaret">
                    <Grid SnapsToDevicePixels="True" UseLayoutRounding="True">
                        <Grid.Resources>
                            <Storyboard x:Key="Animate1" x:Name="Animate">
                                <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="Caret" Storyboard.TargetProperty="Opacity">
                                    <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1" />
                                    <DiscreteDoubleKeyFrame KeyTime="0:0:0.53" Value="0" />
                                    <DiscreteDoubleKeyFrame KeyTime="0:0:1.06" Value="0" />
                                </DoubleAnimationUsingKeyFrames>
                            </Storyboard>
                        </Grid.Resources>
                        <Rectangle
                            x:Name="Caret"
                            VerticalAlignment="Stretch"
                            Fill="Black"
                            SnapsToDevicePixels="True"
                            UseLayoutRounding="True" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Focusable" Value="False" />
    </Style>
