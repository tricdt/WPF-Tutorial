    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.RichEdit\DevExpress.Xpf.RichEdit\themes\Generic\Ruler.xaml-->
    <ControlTemplate x:Key="{dxret:LWKey RichEditRuler.HorizontalRulerControlTemplate}" TargetType="dxrec:HorizontalRulerControl">
        <Grid Canvas.ZIndex="1">
            <ContentControl Focusable="False" Template="{DynamicResource {dxret:LWKey RichEditRuler.HorizontalRulerBackgroundControlTemplate}}" />
            <Canvas
                x:Name="Surface"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                Style="{DynamicResource {dxret:LWKey RichEditRuler.HorizontalRulerContentStyle}}" />
            <dxrec:LeftIndentHotZoneControl x:Name="PART_Indent" Focusable="False" Visibility="Visible">
                <dxrec:LeftIndentHotZoneControl.Clip>
                    <RectangleGeometry Rect="0,0,0,0" />
                </dxrec:LeftIndentHotZoneControl.Clip>
            </dxrec:LeftIndentHotZoneControl>
            <dxrec:RightIndentHotZoneControl x:Name="PART_RightIndent" Focusable="False" Visibility="Visible">
                <dxrec:RightIndentHotZoneControl.Clip>
                    <RectangleGeometry Rect="0,0,0,0" />
                </dxrec:RightIndentHotZoneControl.Clip>
            </dxrec:RightIndentHotZoneControl>
            <dxrec:FirstLineIndentHotZoneControl x:Name="PART_TopIndent" Focusable="False" Visibility="Visible">
                <dxrec:FirstLineIndentHotZoneControl.Clip>
                    <RectangleGeometry Rect="0,0,0,0" />
                </dxrec:FirstLineIndentHotZoneControl.Clip>
            </dxrec:FirstLineIndentHotZoneControl>
            <dxrec:LeftIndentBottomHotZoneControl x:Name="PART_BottomIndent" Focusable="False" Visibility="Visible">
                <dxrec:LeftIndentBottomHotZoneControl.Clip>
                    <RectangleGeometry Rect="0,0,0,0" />
                </dxrec:LeftIndentBottomHotZoneControl.Clip>
            </dxrec:LeftIndentBottomHotZoneControl>
            <dxrec:LeftTabHotZoneControl x:Name="PART_Tab" Focusable="False" Visibility="Visible">
                <dxrec:LeftTabHotZoneControl.Clip>
                    <RectangleGeometry Rect="0,0,0,0" />
                </dxrec:LeftTabHotZoneControl.Clip>
            </dxrec:LeftTabHotZoneControl>
            <dxrec:HorizontalTableHotZoneControl x:Name="PART_HorizontalTable" Focusable="False" Visibility="Visible">
                <dxrec:HorizontalTableHotZoneControl.Clip>
                    <RectangleGeometry Rect="0,0,0,0" />
                </dxrec:HorizontalTableHotZoneControl.Clip>
            </dxrec:HorizontalTableHotZoneControl>
            <dxrec:TabTypeToggleHotZoneControl x:Name="PART_TabTypeToggle" Focusable="False" Visibility="Visible">
                <dxrec:TabTypeToggleHotZoneControl.Clip>
                    <RectangleGeometry Rect="0,0,0,0" />
                </dxrec:TabTypeToggleHotZoneControl.Clip>
            </dxrec:TabTypeToggleHotZoneControl>
            <dxrec:RulerNumberTickMarkControl x:Name="PART_NumberTickMark" Focusable="False" Visibility="Visible" />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxret:LWKey RichEditRuler.VerticalRulerControlTemplate}" TargetType="dxrec:VerticalRulerControl">
        <Grid>
            <ContentControl Focusable="False" Template="{DynamicResource {dxret:LWKey RichEditRuler.VerticalRulerBackgroundControlTemplate}}" />
            <Canvas
                x:Name="Surface"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                Canvas.ZIndex="1"
                Style="{DynamicResource {dxret:LWKey RichEditRuler.VerticalRulerContentStyle}}" />
            <dxrec:RulerNumberTickMarkControl x:Name="PART_NumberTickMark" Focusable="False" Visibility="Visible" />
        </Grid>
    </ControlTemplate>
