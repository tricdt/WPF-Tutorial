    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.RichEdit\DevExpress.Xpf.RichEdit\themes\Generic\NumberingListView.xaml-->
    <Style x:Key="{dxret:LWKey RichEditNumberingListView.SpinEditorBaseStyle}" TargetType="{x:Type dxreui:RichTextIndentEdit}">
        <Setter Property="MaxValue" Value="6600" />
        <Setter Property="MinValue" Value="-6600" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="MinWidth" Value="60" />
        <Setter Property="ValueUnitConverter" Value="{Binding ElementName=CustomizeListWindow, Path=DataContext.ValueUnitConverter}" />
        <Setter Property="DefaultUnitType" Value="{Binding ElementName=CustomizeListWindow, Path=DataContext.DefaultUnitType}" />
        <Setter Property="Margin" Value="0,0,6,0" />
    </Style>

    <Style x:Key="{dxret:LWKey RichEditNumberingListView.IndentSpinEditorStyle}" BasedOn="{StaticResource {dxret:LWKey RichEditNumberingListView.SpinEditorBaseStyle}}" TargetType="{x:Type dxreui:RichTextIndentEdit}">
        <Setter Property="TwipsValue" Value="{Binding IndentAt, Mode=TwoWay}" />
    </Style>
    <Style x:Key="{dxret:LWKey RichEditNumberingListView.AlignmentSpinEditorStyle}" BasedOn="{StaticResource {dxret:LWKey RichEditNumberingListView.SpinEditorBaseStyle}}" TargetType="{x:Type dxreui:RichTextIndentEdit}">
        <Setter Property="TwipsValue" Value="{Binding AlignedAt, Mode=TwoWay}" />
    </Style>

    <Style x:Key="{dxret:LWKey RichEditNumberingListView.FontButtonStyle}" TargetType="{x:Type dx:DialogButton}">
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="Content" Value="{dxre:RichEditLocalizer StringId=Caption_BulletsAndNumbering_Bulleted_Font}" />
        <Setter Property="Margin" Value="9,0,0,0" />
        <Setter Property="Command" Value="{Binding OpenFontFormCommand}" />
    </Style>

    <Style x:Key="{dxret:LWKey RichEditNumberingListView.NumberFormatComboBoxEditStyle}" TargetType="{x:Type dxe:ComboBoxEdit}">
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="IsTextEditable" Value="False" />
        <Setter Property="EditValue" Value="{Binding NumberStyle, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
        <Setter Property="ItemsSource" Value="{Binding NumberStyleItems, Mode=OneWay}" />
        <Setter Property="ApplyItemTemplateToSelectedItem" Value="True" />
        <Setter Property="ItemTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock Margin="3,2,0,2" Text="{Binding Converter={dxrevm:ListNumberAlignmentToDisplayTextConverter}, Mode=OneWay}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>

    </Style>

    <Style x:Key="{dxret:LWKey RichEditNumberingListView.StartSpinEditorStyle}" TargetType="{x:Type dxe:SpinEdit}">
        <Setter Property="Value" Value="{Binding StartAt, Mode=TwoWay}" />
        <Setter Property="MaxValue" Value="{Binding MaxStartValue}" />
        <Setter Property="MinValue" Value="{Binding MinStartValue}" />
        <Setter Property="IsFloatValue" Value="False" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="AllowRoundOutOfRangeValue" Value="True" />
    </Style>
