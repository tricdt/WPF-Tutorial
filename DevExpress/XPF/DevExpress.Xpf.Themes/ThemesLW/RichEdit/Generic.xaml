    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.RichEdit\DevExpress.Xpf.RichEdit\themes\Generic\BarItemTemplates.xaml-->
    <dxo:GalleryStyleItemForeColorConverter x:Key="galleryStyleItemForeColorConverter" />
    <dxo:OfficeDefaultBarItemDataTemplates x:Key="officeDefaultTemplates" />



    <DataTemplate x:Key="{dxret:LWKey BarItemTemplate.StyleGalleryItemCaption}">
        <dx:DXBorder MaxWidth="70" BorderBrush="LightGray" BorderThickness="1">
            <Grid Margin="5">
                <Grid.RowDefinitions>
                    <RowDefinition Height="30" />
                    <RowDefinition Height="20" />
                </Grid.RowDefinitions>
                <dx:DXBorder Grid.Row="0" Grid.Column="0" Padding="0">
                    <dx:DXBorder.Resources>
                        <dxre:RichEditStringIdConverter x:Key="stringIdConverter" />
                    </dx:DXBorder.Resources>
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="0"
                        Margin="0,2,0,2"
                        Padding="0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Bottom"
                        FontFamily="{Binding FontFamily}"
                        FontSize="{Binding FontSize}"
                        FontStyle="{Binding FontStyle}"
                        FontWeight="{Binding FontWeight}"
                        Foreground="{Binding Converter={StaticResource galleryStyleItemForeColorConverter}, Path=FontColor}"
                        Text="{Binding Source={StaticResource stringIdConverter}, ConverterParameter=Caption_StyleGalleryItemText, Converter={StaticResource stringIdConverter}, Mode=OneTime}"
                        TextDecorations="{Binding TextDecorations}"
                        TextWrapping="NoWrap" />
                </dx:DXBorder>
                <TextBlock
                    Grid.Row="1"
                    HorizontalAlignment="Left"
                    Foreground="Gray"
                    Text="{Binding StyleName}"
                    TextWrapping="NoWrap" />
            </Grid>
        </dx:DXBorder>
    </DataTemplate>
    <DataTemplate x:Key="{dxret:LWKey BarItemTemplate.TableStyleGalleryItemCaption}">
        <dx:DXBorder Margin="2,2,2,2">
            <dxb:BarManager.DXContextMenu>
                <dxb:PopupMenu>
                    <dxb:BarButtonItem
                        Command="{Binding RichEditControl.CommandProvider.NewTableStyle}"
                        Content="{dxre:RichEditLocalizedString MenuCmd_NewTableStyle}"
                        Glyph="{dx:ImageSelector Source={dxre:RichEditImage Image=NewTableStyle_16x16.png}, SvgSource={dxre:RichEditSvgImage Name=NewTableStyle}}"
                        IsPrivate="True" />
                    <dxb:BarButtonItem
                        Command="{Binding Path=ModifyTableStyleCommand}"
                        CommandParameter="{Binding Path=Style}"
                        Content="{dxre:RichEditLocalizedString MenuCmd_ModifyTableStyle}"
                        Glyph="{dx:ImageSelector Source={dxre:RichEditImage Image=ModifyTableStyle_16x16.png}, SvgSource={dxre:RichEditSvgImage Name=ModifyTableStyle}}"
                        IsPrivate="True" />
                    <dxb:BarButtonItem
                        Command="{Binding Path=DeleteTableStyleCommand}"
                        CommandParameter="{Binding Path=Style}"
                        Content="{dxre:RichEditLocalizedString MenuCmd_DeleteTableStyle}"
                        Glyph="{dx:ImageSelector Source={dxre:RichEditImage Image=ClearTableStyle_16x16.png}, SvgSource={dxre:RichEditSvgImage Name=ClearTableStyle}}"
                        IsPrivate="True" />
                </dxb:PopupMenu>
            </dxb:BarManager.DXContextMenu>
            <Grid Margin="2,2,2,2">
                <dx:DXBorder>
                    <dx:DXImage Source="{Binding BitmapSource}" ToolTip="{Binding StyleName}" />
                </dx:DXBorder>
            </Grid>
        </dx:DXBorder>
    </DataTemplate>
    <!--#endregion C:\work\2023.1\XPF\DevExpress.Xpf.RichEdit\DevExpress.Xpf.RichEdit\themes\Generic\BarItemTemplates.xaml-->

    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.RichEdit\DevExpress.Xpf.RichEdit\themes\Generic\RibbonTemplateGenerator.xaml-->
    <ControlTemplate x:Key="{dxret:LWKey RichEditControl.EmptyRibbonTemplate}" TargetType="{x:Type Control}" />
    <ControlTemplate x:Key="{dxret:LWKey RichEditControl.RibbonTemplate}" TargetType="{x:Type Control}">
        <DockPanel>
            <dxr:RibbonControl dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonControl}" DockPanel.Dock="Top" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonControl}}">
                <dxmvvm:Interaction.Behaviors>
                    <dxb:ControllerBehavior Actions="{Binding Path=(dxre:RichEditControl.RichEdit).RibbonActions, RelativeSource={RelativeSource TemplatedParent}}" ExecutionMode="OnEvent">
                        <dxb:ControllerBehavior.Triggers>
                            <dxb:ActionTrigger EventName="Loaded" ExecuteOnce="True" />
                        </dxb:ControllerBehavior.Triggers>
                    </dxb:ControllerBehavior>
                </dxmvvm:Interaction.Behaviors>
                <dxr:RibbonControl.ToolbarItemLinks>
                    <dxb:BarButtonItemLink BarItemName="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_SaveAs}" />
                    <dxb:BarButtonItemLink BarItemName="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_Undo}" />
                    <dxb:BarButtonItemLink BarItemName="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_Redo}" />
                </dxr:RibbonControl.ToolbarItemLinks>
                <dxr:RibbonDefaultPageCategory dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonCategory_Default}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonCategory_Default}}">
                    <dxr:RibbonPage dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonPage_File}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonPage_File}}">
                        <dxr:RibbonPage.GroupCollectionTemplate>
                            <DataTemplate>
                                <ItemsControl>
                                    <dxr:RibbonPageGroup dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonGroup_File_Common}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonGroup_File_Common}}">
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_New}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Common_New}}" />
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_Open}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Common_Open}}" />
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_Save}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Common_Save}}" />
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_SaveAs}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Common_SaveAs}}" />
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_QuickPrint}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Common_QuickPrint}}" />
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_Print}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Common_Print}}" />
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_PrintPreview}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Common_PrintPreview}}" />
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_Undo}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Common_Undo}}" />
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Common_Redo}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Common_Redo}}" />
                                    </dxr:RibbonPageGroup>
                                    <dxr:RibbonPageGroup dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonGroup_File_Info}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonGroup_File_Info}}">
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Info_EncryptDocument}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Info_EncryptDocument}}" />
                                        <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonItem_File_Info_DocumentProperties}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonItem_File_Info_DocumentProperties}}" />
                                    </dxr:RibbonPageGroup>
                                </ItemsControl>
                            </DataTemplate>
                        </dxr:RibbonPage.GroupCollectionTemplate>
                    </dxr:RibbonPage>
                    <dxr:RibbonPage dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonPage_Home}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonPage_Home}}">
                        <dxr:RibbonPage.GroupCollectionTemplate>
                            <DataTemplate>
                                <ItemsControl>
                                    <dxr:RibbonPageGroup dx:XamlHelper.Name="{x:Static dxre:DefaultBarItemNames.RibbonGroup_Home_Clipboard}" Style="{DynamicResource {dxret:LWKey BarItemStyle.RibbonGroup_Home_Clipboard}}">
