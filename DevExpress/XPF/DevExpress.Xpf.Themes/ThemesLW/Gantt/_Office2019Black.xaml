<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
    xmlns:dxgnt="http://schemas.devexpress.com/winfx/2008/xaml/gantt/themekeys"
    xmlns:dxgni="http://schemas.devexpress.com/winfx/2008/xaml/gantt/internal"
    xmlns:cs="colorscheme_ignorable_uri">
    <ResourceDictionary.MergedDictionaries>
        <dxi:PaletteResourceDictionary />
    </ResourceDictionary.MergedDictionaries>
    <SolidColorBrush x:Key="{dxgnt:LWKey GanttControl.RowSelectedBackground}" Color="{StaticResource Color.Control.SelectionBackground}" />
    <SolidColorBrush x:Key="{dxgnt:LWKey GanttControl.RowFadeSelectionBackground}" Color="{StaticResource Color.Control.HoverBackground}" />
    <SolidColorBrush x:Key="{dxgnt:LWKey GanttItems.ConnectorHighlightedShadowBrush}" Color="{StaticResource Color.Backstage.Foreground}" Opacity="0.32" />
    <dxgni:GanttResourcePalette x:Key="{dxgnt:LWKey GanttControl.ResourcePalette}">
        #FFF68D99,
        #FFF7B639,
        #FF70D5B0,
        #FF9AD55B,
        #FF79BEF4,
        #FFB09BED,
        #FFE985D7,
        #FF78D6E7
    </dxgni:GanttResourcePalette>
</ResourceDictionary>