<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxtm="http://schemas.devexpress.com/winfx/2008/xaml/treemap">

    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="TreeMap/TreeMap.xaml" />
    </ResourceDictionary.MergedDictionaries>


    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.TreeMap.TreeMapControl'}" BasedOn="{StaticResource TreeMapControl.DefaultStyleBase}" TargetType="{x:Type dxtm:TreeMapControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.TreeMap.Title'}" BasedOn="{StaticResource Title.DefaultStyleBase}" TargetType="{x:Type dxtm:Title}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.TreeMap.Legend'}" BasedOn="{StaticResource Legend.DefaultStyleBase}" TargetType="{x:Type dxtm:Legend}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.TreeMap.LegendTitle'}" BasedOn="{StaticResource LegendTitle.DefaultStyleBase}" TargetType="{x:Type dxtm:LegendTitle}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.TreeMap.TreeMapItemPresentation'}" BasedOn="{StaticResource TreeMapItemPresentation.DefaultStyleBase}" TargetType="{x:Type dxtm:TreeMapItemPresentation}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.TreeMap.TreeMapItemsControl'}" BasedOn="{StaticResource TreeMapItemsControl.DefaultStyleBase}" TargetType="{x:Type dxtm:TreeMapItemsControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.TreeMap.HierarchicalTreeMapItemControl'}" BasedOn="{StaticResource HierarchicalTreeMapItemControl.DefaultStyleBase}" TargetType="{x:Type dxtm:HierarchicalTreeMapItemControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.TreeMap.SunburstControl'}" BasedOn="{StaticResource SunburstControl.DefaultStyleBase}" TargetType="{x:Type dxtm:SunburstControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.TreeMap.SunburstSectorPresentation'}" BasedOn="{StaticResource SunburstSectorPresentation.DefaultStyleBase}" TargetType="{x:Type dxtm:SunburstSectorPresentation}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.TreeMap.SunburstLabelPresentation'}" BasedOn="{StaticResource SunburstLabelPresentation.DefaultStyleBase}" TargetType="{x:Type dxtm:SunburstLabelPresentation}" />


</ResourceDictionary>