<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxdv="http://schemas.devexpress.com/winfx/2008/xaml/documentviewer">

    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="DocumentViewer\DocumentViewer.xaml" />
    </ResourceDictionary.MergedDictionaries>


    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.DocumentViewer.DocumentViewerControl'}" BasedOn="{StaticResource DocumentViewerControl.DefaultStyleBase}" TargetType="{x:Type dxdv:DocumentViewerControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.DocumentViewer.DocumentPresenterControl'}" BasedOn="{StaticResource DocumentPresenterControl.DefaultStyleBase}" TargetType="{x:Type dxdv:DocumentPresenterControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.DocumentViewer.DocumentViewerItemsControl'}" BasedOn="{StaticResource DocumentViewerItemsControl.DefaultStyleBase}" TargetType="{x:Type dxdv:DocumentViewerItemsControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.DocumentViewer.PageControl'}" BasedOn="{StaticResource PageControl.DefaultStyleBase}" TargetType="{x:Type dxdv:PageControl}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.DocumentViewer.PageControlItem'}" BasedOn="{StaticResource PageControlItem.DefaultStyleBase}" TargetType="{x:Type dxdv:PageControlItem}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.DocumentViewer.SearchControlContainer'}" BasedOn="{StaticResource SearchControlContainer.DefaultStyleBase}" TargetType="{x:Type dxdv:SearchControlContainer}" />
    <Style x:Key="{dx:LWDefaultStyleKey 'DevExpress.Xpf.DocumentViewer.DocumentMapControl'}" BasedOn="{StaticResource DocumentMapControl.DefaultStyleBase}" TargetType="{x:Type dxdv:DocumentMapControl}" />


</ResourceDictionary>