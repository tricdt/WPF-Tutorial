    <ControlTemplate x:Key="{dxscht:LWKey WeekView.VerticalCellHeaderNormalTemplate}" TargetType="{x:Type dxschi:VisualDateCellHeader}">
        <Grid x:Name="Root" Background="Transparent">
            <dxschi:HeaderControl
