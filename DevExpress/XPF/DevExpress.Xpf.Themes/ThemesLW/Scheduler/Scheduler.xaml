    <!--#endregion Base\Scheduler\Base\BaseView\Header.xaml-->

    <!--#region Base\Scheduler\Base\Appointments\VerticalAppointment.xaml-->
    <!-- General -->

    <ControlTemplate x:Key="{dxscht:LWKey SchedulerView.VerticalAppointmentTemplate}" TargetType="{x:Type dxschi:VisualVerticalAppointmentControl}">
        <dxschi:AppointmentColorConvertControl ControlColor="{Binding ViewInfo.LabelColor, RelativeSource={RelativeSource TemplatedParent}}" Margin="1">
            <Grid x:Name="PART_ToolTipContainer" dxsch:SchedulerControl.HitTestType="AppointmentContent" dxsch:SchedulerControl.SelectableIntervalViewInfo="{TemplateBinding ViewInfo}">
                <Grid.Resources>
                    <dxschi:BoolToThicknessConverter x:Key="selectionBoolToThicknessConverter" ThicknessWhenFalse="1" ThicknessWhenTrue="1" />
                </Grid.Resources>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="6" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="4" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="6" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="4" />
                </Grid.RowDefinitions>
                <ToolTipService.ToolTip>
                    <ToolTip Content="{TemplateBinding ViewInfo}" ContentTemplate="{Binding ViewInfo.View.AppointmentToolTipContentTemplate, RelativeSource={RelativeSource TemplatedParent}}" Visibility="{Binding ViewInfo.View.AppointmentToolTipVisibility, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ToolTipVisibilityConverter}}" />
                </ToolTipService.ToolTip>
                <dxschi:AppointmentRoundedBorder
                    x:Name="AppointmentRoundedBorder"
                    Grid.RowSpan="2"
                    Grid.ColumnSpan="2"
                    CornerRadius="0"
                    DefaultCornerRadius="0"
                    DefaultRoundBorderThickness="{Binding ViewInfo.Selected, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource selectionBoolToThicknessConverter}}"
                    ViewInfo="{TemplateBinding ViewInfo}">
                    <dxschi:AppointmentRoundedBorder.Style>
                        <Style>
                            <Setter Property="dxschi:AppointmentRoundedBorder.BorderBrush" Value="{StaticResource Brush.Border}" />
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ViewInfo.Selected, RelativeSource={RelativeSource TemplatedParent}}" Value="True">
                                    <Setter Property="dxschi:AppointmentRoundedBorder.BorderBrush" Value="{dx:LWBrush 'Brush.Foreground, VS2019Dark=Brush.Window.Background, Win11=Brush.Foreground.Primary'}" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </dxschi:AppointmentRoundedBorder.Style>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="1">
                            <Grid.Resources>
                                <dxschi:AppointmentStatusDisplayTypeToGridLengthConverter x:Key="StatusDisplayTypeToGridLengthConverter" DefaultValue="7" />
                                <dxschi:AppointmentStatusDisplayTypeToVisibilityConverter x:Key="AppointmentStatusDisplayTypeToVisibilityConverter" />
                            </Grid.Resources>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition x:Name="ContentContainerStatusColumn" Width="{Binding StatusDisplayType, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource StatusDisplayTypeToGridLengthConverter}}" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <dx:DXBorder
                                Background="{dx:LWBrush 'Brush.Button.Background, Win11=Brush.Button.SimpleButtonBackground'}"
                                BorderBrush="{dx:LWBrush 'Brush.Window.Background, Win11=Brush.WindowBackground'}"
                                BorderThickness="0,0,1,0"
                                Visibility="{Binding StatusDisplayType, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource AppointmentStatusDisplayTypeToVisibilityConverter}}">
                                <dxsch:AppointmentStatusControl Background="{dx:LWBrush 'Brush.Button.Background, Win11=Brush.Button.SimpleButtonBackground'}" Orientation="Vertical" ViewInfo="{TemplateBinding ViewInfo}">
                                    <dxschi:AppointmentBorder Background="{Binding ViewInfo.Status.Brush, RelativeSource={RelativeSource TemplatedParent}}" DefaultBorderThickness="0" ViewInfo="{TemplateBinding ViewInfo}" />
                                </dxsch:AppointmentStatusControl>
                            </dx:DXBorder>
                            <dx:DXBorder Grid.Column="1" Background="{Binding ViewInfo.LabelBrush, RelativeSource={RelativeSource TemplatedParent}}" />
                            <ContentPresenter
                                x:Name="PART_Content"
                                Grid.Column="1"
                                Content="{TemplateBinding ViewInfo}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                Margin="3" />
                        </Grid>
                    </Grid>
                </dxschi:AppointmentRoundedBorder>
                <Rectangle
                    Grid.RowSpan="2"
                    Grid.ColumnSpan="2"
                    Cursor="SizeNS"
                    dxsch:SchedulerControl.HitTestType="AppointmentResizingTopEdge"
                    dxsch:SchedulerControl.SelectableIntervalViewInfo="{TemplateBinding ViewInfo}"
                    Fill="Transparent"
                    Height="4"
                    IsHitTestVisible="True"
                    VerticalAlignment="Top"
                    Visibility="{Binding Path=ViewInfo.CanResizeStart, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <Rectangle
                    Grid.RowSpan="2"
                    Grid.ColumnSpan="2"
                    Cursor="SizeNS"
                    dxsch:SchedulerControl.HitTestType="AppointmentResizingBottomEdge"
                    dxsch:SchedulerControl.SelectableIntervalViewInfo="{TemplateBinding ViewInfo}"
                    Fill="Transparent"
                    Height="4"
                    IsHitTestVisible="True"
                    VerticalAlignment="Bottom"
                    Visibility="{Binding Path=ViewInfo.CanResizeEnd, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="DraggedStates">
                    <VisualState x:Name="NotDragged">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="AppointmentRoundedBorder" Storyboard.TargetProperty="Opacity" To="1" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Dragged">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="AppointmentRoundedBorder" Storyboard.TargetProperty="Opacity" To="0.38" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </dxschi:AppointmentColorConvertControl>
    </ControlTemplate>

    <DataTemplate x:Key="{dxscht:LWKey SchedulerView.VerticalAppointmentContentTemplate}">
        <Grid Background="Transparent">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <dxschi:AppointmentImagesControl Grid.Column="0" Orientation="Vertical" ViewInfo="{Binding}" />
            <dxschi:VerticalAppointmentContentPanel Grid.Column="1">
                <dxschi:VerticalAppointmentClockControl ViewInfo="{Binding}" />
                <TextBlock Foreground="{DynamicResource {dxscht:LWKey SchedulerView.AppointmentForegroundColor}}" Text="{Binding Subject}" TextWrapping="Wrap">
                    <TextBlock.TextTrimming>
                        <TextTrimming>CharacterEllipsis</TextTrimming>
                    </TextBlock.TextTrimming>
                </TextBlock>
                <TextBlock Foreground="{DynamicResource {dxscht:LWKey SchedulerView.AppointmentForegroundColor}}" Text="{Binding Location}" TextWrapping="Wrap">
                    <TextBlock.TextTrimming>
                        <TextTrimming>CharacterEllipsis</TextTrimming>
                    </TextBlock.TextTrimming>
                </TextBlock>
                <TextBlock
                    dxschi:VerticalAppointmentContentPanel.AlwaysOnNewRow="True"
                    Foreground="{DynamicResource {dxscht:LWKey SchedulerView.AppointmentForegroundColor}}"
                    Margin="0,4,0,0"
                    Text="{Binding Description}"
                    TextWrapping="Wrap">
                    <TextBlock.TextTrimming>
                        <TextTrimming>CharacterEllipsis</TextTrimming>
                    </TextBlock.TextTrimming>
                </TextBlock>
            </dxschi:VerticalAppointmentContentPanel>
        </Grid>
    </DataTemplate>
    <!--#endregion Base\Scheduler\Base\Appointments\VerticalAppointment.xaml-->

    <!--#region Base\Scheduler\Base\Appointments\HorizontalAppointment.xaml-->
    <!-- General -->

    <DataTemplate x:Key="{dxscht:LWKey SchedulerView.HorizontalAppointmentLongerThanADayContentTemplate}">
        <Grid Background="Transparent" VerticalAlignment="Center">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <StackPanel Grid.Column="0" Orientation="Horizontal" VerticalAlignment="Center">
                <dxschi:AppointmentContinueStartDateControl ViewInfo="{Binding}" />
                <dxschi:HorizontalAppointmentStartClockControl ViewInfo="{Binding}" />
            </StackPanel>
            <dxschi:HorizontalAppointmentContentPanel Grid.Column="1" VerticalAlignment="Center">
                <dxschi:AppointmentImagesControl HorizontalAlignment="Center" Orientation="Horizontal" ViewInfo="{Binding}" />
                <TextBlock
                    Foreground="{DynamicResource {dxscht:LWKey SchedulerView.AppointmentForegroundColor}}"
                    Text="{Binding Subject}"
                    TextWrapping="Wrap"
                    VerticalAlignment="Stretch">
                    <TextBlock.TextTrimming>
                        <TextTrimming>CharacterEllipsis</TextTrimming>
                    </TextBlock.TextTrimming>
                </TextBlock>
                <TextBlock
                    Foreground="{DynamicResource {dxscht:LWKey SchedulerView.AppointmentForegroundColor}}"
                    Text="{Binding Location}"
                    TextWrapping="Wrap"
                    VerticalAlignment="Stretch">
                    <TextBlock.TextTrimming>
                        <TextTrimming>CharacterEllipsis</TextTrimming>
                    </TextBlock.TextTrimming>
                </TextBlock>
            </dxschi:HorizontalAppointmentContentPanel>
            <StackPanel Grid.Column="2" Orientation="Horizontal" VerticalAlignment="Center">
                <dxschi:HorizontalAppointmentEndClockControl ViewInfo="{Binding}" />
                <dxschi:AppointmentContinueEndDateControl ViewInfo="{Binding}" />
            </StackPanel>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="{dxscht:LWKey SchedulerView.AppointmentToolTipContentTemplate}">
        <TextBlock Text="{Binding ToolTipText}" />
    </DataTemplate>

    <!-- Appointment -->

    <ControlTemplate x:Key="{dxscht:LWKey SchedulerView.HorizontalAppointmentTemplate}" TargetType="{x:Type dxschi:VisualHorizontalAppointmentControl}">
        <dxschi:AppointmentColorConvertControl ControlColor="{Binding ViewInfo.LabelColor, RelativeSource={RelativeSource TemplatedParent}}" Margin="1,1,1,0">
            <Grid x:Name="PART_ToolTipContainer" dxsch:SchedulerControl.HitTestType="AppointmentContent" dxsch:SchedulerControl.SelectableIntervalViewInfo="{TemplateBinding ViewInfo}">
                <Grid.Resources>
                    <dxschi:BoolToThicknessConverter x:Key="selectionBoolToThicknessConverter" ThicknessWhenFalse="1" ThicknessWhenTrue="1" />
                </Grid.Resources>
                <ToolTipService.ToolTip>
                    <ToolTip Content="{TemplateBinding ViewInfo}" ContentTemplate="{Binding ViewInfo.View.AppointmentToolTipContentTemplate, RelativeSource={RelativeSource TemplatedParent}}" Visibility="{Binding ViewInfo.View.AppointmentToolTipVisibility, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ToolTipVisibilityConverter}}" />
                </ToolTipService.ToolTip>
                <dxschi:AppointmentRoundedBorder
                    x:Name="AppointmentRoundedBorder"
                    DefaultCornerRadius="0"
                    DefaultRoundBorderThickness="{Binding ViewInfo.Selected, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource selectionBoolToThicknessConverter}}"
                    ViewInfo="{TemplateBinding ViewInfo}">
                    <dxschi:AppointmentRoundedBorder.Style>
                        <Style>
                            <Setter Property="dxschi:AppointmentRoundedBorder.BorderBrush" Value="{StaticResource Brush.Border}" />
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ViewInfo.Selected, RelativeSource={RelativeSource TemplatedParent}}" Value="True">
                                    <Setter Property="dxschi:AppointmentRoundedBorder.BorderBrush" Value="{dx:LWBrush 'Brush.Foreground, VS2019Dark=Brush.Window.Background, Win11=Brush.Foreground.Primary'}" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </dxschi:AppointmentRoundedBorder.Style>
                    <Grid>
                        <Grid.Resources>
                            <dxschi:AppointmentStatusDisplayTypeToDoubleConverter x:Key="StatusDisplayTypeToDoubleConverter" DefaultValue="15" />
                            <dxschi:AppointmentStatusDisplayTypeToVisibilityConverter x:Key="AppointmentStatusDisplayTypeToVisibilityConverter" />
                        </Grid.Resources>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <dx:DXBorder
                            Background="{dx:LWBrush 'Brush.Button.Background, Win11=Brush.Button.SimpleButtonBackground'}"
                            BorderBrush="{dx:LWBrush 'Brush.Window.Background, Win11=Brush.WindowBackground'}"
                            BorderThickness="0,0,1,0"
                            Height="{Binding StatusDisplayType, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource StatusDisplayTypeToDoubleConverter}}"
                            Visibility="{Binding StatusDisplayType, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource AppointmentStatusDisplayTypeToVisibilityConverter}}">
                            <dxsch:AppointmentStatusControl Background="{dx:LWBrush 'Brush.Button.Background, Win11=Brush.Button.SimpleButtonBackground'}" Orientation="Vertical" ViewInfo="{TemplateBinding ViewInfo}">
                                <dxschi:AppointmentBorder Background="{Binding ViewInfo.Status.Brush, RelativeSource={RelativeSource TemplatedParent}}" DefaultBorderThickness="0" ViewInfo="{TemplateBinding ViewInfo}" />
                            </dxsch:AppointmentStatusControl>
                        </dx:DXBorder>
                        <dx:DXBorder Grid.Row="1" Background="{Binding ViewInfo.LabelBrush, RelativeSource={RelativeSource TemplatedParent}}" />
                        <ContentPresenter
                            x:Name="PART_Content"
                            Grid.Row="1"
                            Content="{TemplateBinding ViewInfo}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            Margin="0" />
                    </Grid>
                </dxschi:AppointmentRoundedBorder>
                <Rectangle
                    Cursor="SizeWE"
                    dxsch:SchedulerControl.HitTestType="AppointmentResizingLeftEdge"
                    dxsch:SchedulerControl.SelectableIntervalViewInfo="{TemplateBinding ViewInfo}"
                    Fill="Transparent"
                    HorizontalAlignment="Left"
                    IsHitTestVisible="True"
                    Visibility="{Binding ViewInfo.CanResizeStart, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"
                    Width="4" />
                <Rectangle
                    Cursor="SizeWE"
                    dxsch:SchedulerControl.HitTestType="AppointmentResizingRightEdge"
                    dxsch:SchedulerControl.SelectableIntervalViewInfo="{TemplateBinding ViewInfo}"
                    Fill="Transparent"
                    HorizontalAlignment="Right"
                    IsHitTestVisible="True"
                    Visibility="{Binding Path=ViewInfo.CanResizeEnd, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"
                    Width="4" />
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="DraggedStates">
                    <VisualState x:Name="NotDragged">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="AppointmentRoundedBorder" Storyboard.TargetProperty="Opacity" To="1" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Dragged">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="AppointmentRoundedBorder" Storyboard.TargetProperty="Opacity" To="0.38" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </dxschi:AppointmentColorConvertControl>
    </ControlTemplate>
