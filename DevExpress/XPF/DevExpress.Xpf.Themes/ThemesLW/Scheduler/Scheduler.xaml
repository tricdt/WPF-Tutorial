    <!--#region Base\Scheduler\Base\Appointments\HorizontalAppointmentSameDay.xaml-->
    <!-- General -->

    <DataTemplate x:Key="{dxscht:LWKey SchedulerView.HorizontalAppointmentSameDayContentTemplate}">
        <dxschi:HorizontalAppointmentContentPanel VerticalAlignment="Center">
            <dxschi:HorizontalAppointmentStartClockControl VerticalAlignment="Center" ViewInfo="{Binding}" />
            <dxschi:HorizontalAppointmentEndClockControl VerticalAlignment="Center" ViewInfo="{Binding}" />
            <dxschi:AppointmentImagesControl Orientation="Horizontal" ViewInfo="{Binding}" />
            <TextBlock
                Foreground="{DynamicResource {dxscht:LWKey SchedulerView.AppointmentForegroundColor}}"
                HorizontalAlignment="Stretch"
                Margin="1,0,2,0"
                Text="{Binding Subject}"
                TextAlignment="Left"
                TextWrapping="Wrap"
                VerticalAlignment="Stretch">
                <TextBlock.TextTrimming>
                    <TextTrimming>CharacterEllipsis</TextTrimming>
                </TextBlock.TextTrimming>
            </TextBlock>
            <TextBlock
                Foreground="{DynamicResource {dxscht:LWKey SchedulerView.AppointmentForegroundColor}}"
                HorizontalAlignment="Stretch"
                Text="{Binding Location}"
                TextAlignment="Left"
                TextWrapping="Wrap"
                VerticalAlignment="Stretch">
                <TextBlock.TextTrimming>
                    <TextTrimming>CharacterEllipsis</TextTrimming>
                </TextBlock.TextTrimming>
            </TextBlock>
        </dxschi:HorizontalAppointmentContentPanel>
    </DataTemplate>
    <!--#endregion Base\Scheduler\Base\Appointments\HorizontalAppointmentSameDay.xaml-->

    <!--#region Base\Scheduler\Base\Appointments\DragDropHoverTimeCells.xaml-->
    <!-- General -->

    <Style x:Key="{dxscht:LWKey SchedulerView.DragDropHoverTimeCellsStyle}" TargetType="{x:Type dxschi:VisualDraggedAppointmentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <dx:DXBorder
                        Background="{StaticResource Brush.Editor.Background}"
                        BorderBrush="{StaticResource Brush.Border}"
                        BorderThickness="2"
                        Margin="1,0,1,0"
                        Opacity="0.25" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--#endregion Base\Scheduler\Base\Appointments\DragDropHoverTimeCells.xaml-->

    <!--#region Base\Scheduler\Base\DayView\TimeRuler.xaml-->
    <!-- General -->

    <Style x:Key="{dxscht:LWKey DayView.TimeRulerFontStyle}" TargetType="{x:Type TextBlock}" />

    <!-- Item -->

    <DataTemplate x:Key="{dxscht:LWKey DayView.TimeRulerMinuteItemTemplate}">
        <dx:DXBorder
            BorderBrush="{dx:LWBrush 'Brush.Delimiter, Office2019Black=Brush.Border'}"
            BorderThickness="0,1,0,0"
            HorizontalAlignment="Right"
            Margin="0,-1,0,0"
            VerticalAlignment="Top">
            <TextBlock
                FontSize="10"
                Foreground="{dx:LWBrush 'Brush.Foreground, Win11=Brush.Foreground.Primary'}"
                HorizontalAlignment="Right"
                Margin="16,2,4,0"
                Text="{Binding Caption}"
                VerticalAlignment="Top" />
        </dx:DXBorder>
    </DataTemplate>

    <DataTemplate x:Key="{dxscht:LWKey DayView.TimeRulerHourItemTemplate}">
        <dx:DXBorder
            BorderBrush="{dx:LWBrush 'Brush.Delimiter, Office2019Black=Brush.Border'}"
            BorderThickness="0,1,0,0"
            dxschi:SharedSizePanel.Span="{Binding MinuteItems.Count}"
            HorizontalAlignment="Left"
            Margin="0,-1,0,0"
            VerticalAlignment="Top">
            <TextBlock
                FontSize="16"
                Foreground="{dx:LWBrush 'Brush.Foreground, Win11=Brush.Foreground.Primary'}"
                HorizontalAlignment="Left"
                Margin="4,0,20,0"
                Text="{Binding Caption}"
                VerticalAlignment="Top" />
        </dx:DXBorder>
    </DataTemplate>

    <!-- Top Border -->

    <ControlTemplate x:Key="{dxscht:LWKey DayView.TimeRulerHeaderSeparatorTemplate}">
        <dx:DXBorder Background="{StaticResource Brush.Border}" Height="1" VerticalAlignment="Top" />
    </ControlTemplate>
    <!--#endregion Base\Scheduler\Base\DayView\TimeRuler.xaml-->

    <!--#region Base\Scheduler\Base\DayView\MoreButton.xaml-->
    <!-- General -->

    
    <!--#endregion Base\Scheduler\Base\DayView\MoreButton.xaml-->

    <!--#region Base\Scheduler\Base\DayView\HorizontalTimeMarker.xaml-->
    <!-- General -->

    <DataTemplate x:Key="{dxscht:LWKey DayView.HorizontalTimeMarker}">
        <dx:DXBorder Background="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.ControlLight.Foreground, Office2019Colorful=Brush.Focused, Office2019HighContrast=Brush.Focused, Win11=Brush.Foreground.Primary'}" Height="1" />
    </DataTemplate>
    <!--#endregion Base\Scheduler\Base\DayView\HorizontalTimeMarker.xaml-->

    <!--#region Base\Scheduler\Base\TimelineView\VerticalTimeMarker.xaml-->
    <!-- General -->

    <DataTemplate x:Key="{dxscht:LWKey TimelineView.VerticalTimeMarker}">
        <dx:DXBorder Background="{dx:LWBrush 'Brush.Foreground, Office2019Black=Brush.ControlLight.Foreground, Office2019Colorful=Brush.Focused, Office2019HighContrast=Brush.Focused, Win11=Brush.Foreground.Primary'}" Width="1" />
    </DataTemplate>
    <!--#endregion Base\Scheduler\Base\TimelineView\VerticalTimeMarker.xaml-->

    <!--#region Base\Scheduler\Base\RangeControl\Header.xaml-->
    <!-- General -->

    <ControlTemplate x:Key="{dxscht:LWKey RangeControl.RulerHeaderControlTemplate}" TargetType="{x:Type dxsch:RulerHeaderControl}">
        <dxschi:HeaderControl
            Content="{TemplateBinding Caption}"
            ContentTemplate="{DynamicResource {dxscht:LWKey RangeControl.RulerHeaderContentTemplate}}"
            dxschi:SchedulerItemsControl.ElementPosition="Middle,Middle,End,End"
            Orientation="Horizontal"
            ResourceColor="{x:Null}" />
    </ControlTemplate>

    <DataTemplate x:Key="{dxscht:LWKey RangeControl.RulerHeaderContentTemplate}">
        <TextBlock Foreground="{DynamicResource {dxscht:LWKey SchedulerView.HeaderForegroundColor}}" HorizontalAlignment="Center" Text="{Binding}">
            <TextBlock.TextTrimming>
                <TextTrimming>CharacterEllipsis</TextTrimming>
            </TextBlock.TextTrimming>
        </TextBlock>
    </DataTemplate>
    <!--#endregion Base\Scheduler\Base\RangeControl\Header.xaml-->

    <!--#region Base\Scheduler\Base\RangeControl\Thumbnail.xaml-->
    <!-- General -->

    <!-- NOTE: Pribindit' tsvet -->

    <ControlTemplate x:Key="{dxscht:LWKey RangeControl.ThumbnailControlTemplate}" TargetType="{x:Type dxsch:ThumbnailControl}">
        <dx:DXBorder Background="{Binding LabelColor, Converter={dxschi:DataItemThumbnailColorToBrushConverter DefaultColor=#FFBABABA}}" Margin="0,1,1,1" />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxscht:LWKey RangeControl.ThumbnailGroupControlTemplate}" TargetType="{x:Type dxsch:ThumbnailGroupControl}">
        <Viewbox Stretch="Uniform">
            <TextBlock Foreground="{dx:LWBrush 'Brush.Foreground, Win11=Brush.Foreground.Primary'}" Text="{Binding Count}" />
        </Viewbox>
    </ControlTemplate>
    <!--#endregion Base\Scheduler\Base\RangeControl\Thumbnail.xaml-->

    <!--#region Base\Scheduler\Base\RangeControl\RulerCell.xaml-->
    <!-- General -->

    <ControlTemplate x:Key="{dxscht:LWKey RangeControl.RulerCellControlTemplate}" TargetType="{x:Type dxsch:RulerCellControl}">
        <dx:DXBorder Background="{StaticResource Brush.Editor.Background}" BorderBrush="{dx:LWBrush 'Brush.Delimiter, Office2019Black=Brush.Border'}" BorderThickness="0,0,1,1" />
    </ControlTemplate>
    <!--#endregion Base\Scheduler\Base\RangeControl\RulerCell.xaml-->

    <!--#region Base\Scheduler\Base\SchedulerControl.xaml-->
    <!-- General -->

    <ControlTemplate x:Key="{dxscht:LWKey SchedulerControl.BorderTemplate}" TargetType="{x:Type ContentControl}">
        <dx:DXBorder Background="{dx:LWBrush 'Brush.Window.Background, Win11=Brush.WindowBackground'}" BorderBrush="{StaticResource Brush.Border}" BorderThickness="1">
            <ContentPresenter />
        </dx:DXBorder>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxscht:LWKey SchedulerControl.InvisibleBorderTemplate}" TargetType="{x:Type ContentControl}">
        <dx:DXBorder Background="{dx:LWBrush 'Brush.Window.Background, Win11=Brush.WindowBackground'}" BorderBrush="{StaticResource Brush.Border}" BorderThickness="1">
            <ContentPresenter />
        </dx:DXBorder>
    </ControlTemplate>

    <Style x:Key="{dxscht:LWKey SchedulerUIControl.ColorMarkerStyle}" TargetType="{x:Type FrameworkElement}">
        <Setter Property="Margin" Value="2" />
    </Style>

    <Style x:Key="{dxscht:LWKey SchedulerUIControl.TextBlockStyle}" BasedOn="{dx:TargetStaticResource {dxt:LWKey TextEdit.TextBlockStyle}, TargetDictionary={dxi:ThemeResourceDictionary Path='Core/Editors/Editors.xaml'}}" TargetType="{x:Type TextBlock}" />

    <Style x:Key="{dxscht:LWKey SchedulerUIControl.ItemTextBlockStyle}" TargetType="{x:Type TextBlock}">
        <Setter Property="Margin" Value="4,2,4,2" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Style.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Margin" Value="14,13,14,13" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="{dxscht:LWKey SchedulerControl.SchedulerRoot}" TargetType="{x:Type dx:XPFContentControl}" />
    <!--#endregion Base\Scheduler\Base\SchedulerControl.xaml-->

    <!--#region Base\Scheduler\Base\ScrollBar.xaml-->
    <!--#endregion Base\Scheduler\Base\ScrollBar.xaml-->


    <!--#region DEFAULT STYLES-->

    <Style x:Key="HeaderControl.DefaultStyle" TargetType="{x:Type dxschi:HeaderControl}">
        <Setter Property="AlternateTemplate" Value="{DynamicResource {dxscht:LWKey SchedulerView.HeaderAlternateTemplate}}" />
        <Setter Property="NormalTemplate" Value="{DynamicResource {dxscht:LWKey SchedulerView.HeaderNormalTemplate}}" />
    </Style>
    <Style x:Key="VerticalHeaderControl.DefaultStyle" TargetType="{x:Type dxschi:VerticalHeaderControl}">
        <Setter Property="AlternateTemplate" Value="{DynamicResource {dxscht:LWKey SchedulerView.VerticalHeaderNormalTemplate}}" />
        <Setter Property="NormalTemplate" Value="{DynamicResource {dxscht:LWKey SchedulerView.VerticalHeaderNormalTemplate}}" />
    </Style>
    <Style x:Key="SchedulerScrollBar.DefaultStyle" BasedOn="{dx:TargetStaticResource ScrollBar.DefaultStyle, TargetDictionary={dxi:ThemeResourceDictionary Path='Core/StandardControls/StandardControls.xaml'}}" TargetType="{x:Type dxschi:SchedulerScrollBar}" />
    <!--#endregion DEFAULT STYLES-->


</ResourceDictionary>