    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\BaseView\HorizontalResourceHeader.xaml-->
    <!-- HorizontalResourceHeaderStyle -->
    <Style x:Key="{dxscht:LWKey SchedulerView.HorizontalResourceHeaderStyle}" TargetType="{x:Type dxschd:VisualResourceHeader}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type dxschd:VisualResourceHeader}">
                    <Grid>
                        <dxschd:HeaderControl
                            HorizontalAlignment="Stretch"
                            dxsch:SchedulerControl.HitTestType="ResourceHeader"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            IsHitTestVisible="True"
                            ResourceColor="{Binding Content.Brushes.ResourceHeader, RelativeSource={RelativeSource TemplatedParent}}" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Foreground="{DynamicResource {dxscht:LWKey SchedulerView.HeaderForegroundColor}}"
                            Text="{Binding ResourceCaption}">
                            <TextBlock.TextTrimming>
                                <TextTrimming>
                                    CharacterEllipsis
                                </TextTrimming>
                            </TextBlock.TextTrimming>
                        </TextBlock>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--#endregion C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\BaseView\HorizontalResourceHeader.xaml-->

    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\BaseView\VerticalResourceHeader.xaml-->
    <Style x:Key="{dxscht:LWKey SchedulerView.VerticalResourceHeaderStyle}" TargetType="{x:Type dxschd:VisualResourceHeader}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type dxschd:VisualResourceHeader}">
                    <Grid SnapsToDevicePixels="True">
                        <dxschd:VerticalHeaderControl
                            dxsch:SchedulerControl.HitTestType="ResourceHeader"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            IsHitTestVisible="True"
                            ResourceColor="{Binding ResourceHeader.Brushes.ResourceHeader}"
                            SnapsToDevicePixels="True" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <dx:LayoutTransformPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                            <TextBlock Foreground="{DynamicResource {dxscht:LWKey SchedulerView.HeaderForegroundColor}}" Text="{Binding ResourceCaption}">
                                <TextBlock.TextTrimming>
                                    <TextTrimming>CharacterEllipsis</TextTrimming>
                                </TextBlock.TextTrimming>
                            </TextBlock>
                        </dx:LayoutTransformPanel>

                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--#endregion C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\BaseView\VerticalResourceHeader.xaml-->

    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\DayView\AllDayAreaControl.xaml-->



    <!-- AllDayAreaTemplate -->
    <DataTemplate x:Key="{dxscht:LWKey DayView.AllDayAreaTemplate}">
        <Grid SnapsToDevicePixels="True">

            <dxschd:SchedulerAllDayAreaTimeCellsControl
                x:Name="HorizontalWeek"
                dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                ItemContainerStyle="{Binding View.AllDayAreaCellStyle}"
                ItemsSource="{Binding Cells}"
                SnapsToDevicePixels="True">
                <dxschd:SchedulerAllDayAreaTimeCellsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <dxschd:PixelSnappedUniformGrid
                            x:Name="AllDayAreaCellPanelPSUG"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), ElementName=HorizontalWeek}"
                            Orientation="Horizontal"
                            SnapsToDevicePixels="True" />
                    </ItemsPanelTemplate>
                </dxschd:SchedulerAllDayAreaTimeCellsControl.ItemsPanel>
            </dxschd:SchedulerAllDayAreaTimeCellsControl>

            <dxschd:SelectionPresenter
                x:Name="AllDaySelectionPresenter"
                CellContainer="{Binding}"
                Resource="{Binding Resource}"
                SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}"
                SelectedCells="{Binding SelectedCells}"
                SelectionTemplate="{Binding View.SelectionTemplate}"
                SnapsToDevicePixels="True" />
            <Grid Margin="0,0,0,10">
                <dxschd:DynamicHorizontalAppointmentsPanel
                    x:Name="AllDayAreaAppointmentLayoutPanel"
                    AppointmentControls="{Binding AppointmentControlCollection}"
                    AppointmentsInfoContainer="{Binding}"
                    CellContainer="{Binding}"
                    CellPadding="1,0,0,0"
                    DefaultAppointmentHeight="{DynamicResource {dxscht:LWKey SchedulerControl.DefaultAppointmentHeight}}"
                    IsResizable="True"
                    IsStatusVisible="{Binding View.AppointmentDisplayOptions.AllDayAppointmentsStatusDisplayType, Converter={StaticResource StatusDisplayTypeToVisibileConverter}}"
                    SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}" />
                <dxschd:DynamicHorizontalAppointmentsPanel
                    x:Name="AllDayAreaDragAppointmentLayoutPanel"
                    AppointmentControls="{Binding DraggedAppointmentControlCollection}"
                    AppointmentStyle="{Binding View.DragDropHoverTimeCellsStyle}"
                    AppointmentsInfoContainer="{Binding}"
                    CellContainer="{Binding}"
                    CellPadding="1,0,0,0"
                    DefaultAppointmentHeight="{DynamicResource {dxscht:LWKey SchedulerControl.DefaultAppointmentHeight}}"
                    IsDraggedMode="True"
                    IsResizable="True"
                    SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}" />
            </Grid>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="{dxscht:LWKey DayView.AllDayAreaBestFitTemplate}">
        <Grid SnapsToDevicePixels="True">
            <dxschd:SchedulerAllDayAreaTimeCellsControl
                x:Name="HorizontalWeek"
                dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                ItemContainerStyle="{Binding View.AllDayAreaCellStyle}"
                ItemsSource="{Binding Cells}"
                SnapsToDevicePixels="True">
                <dxschd:SchedulerAllDayAreaTimeCellsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <dxschd:PixelSnappedUniformGrid
                            x:Name="AllDayAreaCellPanelPSUG"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), ElementName=HorizontalWeek}"
                            Orientation="Horizontal"
                            SnapsToDevicePixels="True" />
                    </ItemsPanelTemplate>
                </dxschd:SchedulerAllDayAreaTimeCellsControl.ItemsPanel>
            </dxschd:SchedulerAllDayAreaTimeCellsControl>

            <dxschd:SelectionPresenter
                x:Name="AllDaySelectionPresenter"
                CellContainer="{Binding}"
                Resource="{Binding Resource}"
                SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}"
                SelectedCells="{Binding SelectedCells}"
                SelectionTemplate="{Binding View.SelectionTemplate}"
                SnapsToDevicePixels="True" />
            <Grid>
                <dxschd:HeightLimitRestrictionControl Padding="0,0,0,10">
                    <dxschd:DynamicHorizontalAppointmentsPanel
                        x:Name="AllDayAreaAppointmentLayoutPanel"
                        AppointmentControls="{Binding AppointmentControlCollection}"
                        AppointmentsInfoContainer="{Binding}"
                        CellContainer="{Binding}"
                        CellPadding="1,0,0,0"
                        DefaultAppointmentHeight="{DynamicResource {dxscht:LWKey SchedulerControl.DefaultAppointmentHeight}}"
                        IsResizable="True"
                        IsStatusVisible="{Binding View.AppointmentDisplayOptions.AllDayAppointmentsStatusDisplayType, Converter={StaticResource StatusDisplayTypeToVisibileConverter}}"
                        SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}" />
                </dxschd:HeightLimitRestrictionControl>
                <dxschd:HeightLimitRestrictionControl Padding="0,0,0,10">
                    <dxschd:DynamicHorizontalAppointmentsPanel
                        x:Name="AllDayAreaDragAppointmentLayoutPanel"
                        AppointmentControls="{Binding DraggedAppointmentControlCollection}"
                        AppointmentStyle="{Binding View.DragDropHoverTimeCellsStyle}"
                        AppointmentsInfoContainer="{Binding}"
                        CellContainer="{Binding}"
                        CellPadding="1,0,0,0"
                        DefaultAppointmentHeight="{DynamicResource {dxscht:LWKey SchedulerControl.DefaultAppointmentHeight}}"
                        IsDraggedMode="True"
                        IsResizable="True"
                        SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}" />
                </dxschd:HeightLimitRestrictionControl>
            </Grid>
        </Grid>
    </DataTemplate>
    <!--#endregion C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\DayView\AllDayAreaControl.xaml-->

    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\DayView\Column.xaml-->
    <dxschd:SchedulerDayViewTimeCellAttachedPropertiesCalculator x:Key="{dxscht:LWKey SchedulerControlGeneric.DayViewTimeCellAttachedPropertiesCalculator}" />
    <!-- DayViewColumnTemplate -->
    <DataTemplate x:Key="{dxscht:LWKey DayView.DayViewColumnTemplate}">
        <dxschd:DayViewAppointmentInfoContainer x:Name="VisibleAptInfoContainer" HorizontalAlignment="Stretch">
            <Grid
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                dxschd:HitTestManager.IsHitTestContainer="True"
                Background="Transparent">
                <dxschd:SchedulerTimeCellControl
                    x:Name="VerticalDay"
                    HorizontalAlignment="Stretch"
                    dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                    ItemContainerStyle="{Binding View.CellStyle}"
                    ItemsSource="{Binding VerticalCellContainer.Cells}">
                    <dxschd:SchedulerTimeCellControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <dxschd:PixelSnappedSharedSizePanel
                                x:Name="TimeCellPanel"
                                dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), ElementName=VerticalDay}"
                                AttachedPropertiesCalculator="{DynamicResource {dxscht:LWKey SchedulerControlGeneric.DayViewTimeCellAttachedPropertiesCalculator}}"
                                Orientation="Vertical"
                                SharedSizeGroup="VerticalDayViewCells" />
                        </ItemsPanelTemplate>
                    </dxschd:SchedulerTimeCellControl.ItemsPanel>
                </dxschd:SchedulerTimeCellControl>
                <dxschd:SelectionPresenter
                    CellContainer="{Binding VerticalCellContainer}"
                    Resource="{Binding VerticalCellContainer.Resource}"
                    SchedulerTimeCellControl="{Binding ElementName=VerticalDay}"
                    SelectedCells="{Binding VerticalCellContainer.SelectedCells}"
                    SelectionTemplate="{Binding View.SelectionTemplate}" />
                <dxschd:DynamicVerticalAppointmentsPanel
                    Padding="0,1,0,0"
                    AppointmentControls="{Binding VerticalCellContainer.AppointmentControlCollection}"
                    CellContainer="{Binding VerticalCellContainer}"
                    CellPadding="{Binding View.AppointmentPadding}"
                    SchedulerTimeCellControl="{Binding ElementName=VerticalDay}"
