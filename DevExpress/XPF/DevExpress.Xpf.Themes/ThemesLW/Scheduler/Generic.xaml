    <!-- DayViewGroupByDate -->
    <Style x:Key="{dxscht:LWKey DayView.DayViewGroupByDate}" TargetType="{x:Type dxschd:VisualDayViewGroupByDate}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type dxschd:VisualDayViewGroupByDate}">
