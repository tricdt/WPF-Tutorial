    <Style x:Key="{dxscht:LWKey DayView.DateHeaderStyle}" TargetType="{x:Type dxschd:VisualDateHeader}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type dxschd:VisualDateHeader}">
                    <Grid
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        dxsch:SchedulerControl.HitTestType="DayHeader"
                        Background="Transparent"
                        IsHitTestVisible="True"
                        SnapsToDevicePixels="True">
                        <dxschd:HeaderControl
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            DisableResourceColor="{TemplateBinding DisableResourceColor}"
                            IsAlternate="{Binding IsAlternate}"
                            Orientation="Horizontal"
                            ResourceColor="{Binding Brushes.ResourceHeader}"
                            SnapsToDevicePixels="True" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <dxschd:DayHeaderControl
                        x:Name="DayViewDayHeaderControl"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        Date="{Binding IntervalStart}"
                        DateFormats="{Binding DateFormats}"
                        SnapsToDevicePixels="True">
                        <TextBlock Foreground="{DynamicResource {dxscht:LWKey SchedulerView.HeaderForegroundColor}}" SnapsToDevicePixels="True" TextAlignment="Center" />
                    </dxschd:DayHeaderControl>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--#endregion C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\DayView\DayHeader.xaml-->

    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\DayView\GroupByDate.xaml-->


