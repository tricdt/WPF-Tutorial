                            </dxschd:SchedulerItemsControl.ItemsPanel>
                        </dxschd:SchedulerItemsControl>
                    </Grid>
                </StackPanel>
            </Grid>
            <Grid Background="Transparent">
                <Grid.RowDefinitions>
                    <RowDefinition>
                        <RowDefinition.SharedSizeGroup>
                            <sys:String>TimeRuler</sys:String>
