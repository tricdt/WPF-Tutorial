    <!-- WeekViewVerticalWeekTemplate -->
    <DataTemplate x:Key="{dxscht:LWKey WeekView.WeekViewVerticalWeekTemplate}">
        <dx:XPFContentControl dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" Content="{Binding CellContainer}">
            <ContentControl.Template>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <ContentPresenter dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" />
                </ControlTemplate>
            </ContentControl.Template>
            <ContentControl.ContentTemplate>
                <DataTemplate>
                    <Grid>
                        <Grid.Resources>
                            <dxschd:AppointmentStatusDisplayTypeToVisibileConverter x:Key="StatusDisplayTypeToVisibileConverter" />
                        </Grid.Resources>
                        <dxschd:SchedulerHorizontalTimeCellsControl
                            x:Name="HorizontalWeek"
                            HorizontalContentAlignment="Stretch"
                            VerticalContentAlignment="Stretch"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                            ItemContainerStyle="{Binding View.VerticalWeekCellStyle}"
                            ItemsSource="{Binding Cells}">
                            <dxschd:SchedulerHorizontalTimeCellsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <dxschd:TwoColumnVerticalWeekPanel
                                        Margin="0,0,0,0"
                                        dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), ElementName=HorizontalWeek}"
                                        CompressedIndex="{Binding CompressedIndex}"
                                        IsCompressed="True" />
                                </ItemsPanelTemplate>
                            </dxschd:SchedulerHorizontalTimeCellsControl.ItemsPanel>
                        </dxschd:SchedulerHorizontalTimeCellsControl>
                        <dxschd:SelectionPresenter
                            CellContainer="{Binding}"
                            Resource="{Binding Resource}"
                            SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}"
                            SelectedCells="{Binding SelectedCells}"
                            SelectionTemplate="{Binding View.SelectionTemplate}" />
                        <dxschd:DynamicHorizontalAppointmentsPanel
                            x:Name="AppointmentsPanel"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), ElementName=HorizontalWeek}"
                            AppointmentControls="{Binding AppointmentControlCollection}"
                            AutoHeight="{Binding View.AppointmentDisplayOptions.AppointmentAutoHeight}"
                            CellContainer="{Binding}"
                            CellPadding="1,0,0,0"
                            DefaultAppointmentHeight="{DynamicResource {dxscht:LWKey SchedulerControl.DefaultAppointmentHeight}}"
                            IsStatusVisible="{Binding View.AppointmentDisplayOptions.StatusDisplayType, Converter={StaticResource StatusDisplayTypeToVisibileConverter}}"
                            SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}">
                            <dxschd:DynamicHorizontalAppointmentsPanel.Margin>
                                <Binding Converter="{StaticResource horizontalPositionToMarginConverter}" ElementName="HorizontalWeek" Path="(dxschd:SchedulerItemsControl.ElementPosition)">
                                    <Binding.ConverterParameter>
                                        <Thickness>1,0,0,0</Thickness>
                                    </Binding.ConverterParameter>
                                </Binding>
                            </dxschd:DynamicHorizontalAppointmentsPanel.Margin>
                        </dxschd:DynamicHorizontalAppointmentsPanel>
                        <dxschd:DynamicHorizontalAppointmentsPanel
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), ElementName=HorizontalWeek}"
                            AppointmentControls="{Binding DraggedAppointmentControlCollection}"
                            AppointmentStyle="{Binding View.DragDropHoverTimeCellsStyle}"
                            CellContainer="{Binding}"
                            DefaultAppointmentHeight="{DynamicResource {dxscht:LWKey SchedulerControl.DefaultAppointmentHeight}}"
                            IsDraggedMode="True"
                            SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}">
                            <dxschd:DynamicHorizontalAppointmentsPanel.Margin>
                                <Binding Converter="{StaticResource horizontalPositionToMarginConverter}" ElementName="HorizontalWeek" Path="(dxschd:SchedulerItemsControl.ElementPosition)">
                                    <Binding.ConverterParameter>
                                        <Thickness>1,0,0,0</Thickness>
                                    </Binding.ConverterParameter>
                                </Binding>
                            </dxschd:DynamicHorizontalAppointmentsPanel.Margin>
                        </dxschd:DynamicHorizontalAppointmentsPanel>

                        <dxschd:MoreButtonItemsControl
                            HorizontalContentAlignment="Stretch"
                            VerticalContentAlignment="Stretch"
                            ItemTemplate="{DynamicResource {dxscht:LWKey SchedulerView.MoreButtonCellTemplate}}"
                            ItemsSource="{Binding ElementName=AppointmentsPanel, Path=MoreButtonInfos}"
                            View="{Binding Path=View}">
                            <dxschd:MoreButtonItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <dxschd:HorizontalWeekMoreButtonPanel2 />
                                </ItemsPanelTemplate>
                            </dxschd:MoreButtonItemsControl.ItemsPanel>
                        </dxschd:MoreButtonItemsControl>
                    </Grid>
                </DataTemplate>
            </ContentControl.ContentTemplate>
        </dx:XPFContentControl>
    </DataTemplate>
    <!--#endregion C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\WeekView\VerticalWeek.xaml-->

