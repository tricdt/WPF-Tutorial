                                            <dxschd:VisualResourceBrushes />
                                        </dxschd:VisualDayViewColumn.Brushes>
                                    </dxschd:VisualDayViewColumn>
                                </dxschd:VisualDayOfWeekHeader.DataContext>
                                <dxschd:VisualDayOfWeekHeader.ContentTemplate>
                                    <DataTemplate />
                                </dxschd:VisualDayOfWeekHeader.ContentTemplate>
                            </dxschd:VisualDayOfWeekHeader>
                            <!-- Day header -->
                            <dx:XPFContentControl
                                Grid.Column="1"
                                VerticalAlignment="Top"
                                DataContext="{Binding View.Control, RelativeSource={RelativeSource TemplatedParent}}"
                                Style="{DynamicResource {dxscht:LWKey SchedulerView.HorizontalHeadersContainerStyle}}">
                                <dxschd:SchedulerDayOfWeekHeaderItemsControl
                                    x:Name="ResourcesItemsControl"
                                    HorizontalContentAlignment="Stretch"
                                    VerticalContentAlignment="Stretch"
                                    ItemContainerStyle="{Binding View.DayOfWeekHeaderStyle, RelativeSource={RelativeSource TemplatedParent}}"
                                    ItemsSource="{TemplateBinding DayOfWeekHeaders}">
                                    <dxschd:SchedulerDayOfWeekHeaderItemsControl.Resources>
                                        <dxschd:ResourceNavigatorVisibilityToElementPositionConverter x:Key="ElementPositionConverter" OuterSeparatorWhenHidden="{DynamicResource {dxscht:LWKey WeekView.DayOfWeekHeadersGroupByDateRightCornerSeparatorPosition}}" VisibleElementPosition="{DynamicResource {dxscht:LWKey WeekView.DayOfWeekHeadersGroupByDateElementPosition}}" />
