    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\AppointmentResourceEdit.xaml-->

    <DataTemplate x:Key="{dxscht:LWKey SchedulerUIControl.SchedulerResourceItemTemplate}">
        <StackPanel Orientation="Horizontal">
            <dxschd:SquareBorder
                Padding="1"
                Background="Gray"
                CornerRadius="1"
                Style="{DynamicResource {dxscht:LWKey SchedulerUIControl.ColorMarkerStyle}}">
                <dx:DXBorder Background="{Binding Path=Color, Converter={StaticResource colorToBrushConverter}}" />
            </dxschd:SquareBorder>
            <TextBlock x:Name="PART_Editor" Style="{DynamicResource {dxscht:LWKey SchedulerUIControl.ItemTextBlockStyle}}" Text="{Binding Caption}" />
        </StackPanel>
    </DataTemplate>

    <ControlTemplate x:Key="{dxscht:LWKey SchedulerUIControl.SchedulerResourceNonEditableTemplate}">
        <dx:DXBorder Margin="{Binding Path=(dxe:BaseEdit.OwnerEdit).Padding, RelativeSource={RelativeSource TemplatedParent}}">
            <StackPanel Orientation="Horizontal">
                <dxschd:SquareBorder
                    Padding="1"
                    Background="Gray"
                    CornerRadius="1"
                    Style="{DynamicResource {dxscht:LWKey SchedulerUIControl.ColorMarkerStyle}}">
                    <dx:DXBorder Background="{Binding Path=(dxe:BaseEdit.OwnerEdit).SelectedItem.Color, Converter={StaticResource colorToBrushConverter}, RelativeSource={RelativeSource TemplatedParent}}" />
                </dxschd:SquareBorder>
                <TextBlock x:Name="PART_Editor" Style="{DynamicResource {dxscht:LWKey SchedulerUIControl.ItemTextBlockStyle}}">
                    <TextBlock.FocusVisualStyle>
                        <Style>
                            <Setter Property="Control.Template">
                                <Setter.Value>
                                    <ControlTemplate>
                                        <Rectangle
                                            SnapsToDevicePixels="True"
                                            Stroke="#FF000000"
                                            StrokeDashArray="1 2"
                                            StrokeThickness="1" />
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </TextBlock.FocusVisualStyle>
                </TextBlock>
            </StackPanel>
        </dx:DXBorder>
    </ControlTemplate>
    <!--#endregion C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\AppointmentResourceEdit.xaml-->

    <!--#region C:\work\2023.1\XPF\DevExpress.Xpf.Scheduler\DevExpress.Xpf.Scheduler\Themes\Generic\AppointmentStatusEdit.xaml-->

