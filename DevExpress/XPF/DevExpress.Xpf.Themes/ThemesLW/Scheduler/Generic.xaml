                                        SnapsToDevicePixels="True"
                                        Style="{DynamicResource {dxscht:LWKey DayView.TopHeadersStyle}}"
                                        Visibility="{Binding ShowDayHeaders, RelativeSource={RelativeSource Mode=TemplatedParent}, Converter={StaticResource boolToVisibilityConverter}}">
                                        <dxschd:SchedulerItemsControl.ItemTemplate>
                                            <DataTemplate>
                                                <dxschd:VisualDateHeader
                                                    dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                                                    Content="{Binding Content.FirstSimpleInterval, RelativeSource={RelativeSource TemplatedParent}}"
                                                    DataContext="{Binding Content.FirstSimpleInterval, RelativeSource={RelativeSource TemplatedParent}}"
                                                    DisableResourceColor="True"
                                                    Style="{Binding Content.View.DateHeaderStyle, RelativeSource={RelativeSource TemplatedParent}}" />
                                            </DataTemplate>
                                        </dxschd:SchedulerItemsControl.ItemTemplate>
                                        <dxschd:SchedulerItemsControl.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <dxschd:PixelSnappedUniformGrid dxschd:SchedulerItemsControl.ElementPosition="{DynamicResource {dxscht:LWKey DayView.TopHeadersElementPosition}}" Orientation="Horizontal" SnapsToDevicePixels="True" />
                                            </ItemsPanelTemplate>
                                        </dxschd:SchedulerItemsControl.ItemsPanel>
                                    </dxschd:SchedulerItemsControl>
                                    <dxschd:SharedSizePanel
                                        Grid.Column="1"
                                        dxschd:SchedulerItemsControl.ElementPosition="{DynamicResource {dxscht:LWKey DayView.TopRightHeaderCornerPosition}}"
                                        IsBaseSizePanel="False"
                                        Orientation="Horizontal"
                                        SharedSizeGroup="VerticalScrollBarSizeGroup"
                                        SnapsToDevicePixels="True"
                                        Visibility="{Binding ShowDayHeaders, RelativeSource={RelativeSource Mode=TemplatedParent}, Converter={StaticResource boolToVisibilityConverter}}">
                                        <dxschd:VisualDateHeader SnapsToDevicePixels="True" Style="{Binding View.DateHeaderStyle, RelativeSource={RelativeSource TemplatedParent}}" Visibility="{DynamicResource {dxscht:LWKey SchedulerView.HeaderCornerVisibility}}">
                                            <dxschd:VisualDateHeader.DataContext>
                                                <dxschd:VisualDayViewColumn IsAlternate="false">
                                                    <dxschd:VisualDayViewColumn.Brushes>
                                                        <dxschd:VisualResourceBrushes />
                                                    </dxschd:VisualDayViewColumn.Brushes>
                                                </dxschd:VisualDayViewColumn>
                                            </dxschd:VisualDateHeader.DataContext>
                                        </dxschd:VisualDateHeader>
                                    </dxschd:SharedSizePanel>
                                    <!-- Resource headers -->
                                    <dxschd:SchedulerItemsControl
                                        Grid.Row="1"
                                        HorizontalAlignment="Stretch"
                                        Background="Transparent"
                                        ItemTemplate="{DynamicResource {dxscht:LWKey DayView.GroupByDateDayViewHeadersTemplate}}"
                                        ItemsSource="{TemplateBinding Intervals}"
                                        SnapsToDevicePixels="True"
                                        Style="{DynamicResource {dxscht:LWKey DayView.BottomHeadersStyle}}">
                                        <dxschd:SchedulerItemsControl.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <dxschd:PixelSnappedUniformGrid
                                                    HorizontalAlignment="Stretch"
                                                    dxschd:SchedulerItemsControl.ElementPosition="{DynamicResource {dxscht:LWKey DayView.NonTopHeadersElementPosition}}"
                                                    Orientation="Horizontal"
                                                    SnapsToDevicePixels="True" />
                                            </ItemsPanelTemplate>
                                        </dxschd:SchedulerItemsControl.ItemsPanel>
                                    </dxschd:SchedulerItemsControl>
                                    <dxschd:SharedSizePanel
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        dxschd:SchedulerItemsControl.ElementPosition="End,Middle,Both,End"
                                        IsBaseSizePanel="False"
                                        Orientation="Horizontal"
                                        SharedSizeGroup="VerticalScrollBarSizeGroup">
                                        <dxschd:VisualResourceHeader
                                            Content="{Binding Intervals.Last.ResourceHeaders.Last, RelativeSource={RelativeSource TemplatedParent}}"
                                            SnapsToDevicePixels="True"
                                            Style="{Binding View.HorizontalResourceHeaderStyle, RelativeSource={RelativeSource TemplatedParent}}"
                                            Visibility="{DynamicResource {dxscht:LWKey SchedulerView.HeaderCornerVisibility}}">
                                            <dxschd:VisualResourceHeader.ContentTemplate>
                                                <DataTemplate />
                                            </dxschd:VisualResourceHeader.ContentTemplate>
                                        </dxschd:VisualResourceHeader>
                                    </dxschd:SharedSizePanel>

                                    <!-- AllDayArea -->
                                    <dx:XPFContentControl
                                        x:Name="AllDayAreaContainer"
                                        Grid.Row="2"
                                        Grid.ColumnSpan="2"
                                        Style="{DynamicResource {dxscht:LWKey DayView.AllDayAreaContentContainerStyle}}"
                                        Visibility="{Binding View.ShowAllDayArea, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}">
                                        <dxschd:SharedSizePanel
                                            dxschd:SchedulerItemsControl.ElementPosition="Standalone, Standalone, Start, End, Start, End"
                                            IsBaseSizePanel="True"
                                            Orientation="Vertical"
                                            SharedSizeGroup="AllDayAreaVerticalGroup"
                                            SnapsToDevicePixels="True">
                                            <dxschd:AllDayAreaControlContainer
                                                dxschd:HitTestManager.IsHitTestContainer="True"
                                                dxschd:SchedulerItemsControl.ElementPosition="Standalone, Standalone, Start, End, Start, End"
                                                AllowScrolling="{Binding View.AllDayAreaScrollBarVisible, RelativeSource={RelativeSource TemplatedParent}}"
                                                Content="{Binding AllDayAreaContainerGroups, RelativeSource={RelativeSource TemplatedParent}}"
                                                ContentTemplate="{Binding View.AllDayAreaScrollBarVisible, Converter={StaticResource DayViewThemeKey_AllDayTemplateConverter}, RelativeSource={RelativeSource TemplatedParent}}"
                                                Template="{DynamicResource {dxscht:LWKey SchedulerControlGeneric.ElementPositionAttachedPropertySetterTemplate}}"
                                                Visibility="{Binding View.ShowAllDayArea, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}" />
                                        </dxschd:SharedSizePanel>
                                    </dx:XPFContentControl>
                                </Grid>

                                <!-- Time rulers -->
                                <dx:XPFContentControl
                                    Grid.Row="1"
                                    MinWidth="{DynamicResource {dxscht:LWKey DayView.TimeRulersMinWidth}}"
                                    Style="{DynamicResource {dxscht:LWKey DayView.TimeRulersContainerStyle}}"
                                    Visibility="{Binding ShowTimeRulers, RelativeSource={RelativeSource Mode=TemplatedParent}, Converter={StaticResource boolToVisibilityConverter}}">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <dxschd:DayViewScrollViewer
                                            x:Name="TimeRulersScrollViewer"
                                            dxschd:ScrollViewerSynchronizer.IsSynchronize="True"
                                            SnapsToDevicePixels="True"
                                            Style="{DynamicResource {dxscht:LWKey DayView.SchedulerScrollViewer}}"
                                            VerticalScrollBarVisibility="Hidden">
                                            <dxmvvm:Interaction.Behaviors>
                                                <dxmvvm:DependencyPropertyBehavior Binding="{Binding DateTimeScrollOffset, RelativeSource={RelativeSource Mode=TemplatedParent}, Mode=OneWayToSource}" EventName="ScrollChanged" PropertyName="ContentVerticalOffset" />
                                            </dxmvvm:Interaction.Behaviors>
                                            <Grid SnapsToDevicePixels="True">
                                                <dxschd:SchedulerItemsControl
                                                    x:Name="TimeRuler"
                                                    HorizontalAlignment="Right"
                                                    VerticalContentAlignment="Stretch"
                                                    ItemTemplate="{DynamicResource {dxscht:LWKey DayView.TimeRulerTemplate}}"
                                                    ItemsSource="{TemplateBinding TimeRulers}"
                                                    SnapsToDevicePixels="True">
                                                    <dxschd:SchedulerItemsControl.ItemsPanel>
                                                        <ItemsPanelTemplate>
                                                            <dxschd:SharedSizePanel Orientation="Horizontal" SharedSizeGroup="TimeRulers" />
                                                        </ItemsPanelTemplate>
                                                    </dxschd:SchedulerItemsControl.ItemsPanel>
                                                </dxschd:SchedulerItemsControl>
                                            </Grid>
                                        </dxschd:DayViewScrollViewer>
                                        <dxschd:DayViewMoreButton
                                            x:Name="TimeRulerGoUpButton"
                                            Margin="0,2,1,0"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Top"
                                            Command="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MoreButtonGoUpCommand}"
                                            CommandParameter="{Binding ElementName=TimeRulersScrollViewer}"
                                            Style="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=View.MoreButtonUpStyle}">
                                            <dxschd:DayViewMoreButton.Visibility>
                                                <MultiBinding Converter="{dxschd:LogicalAndToVisibilityMultiValueConverter}">
                                                    <Binding Path="IsEnabled" RelativeSource="{RelativeSource Self}" />
                                                    <Binding Path="IsMoreButtonsOnTimeRulerVisible" RelativeSource="{RelativeSource TemplatedParent}" />
                                                </MultiBinding>
                                            </dxschd:DayViewMoreButton.Visibility>
                                        </dxschd:DayViewMoreButton>
                                        <dxschd:DayViewMoreButton
                                            x:Name="TimeRulerGoDownButton"
                                            Margin="0,0,1,2"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Bottom"
                                            Command="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MoreButtonGoDownCommand}"
                                            CommandParameter="{Binding ElementName=TimeRulersScrollViewer}"
                                            Style="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=View.MoreButtonDownStyle}">
                                            <dxschd:DayViewMoreButton.Visibility>
                                                <MultiBinding Converter="{dxschd:LogicalAndToVisibilityMultiValueConverter}">
                                                    <Binding Path="IsEnabled" RelativeSource="{RelativeSource Self}" />
                                                    <Binding Path="IsMoreButtonsOnTimeRulerVisible" RelativeSource="{RelativeSource TemplatedParent}" />
                                                </MultiBinding>
                                            </dxschd:DayViewMoreButton.Visibility>
                                        </dxschd:DayViewMoreButton>
                                        <dxschd:SharedSizePanel
                                            Grid.Row="1"
                                            IsBaseSizePanel="False"
                                            Orientation="Vertical"
                                            SharedSizeGroup="ResourceNavigatorRowGroup"
                                            SnapsToDevicePixels="True">
                                            <dx:DXBorder />
                                        </dxschd:SharedSizePanel>
                                    </Grid>
                                </dx:XPFContentControl>

                                <!-- MainCells -->
                                <dx:XPFContentControl
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    SnapsToDevicePixels="True"
                                    Style="{DynamicResource {dxscht:LWKey DayView.VerticalContentContainerStyle}}"
                                    UseLayoutRounding="True">
                                    <Grid SnapsToDevicePixels="True" UseLayoutRounding="True">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <dxschd:DayViewScrollViewer
                                            x:Name="PART_DayViewScrollViewer"
                                            Grid.ColumnSpan="2"
                                            HorizontalContentAlignment="Stretch"
                                            dxsch:TouchSurfaceSynchronizer.Surface="{Binding RelativeSource={RelativeSource AncestorType=dxsch:SchedulerControl}}"
                                            dxschd:ScrollViewerSynchronizer.IsSynchronize="True"
                                            SnapsToDevicePixels="True"
                                            Style="{DynamicResource {dxscht:LWKey DayView.SchedulerScrollViewer}}"
                                            VerticalScrollBarVisibility="Visible">
                                            <Grid>
                                                <dxschd:SchedulerItemsControl
                                                    HorizontalAlignment="Stretch"
                                                    Background="Transparent"
                                                    ItemTemplate="{DynamicResource {dxscht:LWKey DayView.GroupByDateDayViewResourceDaysTemplate}}"
                                                    ItemsSource="{TemplateBinding Intervals}">
                                                    <dxschd:SchedulerItemsControl.ItemsPanel>
                                                        <ItemsPanelTemplate>
                                                            <dxschd:PixelSnappedUniformGrid HorizontalAlignment="Stretch" dxschd:SchedulerItemsControl.ElementPosition="Standalone, Standalone, Start, Both" Orientation="Horizontal" />
                                                        </ItemsPanelTemplate>
                                                    </dxschd:SchedulerItemsControl.ItemsPanel>
                                                </dxschd:SchedulerItemsControl>
                                            </Grid>
                                        </dxschd:DayViewScrollViewer>
                                        <ItemsControl ItemTemplate="{DynamicResource {dxscht:LWKey DayView.DayViewGroupByDateNavigationButtonsTemplate}}" ItemsSource="{TemplateBinding Intervals}">
                                            <ItemsControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <dxschd:PixelSnappedUniformGrid dxschd:SchedulerItemsControl.ElementPosition="Standalone, Standalone, Start, End" Orientation="Horizontal" />
                                                </ItemsPanelTemplate>
                                            </ItemsControl.ItemsPanel>
                                        </ItemsControl>
                                        <Grid
                                            Grid.Row="1"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            SnapsToDevicePixels="True"
                                            UseLayoutRounding="True">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>
                                            <dxschd:SharedSizePanel
                                                IsBaseSizePanel="True"
                                                Orientation="Vertical"
                                                SharedSizeGroup="ResourceNavigatorRowGroup"
                                                SnapsToDevicePixels="True">
                                                <dxschd:SharedSizePanel.Resources>
                                                    <dxschd:ResourceNavigatorVisibilityToConverter x:Key="ResourceNavigatorVisibilityToConverter" />
                                                </dxschd:SharedSizePanel.Resources>
                                                <dxschd:ResourceNavigatorControl
                                                    Grid.Row="1"
                                                    Margin="{DynamicResource {dxscht:LWKey DayView.ResourceNavigatorMargin}}"
                                                    dxsch:SchedulerControl.HitTestType="None"
                                                    dxschd:HitTestManager.IsHitTestContainer="True"
                                                    IsHitTestVisible="True"
                                                    SchedulerControl="{TemplateBinding Control}"
                                                    Style="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Control.ResourceNavigatorHorizontalStyle}"
                                                    Visibility="{Binding Control.ResourceNavigator.Visibility, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ResourceNavigatorVisibilityToConverter}}" />
                                            </dxschd:SharedSizePanel>
                                        </Grid>
                                        <dxschd:SharedSizePanel
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            IsBaseSizePanel="False"
                                            Orientation="Horizontal"
                                            SharedSizeGroup="VerticalScrollBarSizeGroup"
                                            SnapsToDevicePixels="True">
                                            <dx:CornerBox x:Name="Corner" />
                                        </dxschd:SharedSizePanel>
                                    </Grid>
                                </dx:XPFContentControl>
                            </Grid>
                        </dxschd:ScrollViewerSynchronizer>
                    </dxschd:SharedGroupSizeContainer>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- END TOSLONLY REPLACE 'dxschd:DayViewScrollViewer'ScrollViewer' -->
