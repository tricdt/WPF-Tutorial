    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.Themes\Base\DocumentViewer\Base\DocumentViewerBar.xaml-->
    <!-- General -->

    <ControlTemplate x:Key="{dxdvt:LWKey DocumentViewer.DocumentViewerBarTemplate}" TargetType="{x:Type ContentControl}">
        <Grid x:Name="Root">
            <dx:DXBorder Background="{dx:LWBrush 'Brush.Window.Background, Win11=Brush.WindowBackground'}" Margin="0,0,5,5">
                <dx:DXBorder.Effect>
                    <DropShadowEffect
                        x:Name="Shadow"
                        BlurRadius="4"
                        Direction="315"
                        Opacity="0.14"
                        ShadowDepth="3" />
                </dx:DXBorder.Effect>
            </dx:DXBorder>
            <dx:DXBorder
                Background="{dx:LWBrush 'Brush.Window.Background, Win11=Brush.WindowBackground'}"
                BorderBrush="{StaticResource Brush.Border}"
                BorderThickness="0,0,0,1"
                Margin="0,0,5,5">
                <ContentPresenter />
            </dx:DXBorder>
        </Grid>
    </ControlTemplate>
