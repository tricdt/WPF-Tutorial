    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ChooseReportColorSchemePage.xaml-->
    <DataTemplate x:Key="defaultTemplate">
        <dx:DXBorder
            Height="68"
            Margin="3"
            Background="{Binding BaseColor, Converter={dxrudi:DrawingColorToMediaBrushConverter}}"
            CornerRadius="4">
            <TextBlock
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                FontWeight="Bold"
                Foreground="{Binding BaseColor, Converter={dxrudi:ColorToBlendedBrushConverter BlendColor=#A0FFFFFF}}"
                Text="{Binding DisplayName, Converter={dxrudi:StringToUpperConverter}}" />
        </dx:DXBorder>
    </DataTemplate>
    <DataTemplate x:Key="customTemplate">
        <dx:DXBorder
            Height="68"
            Margin="3"
            Background="{Binding BaseColor, Converter={dxrudi:DrawingColorToMediaBrushConverter}}"
            CornerRadius="4">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition Width="auto" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Margin="0,0,4,0"
                    VerticalAlignment="Center"
                    FontWeight="Bold"
                    Foreground="{Binding BaseColor, Converter={dxrudi:ColorToBlendedBrushConverter BlendColor=#A0FFFFFF}}"
                    Text="{Binding DisplayName, Converter={dxrudi:StringToUpperConverter}}"
                    TextAlignment="Right" />
                <ToggleButton Name="customColor" Grid.Column="1">
                    <dx:DXImage
                        Name="gear"
                        Width="24"
                        Height="24"
                        Source="{dx:SvgImageSource Uri={dx:ReportDesignerResource Images/SVG/Wizard/CustomTheme.svg}}" />
                    <ToggleButton.Style>
                        <Style TargetType="ToggleButton">
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="ToggleButton">
                                        <Grid>
                                            <dx:DXBorder
                                                Name="border"
                                                Background="Gray"
                                                CornerRadius="0,4,4,0"
                                                Opacity="0.1" />
                                            <ContentPresenter
                                                Name="contentPresenter"
                                                Margin="9,0"
                                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                ContentSource="Content" />

                                        </Grid>
                                        <ControlTemplate.Triggers>
                                            <Trigger Property="IsMouseOver" Value="True">
                                                <Setter TargetName="border" Property="Opacity" Value="0.2" />
                                            </Trigger>
                                            <Trigger Property="IsChecked" Value="True">
                                                <Setter TargetName="border" Property="Opacity" Value="0.3" />
                                            </Trigger>
                                        </ControlTemplate.Triggers>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </ToggleButton.Style>
                </ToggleButton>
                <dxe:FlyoutControl VerticalAlignment="Center" IsOpen="{Binding IsChecked, ElementName=customColor, Mode=TwoWay}" PlacementTarget="{Binding ElementName=gear}">
                    <dxe:FlyoutControl.Settings>
                        <dxe:FlyoutSettings Placement="Right" ShowIndicator="True" />
                    </dxe:FlyoutControl.Settings>
                    <dxrudwi:FlyoutColorChooserControl CustomColorScheme="{Binding Mode=OneWay}" />
                </dxe:FlyoutControl>
            </Grid>
        </dx:DXBorder>
    </DataTemplate>



    <dxrudwi:ColorSchemeTemplateSelector x:Key="schemeTemplateSelector" CustomTemplate="{StaticResource customTemplate}" DefaultTemplate="{StaticResource defaultTemplate}" />

    <DataTemplate x:Key="{x:Type dxrudwp:ChooseReportColorSchemePage}">
        <dxe:ListBoxEdit
            Margin="15,24,15,0"
            HorizontalContentAlignment="Stretch"
            VerticalContentAlignment="Top"
            AllowItemHighlighting="True"
            EditValue="{Binding ColorScheme}"
            ItemTemplateSelector="{StaticResource schemeTemplateSelector}"
            ItemsSource="{Binding AvailableColorSchemes}"
            ShowBorder="False">
            <dxe:ListBoxEdit.ItemsPanel>
                <ItemsPanelTemplate>
                    <UniformGrid VerticalAlignment="Top" Columns="5" />
                </ItemsPanelTemplate>
            </dxe:ListBoxEdit.ItemsPanel>
            <dxe:ListBoxEdit.ItemContainerStyle>
                <Style TargetType="{x:Type dxe:ListBoxEditItem}">
                    <Setter Property="Margin" Value="9,0,9,12" />
                    <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                        <Setter.Value>
                            <DataTemplate>
                                <ItemsControl>
                                    <dxrudwi:ColorSchemeListBoxItemBehavior AssociatedListBox="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type dxe:ListBoxEdit}}}" />
                                    <dxdai:DoubleClickCommandExecutor Command="{Binding Path=AssociatedObject.(dxda:DataSourceWizardControlBase.NavigationModel).NextCommand, RelativeSource={RelativeSource Self}}" FinishCommand="{Binding Path=AssociatedObject.(dxda:DataSourceWizardControlBase.NavigationModel).FinishCommand, RelativeSource={RelativeSource Self}}" />
                                </ItemsControl>
                            </DataTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </dxe:ListBoxEdit.ItemContainerStyle>
        </dxe:ListBoxEdit>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ChooseReportColorSchemePage.xaml-->

