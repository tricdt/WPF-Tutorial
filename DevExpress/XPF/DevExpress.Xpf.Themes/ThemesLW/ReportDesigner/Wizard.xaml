    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\MapReportTemplateFieldsPage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:MapReportTemplateFieldsPage}">
        <Grid Grid.Row="1" Margin="12">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition Width="4" />
                <ColumnDefinition Width="2*" />
            </Grid.ColumnDefinitions>

            <dxrudfl:ReportDesignerFieldListControl Nodes="{Binding Nodes}" ShowBorder="True">
                <dxrudfl:ReportDesignerFieldListControl.Resources>
                    <DataTemplate x:Key="{dxrudt:LWKey ReportDesigner.FieldListNodeTemplate}">
                        <StackPanel Orientation="Horizontal">
                            <dx:DXImage Margin="0,0,5,0" VerticalAlignment="Center" Source="{Binding RowData.Row, Converter={dxrudi:TypeSpecificsToImageSourceConverter}}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding Data.DisplayName, Converter={dxrudi:FieldListNodeDisplayNameToDisplayTextConverter}}" />
                        </StackPanel>
                    </DataTemplate>

                    <Style x:Key="{dxrudt:LWKey ReportDesigner.FieldListViewStyle}" BasedOn="{StaticResource ReportDesignerFielsListControlStyleBase}" TargetType="{x:Type dxg:TreeListView}">
                        <Setter Property="ChildNodesPath" Value="Children" />
                        <Setter Property="AllowDragDrop" Value="True" />
                        <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                            <Setter.Value>
                                <DataTemplate>
                                    <ItemsControl>
                                        <dxdai:ProhibitDropToSelfBehavior />
                                    </ItemsControl>
                                </DataTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </dxrudfl:ReportDesignerFieldListControl.Resources>
            </dxrudfl:ReportDesignerFieldListControl>

