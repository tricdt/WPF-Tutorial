    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\CustomizeLabelPage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:CustomizeLabelPage}">
        <Grid Name="root" dxe:ValidationService.IsValidationContainer="True">
            <dxlc:LayoutControl
                Grid.Row="1"
                Margin="24"
                Padding="0"
                Grid.IsSharedSizeScope="True"
                Orientation="Horizontal">
                <dxlc:LayoutControl.Resources>
                    <Style TargetType="dxlc:LayoutItem">
                        <Setter Property="LabelPosition" Value="Left" />
                        <Setter Property="AddColonToLabel" Value="True" />
                    </Style>
                </dxlc:LayoutControl.Resources>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="3*" />
                        <ColumnDefinition Width="5*" />
                    </Grid.ColumnDefinitions>
                    <dxlc:LayoutGroup Margin="0,0,6,0" ItemLabelsAlignment="Local" Orientation="Vertical">
                        <dxlc:LayoutItem
                            Name="pageSizeItem"
                            Padding="0,0,0,6"
                            VerticalAlignment="Top"
                            Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_CustomizeLabel_PageSize}"
                            LabelVerticalAlignment="Top">
                            <StackPanel>
                                <dxe:ComboBoxEdit
                                    ApplyItemTemplateToSelectedItem="True"
                                    EditValue="{Binding SelectedPaperKind, UpdateSourceTrigger=PropertyChanged}"
                                    IsTextEditable="False"
                                    ItemsSource="{Binding PaperKinds}">
                                    <dxe:ComboBoxEdit.ItemTemplate>
                                        <DataTemplate>
                                            <TextBlock
                                                Margin="2,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Text="{Binding DisplayName}" />
                                        </DataTemplate>
                                    </dxe:ComboBoxEdit.ItemTemplate>
                                </dxe:ComboBoxEdit>
                                <TextBlock Margin="6,0,0,0" Foreground="Gray" Text="{Binding PaperKindFormattedText}" />
                            </StackPanel>
                        </dxlc:LayoutItem>
                        <dxlc:LayoutGroup Grid.Row="1" VerticalAlignment="Stretch" Orientation="Vertical">
                            <dxlc:LayoutGroup.Resources>
                                <Style TargetType="dxe:SpinEdit">
                                    <Setter Property="MaskUseAsDisplayFormat" Value="True" />
                                    <Setter Property="Mask" Value="{Binding Unit, Converter={dxrudwi:GraphicsUnitsToMaskConverter}}" />
                                    <Setter Property="EditValueType" Value="{x:Type sys:Single}" />
                                    <Setter Property="Increment" Value="{Binding Unit, Converter={dxrudwi:GraphicsUnitsToIncrementConverter}}" />
                                </Style>
                            </dxlc:LayoutGroup.Resources>
                            <dxlc:LayoutGroup Padding="0,0,0,6" Orientation="Vertical">
                                <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_CustomizeLabel_LabelWidth}">
                                    <dxe:SpinEdit EditValue="{Binding Width, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" />
                                </dxlc:LayoutItem>
                                <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_CustomizeLabel_LabelHeight}">
                                    <dxe:SpinEdit EditValue="{Binding Height, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" />
                                </dxlc:LayoutItem>
                            </dxlc:LayoutGroup>
                            <dxlc:LayoutGroup Padding="0,0,0,6" Orientation="Vertical">
                                <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_CustomizeLabel_VerticalPitch}">
                                    <dxe:SpinEdit EditValue="{Binding VerticalPitch, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" />
                                </dxlc:LayoutItem>
                                <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_CustomizeLabel_HorizontalPitch}">
                                    <dxe:SpinEdit EditValue="{Binding HorizontalPitch, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" />
                                </dxlc:LayoutItem>
                            </dxlc:LayoutGroup>
                            <dxlc:LayoutGroup Orientation="Vertical">
                                <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_CustomizeLabel_TopMargin}">
                                    <dxe:SpinEdit EditValue="{Binding TopMargin, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" />
                                </dxlc:LayoutItem>
                                <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_CustomizeLabel_LeftMargin}">
                                    <dxe:SpinEdit EditValue="{Binding LeftMargin, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" />
                                </dxlc:LayoutItem>
                                <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_CustomizeLabel_RightMargin}">
                                    <dxe:SpinEdit EditValue="{Binding RightMargin, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" />
                                </dxlc:LayoutItem>
                                <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_CustomizeLabel_BottomMargin}">
                                    <dxe:SpinEdit EditValue="{Binding BottomMargin, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" />
                                </dxlc:LayoutItem>
                            </dxlc:LayoutGroup>
                        </dxlc:LayoutGroup>
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup
                        Grid.Column="1"
                        Margin="12,0,0,0"
                        VerticalAlignment="Stretch"
                        Orientation="Vertical">
                        <dxlc:LayoutItem Height="{Binding ActualHeight, ElementName=pageSizeItem}" VerticalContentAlignment="Top">
                            <dxe:ListBoxEdit EditValue="{Binding Unit, UpdateSourceTrigger=PropertyChanged}" ItemsSource="{Binding Units}" ShowBorder="False">
                                <dxe:ListBoxEdit.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Margin="0,0,12,0" Text="{Binding}" />
                                    </DataTemplate>
                                </dxe:ListBoxEdit.ItemTemplate>
                                <dxe:ListBoxEdit.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <StackPanel Orientation="Horizontal" />
                                    </ItemsPanelTemplate>
                                </dxe:ListBoxEdit.ItemsPanel>
                                <dxe:ListBoxEdit.StyleSettings>
                                    <dxe:RadioListBoxEditStyleSettings />
                                </dxe:ListBoxEdit.StyleSettings>
                            </dxe:ListBoxEdit>
                        </dxlc:LayoutItem>
                        <dxlc:LayoutItem HorizontalAlignment="Stretch" VerticalAlignment="Top">
                            <dx:ThemedBorder BorderThickness="1">
                                <dx:DXImage Source="pack://application:,,,/DevExpress.Xpf.ReportDesigner.v24.1;component/Images/Wizard/labelReportPage.png" Stretch="Uniform" UseLayoutRounding="False" />
                            </dx:ThemedBorder>
                        </dxlc:LayoutItem>
                    </dxlc:LayoutGroup>
                </Grid>
            </dxlc:LayoutControl>
            <dx:DialogFooter>
                <Grid>
                    <ContentControl HorizontalAlignment="Right" Content="{x:Null}" ContentTemplate="{DynamicResource {dxdat:LWKey DataAccess.WizardDefaultFooterButtonsTemplate}}" />
                </Grid>
            </dx:DialogFooter>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\CustomizeLabelPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\SelectColumnsPage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:SelectColumnsPage}">
        <Grid>
            <dxlc:LayoutControl Grid.Row="1" Margin="24" Padding="0">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <dxlc:LayoutItem
                        Grid.RowSpan="2"
                        VerticalAlignment="Stretch"
                        Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SelectColumns_AvailableColumns}"
                        LabelPosition="Top">
                        <dxe:ListBoxEdit
                            Name="availableColumns"
                            DisplayMember="DisplayName"
                            EditValue="{Binding ActiveAvailableColumns, Mode=TwoWay, Converter={dxrudwi:ObjectListToTypedListConverter ObjectType=dataproviders:ColumnInfo}}"
                            ItemsSource="{Binding AvailableColumns}"
                            SelectedIndex="0"
                            SelectionMode="Multiple" />
                    </dxlc:LayoutItem>
                    <StackPanel
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="12,0,12,60"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
