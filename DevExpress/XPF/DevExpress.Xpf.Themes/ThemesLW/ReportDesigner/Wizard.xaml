                <RowDefinition />
                <RowDefinition Height="auto" />
            </Grid.RowDefinitions>
            <dxlc:LayoutControl>
                <dxlc:LayoutGroup Orientation="Vertical">
                    <dxlc:LayoutItem>
                        <dxe:ListBoxEdit
                                ShowBorder="False"
                                EditValue="{Binding ShouldCreateNewConnection, UpdateSourceTrigger=PropertyChanged}"
                                StyleSettings="{dxdai:RadioListBoxEditStyleSettingsEx}" >
