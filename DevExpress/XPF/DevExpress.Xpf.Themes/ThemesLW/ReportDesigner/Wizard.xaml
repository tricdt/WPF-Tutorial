<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:crosstab="clr-namespace:DevExpress.XtraReports.UI.CrossTab;assembly=DevExpress.XtraReports.v24.1"
    xmlns:dataproviders="clr-namespace:DevExpress.Data.XtraReports.DataProviders;assembly=DevExpress.Data.v24.1"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxd="clr-namespace:DevExpress.Data;assembly=DevExpress.Data.v24.1"
    xmlns:dxda="http://schemas.devexpress.com/winfx/2008/xaml/dataaccess"
    xmlns:dxdai="http://schemas.devexpress.com/winfx/2008/xaml/dataaccess/internal"
    xmlns:dxdat="http://schemas.devexpress.com/winfx/2008/xaml/dataaccess/themekeys"
    xmlns:dxdawn="clr-namespace:DevExpress.Xpf.DataAccess.DataSourceWizard.Native;assembly=DevExpress.Xpf.DataAccess.v24.1"
    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
    xmlns:dxpt="http://schemas.devexpress.com/winfx/2008/xaml/printing/themekeys"
    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
    xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
    xmlns:dxrud="clr-namespace:DevExpress.Xpf.Reports.UserDesigner;assembly=DevExpress.Xpf.ReportDesigner.v24.1"
    xmlns:dxrude="clr-namespace:DevExpress.Xpf.Reports.UserDesigner.Editors;assembly=DevExpress.Xpf.ReportDesigner.v24.1"
    xmlns:dxrudei="clr-namespace:DevExpress.Xpf.Reports.UserDesigner.Editors.Native;assembly=DevExpress.Xpf.ReportDesigner.v24.1"
    xmlns:dxrudfl="clr-namespace:DevExpress.Xpf.Reports.UserDesigner.FieldList;assembly=DevExpress.Xpf.ReportDesigner.v24.1"
    xmlns:dxrudi="clr-namespace:DevExpress.Xpf.Reports.UserDesigner.Native;assembly=DevExpress.Xpf.ReportDesigner.v24.1"
    xmlns:dxrudl="clr-namespace:DevExpress.Xpf.Reports.UserDesigner.Localization;assembly=DevExpress.Xpf.ReportDesigner.v24.1"
    xmlns:dxrudt="clr-namespace:DevExpress.Xpf.Reports.UserDesigner.Themes;assembly=DevExpress.Xpf.ReportDesigner.v24.1"
    xmlns:dxrudwi="clr-namespace:DevExpress.Xpf.Reports.UserDesigner.ReportWizard.Native;assembly=DevExpress.Xpf.ReportDesigner.v24.1"
    xmlns:dxrudwp="clr-namespace:DevExpress.Xpf.Reports.UserDesigner.ReportWizard.Pages;assembly=DevExpress.Xpf.ReportDesigner.v24.1"
    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
    xmlns:localization="http://schemas.devexpress.com/winfx/2008/xaml/dataaccess"
    xmlns:printingthemes="http://schemas.devexpress.com/winfx/2008/xaml/printing/themekeys"
    xmlns:reportsui="clr-namespace:DevExpress.XtraReports.UI;assembly=DevExpress.XtraReports.v24.1"
    xmlns:utils="clr-namespace:DevExpress.Utils;assembly=DevExpress.Data.v24.1">

    <ResourceDictionary.MergedDictionaries>
        <dxi:ThemeResourceDictionary Path="ReportDesigner/BaseStyles.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <dxmvvm:ObjectToObjectConverter x:Key="removeButtonVisibilityConverter" DefaultTarget="{x:Static Visibility.Visible}">
        <dxmvvm:MapItem Source="{x:Static dxg:GridControl.NewItemRowHandle}" Target="{x:Static Visibility.Hidden}" />
    </dxmvvm:ObjectToObjectConverter>

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ChooseReportTypePage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:ChooseReportTypePage}">
        <Grid Margin="36,0">
            <dxe:ListBoxEdit
                Grid.Row="1"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                EditValue="{Binding ReportType, UpdateSourceTrigger=PropertyChanged}"
                ItemsSource="{Binding ReportTypes}"
                ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                ShowBorder="False">
                <dxe:ListBoxEdit.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Margin="10">
                            <dx:DXImage Width="96" Source="{Binding Converter={dxrudwi:ReportTypeToImageSourceConverter}}" />
                            <TextBlock HorizontalAlignment="Center" Text="{Binding Converter={dxrudwi:ReportTypeToDisplayNameConverter}}" />
                        </StackPanel>
                    </DataTemplate>
                </dxe:ListBoxEdit.ItemTemplate>
                <dxe:ListBoxEdit.ItemsPanel>
                    <ItemsPanelTemplate>
                        <dxdai:AlignWrapPanel HorizontalContentAlignment="Center" />
                    </ItemsPanelTemplate>
                </dxe:ListBoxEdit.ItemsPanel>
                <dxe:ListBoxEdit.ItemContainerStyle>
                    <Style TargetType="dxe:ListBoxEditItem">
                        <Setter Property="Margin" Value="24,6" />
                        <Setter Property="HorizontalContentAlignment" Value="Center" />
                        <Setter Property="VerticalContentAlignment" Value="Center" />
                        <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                            <Setter.Value>
                                <DataTemplate>
                                    <ContentControl>
                                        <dxdai:DoubleClickCommandExecutor Command="{Binding Path=AssociatedObject.(dxda:DataSourceWizardControlBase.NavigationModel).NextCommand, RelativeSource={RelativeSource Self}}" FinishCommand="{Binding Path=AssociatedObject.(dxda:DataSourceWizardControlBase.NavigationModel).FinishCommand, RelativeSource={RelativeSource Self}}" />
                                    </ContentControl>
                                </DataTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </dxe:ListBoxEdit.ItemContainerStyle>
            </dxe:ListBoxEdit>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ChooseReportTypePage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\MasterDetailConfigurePage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:MasterDetailSelectDataMembersPage}">
        <dxlc:LayoutControl ItemSpace="6" Orientation="Horizontal">
            <dxlc:LayoutItem VerticalAlignment="Stretch" Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_MasterDetailConfigurePage_SelectDataMembers}" LabelPosition="Top">
                <dxg:GridControl ItemsSource="{Binding AvailableQueries}">
                    <dxg:GridControl.View>
                        <dxg:TreeListView
                            AllowEditing="False"
                            AllowIndeterminateCheckState="False"
                            AllowRecursiveNodeChecking="False"
                            AutoWidth="True"
                            CheckBoxFieldName="CheckStateXpf"
                            KeyFieldName="ID"
                            NavigationStyle="Row"
                            ParentFieldName="ParentID"
                            ShowCheckboxes="True"
                            ShowColumnHeaders="False"
                            ShowHorizontalLines="False"
                            ShowIndicator="False"
                            ShowVerticalLines="False"
                            VerticalScrollbarVisibility="Auto">
                            <dxmvvm:Interaction.Behaviors>
                                <dxrudwi:QueriesTreeListSyncStateBehavior QueryChangedCommand="{Binding OnQueryChangedCommand}" />
                            </dxmvvm:Interaction.Behaviors>
                        </dxg:TreeListView>
                    </dxg:GridControl.View>
                    <dxg:GridControl.Columns>
                        <dxg:GridColumn FieldName="Name" AllowEditing="False" />
                    </dxg:GridControl.Columns>
                </dxg:GridControl>
            </dxlc:LayoutItem>
            <dxlc:LayoutItem VerticalAlignment="Stretch" Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_MasterDetailConfigurePage_SelectDataFields}" LabelPosition="Top">
                <dxg:GridControl Name="fieldsGrid" Grid.Column="1" ItemsSource="{Binding AvailableFields}">
                    <dxg:GridControl.View>
                        <dxg:TreeListView
                            AllowEditing="False"
                            AllowRecursiveNodeChecking="True"
                            CheckBoxFieldName="Checked"
                            KeyFieldName="ID"
                            NavigationStyle="Row"
                            ParentFieldName="ParentID"
                            ShowCheckboxes="True"
                            ShowColumnHeaders="False"
                            ShowHorizontalLines="False"
                            ShowIndicator="False"
                            ShowVerticalLines="False"
                            VerticalScrollbarVisibility="Auto">
                            <dxmvvm:Interaction.Behaviors>
                                <dxrudwi:FieldsTreeListStateBehavior FieldChangedCommand="{Binding OnFieldChangedCommand}" />
                            </dxmvvm:Interaction.Behaviors>
                        </dxg:TreeListView>
                    </dxg:GridControl.View>
                    <dxg:GridControl.Columns>
                        <dxg:GridColumn FieldName="DisplayName" />
                        <dxg:GridColumn FieldName="ID" SortIndex="1" Visible="False" />
                        <dxg:GridColumn
                            FieldName="IsList"
                            SortIndex="0"
                            SortOrder="Descending"
                            Visible="False" />
                    </dxg:GridControl.Columns>
                </dxg:GridControl>
            </dxlc:LayoutItem>
        </dxlc:LayoutControl>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\MasterDetailConfigurePage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\MasterDetailAddGroupingLevelPage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:MasterDetailAddGroupingLevelPage}">
        <dxlc:LayoutControl ItemLabelsAlignment="Local" Orientation="Horizontal">
            <dxlc:LayoutGroup Orientation="Horizontal">
                <dxlc:LayoutGroup Orientation="Vertical">
                    <dxlc:LayoutItem VerticalAlignment="Top" Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_MasterDetailAddGroupingLevelPage_SelectReport}" LabelPosition="Top">
                        <dxg:LookUpEdit
                            VerticalAlignment="Top"
                            AutoPopulateColumns="False"
                            DisplayMember="DisplayName"
                            IsTextEditable="False"
                            ItemsSource="{Binding AvailableQueries, Converter={dxrudwi:QueriesTreeToListConverter}}"
                            PopupMinHeight="130"
                            SelectedItem="{Binding SelectedQuery, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                            <dxg:LookUpEdit.PopupContentTemplate>
                                <ControlTemplate>
                                    <dxg:GridControl x:Name="PART_GridControl" AutoGenerateColumns="None" ItemsSource="{Binding AvailableQueries}">
                                        <dxg:GridControl.Columns>
                                            <dxg:GridColumn FieldName="Name" AllowEditing="False" />
                                        </dxg:GridControl.Columns>
                                        <dxg:GridControl.View>
                                            <dxg:TreeListView
                                                AutoExpandAllNodes="True"
                                                AutoWidth="True"
                                                ChildNodesSelector="{dxrudwi:QueriesChildNodeSelector}"
                                                NavigationStyle="Row"
                                                ShowColumnHeaders="False"
                                                ShowHorizontalLines="False"
                                                ShowIndicator="False"
                                                ShowVerticalLines="False"
                                                TreeDerivationMode="ChildNodesSelector"
                                                VerticalScrollbarVisibility="Auto" />
                                        </dxg:GridControl.View>
                                    </dxg:GridControl>
                                </ControlTemplate>
                            </dxg:LookUpEdit.PopupContentTemplate>
                        </dxg:LookUpEdit>
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem
                        Margin="0,6,0,0"
                        VerticalAlignment="Stretch"
                        Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_MasterDetailAddGroupingLevelPage_SelectFields}"
                        LabelPosition="Top">
                        <dxe:ListBoxEdit DisplayMember="DisplayName" ItemsSource="{Binding AvailableColumns}" SelectionMode="Extended">
                            <dxe:ListBoxEdit.EditValue>
                                <Binding Path="SelectedColumns">
                                    <Binding.Converter>
                                        <dxrudwi:ObjectListToTypedListConverter ObjectType="{x:Type dataproviders:ColumnInfo}" />
                                    </Binding.Converter>
                                </Binding>
                            </dxe:ListBoxEdit.EditValue>
                            <dxe:ListBoxEdit.ItemContainerStyle>
                                <Style TargetType="dxe:ListBoxEditItem">
                                    <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                                        <Setter.Value>
                                            <DataTemplate>
                                                <ContentControl>
                                                    <dxdai:DoubleClickCommandExecutor Command="{Binding Path=DataContext.AddLevelCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type dxe:ListBoxEdit}}}" />
                                                </ContentControl>
                                            </DataTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </dxe:ListBoxEdit.ItemContainerStyle>
                        </dxe:ListBoxEdit>
                    </dxlc:LayoutItem>
                </dxlc:LayoutGroup>
                <dxlc:LayoutGroup
                    Margin="0,24,0,0"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    ItemLabelsAlignment="Local"
                    Orientation="Vertical">
                    <dxlc:LayoutItem>
                        <Button Command="{Binding AddLevelCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName=Images/Svg/Wizard/ArrowRight.svg}" Stretch="None" />
                        </Button>
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem>
                        <Button Command="{Binding CombineLevelCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName=Images/Svg/Wizard/ArrowPlus.svg}" Stretch="None" />
                        </Button>
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem>
                        <Button Command="{Binding RemoveLevelCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName=Images/Svg/Wizard/ArrowLeft.svg}" Stretch="None" />
                        </Button>
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem>
                        <Button Margin="0,18,0,0" Command="{Binding MoveUpCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName=Images/Svg/Wizard/ArrowUp.svg}" Stretch="None" />
                        </Button>
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem>
                        <Button Command="{Binding MoveDownCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName=Images/Svg/Wizard/ArrowDown.svg}" Stretch="None" />
                        </Button>
                    </dxlc:LayoutItem>
                </dxlc:LayoutGroup>
            </dxlc:LayoutGroup>
            <dxlc:LayoutItem VerticalAlignment="Stretch">
                <dx:DXBorder
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    Background="White"
                    BorderBrush="LightGray"
                    BorderThickness="1,1,1,1"
                    CornerRadius="3,3,3,3">
                    <Grid>
                        <dx:DXBorder Margin="26,0,26,0" BorderBrush="LightGray" BorderThickness="1,0,1,0" />
                        <dx:DXBorder Margin="0,26,0,26" BorderBrush="LightGray" BorderThickness="0,1,0,1" />
                        <dx:DXBorder Margin="26,26,26,26">
                            <dxe:ListBoxEdit
                                DisplayMember="DisplayName"
                                EditValue="{Binding SelectedGroup}"
                                ItemsSource="{Binding Groups}"
                                ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                SelectedIndex="0"
                                ShowBorder="False">
                                <dxe:ListBoxEdit.ItemContainerStyle>
                                    <Style TargetType="dxe:ListBoxEditItem">
                                        <Setter Property="Margin">
                                            <Setter.Value>
                                                <MultiBinding Converter="{dxrudwi:GroupIndexToHierarchicalMarginConverter}">
                                                    <Binding Path="ItemsSource" RelativeSource="{RelativeSource AncestorType=dxe:ListBoxEdit}" />
                                                    <Binding />
                                                </MultiBinding>
                                            </Setter.Value>
                                        </Setter>
                                        <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                                            <Setter.Value>
                                                <DataTemplate>
                                                    <ContentControl>
                                                        <dxdai:DoubleClickCommandExecutor Command="{Binding Path=DataContext.RemoveLevelCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type dxe:ListBoxEdit}}}" />
                                                    </ContentControl>
                                                </DataTemplate>
                                            </Setter.Value>
                                        </Setter>
                                    </Style>
                                </dxe:ListBoxEdit.ItemContainerStyle>
                                <dxe:ListBoxEdit.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding DisplayName}" TextWrapping="Wrap" />
                                    </DataTemplate>
                                </dxe:ListBoxEdit.ItemTemplate>
                            </dxe:ListBoxEdit>
                        </dx:DXBorder>
                    </Grid>
                </dx:DXBorder>
            </dxlc:LayoutItem>
        </dxlc:LayoutControl>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\MasterDetailAddGroupingLevelPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\MasterDetailSummaryOptionsPage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:MasterDetailChooseSummaryOptionsPage}">
        <dxlc:LayoutControl ItemLabelsAlignment="Local" Orientation="Horizontal">
            <dxlc:LayoutGroup Orientation="Vertical">
                <dxlc:LayoutItem VerticalAlignment="Top" Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_MasterDetailAddGroupingLevelPage_SelectReport}" LabelPosition="Top">
                    <dxg:LookUpEdit
                        VerticalAlignment="Top"
                        AutoPopulateColumns="False"
                        DisplayMember="DisplayName"
                        IsTextEditable="False"
                        ItemsSource="{Binding AvailableQueries, Converter={dxrudwi:QueriesTreeToListConverter}}"
                        PopupMinHeight="130"
                        SelectedItem="{Binding SelectedQuery, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                        <dxg:LookUpEdit.PopupContentTemplate>
                            <ControlTemplate>
                                <dxg:GridControl x:Name="PART_GridControl" AutoGenerateColumns="None" ItemsSource="{Binding AvailableQueries}">
                                    <dxg:GridControl.Columns>
                                        <dxg:GridColumn FieldName="Name" AllowEditing="False" />
                                    </dxg:GridControl.Columns>
                                    <dxg:GridControl.View>
                                        <dxg:TreeListView
                                            AutoExpandAllNodes="True"
                                            AutoWidth="True"
                                            ChildNodesSelector="{dxrudwi:QueriesChildNodeSelector}"
                                            NavigationStyle="Row"
                                            ShowColumnHeaders="False"
                                            ShowHorizontalLines="False"
                                            ShowIndicator="False"
                                            ShowVerticalLines="False"
                                            TreeDerivationMode="ChildNodesSelector"
                                            VerticalScrollbarVisibility="Auto" />
                                    </dxg:GridControl.View>
                                </dxg:GridControl>
                            </ControlTemplate>
                        </dxg:LookUpEdit.PopupContentTemplate>
                    </dxg:LookUpEdit>
                </dxlc:LayoutItem>
                <dxlc:LayoutItem VerticalAlignment="Stretch">
                    <dxg:GridControl ItemsSource="{Binding Summaries}">
                        <dxg:GridControl.View>
                            <dxg:TableView
                                AllowCommitOnValidationAttributeError="False"
                                AutoWidth="True"
                                IsColumnChooserVisible="False"
                                NewItemRowPosition="Bottom"
                                ShowGroupPanel="False"
                                ShowValidationAttributeErrors="True"
                                VerticalScrollbarVisibility="Auto">
                                <dxmvvm:Interaction.Behaviors>
                                    <dxrudwi:SummariesGridBehavior AvailableColumns="{Binding AvailableColumns}" NewSummariesAddedCommand="{Binding OnNewColumnSummariesAddedCommand}" />
                                </dxmvvm:Interaction.Behaviors>
                            </dxg:TableView>
                        </dxg:GridControl.View>
                        <dxg:GridControl.Columns>
                            <dxg:GridColumn
                                Width="*"
                                AllowEditing="True"
                                DisplayMemberBinding="{Binding RowData.Row.Column, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                Header="Column">
                                <dxg:GridColumn.DisplayTemplate>
                                    <ControlTemplate>
                                        <TextBlock Margin="4,2" Text="{Binding Value.DisplayName}" />
                                    </ControlTemplate>
                                </dxg:GridColumn.DisplayTemplate>
                                <dxg:GridColumn.EditTemplate>
                                    <ControlTemplate>
                                        <dxe:ComboBoxEdit
                                            x:Name="PART_Editor"
                                            DisplayMember="DisplayName"
                                            IsTextEditable="False"
                                            ShowBorder="False">
                                            <dxe:ComboBoxEdit.ItemsSource>
                                                <MultiBinding Converter="{dxrudwi:AvailableColumnsEditSettingsConverter}">
                                                    <Binding Path="DataContext.AvailableColumns" RelativeSource="{RelativeSource AncestorType=dxg:GridControl}" />
                                                    <Binding Path="RowData" />
                                                </MultiBinding>
                                            </dxe:ComboBoxEdit.ItemsSource>
                                            <dxe:ComboBoxEdit.EditValue>
                                                <Binding
                                                    Mode="TwoWay"
                                                    Path="EditValue"
                                                    RelativeSource="{RelativeSource TemplatedParent}"
                                                    UpdateSourceTrigger="PropertyChanged" />
                                            </dxe:ComboBoxEdit.EditValue>
                                        </dxe:ComboBoxEdit>
                                    </ControlTemplate>
                                </dxg:GridColumn.EditTemplate>
                            </dxg:GridColumn>
                            <dxg:GridColumn
                                Width="*"
                                AllowEditing="True"
                                DisplayMemberBinding="{Binding RowData.Row.SummaryFunctions, Converter={dxrudwi:ObjectListToTypedListConverter ObjectType=reportsui:SummaryFunc}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                Header="Summary Functions">
                                <dxg:GridColumn.EditSettings>
                                    <dxe:ComboBoxEditSettings
                                        AutoComplete="False"
                                        ImmediatePopup="True"
                                        IncrementalFiltering="True"
                                        ItemsSource="{Binding AvailableSummaryFunctions}">
                                        <dxe:ComboBoxEditSettings.StyleSettings>
                                            <dxe:CheckedTokenComboBoxStyleSettings AllowEditTokens="False" NewTokenPosition="Far" />
                                        </dxe:ComboBoxEditSettings.StyleSettings>
                                    </dxe:ComboBoxEditSettings>
                                </dxg:GridColumn.EditSettings>
                            </dxg:GridColumn>
                            <dxg:GridColumn
                                Name="PART_ServiceColumn"
                                AllowBestFit="True"
                                AllowColumnFiltering="False"
                                AllowGrouping="False"
                                BestFitArea="All"
                                BestFitMode="VisibleRows">
                                <dxg:GridColumn.CellTemplateSelector>
                                    <dxdawn:RemoveRowCellTemplateSelector>
                                        <dxdawn:RemoveRowCellTemplateSelector.ExistingRowCellTemplate>
                                            <DataTemplate>
                                                <dxe:ButtonsControl HorizontalAlignment="Stretch" HorizontalContentAlignment="Center">
                                                    <dxe:ButtonInfo Command="{Binding DataContext.RemoveColumnSummariesCommand, RelativeSource={RelativeSource AncestorType={x:Type dxg:GridControl}}}" CommandParameter="{Binding RowData.Row}" GlyphKind="Cancel" />
                                                </dxe:ButtonsControl>
                                            </DataTemplate>
                                        </dxdawn:RemoveRowCellTemplateSelector.ExistingRowCellTemplate>
                                    </dxdawn:RemoveRowCellTemplateSelector>
                                </dxg:GridColumn.CellTemplateSelector>
                            </dxg:GridColumn>
                        </dxg:GridControl.Columns>
                    </dxg:GridControl>
                </dxlc:LayoutItem>
                <dxlc:LayoutItem HorizontalAlignment="Stretch" VerticalAlignment="Bottom">
                    <dxe:CheckEdit Content="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SummaryOptions_IgnoreNullValues}" EditValue="{Binding IgnoreNullValues, UpdateSourceTrigger=PropertyChanged}" />
                </dxlc:LayoutItem>
            </dxlc:LayoutGroup>
        </dxlc:LayoutControl>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\MasterDetailSummaryOptionsPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\SelectLabelTypePage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:SelectLabelTypePage}">
        <Grid>
            <Grid Grid.Row="1" Margin="24">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="3*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <dxlc:LayoutControl
                    Grid.Column="1"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Center"
                    Orientation="Vertical">
                    <dxlc:LayoutControl.Resources>
                        <Style TargetType="dxlc:LayoutItem">
                            <Setter Property="ElementSpace" Value="12" />
                            <Setter Property="Margin" Value="0,0,0,6" />
                        </Style>
                    </dxlc:LayoutControl.Resources>
                    <dxlc:LayoutGroup Orientation="Vertical">
                        <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SelectLabelType_LabelProduct}" LabelPosition="Left">
                            <dxe:ComboBoxEdit
                                DisplayMember="Name"
                                EditValue="{Binding SelectedProduct, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                IsTextEditable="False"
                                ItemsSource="{Binding AvailableProducts}" />
                        </dxlc:LayoutItem>
                        <dxlc:LayoutItem Margin="0,0,0,24" Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SelectLabelType_ProductNumber}" LabelPosition="Left">
                            <dxe:ComboBoxEdit
                                DisplayMember="Name"
                                EditValue="{Binding SelectedDetails, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                IsTextEditable="False"
                                ItemsSource="{Binding AvailableLabelDetails}" />
                        </dxlc:LayoutItem>
                        <dxlc:LayoutGroup VerticalAlignment="Stretch" Orientation="Vertical">
                            <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SelectLabelType_Width}" LabelPosition="Left">
                                <TextBlock Text="{Binding SelectedDetails.Width}" />
                            </dxlc:LayoutItem>
                            <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SelectLabelType_Height}" LabelPosition="Left">
                                <TextBlock Text="{Binding SelectedDetails.Height}" />
                            </dxlc:LayoutItem>
                            <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SelectLabelType_PaperType}" LabelPosition="Left">
                                <TextBlock>
                                    <TextBlock.Text>
                                        <MultiBinding Converter="{dxrudwi:PaperKindToDisplayNameConverter}">
                                            <Binding Path="CurrentPaperKindData.Name" />
                                            <Binding Path="CurrentPaperKindData.IsRollPaper" />
                                        </MultiBinding>
                                    </TextBlock.Text>
                                </TextBlock>
                            </dxlc:LayoutItem>
                            <dxlc:LayoutItem Label="Paper Size:" LabelPosition="Left" LabelVerticalAlignment="Top">
                                <StackPanel>
                                    <TextBlock>
                                        <!--<TextBlock.Text>
