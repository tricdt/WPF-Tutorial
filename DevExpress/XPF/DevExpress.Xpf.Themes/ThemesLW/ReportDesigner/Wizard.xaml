                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName=Images/Svg/Wizard/ArrowLeft.svg}" Stretch="None" />
                        </Button>
                        <Button Command="{Binding RemoveAllCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName=Images/Svg/Wizard/ArrowDoubleLeft.svg}" Stretch="None" />
                        </Button>
                    </StackPanel>
                    <dxlc:LayoutItem
                        Grid.RowSpan="2"
                        Grid.Column="2"
                        VerticalAlignment="Stretch"
                        Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SelectColumns_SelectedColumns}"
                        LabelPosition="Top">
                        <dxe:ListBoxEdit
                            Name="selectedColumns"
                            DisplayMember="DisplayName"
                            EditValue="{Binding ActiveSelectedColumns, Converter={dxrudwi:ObjectListToTypedListConverter ObjectType=dataproviders:ColumnInfo}}"
                            ItemsSource="{Binding SelectedColumns}"
                            SelectedIndex="0"
                            SelectionMode="Multiple" />
                    </dxlc:LayoutItem>
                </Grid>
            </dxlc:LayoutControl>
        </Grid>

    </DataTemplate>

    <DataTemplate x:Key="{x:Type dxrudwp:MultiQuerySelectFieldsPage}">
        <Grid>
            <dxlc:LayoutControl Grid.Row="1" ItemSpace="24" Orientation="Horizontal">
                <dxlc:LayoutItem VerticalAlignment="Stretch" Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_MultiQuerySelectFieldsPage_SelectDataMember}" LabelPosition="Top">
                    <dxe:ListBoxEdit
                        DisplayMember="DisplayName"
                        EditValue="{Binding SelectedTable, Mode=TwoWay}"
                        ItemsSource="{Binding AvailableTables}"
                        ValueMember="Name" />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem VerticalAlignment="Stretch" Label="{dxrudl:ReportDesignerStringId StringId=ReportWizard_MultiQuerySelectFieldsPage_SelectDataFields}" LabelPosition="Top">
                    <dxe:ListBoxEdit
                        DisplayMember="DisplayName"
                        EditValue="{Binding SelectedColumns, Converter={dxrudwi:ObjectListToTypedListConverter ObjectType=sys:String}, Mode=TwoWay}"
                        ItemsSource="{Binding AvailableColumns}"
                        StyleSettings="{dxe:CheckedListBoxStyleSettings}"
                        ValueMember="Name" />
                </dxlc:LayoutItem>
            </dxlc:LayoutControl>
