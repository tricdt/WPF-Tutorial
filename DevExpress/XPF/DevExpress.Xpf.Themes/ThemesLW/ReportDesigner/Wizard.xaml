                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName=Images/Svg/Wizard/ArrowRight.svg}" Stretch="None" />
                        </Button>
                        <Button Margin="0,0,0,18" Command="{Binding AddAllCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName=Images/Svg/Wizard/ArrowDoubleRight.svg}" Stretch="None" />
