    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\AddGroupingLevelPage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:AddGroupingLevelPage}">
        <Grid>
            <dxlc:LayoutControl Grid.Row="1" Padding="12" Orientation="Horizontal">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="3*" />
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="5*" />
                    </Grid.ColumnDefinitions>
                    <TextBlock Text="{dxrudl:ReportDesignerStringId StringId=ReportWizard_AddGroupingLevel_AvailableColumns}" />
                    <dxe:ListBoxEdit
                        Name="availableColumns"
                        Grid.Row="1"
                        DisplayMember="DisplayName"
                        ItemsSource="{Binding AvailableColumns}"
                        SelectedIndex="0"
                        SelectionMode="Extended">
                        <dxe:ListBoxEdit.EditValue>
                            <Binding Path="SelectedColumns">
                                <Binding.Converter>
                                    <dxrudwi:ObjectListToTypedListConverter ObjectType="{x:Type dataproviders:ColumnInfo}" />
                                </Binding.Converter>
                            </Binding>
                        </dxe:ListBoxEdit.EditValue>
                        <dxe:ListBoxEdit.ItemContainerStyle>
                            <Style TargetType="dxe:ListBoxEditItem">
                                <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                                    <Setter.Value>
                                        <DataTemplate>
                                            <ContentControl>
                                                <dxdai:DoubleClickCommandExecutor Command="{Binding Path=DataContext.AddLevelCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type dxe:ListBoxEdit}}}" />
                                            </ContentControl>
                                        </DataTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </dxe:ListBoxEdit.ItemContainerStyle>
                    </dxe:ListBoxEdit>

                    <StackPanel
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="12,0,12,60"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
                        <Button Margin="0,0,0,6" Command="{Binding AddLevelCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName='Images/Svg/Wizard/ArrowRight.svg'}" Stretch="None" />
                        </Button>
                        <Button Margin="0,0,0,6" Command="{Binding CombineLevelCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName='Images/Svg/Wizard/ArrowPlus.svg'}" Stretch="None" />
                        </Button>
                        <Button Margin="0,0,0,18" Command="{Binding RemoveLevelCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName='Images/Svg/Wizard/ArrowLeft.svg'}" Stretch="None" />
                        </Button>
                        <Button Margin="0,0,0,6" Command="{Binding MoveUpCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName='Images/Svg/Wizard/ArrowUp.svg'}" Stretch="None" />
                        </Button>
                        <Button Command="{Binding MoveDownCommand}">
                            <dx:DXImage Source="{dxrud:ReportDesignerResourceImage ResourceName='Images/Svg/Wizard/ArrowDown.svg'}" Stretch="None" />
                        </Button>
                    </StackPanel>
                    <Grid Grid.RowSpan="2" Grid.Column="2">
                        <dx:DXBorder
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Background="White"
                            BorderBrush="LightGray"
                            BorderThickness="1,1,1,1"
                            CornerRadius="3,3,3,3">
                            <Grid>
                                <dx:DXBorder Margin="26,0,26,0" BorderBrush="LightGray" BorderThickness="1,0,1,0" />
                                <dx:DXBorder Margin="0,26,0,26" BorderBrush="LightGray" BorderThickness="0,1,0,1" />
                                <dx:DXBorder Margin="26,26,26,26">
                                    <dxe:ListBoxEdit
                                        DisplayMember="DisplayName"
                                        EditValue="{Binding SelectedGroup}"
                                        ItemsSource="{Binding Groups}"
                                        ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                        SelectedIndex="0"
                                        ShowBorder="False">
                                        <dxe:ListBoxEdit.ItemContainerStyle>
                                            <Style TargetType="dxe:ListBoxEditItem">
                                                <Setter Property="Margin">
                                                    <Setter.Value>
                                                        <MultiBinding Converter="{dxrudwi:GroupIndexToHierarchicalMarginConverter}">
                                                            <Binding Path="ItemsSource" RelativeSource="{RelativeSource AncestorType=dxe:ListBoxEdit}" />
                                                            <Binding />
                                                        </MultiBinding>
                                                    </Setter.Value>
                                                </Setter>
                                                <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                                                    <Setter.Value>
                                                        <DataTemplate>
                                                            <ContentControl>
                                                                <dxdai:DoubleClickCommandExecutor Command="{Binding Path=DataContext.RemoveLevelCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type dxe:ListBoxEdit}}}" />
                                                            </ContentControl>
                                                        </DataTemplate>
                                                    </Setter.Value>
                                                </Setter>
                                            </Style>
                                        </dxe:ListBoxEdit.ItemContainerStyle>
                                        <dxe:ListBoxEdit.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding DisplayName}" TextWrapping="Wrap" />
                                            </DataTemplate>
                                        </dxe:ListBoxEdit.ItemTemplate>
                                    </dxe:ListBoxEdit>
                                </dx:DXBorder>
                            </Grid>
                        </dx:DXBorder>
                    </Grid>
                </Grid>
            </dxlc:LayoutControl>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\AddGroupingLevelPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ChooseSummaryOptionsPage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:ChooseSummaryOptionsPage}">
        <Grid>
            <dxlc:LayoutControl
                Grid.Row="1"
                Margin="24"
                Padding="0"
                Orientation="Vertical">
                <dxlc:LayoutItem HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                    <Grid Grid.IsSharedSizeScope="True">
                        <Grid.RowDefinitions>
                            <RowDefinition Name="headerRow" Height="auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Name="displayNameColumn" Width="auto" SharedSizeGroup="sharedGroup" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <TextBlock
                            Grid.Column="1"
                            Margin="0,0,0,6"
                            Text="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SummaryOptions_Sum}"
                            TextAlignment="Center" />
                        <TextBlock
                            Grid.Column="2"
                            Margin="0,0,0,6"
                            Text="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SummaryOptions_Avg}"
                            TextAlignment="Center" />
                        <TextBlock
                            Grid.Column="3"
                            Margin="0,0,0,6"
                            Text="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SummaryOptions_Min}"
                            TextAlignment="Center" />
                        <TextBlock
                            Grid.Column="4"
                            Margin="0,0,0,6"
                            Text="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SummaryOptions_Max}"
                            TextAlignment="Center" />
                        <TextBlock
                            Grid.Column="5"
                            Margin="0,0,0,6"
                            Text="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SummaryOptions_Count}"
                            TextAlignment="Center" />
                        <ScrollViewer
                            Name="scrollViewer"
                            Grid.Row="1"
                            Grid.ColumnSpan="6"
                            HorizontalScrollBarVisibility="Disabled"
                            VerticalScrollBarVisibility="Hidden">
                            <ItemsControl HorizontalAlignment="Stretch" ItemsSource="{Binding SummaryOptions}">
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <Grid Margin="0,3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" SharedSizeGroup="sharedGroup" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Margin="0,0,24,0" Text="{Binding ColumnInfo.DisplayName}" />
                                            <dxe:CheckEdit Grid.Column="1" HorizontalAlignment="Center" EditValue="{Binding Options.Sum}" />
                                            <dxe:CheckEdit Grid.Column="2" HorizontalAlignment="Center" EditValue="{Binding Options.Avg}" />
                                            <dxe:CheckEdit Grid.Column="3" HorizontalAlignment="Center" EditValue="{Binding Options.Min}" />
                                            <dxe:CheckEdit Grid.Column="4" HorizontalAlignment="Center" EditValue="{Binding Options.Max}" />
                                            <dxe:CheckEdit Grid.Column="5" HorizontalAlignment="Center" EditValue="{Binding Options.Count}" />
                                        </Grid>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                        </ScrollViewer>
                        <dx:DXBorder
                            BorderBrush="{DynamicResource {dxt:LWKey Brushes.PanelBorder}}"
                            Grid.RowSpan="2"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            BorderThickness="0,0,1,0" />
                        <dx:DXBorder
                            BorderBrush="{DynamicResource {dxt:LWKey Brushes.PanelBorder}}"
                            Grid.ColumnSpan="6"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            BorderThickness="0,0,0,1" />
                    </Grid>
                </dxlc:LayoutItem>
                <dxlc:LayoutItem HorizontalAlignment="Stretch" VerticalAlignment="Bottom">
                    <dxe:CheckEdit Content="{dxrudl:ReportDesignerStringId StringId=ReportWizard_SummaryOptions_IgnoreNullValues}" EditValue="{Binding IgnoreNullValues, UpdateSourceTrigger=PropertyChanged}" />
                </dxlc:LayoutItem>
            </dxlc:LayoutControl>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ChooseSummaryOptionsPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ChooseReportLayoutPage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:ChooseReportLayoutPage}">
        <Grid Grid.Row="1" Margin="24">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <dxe:ListBoxEdit
                Margin="4,0"
                EditValue="{Binding Portrait, UpdateSourceTrigger=PropertyChanged}"
                ItemsSource="{dxdai:BooleanItemsSource}"
                ShowBorder="False">
                <dxe:ListBoxEdit.ItemTemplate>
                    <DataTemplate>
                        <DataTemplate.Resources>
                            <dxmvvm:BooleanToObjectConverter x:Key="boolToTextConverter" FalseValue="{dxrudl:ReportDesignerStringId StringId=PageOrientation_Landscape}" TrueValue="{dxrudl:ReportDesignerStringId StringId=PageOrientation_Portrait}" />
                        </DataTemplate.Resources>
                        <TextBlock Margin="0,0,36,0" Text="{Binding Converter={StaticResource boolToTextConverter}}" />
                    </DataTemplate>
                </dxe:ListBoxEdit.ItemTemplate>
                <dxe:ListBoxEdit.ItemsPanel>
                    <ItemsPanelTemplate>
                        <UniformGrid Columns="2" />
                    </ItemsPanelTemplate>
                </dxe:ListBoxEdit.ItemsPanel>
                <dxe:ListBoxEdit.StyleSettings>
                    <dxe:RadioListBoxEditStyleSettings />
                </dxe:ListBoxEdit.StyleSettings>
            </dxe:ListBoxEdit>
            <dxe:ListBoxEdit
                Grid.Row="1"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                EditValue="{Binding SelectedReportLayout}"
                ItemsSource="{Binding ReportLayouts}"
                ShowBorder="False">
                <dxe:ListBoxEdit.ItemContainerStyle>
                    <Style TargetType="dxe:ListBoxEditItem">
                        <Setter Property="Margin" Value="4" />
                    </Style>
                </dxe:ListBoxEdit.ItemContainerStyle>
                <dxe:ListBoxEdit.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Margin="3,6">
                            <dx:DXImage Source="{Binding Converter={dxrudwi:ReportLayoutToImageSourceConverter}}" />
                            <TextBlock HorizontalAlignment="Center" Text="{Binding Converter={dxrudwi:ReportLayoutToTextConverter}}" />
                        </StackPanel>
                    </DataTemplate>
                </dxe:ListBoxEdit.ItemTemplate>
                <dxe:ListBoxEdit.ItemsPanel>
                    <ItemsPanelTemplate>
                        <StackPanel Orientation="Horizontal" />
                    </ItemsPanelTemplate>
                </dxe:ListBoxEdit.ItemsPanel>
            </dxe:ListBoxEdit>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ChooseReportLayoutPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ChooseReportStylePage.xaml-->
    <DataTemplate x:Key="{x:Type dxrudwp:ChooseReportStylePage}">
        <Grid Grid.Row="1" Margin="24">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="2*" />
                <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Grid>
                <Grid.Resources>
                    <Style TargetType="Border">
                        <Setter Property="BorderBrush">
                            <Setter.Value>
                                <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                    <GradientStop Offset="0.6" Color="LightGray" />
                                    <GradientStop Offset="0.9" Color="Transparent" />
                                </LinearGradientBrush>
