                Grid.Column="1"
                Width="4"
                HorizontalAlignment="Center"
                VerticalAlignment="Stretch"
                Background="Transparent" />

            <dxg:GridControl Grid.Column="2" ItemsSource="{Binding TemplateFields}" ShowBorder="True">
                <dxg:GridControl.Columns>
                    <dxg:GridColumn FieldName="DisplayName" Header="Template Field Name">
                        <dxg:GridColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel Orientation="Horizontal">
                                    <dx:DXImage Margin="0,0,5,0" VerticalAlignment="Center" Source="{Binding RowData.Row, Converter={dxrudi:TemplateFieldToImageSourceConverter}}" />
                                    <TextBlock VerticalAlignment="Center" Text="{Binding Data.DisplayName}" />
                                </StackPanel>
                            </DataTemplate>
                        </dxg:GridColumn.CellTemplate>
                    </dxg:GridColumn>

                    <dxg:GridColumn FieldName="Value" Header="Template Field Value">
                        <dxg:GridColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel>
                                    <dxe:PopupBaseEdit x:Name="PART_Editor" ShowBorder="False">
                                        <dxe:PopupBaseEdit.Mask>
                                            <MultiBinding Converter="{dxrudwi:IsBindingValueToMaskConverter}">
                                                <Binding RelativeSource="{RelativeSource Self}" />
                                                <Binding Path="RowData.Row.IsBindingValue" />
                                            </MultiBinding>
                                        </dxe:PopupBaseEdit.Mask>
                                        <dxe:PopupBaseEdit.PopupContentTemplate>
                                            <ControlTemplate>
                                                <dxg:GridControl
                                                    MinHeight="100"
                                                    MaxHeight="300"
                                                    ItemsSource="{Binding Column.DataContext.Nodes, Converter={dxrudwi:FieldListNodesWithNoneConverter}}"
                                                    SelectionMode="Cell"
                                                    ShowBorder="False">
                                                    <dxg:GridControl.Resources>
                                                        <DataTemplate x:Key="{dxrudt:LWKey ReportDesigner.FieldListNodeTemplate}">
                                                            <StackPanel Orientation="Horizontal">
                                                                <dx:DXImage Margin="0,0,5,0" VerticalAlignment="Center" Source="{Binding RowData.Row, Converter={dxrudi:TypeSpecificsToImageSourceConverter}}" />
                                                                <TextBlock VerticalAlignment="Center" Text="{Binding Data.DisplayName, Converter={dxrudi:FieldListNodeDisplayNameToDisplayTextConverter}}" />
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </dxg:GridControl.Resources>
                                                    <dxg:GridControl.View>
                                                        <dxg:TreeListView
                                                            AllowEditing="False"
                                                            AutoWidth="True"
                                                            ChildNodesPath="Children"
                                                            ExpandCollapseNodesOnNavigation="True"
                                                            HorizontalScrollbarVisibility="Auto"
                                                            ShowColumnHeaders="False"
                                                            ShowHorizontalLines="False"
                                                            ShowIndicator="False"
                                                            ShowVerticalLines="False"
                                                            TreeDerivationMode="ChildNodesSelector"
                                                            VerticalScrollbarVisibility="Auto">
                                                            <dxmvvm:Interaction.Behaviors>
                                                                <dxdai:ExpandTreeListNodeBehavior />
                                                            </dxmvvm:Interaction.Behaviors>
                                                        </dxg:TreeListView>
                                                    </dxg:GridControl.View>
                                                    <dxg:GridControl.Columns>
                                                        <dxg:GridColumn FieldName="DisplayName" CellTemplate="{DynamicResource {dxrudt:LWKey ReportDesigner.FieldListNodeTemplate}}" />
                                                    </dxg:GridControl.Columns>
                                                    <dxmvvm:Interaction.Behaviors>
                                                        <dxrudwi:CloseEditorOnTemplateFieldSelectedBehavior Popup="{Binding Path=AssociatedObject.(dxe:BaseEdit.OwnerEdit), RelativeSource={RelativeSource Self}, UpdateSourceTrigger=PropertyChanged}" />
                                                    </dxmvvm:Interaction.Behaviors>
                                                </dxg:GridControl>
                                            </ControlTemplate>
                                        </dxe:PopupBaseEdit.PopupContentTemplate>

                                        <dxmvvm:Interaction.Behaviors>
                                            <dxrudwi:TextInputBehavior />
                                        </dxmvvm:Interaction.Behaviors>
                                    </dxe:PopupBaseEdit>
                                </StackPanel>
                            </DataTemplate>
                        </dxg:GridColumn.CellTemplate>
                    </dxg:GridColumn>
                    <dxg:GridColumn FieldName="Category" GroupIndex="0" />
                </dxg:GridControl.Columns>

                <dxg:GridControl.View>
                    <dxg:TableView
                        AllowColumnFiltering="False"
                        AllowColumnMoving="False"
                        AllowDragDrop="True"
                        AllowEditing="True"
                        AllowGrouping="False"
                        AllowSortedDataDragDrop="True"
                        AllowSorting="False"
                        AutoWidth="True"
                        EditorShowMode="MouseDown"
                        IsColumnMenuEnabled="False"
                        NavigationStyle="Cell"
                        ShowGroupPanel="False"
                        ShowIndicator="False"
                        ShowSearchPanelMode="Never"
                        VerticalScrollbarVisibility="Auto">
                        <dxmvvm:Interaction.Behaviors>
                            <dxrudwi:TemplateFieldsDragDropBehavior />
                        </dxmvvm:Interaction.Behaviors>
                    </dxg:TableView>
                </dxg:GridControl.View>
                <dxmvvm:Interaction.Behaviors>
                    <dxrudwi:FormatBindingValueBehavior />
                </dxmvvm:Interaction.Behaviors>
            </dxg:GridControl>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\MapReportTemplateFieldsPage.xaml-->

    <!--#region c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ConfigureReportTemplateQueryPage.xaml-->
    <DataTemplate x:Key="queriesTemplate">
        <dxe:ButtonEdit x:Name="PART_Editor" AllowDefaultButton="False" IsTextEditable="True">
            <dxe:ButtonEdit.Buttons>
                <dxe:ButtonInfo Command="{Binding DataContext.AddQueryCommand, RelativeSource={RelativeSource AncestorType=dxg:GridControl}}" GlyphKind="Plus" />
            </dxe:ButtonEdit.Buttons>
        </dxe:ButtonEdit>
    </DataTemplate>
    <DataTemplate x:Key="queryTemplate">
        <dxe:ButtonEdit x:Name="PART_Editor" AllowDefaultButton="False">
            <dxe:ButtonEdit.Buttons>
                <dxe:ButtonInfo Command="{Binding DataContext.ConfigureQueryCommand, RelativeSource={RelativeSource AncestorType=dxg:GridControl}}" CommandParameter="{Binding RowData.Row}" GlyphKind="Edit" />
                <dxe:ButtonInfo Command="{Binding DataContext.DeleteQueryCommand, RelativeSource={RelativeSource AncestorType=dxg:GridControl}}" CommandParameter="{Binding RowData.Row}" GlyphKind="Minus" />
            </dxe:ButtonEdit.Buttons>
        </dxe:ButtonEdit>
    </DataTemplate>


    <DataTemplate x:Key="{x:Type dxrudwp:ConfigureReportTemplateQueryPage}">
        <Grid Margin="12">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="2*" />
                <ColumnDefinition Width="4" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <TextBlock Margin="0,0,0,4" Text="Data source structure:" />
            <TextBlock Grid.Column="2" Margin="0,0,0,4" Text="Tempalte fields:" />
            <dxg:GridControl
                x:Name="grid"
                Grid.Row="1"
                ItemsSource="{Binding Items}"
                SelectedItem="{Binding SelectedItem}"
                ShowBorder="True">
                <dxg:GridControl.Resources>
                    <dx:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
                    <Style TargetType="{x:Type dxg:RowMarginControl}">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate>
                                    <Grid>
                                        <Path x:Name="PART_OffsetPath" Stroke="{TemplateBinding BorderBrush}" StrokeThickness="1" />
                                        <Path x:Name="PART_TreeLinePath" Stroke="{TemplateBinding BorderBrush}" StrokeThickness="1" />
                                        <dxg:IndentsPanel HorizontalAlignment="Right" RowIndent="{Binding View.RowIndent}">
                                            <dxg:TreeListNodeExpandButton
                                                x:Name="PART_ExpandButton"
                                                HorizontalAlignment="Left"
                                                Command="{Binding View.Commands.ChangeNodeExpanded}"
                                                CommandParameter="{Binding RowHandle.Value}"
                                                IsChecked="{Binding IsExpanded}"
                                                IsExpandButtonVisible="{Binding Path=IsButtonVisible}" />
                                            <dxe:CheckEdit
                                                x:Name="PART_NodeCheckBox"
                                                Focusable="False"
                                                IsChecked="{Binding IsChecked, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                IsEnabled="{DXBinding 'Level == 0 ? `False` : `True`'}"
                                                IsThreeState="{Binding View.AllowIndeterminateCheckState}">
                                                <dxg:GridViewHitInfoBase.HitTestAcceptor>
                                                    <dxg:TreeListNodeCheckboxHitTestAcceptor />
                                                </dxg:GridViewHitInfoBase.HitTestAcceptor>
                                            </dxe:CheckEdit>
                                            <dx:MeasurePixelSnapper Visibility="{Binding IsImageVisible, Converter={StaticResource BooleanToVisibilityConverter}}">
                                                <dx:DXImage
                                                    x:Name="PART_NodeImage"
                                                    Width="{Binding Path=View.NodeImageSize.Width}"
                                                    Height="{Binding Path=View.NodeImageSize.Height}"
                                                    Focusable="False"
                                                    Source="{Binding Image, UpdateSourceTrigger=PropertyChanged}">
                                                    <dxg:GridViewHitInfoBase.HitTestAcceptor>
                                                        <dxg:TreeListNodeImageHitTestAcceptor />
                                                    </dxg:GridViewHitInfoBase.HitTestAcceptor>
                                                    <RenderOptions.BitmapScalingMode>NearestNeighbor</RenderOptions.BitmapScalingMode>
                                                </dx:DXImage>
                                            </dx:MeasurePixelSnapper>
                                        </dxg:IndentsPanel>
                                    </Grid>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </dxg:GridControl.Resources>

                <dxg:GridControl.View>
                    <dxg:TreeListView
                        AllowEditing="False"
                        AllowRecursiveNodeChecking="True"
                        AutoWidth="True"
                        CheckBoxFieldName="Selected"
                        EditorButtonShowMode="ShowAlways"
                        KeyFieldName="Id"
                        NavigationStyle="Row"
                        ParentFieldName="ParentId"
                        ShowCheckboxes="True"
                        ShowColumnHeaders="False"
                        ShowHorizontalLines="False"
                        ShowIndicator="False"
                        ShowVerticalLines="False"
                        VerticalScrollbarVisibility="Auto">
                        <dxmvvm:Interaction.Behaviors>
                            <dxmvvm:EventToCommand
                                Command="{Binding OnNodeExpandingCommand}"
                                EventArgsConverter="{dxdawn:TreeListNodeEventArgsToMultiQueryItemConverter}"
                                EventName="NodeExpanding"
                                PassEventArgsToCommand="True" />
                            <dxmvvm:EventToCommand
                                Command="{Binding OnNodeCheckedChangedCommand}"
                                EventArgsConverter="{dxdawn:TreeListNodeEventArgsToMultiQueryItemConverter}"
                                EventName="NodeCheckStateChanged"
                                PassEventArgsToCommand="True" />
                        </dxmvvm:Interaction.Behaviors>
                    </dxg:TreeListView>
                </dxg:GridControl.View>
                <dxg:GridControl.Columns>
                    <dxg:GridColumn FieldName="DisplayName" AllowEditing="False" ReadOnly="True">
                        <dxg:GridColumn.CellTemplateSelector>
                            <dxdawn:MultiQueryColumnTemplateSelector QueriesCollectionTemplate="{StaticResource queriesTemplate}" QueryTemplate="{StaticResource queryTemplate}" />
                        </dxg:GridColumn.CellTemplateSelector>
                    </dxg:GridColumn>
                </dxg:GridControl.Columns>
            </dxg:GridControl>

            <GridSplitter
                Grid.RowSpan="2"
                Grid.Column="1"
                Width="4"
                HorizontalAlignment="Center"
                VerticalAlignment="Stretch"
                Background="Transparent" />

            <dxe:ListBoxEdit
                Grid.Row="1"
                Grid.Column="2"
                IsReadOnly="True"
                ItemsSource="{Binding RequiredTemplateFields}">
                <dxe:ListBoxEdit.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <dx:DXImage Margin="0,0,5,0" VerticalAlignment="Center" Source="{Binding Type, Converter={dxrudwi:TypeToImageSourceConverter}}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding DisplayName}" />
                        </StackPanel>
                    </DataTemplate>
                </dxe:ListBoxEdit.ItemTemplate>
            </dxe:ListBoxEdit>
        </Grid>
    </DataTemplate>
    <!--#endregion c:\work\2023.1\XPF\DevExpress.Xpf.ReportDesigner\DevExpress.Xpf.ReportDesigner\Themes\Generic\Wizard\Pages\ConfigureReportTemplateQueryPage.xaml-->

