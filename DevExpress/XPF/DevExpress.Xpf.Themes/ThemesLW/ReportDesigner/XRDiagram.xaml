    <Style x:Key="ExpressionEditorButtonStyle" x:Shared="False" TargetType="{x:Type dxe:ButtonInfo}">
        <Setter Property="GlyphKind" Value="Regular" />
        <Setter Property="Command" Value="{Binding Path=ReportModel.Diagram.(dxrud:ReportDesigner.Designer).ActualCommands.RunExpressionCollectionEditorCommand}" />
        <Setter Property="CommandParameter">
            <Setter.Value>
                <MultiBinding Converter="{dxrudei:ExpressionDataConverter}">
                    <Binding Path="(smarttagsnative:ExpressionDataExtensions.ExpressionPath)" RelativeSource="{RelativeSource Self}" />
                    <Binding Source="{x:Static reportsui:XRControl+EventNames.BeforePrint}" />
                    <Binding Path="(smarttagsnative:ExpressionDataExtensions.ExpressionResultType)" RelativeSource="{RelativeSource Self}" />
                </MultiBinding>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxrudt:LWKey SmartTagEditors.ImageEditor}" TargetType="{x:Type dxe:ImageEdit}" />

    <Style x:Key="{dxrudt:LWKey SmartTagEditors.TextEditStyle}" x:Shared="False" TargetType="{x:Type dxe:TextEdit}">
        <Setter Property="TextWrapping" Value="Wrap" />
        <Setter Property="VerticalContentAlignment" Value="Top" />
        <Setter Property="AcceptsReturn" Value="True" />
        <Setter Property="ValidateOnEnterKeyPressed" Value="True" />
        <Setter Property="ValidateOnTextInput" Value="True" />
        <Setter Property="EditValuePostMode" Value="Delayed" />
        <Setter Property="EditValuePostDelay" Value="750" />
    </Style>

