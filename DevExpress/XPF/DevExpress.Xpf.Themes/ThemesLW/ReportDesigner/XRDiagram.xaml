    <Style x:Key="{dxrudt:LWKey SmartTagEditors.SummaryEditorStyle}" x:Shared="False" TargetType="{x:Type dxe:PopupBaseEdit}">
        <Setter Property="DisplayTextConverter" Value="{dxrudi:SummaryToDisplayTextConverter}" />
        <Setter Property="IsTextEditable" Value="False" />
        <Setter Property="IsReadOnly" Value="True" />
        <Setter Property="PopupContentTemplate">
            <Setter.Value>
                <ControlTemplate>
                    <dxe:ListBoxEdit
                        AllowItemHighlighting="True"
                        EditValue="{Binding SummaryRunning}"
                        ItemsSource="{dxe:EnumItemsSource EnumType={x:Type reportsui:SummaryRunning}, NameConverter={dxrudi:EnumToDisplayNameConverter}}"
                        ShowBorder="False"
                        ValueMember="Id">
                        <dxmvvm:Interaction.Behaviors>
                            <dxrudi:ClosePopupOnSummaryRunningSelected Popup="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxe:PopupBaseEdit}}}" />
                        </dxmvvm:Interaction.Behaviors>

                    </dxe:ListBoxEdit>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Buttons">
            <Setter.Value>
                <dxe:ButtonInfoCollection>
                    <dxe:ButtonInfo GlyphKind="Regular">
                        <dxmvvm:Interaction.Behaviors>
                            <dxe:DialogServiceEditorBehavior Commands="{dxe:UICommandContainerGenerator Buttons=OKCancel}" Content="{Binding ReportModel}">
                                <dxe:DialogServiceEditorBehavior.DialogServiceTemplate>
                                    <Binding Path="AssociatedObject.DataContext.ReportModel.DataBindingMode" RelativeSource="{RelativeSource Self}">
                                        <Binding.Converter>
                                            <dxmvvm:ObjectToObjectConverter DefaultTarget="{StaticResource summaryExpressionModeDialogTemplate}">
                                                <dxmvvm:MapItem Source="{x:Static reportsui:DataBindingMode.Bindings}" Target="{StaticResource summaryBindingModeDialogTemplate}" />
                                            </dxmvvm:ObjectToObjectConverter>
                                        </Binding.Converter>
                                    </Binding>
                                </dxe:DialogServiceEditorBehavior.DialogServiceTemplate>
                            </dxe:DialogServiceEditorBehavior>
                        </dxmvvm:Interaction.Behaviors>
                    </dxe:ButtonInfo>
                </dxe:ButtonInfoCollection>
            </Setter.Value>
        </Setter>
    </Style>

