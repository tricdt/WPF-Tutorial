                    </dxlc:LayoutItem>

                    <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=Properties_SummaryRunning}" Visibility="{Binding Converter={smarttagsnative:SmartTagModelToPropertyVisibilityConverter}, ConverterParameter=Summary.Running}">
                        <dxe:PopupBaseEdit EditValue="{Binding ReportModel.Diagram.SelectionModel.Summary, ValidatesOnExceptions=True, Mode=OneWay}" Style="{DynamicResource {dxrudt:LWKey SmartTagEditors.SummaryEditorStyle}}" />
                    </dxlc:LayoutItem>

