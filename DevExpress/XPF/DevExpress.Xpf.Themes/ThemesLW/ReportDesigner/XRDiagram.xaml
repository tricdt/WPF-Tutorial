                                    <DataTemplate>
                                        <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=Properties_Angle}" Visibility="{Binding Converter={smarttagsnative:SmartTagModelToPropertyVisibilityConverter}, ConverterParameter=Angle}">
                                            <dxe:SpinEdit EditValue="{Binding Angle, UpdateSourceTrigger=PropertyChanged}" Style="{DynamicResource {dxrudt:LWKey SmartTagEditors.AngleEditorStyle}}" />
