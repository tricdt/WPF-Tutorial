            <dxlc:LayoutControl.Resources>
                <Style BasedOn="{StaticResource layoutItemStyle}" TargetType="{x:Type dxlc:LayoutItem}" />
            </dxlc:LayoutControl.Resources>

            <dxlc:LayoutGroup>
                <dxlc:LayoutGroup Orientation="Vertical">
                    <dxlc:LayoutGroup.Resources>
                        <DataTemplate x:Key="binding">
                            <dxlc:LayoutItem x:Name="DataBindings" Label="{dxrudl:ReportDesignerStringId StringId=Properties_DataBinding}" Visibility="{Binding Converter={smarttagsnative:SmartTagModelToPropertyVisibilityConverter IsBindingProperty=True}, ConverterParameter=Text}">
                                <dxrude:PopupTreeListEdit EditValue="{Binding TextBinding, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Style="{DynamicResource {dxrudt:LWKey SmartTagEditors.DataBindingEditorStyle}}">
                                    <dxrude:PopupTreeListEdit.HierarchicalPathProvider>
                                        <MultiBinding Converter="{dxrudi:BindingModeToHierarchicalPathProviderConverter}">
                                            <Binding Path="ReportModel.DataBindingMode" />
                                            <Binding Path="ItemsSource" RelativeSource="{RelativeSource Self}" />
                                            <Binding Path="ReportModel" />
                                        </MultiBinding>
                                    </dxrude:PopupTreeListEdit.HierarchicalPathProvider>
                                </dxrude:PopupTreeListEdit>
                            </dxlc:LayoutItem>
                        </DataTemplate>
                        <DataTemplate x:Key="expression">
                            <dxlc:LayoutItem Label="{dxrudl:ReportDesignerStringId StringId=Properties_Expression}" Visibility="{Binding Converter={smarttagsnative:SmartTagModelToPropertyVisibilityConverter IsBindingProperty=True}, ConverterParameter=Text}">
                                <dxe:PopupBaseEdit
                                    smarttagsnative:ExpressionDataExtensions.ExpressionPath="{x:Static reportsdesign:XRComponentPropertyNames.Text}"
                                    EditValue="{Binding TextBinding, UpdateSourceTrigger=PropertyChanged}"
                                    Style="{DynamicResource {dxrudt:LWKey SmartTagEditors.PopupWithExpressionEditorStyle}}"
                                    ValidateOnEnterKeyPressed="True"
                                    ValidateOnTextInput="False" />
                            </dxlc:LayoutItem>
                        </DataTemplate>
                        <smarttagsnative:BindingModeToPropertyTemplateConverter x:Key="textTemplateConverter" BindingTemplate="{StaticResource binding}" ExpressionTemplate="{StaticResource expression}" />
                    </dxlc:LayoutGroup.Resources>
                    <dxlc:LayoutItem
                        AddColonToLabel="True"
                        Label="{dxrudl:ReportDesignerStringId StringId=Properties_Text}"
                        LabelPosition="Top"
                        Visibility="{Binding Converter={smarttagsnative:SmartTagModelToPropertyVisibilityConverter}, ConverterParameter=Text}">
                        <dxe:TextEdit Height="75" Style="{DynamicResource {dxrudt:LWKey SmartTagEditors.TextEditStyle}}">
                            <dxmvvm:Interaction.BehaviorsTemplate>
                                <DataTemplate>
                                    <ItemsControl>
                                        <smarttagsnative:XRLabelSmartTagTextPropertyBehavior />
                                    </ItemsControl>
                                </DataTemplate>
                            </dxmvvm:Interaction.BehaviorsTemplate>
                        </dxe:TextEdit>
                    </dxlc:LayoutItem>
                    <ContentControl IsTabStop="False" Content="{Binding}" ContentTemplate="{Binding ReportModel.DataBindingMode, Converter={StaticResource textTemplateConverter}}" />

