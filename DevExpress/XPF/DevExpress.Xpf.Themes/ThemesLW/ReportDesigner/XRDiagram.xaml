        <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
            <Setter.Value>
                <DataTemplate>
                    <ItemsControl>
                        <dxrudi:ExpressionValidationBehavior />
                    </ItemsControl>
                </DataTemplate>
            </Setter.Value>
        </Setter>

        <Setter Property="PopupContentTemplate" Value="{StaticResource PopupExpressionEditorTemplate}" />

        <Setter Property="Buttons">
            <Setter.Value>
                <dxe:ButtonInfoCollection>
                    <dxe:ButtonInfo Style="{StaticResource ExpressionEditorButtonStyle}" />
                </dxe:ButtonInfoCollection>
            </Setter.Value>
        </Setter>
    </Style>

