<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="LayerSeparator.NormalColor">#FF8C97A8</Color>

    <Color x:Key="LayerSeparator.MouseOverColor">#FF5C6778</Color>

    <Color x:Key="ElementSizer.GlyphNormalColor">#FF8C97A8</Color>

    <Color x:Key="ElementSizer.GlyphMouseOverColor">#FF5C6778</Color>

    <Color x:Key="ElementSizer.GlyphBackgroundColor">#3FFFFFFF</Color>

    <Color x:Key="GroupBoxButton.NormalColor">#FF1B293E</Color>

    <Color x:Key="GroupBoxButton.MouseOverColor">#FF7F90AE</Color>

    <Color x:Key="GroupBoxButton.PressedColor">#FF6F809E</Color>

    <SolidColorBrush x:Key="GroupBox.Foreground" Color="#FF1B293E"/>

    <SolidColorBrush x:Key="GroupBox.Background" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="GroupBox.BorderBrush" Color="#FF8591A2"/>

    <SolidColorBrush x:Key="GroupBox.SeparatorBrush" Color="#FFA8B3C8"/>

    <LinearGradientBrush x:Key="GroupBox.TitleBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFEEF1F3" Offset="0"/>
        <GradientStop Color="#FFDADFE7" Offset="1"/>
        <GradientStop Color="#FFE2E6EB" Offset="0.5"/>
        <GradientStop Color="#FFDADFE7" Offset="0.5"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="GroupSeparator.Brush1" Color="#FFA8B3C8"/>

    <SolidColorBrush x:Key="GroupSeparator.Brush2" Color="#3FFFFFFF"/>

</ResourceDictionary>