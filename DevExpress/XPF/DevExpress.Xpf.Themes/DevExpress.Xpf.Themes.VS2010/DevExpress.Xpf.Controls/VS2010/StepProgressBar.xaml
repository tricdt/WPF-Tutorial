<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxcot="http://schemas.devexpress.com/winfx/2008/xaml/controls/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Duration x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressAnimationDuration}">0:0:0.5</Duration>

    <sys:Boolean x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ShowOuterBorder}">False</sys:Boolean>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderBrush}" Color="Transparent"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterIndentBrush}" Color="Transparent"/>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderIndent}">0</sys:Double>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderSize}">0</sys:Double>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBarThickness}">4</sys:Double>

    <Thickness x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBorderThickness}">1</Thickness>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBackground}" Color="#97A4B9"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressForeground}" Color="#FFCC00"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressAccentBorderBrush}" Color="#CDC8A3"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressNeutralBorderBrush}" Color="#97A4B9"/>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorBorderSize}">1</sys:Double>

    <Thickness x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorPanelMargin}">3</Thickness>

    <EllipseGeometry x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorGeometry}"
                     Center="20,20"
                     RadiusX="20"
                     RadiusY="20"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedItemForeground}" Color="#1B293E"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedIndicatorFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0"
                         cs:id="LinearGradientBrush_0001">
        <GradientStop Color="#FFF7F8F9"
                      Offset="0"
                      cs:id="GradientStop_0002"/>
        <GradientStop Color="#FFCED4DF"
                      Offset="1"
                      cs:id="GradientStop_0003"/>
        <GradientStop Color="#FFD8DDE5"
                      Offset="0.5"
                      cs:id="GradientStop_0004"/>
        <GradientStop Color="#FFCED4DF"
                      Offset="0.5"
                      cs:id="GradientStop_0005"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedIndicatorStroke}" Color="#97A4B9"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedMouseOverFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0"
                         cs:id="LinearGradientBrush_0002">
        <GradientStop Color="#FFFFFEF8"
                      Offset="0"
                      cs:id="GradientStop_0006"/>
        <GradientStop Color="#FFFFF4D3"
                      Offset="1"
                      cs:id="GradientStop_0007"/>
        <GradientStop Color="#FFFFF9E8"
                      Offset="0.5"
                      cs:id="GradientStop_0008"/>
        <GradientStop Color="#FFFFF4D3"
                      Offset="0.5"
                      cs:id="GradientStop_0009"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedMouseOverStroke}" Color="#97A4B9"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateItemForeground}" Color="#1B293E"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateIndicatorFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFEF8"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFF3D2"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FFE8A6"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
        <GradientStop Color="#FFE8A6"
                      Offset="0.5"
                      cs:id="GradientStop_0003"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateIndicatorStroke}" Color="#FFA9ACB5"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateMouseOverFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFEF8"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFF3D2"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FFE8A6"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
        <GradientStop Color="#FFE8A6"
                      Offset="0.5"
                      cs:id="GradientStop_0003"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateMouseOverStroke}" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedItemForeground}" Color="#1B293E"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedIndicatorFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFEF8"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFF3D2"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FFE8A6"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
        <GradientStop Color="#FFE8A6"
                      Offset="0.5"
                      cs:id="GradientStop_0003"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedIndicatorStroke}" Color="#97A4B9"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedMouseOverFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0"
                         cs:id="LinearGradientBrush_0002">
        <GradientStop Color="#FFFFFEF8"
                      Offset="0"
                      cs:id="GradientStop_0006"/>
        <GradientStop Color="#FFFFF4D3"
                      Offset="1"
                      cs:id="GradientStop_0007"/>
        <GradientStop Color="#FFFFF9E8"
                      Offset="0.5"
                      cs:id="GradientStop_0008"/>
        <GradientStop Color="#FFFFF4D3"
                      Offset="0.5"
                      cs:id="GradientStop_0009"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedMouseOverStroke}" Color="#97A4B9"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledNotStartedIndicatorFill}" Color="#CAD3DF"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledNotStartedIndicatorStroke}" Color="#ADB9CC"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledIndeterminateIndicatorFill}" Color="#D7D5C7"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledIndeterminateIndicatorStroke}" Color="#B4BCCA"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledCompletedIndicatorFill}" Color="#D7D5C7"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledCompletedIndicatorStroke}" Color="#B4BCCA"/>

</ResourceDictionary>