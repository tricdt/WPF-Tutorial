<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey ErrorForeground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey ErrorBackground}" Color="#FFB92200"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey ErrorBorderBrush}" Color="#FFB92200"/>

    <Thickness x:Key="{dxt:BadgesThemeKey ErrorBorderThickness}">0</Thickness>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey WarningForeground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey WarningBackground}" Color="#FFB98E00"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey WarningBorderBrush}" Color="#FFB98E00"/>

    <Thickness x:Key="{dxt:BadgesThemeKey WarningBorderThickness}">0</Thickness>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey SuccessForeground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey SuccessBackground}" Color="#FF007E65"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey SuccessBorderBrush}" Color="#FF007E65"/>

    <Thickness x:Key="{dxt:BadgesThemeKey SuccessBorderThickness}">0</Thickness>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey InformationForeground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey InformationBackground}" Color="#FF00639A"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey InformationBorderBrush}" Color="#FF00639A"/>

    <Thickness x:Key="{dxt:BadgesThemeKey InformationBorderThickness}">0</Thickness>

    <Thickness x:Key="{dxt:BadgesThemeKey Padding}">3,3,3,4</Thickness>

    <Thickness x:Key="{dxt:BadgesThemeKey TouchPadding}">5,5,5,6</Thickness>

</ResourceDictionary>