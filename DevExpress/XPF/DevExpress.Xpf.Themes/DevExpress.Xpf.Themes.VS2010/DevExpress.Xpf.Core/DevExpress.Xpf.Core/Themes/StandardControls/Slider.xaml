                    <Border Background="{TemplateBinding Background}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="SliderThumbStyle" TargetType="{x:Type Thumb}">
        <Setter Property="Foreground" Value="{x:Null}"/>
        <Setter Property="Background" Value="{x:Null}"/>
        <Setter Property="MinWidth" Value="11"/>
        <Setter Property="MinHeight" Value="11"/>
        <Setter Property="MaxWidth" Value="21"/>
        <Setter Property="MaxHeight" Value="21"/>
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Thumb}">
                    <Grid>
                        <Grid x:Name="DefaultState">
                            <Border Background="{StaticResource SliderThumb.DefaultState.Border1}"
                                    CornerRadius="2"
                                    Padding="1">
                                <Border Background="{StaticResource SliderThumb.DefaultState.Border2}"
                                        CornerRadius="1"
                                        Padding="1">
                                    <Border Background="{StaticResource SliderThumb.DefaultState.Border3}" CornerRadius="1"/>
                                </Border>
                            </Border>
                        </Grid>
                        <Grid x:Name="HoverState" Opacity="0">
                            <Border Background="{StaticResource SliderThumb.HoverState.Border1}"
                                    CornerRadius="2"
                                    Padding="1">
                                <Border Background="{StaticResource SliderThumb.HoverState.Border2}"
                                        CornerRadius="1"
                                        Padding="1">
                                    <Border Background="{StaticResource SliderThumb.HoverState.Border3}" CornerRadius="1"/>
                                </Border>
                            </Border>
                        </Grid>
                        <Grid x:Name="PressedState" Opacity="0">
                            <Border Background="{StaticResource SliderThumb.PressedState.Border1}"
                                    CornerRadius="2"
                                    Padding="1">
                                <Border Background="{StaticResource SliderThumb.PressedState.Border2}"
                                        CornerRadius="1"
                                        Padding="1">
                                    <Border Background="{StaticResource SliderThumb.PressedState.Border3}" CornerRadius="1"/>
                                </Border>
                            </Border>
                        </Grid>
                        <Grid x:Name="DisabledState" Opacity="0">
                            <Border Background="{StaticResource SliderThumb.DisabledState.Border1}"
                                    CornerRadius="2"
                                    Padding="1">
                                <Border Background="{StaticResource SliderThumb.DisabledState.Border2}" CornerRadius="1"/>
                            </Border>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Opacity"
                                    TargetName="DefaultState"
                                    Value="0"/>
                            <Setter Property="Opacity"
                                    TargetName="HoverState"
                                    Value="1"/>
                            <Setter Property="Opacity"
                                    TargetName="PressedState"
                                    Value="0"/>
                        </Trigger>
                        <Trigger Property="IsDragging" Value="True">
                            <Setter Property="Opacity"
                                    TargetName="DefaultState"
                                    Value="0"/>
                            <Setter Property="Opacity"
                                    TargetName="HoverState"
                                    Value="0"/>
                            <Setter Property="Opacity"
                                    TargetName="PressedState"
                                    Value="1"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity"
                                    TargetName="DefaultState"
                                    Value="0"/>
                            <Setter Property="Opacity"
                                    TargetName="HoverState"
                                    Value="0"/>
                            <Setter Property="Opacity"
                                    TargetName="PressedState"
                                    Value="0"/>
                            <Setter Property="Opacity"
                                    TargetName="DisabledState"
                                    Value="1"/>
                            <Setter Property="Foreground" Value="{StaticResource Text.DisabledState.Foreground}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

