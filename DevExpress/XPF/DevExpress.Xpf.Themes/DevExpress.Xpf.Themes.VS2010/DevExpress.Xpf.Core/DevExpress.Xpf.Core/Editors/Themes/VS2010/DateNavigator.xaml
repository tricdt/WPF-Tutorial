    <CornerRadius x:Key="{dxet:DateNavigatorThemeKey ResourceKey=CellButtonCornerRadius}">2</CornerRadius>

    <dxepc:DateNavigatorStateAppearance x:Key="{dxet:DateNavigatorThemeKey ResourceKey=AppearanceNormalState}" CornerRadius="2"/>

    <dxepc:DateNavigatorStateAppearance x:Key="{dxet:DateNavigatorThemeKey ResourceKey=AppearanceDisabledState}"
                                        Background="Transparent"
                                        BorderBrush="Transparent"
                                        Foreground="#44000000"
                                        PriorityIndex="8"/>

    <dxepc:DateNavigatorStateAppearance x:Key="{dxet:DateNavigatorThemeKey ResourceKey=AppearanceInactiveState}"
                                        Foreground="#7F000000"
                                        PriorityIndex="7"/>

    <dxepc:DateNavigatorStateAppearance x:Key="{dxet:DateNavigatorThemeKey ResourceKey=AppearanceFocusedState}"
                                        BorderBrush="#5A64490F"
                                        BorderThickness="1"
                                        PriorityIndex="3">
        <dxepc:DateNavigatorStateAppearance.Background>
            <LinearGradientBrush EndPoint="0.5,1"
                                 StartPoint="0.5,0"
                                 cs:id="LinearGradientBrush_0001">
                <GradientStop Color="#5AFFFBF0"
                              Offset="0"
                              cs:id="GradientStop_0004"/>
                <GradientStop Color="#5AFFE8A6"
                              Offset="1"
                              cs:id="GradientStop_0005"/>
                <GradientStop Color="#5AFFF4D2"
                              Offset="0.5"
                              cs:id="GradientStop_0006"/>
                <GradientStop Color="#5AFFE8A6"
                              Offset="0.5"
                              cs:id="GradientStop_0007"/>
            </LinearGradientBrush>
        </dxepc:DateNavigatorStateAppearance.Background>
    </dxepc:DateNavigatorStateAppearance>

    <dxepc:DateNavigatorStateAppearance x:Key="{dxet:DateNavigatorThemeKey ResourceKey=AppearanceMouseOverState}"
                                        BorderBrush="#FFE4C97F"
                                        BorderThickness="1"
                                        PriorityIndex="1">
        <dxepc:DateNavigatorStateAppearance.Background>
            <LinearGradientBrush EndPoint="0.5,1"
                                 StartPoint="0.5,0"
                                 cs:id="LinearGradientBrush_0000">
                <GradientStop Color="#FFFFFBF0"
                              Offset="0"
                              cs:id="GradientStop_0000"/>
                <GradientStop Color="#FFFFE8A6"
                              Offset="1"
                              cs:id="GradientStop_0001"/>
                <GradientStop Color="#FFFFF4D2"
                              Offset="0.5"
                              cs:id="GradientStop_0002"/>
                <GradientStop Color="#FFFFE8A6"
                              Offset="0.5"
                              cs:id="GradientStop_0003"/>
            </LinearGradientBrush>
        </dxepc:DateNavigatorStateAppearance.Background>
    </dxepc:DateNavigatorStateAppearance>

    <dxepc:DateNavigatorStateAppearance x:Key="{dxet:DateNavigatorThemeKey ResourceKey=AppearanceHolidayState}"
                                        Foreground="#FFCC3333"
                                        PriorityIndex="5"/>

    <dxepc:DateNavigatorStateAppearance x:Key="{dxet:DateNavigatorThemeKey ResourceKey=AppearanceSelectedState}"
                                        Background="#FFFFE8A6"
                                        BorderBrush="#FFE4C97F"
                                        BorderThickness="1"
                                        PriorityIndex="2"/>

    <dxepc:DateNavigatorStateAppearance x:Key="{dxet:DateNavigatorThemeKey ResourceKey=AppearanceTodayState}"
                                        Background="#7FBCC7D8"
                                        PriorityIndex="4"/>

    <dxepc:DateNavigatorStateAppearance x:Key="{dxet:DateNavigatorThemeKey ResourceKey=AppearanceSpecialDateState}"
                                        FontWeight="Bold"
                                        PriorityIndex="6"/>

    <dxepc:DateNavigatorCellAppearance x:Key="{dxet:DateNavigatorThemeKey ResourceKey=Appearance}"
                                       DisabledState="{StaticResource {dxet:DateNavigatorThemeKey ResourceKey=AppearanceDisabledState}}"
                                       FocusedState="{StaticResource {dxet:DateNavigatorThemeKey ResourceKey=AppearanceFocusedState}}"
                                       HolidayState="{StaticResource {dxet:DateNavigatorThemeKey ResourceKey=AppearanceHolidayState}}"
                                       InactiveState="{StaticResource {dxet:DateNavigatorThemeKey ResourceKey=AppearanceInactiveState}}"
                                       MouseOverState="{StaticResource {dxet:DateNavigatorThemeKey ResourceKey=AppearanceMouseOverState}}"
                                       NormalState="{StaticResource {dxet:DateNavigatorThemeKey ResourceKey=AppearanceNormalState}}"
                                       SelectedState="{StaticResource {dxet:DateNavigatorThemeKey ResourceKey=AppearanceSelectedState}}"
                                       SpecialDateState="{StaticResource {dxet:DateNavigatorThemeKey ResourceKey=AppearanceSpecialDateState}}"
                                       TodayState="{StaticResource {dxet:DateNavigatorThemeKey ResourceKey=AppearanceTodayState}}"/>

    <Size x:Key="{dxet:DateNavigatorThemeKey ResourceKey=CalendarCellSize}">30,17</Size>

    <Size x:Key="{dxet:DateNavigatorThemeKey ResourceKey=CalendarTouchCellSize}">44,44</Size>

    <Thickness x:Key="{dxet:DateNavigatorThemeKey ResourceKey=DateNavigatorCalendarPadding}">6,12,6,12</Thickness>

    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=OutlookHeaderStyle}" TargetType="{x:Type Border}">
        <Setter Property="Background"
                Value="#1F9CAAC1"
                cs:id="Setter_0000"/>
        <Setter Property="MinHeight" Value="22"/>
    </Style>

    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=TodayButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Content" Value="{Binding Path=IsTabStop, RelativeSource={RelativeSource Self}, ConverterParameter=Today, Converter={StaticResource EditorStringIdConverter}}"/>
        <Setter Property="Visibility" Value="{Binding Path=ShowTodayButton, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BoolToVisibilityConverter}}"/>
        <Setter Property="dx:FocusHelper2.Focusable" Value="False"/>
        <Setter Property="Margin" Value="0,0,0,12"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>

    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=ClearButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Content" Value="{Binding Path=IsTabStop, RelativeSource={RelativeSource Self}, ConverterParameter=Clear, Converter={StaticResource EditorStringIdConverter}}"/>
        <Setter Property="Visibility" Value="{Binding Path=ShowClearButton, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BoolToVisibilityConverter}}"/>
        <Setter Property="dx:FocusHelper2.Focusable" Value="False"/>
        <Setter Property="Margin" Value="0,0,0,12"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding Path=ShowTodayButton, RelativeSource={RelativeSource TemplatedParent}}" Value="True">
                <Setter Property="Margin" Value="8,0,0,12"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=CurrentDateButtonTemplate}" TargetType="{x:Type Button}">
        <Grid Background="Transparent" cs:id="Grid_0000">
            <ContentPresenter Content="{TemplateBinding Content}"
                              HorizontalAlignment="Center"
                              SnapsToDevicePixels="True"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=CellButtonTemplate}" TargetType="{x:Type Button}">
        <Grid Background="Transparent" cs:id="Grid_0000">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible"
                                                           Storyboard.TargetName="BorderMouseOver"
                                                           Storyboard.TargetProperty="Visibility"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed"/>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CellStatesSpecial">
                    <VisualState x:Name="CellStateSpecial">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.FontWeight="Bold"
                                                           Storyboard.TargetName="Content"
                                                           Storyboard.TargetProperty="FontWeight"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="CellStateNotSpecial"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CellStatesHoliday">
                    <VisualState x:Name="CellStateHoliday">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Content"
                                            Storyboard.TargetProperty="ForegroundSolidColor5"
                                            To="#FFCC3333"
                                            cs:id="ColorAnimation_0000"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="CellStateNotHoliday"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CellStatesSelected">
                    <VisualState x:Name="CellStateSelected">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible"
                                                           Storyboard.TargetName="BorderSelected"
                                                           Storyboard.TargetProperty="Visibility"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="CellStateNotSelected"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CellStatesToday">
                    <VisualState x:Name="CellStateToday">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible"
                                                           Storyboard.TargetName="BorderToday"
                                                           Storyboard.TargetProperty="Visibility"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="CellStateNotToday"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CellStatesFocused">
                    <VisualState x:Name="CellStateNotFocused"/>
                    <VisualState x:Name="CellStateFocused">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible"
                                                           Storyboard.TargetName="BorderFocused"
                                                           Storyboard.TargetProperty="Visibility"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CellStatesInactive">
                    <VisualState x:Name="CellStateActive"/>
                    <VisualState x:Name="CellStateInactive">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Content"
                                            Storyboard.TargetProperty="ForegroundSolidColor4"
                                            To="#7F000000"
                                            cs:id="ColorAnimation_0001"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CellStatesDisabled">
                    <VisualState x:Name="CellStateEnabled"/>
                    <VisualState x:Name="CellStateDisabled">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Content"
                                            Storyboard.TargetProperty="ForegroundSolidColor4"
                                            To="#44000000"
                                            cs:id="ColorAnimation_0002"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="BorderMouseOver" Visibility="Collapsed">
                <Border Background="#FFE4C97F"
                        CornerRadius="2"
                        Padding="1"
                        cs:id="Border_0000">
                    <Border CornerRadius="1"
                            Padding="1"
                            Visibility="Visible">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                 cs:id="LinearGradientBrush_0000">
                                <GradientStop Color="#FFFFFBF0"
                                              Offset="0"
                                              cs:id="GradientStop_0000"/>
                                <GradientStop Color="#FFFFE8A6"
                                              Offset="1"
                                              cs:id="GradientStop_0001"/>
                                <GradientStop Color="#FFFFF4D2"
                                              Offset="0.5"
                                              cs:id="GradientStop_0002"/>
                                <GradientStop Color="#FFFFE8A6"
                                              Offset="0.5"
                                              cs:id="GradientStop_0003"/>
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="BorderSelected" Visibility="Collapsed">
                <Border Background="#FFE4C97F"
                        CornerRadius="2"
                        Padding="1"
                        cs:id="Border_0001">
                    <Border Background="#FFFFF9E9"
                            CornerRadius="1"
                            Padding="1"
                            cs:id="Border_0002">
                        <Border Background="#FFFFE8A6" cs:id="Border_0003"/>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="BorderFocused" Visibility="Collapsed">
                <Border Background="#FF64490F"
                        CornerRadius="2"
                        Opacity="0.35"
                        Padding="1"
                        cs:id="Border_0004">
                    <Border CornerRadius="1"
                            Padding="1"
                            Visibility="Visible">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1"
                                                 StartPoint="0.5,0"
                                                 cs:id="LinearGradientBrush_0001">
                                <GradientStop Color="#FFFFFBF0"
                                              Offset="0"
                                              cs:id="GradientStop_0004"/>
                                <GradientStop Color="#FFFFE8A6"
                                              Offset="1"
                                              cs:id="GradientStop_0005"/>
                                <GradientStop Color="#FFFFF4D2"
                                              Offset="0.5"
                                              cs:id="GradientStop_0006"/>
                                <GradientStop Color="#FFFFE8A6"
                                              Offset="0.5"
                                              cs:id="GradientStop_0007"/>
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="BorderToday"
                    Background="#7FBCC7D8"
                    CornerRadius="2"
                    Visibility="Collapsed"
                    cs:id="Border_0005"/>
            <dxc:ContentContainer x:Name="Content"
                                  Content="{TemplateBinding Content}"
                                  HorizontalAlignment="Center"
                                  Margin="1,0,1,1"
                                  VerticalAlignment="Center"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=HeaderButtonTemplate}" TargetType="{x:Type Button}">
        <Grid SnapsToDevicePixels="True">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="Content"
                                                           Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FF063A9A" cs:id="SolidColorBrush_0000"/>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <dxc:ContentContainer x:Name="Content"
                                  Foreground="#FF0066CC"
                                  cs:id="ContentContainer_0000">
                <ContentPresenter Content="{TemplateBinding Content}"
                                  ContentTemplate="{TemplateBinding ContentTemplate}"
                                  HorizontalAlignment="Center"
                                  VerticalAlignment="Center"/>
            </dxc:ContentContainer>
        </Grid>
    </ControlTemplate>

    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=HeaderButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Background"
                Value="#00000000"
                cs:id="Setter_0000"/>
        <Setter Property="Template" Value="{DynamicResource {dxet:DateNavigatorThemeKey ResourceKey=HeaderButtonTemplate}}"/>
        <Setter Property="dx:FocusHelper2.Focusable" Value="False"/>
    </Style>

    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=CurrentDateButtonStyle}"
           BasedOn="{StaticResource {dxet:DateNavigatorThemeKey ResourceKey=HeaderButtonStyle}}"
           TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource {dxet:DateNavigatorThemeKey ResourceKey=CurrentDateButtonTemplate}}"/>
        <Setter Property="Margin" Value="0,2,0,2"/>
    </Style>

    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=WeekdayNameStyle}" TargetType="{x:Type TextBlock}"/>

    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=WeekNumberStyle}" TargetType="{x:Type TextBlock}">
        <Setter Property="Foreground"
                Value="#FFB3B3B3"
                cs:id="Setter_0000"/>
        <Setter Property="TextElement.FontSize" Value="9"/>
        <Setter Property="Margin" Value="0,0,4,0"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=WeekdayAbbreviationStyle}" TargetType="{x:Type TextBlock}">
        <Setter Property="Foreground"
                Value="#FF1B293E"
                cs:id="Setter_0000"/>
        <Setter Property="Margin" Value="0,0,0,4"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
    </Style>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=WeekNumberDelimeter}">
        <Border Background="#FF999999"
                Height="1"
                SnapsToDevicePixels="True"
                cs:id="Border_0000"/>
    </ControlTemplate>

    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=CellButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource {dxet:DateNavigatorThemeKey ResourceKey=CellButtonTemplate}}"/>
        <Setter Property="Background"
                Value="#FFFFFFFF"
                cs:id="Setter_0000"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
    </Style>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=YearInfoTemplate}">
        <Grid x:Name="PART_ContentGrid" SnapsToDevicePixels="True">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=MonthInfoDelimeter}">
        <Border Background="#FFE3E3E3"
                Height="1"
                Margin="0,0,0,4"
                SnapsToDevicePixels="True"
                cs:id="Border_0000"/>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=MonthInfoTemplate}">
        <Grid x:Name="PART_ContentGrid" SnapsToDevicePixels="True">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <ContentControl Grid.Column="2"
                            Grid.ColumnSpan="7"
                            Grid.Row="1"
                            IsTabStop="False"
                            Template="{DynamicResource {dxet:DateNavigatorThemeKey ResourceKey=MonthInfoDelimeter}}"/>
            <ContentControl x:Name="WeekNumberDelimeter"
                            Grid.Column="1"
                            Grid.Row="2"
                            Grid.RowSpan="6"
                            IsTabStop="False"
                            Template="{DynamicResource {dxet:DateNavigatorThemeKey ResourceKey=WeekNumberDelimeter}}"
                            Visibility="{Binding Path=(dxe:DateNavigator.Navigator).ShowWeekNumbers, RelativeSource={RelativeSource Self}, Converter={StaticResource BoolToVisibilityConverter}}"/>
        </Grid>
    </ControlTemplate>

    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=HeaderStyle}" TargetType="{x:Type Border}">
        <Setter Property="Background"
                Value="Transparent"
                cs:id="Setter_0000"/>
        <Setter Property="Margin" Value="4,4,4,8"/>
    </Style>

    <dxc:EditorStringIdConverter x:Key="EditorStringIdConverter"/>

    <SolidColorBrush x:Key="{dxet:DateNavigatorThemeKey ResourceKey=BackgroundBrush}" Color="#FFFFFFFF"/>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=ClearButtonTemplate}" TargetType="{x:Type Button}">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FFC1A965" cs:id="SolidColorBrush_0000"/>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="Border1"
                                                           Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0"
                                                             cs:id="LinearGradientBrush_0000">
                                            <GradientStop Color="#FFFFFEFB"
                                                          Offset="0"
                                                          cs:id="GradientStop_0000"/>
                                            <GradientStop Color="#FFFFF5D7"
                                                          Offset="1"
                                                          cs:id="GradientStop_0001"/>
                                            <GradientStop Color="#FFFFF6DC"
                                                          Offset="0.5"
                                                          cs:id="GradientStop_0002"/>
                                        </LinearGradientBrush>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="Border2"
                                                           Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0"
                                                             cs:id="LinearGradientBrush_0001">
                                            <GradientStop Color="#FFFFFBE5"
                                                          Offset="0"
                                                          cs:id="GradientStop_0003"/>
                                            <GradientStop Color="#FFFCEBB7"
                                                          Offset="1"
                                                          cs:id="GradientStop_0004"/>
                                            <GradientStop Color="#FFFADF8F"
                                                          Offset="0.5"
                                                          cs:id="GradientStop_0005"/>
                                            <GradientStop Color="#FFF4C233"
                                                          Offset="0.5"
                                                          cs:id="GradientStop_0006"/>
                                        </LinearGradientBrush>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0"
                                                             cs:id="LinearGradientBrush_0002">
                                            <GradientStop Color="#FF7B6645"
                                                          Offset="0"
                                                          cs:id="GradientStop_0007"/>
                                            <GradientStop Color="#FFA78E66"
                                                          Offset="1"
                                                          cs:id="GradientStop_0008"/>
                                        </LinearGradientBrush>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="Border1"
                                                           Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0"
                                                             cs:id="LinearGradientBrush_0003">
                                            <GradientStop Color="#FFC18C5D"
                                                          Offset="0"
                                                          cs:id="GradientStop_0009"/>
                                            <GradientStop Color="#FFFFC14C"
                                                          Offset="1"
                                                          cs:id="GradientStop_0010"/>
                                            <GradientStop Color="#FFFFAB49"
                                                          Offset="0.5"
                                                          cs:id="GradientStop_0011"/>
                                            <GradientStop Color="#FFFF8F08"
                                                          Offset="0.5"
                                                          cs:id="GradientStop_0012"/>
                                            <GradientStop Color="#FFFCB06A"
                                                          Offset="0.1"
                                                          cs:id="GradientStop_0013"/>
                                        </LinearGradientBrush>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Border2"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
