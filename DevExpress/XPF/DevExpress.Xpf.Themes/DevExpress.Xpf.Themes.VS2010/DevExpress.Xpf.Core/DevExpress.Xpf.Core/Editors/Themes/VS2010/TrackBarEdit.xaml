    <ControlTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=LeftStepButtonTemplate}" TargetType="{x:Type RepeatButton}">
        <Grid>
            <Ellipse x:Name="Shadow"
                     Fill="#FF000000"
                     Margin="1,1,-1,-1"
                     Opacity="0.05"/>
            <Grid x:Name="DefaultState">
                <Border Background="{StaticResource SliderThumb.DefaultState.Border1}"
                        CornerRadius="64"
                        Padding="1">
                    <Border Background="{StaticResource SliderThumb.DefaultState.Border2}"
                            CornerRadius="64"
                            Padding="1">
                        <Border Background="{StaticResource SliderThumb.DefaultState.Border3}" CornerRadius="64"/>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Border Background="{StaticResource SliderThumb.HoverState.Border1}"
                        CornerRadius="64"
                        Padding="1">
                    <Border Background="{StaticResource SliderThumb.HoverState.Border2}"
                            CornerRadius="64"
                            Padding="1">
                        <Border Background="{StaticResource SliderThumb.HoverState.Border3}" CornerRadius="64"/>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border Background="{StaticResource SliderThumb.PressedState.Border1}"
                        CornerRadius="64"
                        Padding="1">
                    <Border Background="{StaticResource SliderThumb.PressedState.Border2}"
                            CornerRadius="64"
                            Padding="1">
                        <Border Background="{StaticResource SliderThumb.PressedState.Border3}" CornerRadius="64"/>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="DisabledState" Opacity="0">
                <Border Background="{StaticResource SliderThumb.DisabledState.Border1}"
                        CornerRadius="64"
                        Padding="1">
                    <Border Background="{StaticResource SliderThumb.DisabledState.Border2}" CornerRadius="64"/>
                </Border>
            </Grid>
            <Rectangle x:Name="Glyph"
                       Fill="#FF6D7484"
                       Height="2"
                       HorizontalAlignment="Center"
                       Stretch="Fill"
                       VerticalAlignment="Center"
                       Width="6"/>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DefaultState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)"
                                            To="#FF1B293E"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)"
                                            To="#FF1B293E"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DisabledState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

