<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/treemap/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- General -->

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ControlBorderBrush}" Color="#FF8591A2"/>

    <!-- Group -->

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupBorderBrush}" Color="#FF8591A2"/>

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupBackground}" Color="#FFBCC7D8"/>

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupForeground}" Color="#FF000000"/>

    <sys:Double x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupFontSize}">12</sys:Double>

    <FontWeight x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupFontWeight}">Bold</FontWeight>

    <!-- SubMenu -->

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupBackground}" Color="#FFD5DCE7"/>

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupForeground}" Color="#FF000000"/>

    <sys:Double x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupFontSize}">12</sys:Double>

    <FontWeight x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupFontWeight}">Bold</FontWeight>

    <!-- ToolTip -->

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipBackground}" Color="#FF2D3F5D"/>

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipForeground}" Color="#FFBCC7D8"/>

    <sys:Double x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipFontSize}">12</sys:Double>

    <FontWeight x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipFontWeight}">Bold</FontWeight>

</ResourceDictionary>