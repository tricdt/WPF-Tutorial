                    Background="#FF939EB0"
                    CornerRadius="64"
                    Opacity="0"
                    cs:id="Border_0000">
                <Border CornerRadius="64" Margin="1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1"
                                             StartPoint="0.5,0"
                                             cs:id="LinearGradientBrush_0000">
