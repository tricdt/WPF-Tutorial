                                <GradientStop Color="#FFFFE8A6"
                                              Offset="1"
                                              cs:id="GradientStop_0011"/>
                                <GradientStop Color="#FFFFF3D1"
                                              Offset="0.5"
                                              cs:id="GradientStop_0012"/>
                                <GradientStop Color="#FFFFE8A6"
                                              Offset="0.5"
                                              cs:id="GradientStop_0013"/>
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="PressedState"
                    Background="#FF939EB0"
                    CornerRadius="64"
                    Opacity="0"
                    cs:id="Border_0002">
