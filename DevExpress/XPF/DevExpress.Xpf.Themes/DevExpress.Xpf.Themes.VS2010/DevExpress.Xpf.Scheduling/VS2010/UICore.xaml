    <Color x:Key="BrushSet.Resource8.Color">#FFF1EF</Color>

    <Color x:Key="BrushSet.Resource9.Color">#E6FAFC</Color>

    <SolidColorBrush x:Key="BrushSet.LabelNone" Color="Transparent"/>

