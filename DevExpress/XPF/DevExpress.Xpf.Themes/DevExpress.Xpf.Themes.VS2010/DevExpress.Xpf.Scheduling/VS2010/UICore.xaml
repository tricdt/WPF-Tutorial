    <LinearGradientBrush x:Key="MonthCellControl.TodayBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFFFBEE" Offset="0"/>
        <GradientStop Color="#FFFFF3D1" Offset="1"/>
        <GradientStop Color="#FFFFE8A6" Offset="0.5"/>
        <GradientStop Color="#FFFFE8A6" Offset="0.5"/>
    </LinearGradientBrush>

    <!--AppointmentControl-->

    <SolidColorBrush x:Key="AppointmentControl.StatusContainer.Background" Color="White"/>

    <SolidColorBrush x:Key="AppointmentControl.StatusContainer.BorderBrush" Color="#98a4b8"/>

