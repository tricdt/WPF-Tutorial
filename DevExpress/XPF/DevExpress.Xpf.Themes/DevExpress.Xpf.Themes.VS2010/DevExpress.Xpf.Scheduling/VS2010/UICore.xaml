    <SolidColorBrush x:Key="HeaderControl.BorderBrush" Color="#98a4b8"/>

    <LinearGradientBrush x:Key="HeaderControl.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFEEF1F3" Offset="0"/>
        <GradientStop Color="#FFDADFE7" Offset="1"/>
        <GradientStop Color="#FFDFE3E9" Offset="0.5"/>
        <GradientStop Color="#FFDADFE7" Offset="0.5"/>
    </LinearGradientBrush>

