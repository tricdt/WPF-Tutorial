<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="http://schemas.devexpress.com/winfx/2008/xaml/gauges" xmlns:core="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.DefaultLinearScaleRangeControl}" TargetType="local:DefaultLinearScaleRangeControl">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="local:DefaultLinearScaleRangeControl">
          <Rectangle RenderTransformOrigin="0.5, 1" Fill="{Binding Path=Presentation.ActualFill}" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>