                            cs:Name="Border_0013"
                            cs:MetropolisLight.Background="#FFFFFFFF"
                           />
                    <Border x:Name="HeaderHoverBackground"
                            Background="#FF333333"
                            cs:Name="Border_0014"
                            Opacity="0"
                            cs:MetropolisLight.Background="#FFFFFFFF"
                           />
                    <Border x:Name="HeaderPressedBackground"
                            Background="#FF333333"
                            cs:Name="Border_0015"
                            Opacity="0"
                            cs:MetropolisLight.Background="#FFFFFFFF"
                           />
