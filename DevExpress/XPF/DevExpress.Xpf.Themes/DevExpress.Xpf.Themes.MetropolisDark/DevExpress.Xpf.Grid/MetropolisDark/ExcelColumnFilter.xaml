<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxe_helpers="clr-namespace:DevExpress.Xpf.Editors.Helpers;assembly=DevExpress.Xpf.Core.v24.1"
                    xmlns:dxe1="clr-namespace:DevExpress.Xpf.Editors.Filtering;assembly=DevExpress.Xpf.Core.v24.1"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                    xmlns:dxgcore="clr-namespace:DevExpress.Xpf.Grid;assembly=DevExpress.Xpf.Grid.v24.1.Core"
                    xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
                    xmlns:dxgtl="clr-namespace:DevExpress.Xpf.Grid.TreeList"
                    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=HeaderPanelBottomBorderColor}"
                     Color="#FF515151"
                     cs:Name="SolidColorBrush_0001"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=HeaderPanelSeparatorColor}"
                     Color="#FF515151"
                     cs:Name="SolidColorBrush_0002"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=ButtonHeaderForeground}"
                     Color="#BFFFFFFF"
                     cs:Name="SolidColorBrush_0003"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=ButtonHeaderSelectedForeground}"
                     Color="#BFFFFFFF"
                     cs:Name="SolidColorBrush_0008"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=ButtonHeaderFocusedForeground}"
                     Color="#FFF78A09"
                     cs:Name="SolidColorBrush_0009"
                     cs:MetropolisLight.Color="#FFF78A09"/>

    <SolidColorBrush x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=ButtonHeaderBackground}"
                     Color="#FF333333"
                     cs:Name="SolidColorBrush_0004"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=HeaderPanelBackground}"
                     Color="#FF333333"
                     cs:Name="SolidColorBrush_0005"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=SearchControlAdditionalButtonColor}"
                     Color="#FF7F7F7F"
                     cs:Name="SolidColorBrush_0006"
                     cs:MetropolisLight.Color="#FF7F7F7F"/>

    <SolidColorBrush x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=ButtonHeaderDecorationBackground}"
                     Color="#FF333333"
                     cs:Name="SolidColorBrush_0007"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=FilterPanelBackground}"
                     Color="Transparent"
                     cs:MetropolisLight.Color="Transparent"/>

    <Thickness x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=ContentMargin}">14</Thickness>

    <Thickness x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=ButtonHeaderPaddingTouch}">0</Thickness>

    <Thickness x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=ButtonHeaderPadding}">14,3,14,6</Thickness>

    <Thickness x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=RowPresenterGridMargin}">0,0,0,0</Thickness>

    <ControlTemplate x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=DataPresenterTemplate}" TargetType="{x:Type dxg:DataPresenter}">
        <Border Background="Transparent"
                cs:Name="Border_0001"
                cs:MetropolisLight.Background="Transparent"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <Thickness x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=HorizontalScrollbarMargin}">0,0,0,0</Thickness>

    <Style x:Key="ButtonEditButtonGlyphStyle" TargetType="FrameworkElement">
        <Setter Property="FlowDirection" Value="LeftToRight"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>

    <ControlTemplate x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=SearchGlyphTemplate}">
        <Grid Background="Transparent"
              cs:Name="Grid_0001"
              Height="Auto"
              Margin="2,0,1,0"
              Style="{StaticResource ButtonEditButtonGlyphStyle}"
              VerticalAlignment="Center"
              Width="Auto"
              cs:MetropolisLight.Background="Transparent"
             >
            <Path Height="12"
                  Stretch="Fill"
                  Stroke="{DynamicResource {dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=SearchControlAdditionalButtonColor}}"
                  StrokeThickness="2"
                  Width="12">
                <Path.Data>
                    <GeometryGroup>
                        <EllipseGeometry Center="2,-2"
                                         RadiusX="1"
                                         RadiusY="1"/>
                        <PathGeometry Figures="M0,0 L1.2,-1.2"/>
                    </GeometryGroup>
                </Path.Data>
            </Path>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=DropDownGlyphTemplate}">
        <Grid Background="Transparent"
              cs:Name="Grid_0002"
              Height="Auto"
              Margin="2,0,1,0"
              Style="{StaticResource ButtonEditButtonGlyphStyle}"
              Width="Auto"
              cs:MetropolisLight.Background="Transparent"
             >
            <Path Data="M0,0 L1,1 2,0 Z"
                  Fill="{DynamicResource {dxgt:ExcelColumnFilterPopupThemeKey ResourceKey=SearchControlAdditionalButtonColor}}"
                  Height="4"
                  HorizontalAlignment="Center"
                  Stretch="Fill"
                  StrokeThickness="0"
                  VerticalAlignment="Center"
                  Width="7"/>
        </Grid>
    </ControlTemplate>

</ResourceDictionary>