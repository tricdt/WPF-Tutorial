                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Shared: MetropolisDark, TouchlineDark-->

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="{dx:SchedulingResource Generic/DefaultUICore.xaml}"/>
    </ResourceDictionary.MergedDictionaries>

    <Color x:Key="BrushSet.Resource1.Color" cs:MetropolisLight.Color="#c1e9fe">#0078d7</Color>

    <Color x:Key="BrushSet.Resource2.Color" cs:MetropolisLight.Color="#b3e5b8">#298835</Color>

    <Color x:Key="BrushSet.Resource3.Color" cs:MetropolisLight.Color="#fdc99d">#d83b01</Color>

