    <SolidColorBrush x:Key="Scheduler.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#ffffff"/>

    <SolidColorBrush x:Key="Scheduler.Foreground"
                     Color="#BFFFFFFF"
