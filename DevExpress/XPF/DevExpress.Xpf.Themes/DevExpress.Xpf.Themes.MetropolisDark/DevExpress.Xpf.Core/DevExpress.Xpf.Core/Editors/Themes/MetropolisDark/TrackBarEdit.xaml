    <ControlTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=LeftStepButtonTemplate}" TargetType="{x:Type RepeatButton}">
        <Grid x:Name="Root"
              Background="#FF333333"
              cs:Name="Grid_0001"
              cs:MetropolisLight.Background="#00000000"
             >
            <Border Background="#FF333333"
                    BorderBrush="#FF515151"
                    BorderThickness="1"
                    cs:Name="Border_0004"
                    Margin="1"
                    cs:MetropolisLight.Background="#FFFFFFFF"
                    cs:MetropolisLight.BorderBrush="#FFB9B9B9"
                   />
            <Rectangle x:Name="Glyph"
                       cs:Name="Rectangle_0001"
                       Fill="#FF3C3C3C"
                       Height="1"
                       Stretch="Fill"
                       Width="5"
                       cs:MetropolisLight.Fill="#FF373A3D"
                      />
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)"
                                            To="#FFF0F0F0"
                                            cs:MetropolisLight.To="#FF373A3D"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)"
                                            To="#FFF78A09"
                                            cs:MetropolisLight.To="#FFF78A09"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0003"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)"
                                            To="#FFF78A09"
                                            cs:MetropolisLight.To="#FFF78A09"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0004"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)"
                                            To="#FFA0A0A0"
                                            cs:MetropolisLight.To="#FFB9B9B9"
                                           />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

