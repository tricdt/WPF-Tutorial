    <ControlTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=VerticalBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <Grid x:Name="TrackBackground" HorizontalAlignment="Center">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="InternalFocusStates">
                    <VisualState x:Name="InternalFocused">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="FocusElement"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="InternalUnfocused"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border Background="#FF5B5B5B"
                    BorderBrush="#FF333333"
                    BorderThickness="1,0,1,0"
                    cs:Name="Border_0001"
                    Width="6"
                    cs:MetropolisLight.Background="#FFB9B9B9"
                    cs:MetropolisLight.BorderBrush="#FFFFFFFF"
                   />
            <Border x:Name="FocusElement"
                    BorderBrush="{StaticResource TrackBarEdit.Focus.Background}"
                    BorderThickness="1"
                    Margin="1"
                    Opacity="0"/>
        </Grid>
    </ControlTemplate>

