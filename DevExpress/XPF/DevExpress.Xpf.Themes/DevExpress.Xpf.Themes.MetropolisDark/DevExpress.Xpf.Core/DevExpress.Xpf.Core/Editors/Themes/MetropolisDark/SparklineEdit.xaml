<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.MetropolisDark.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/SparklineEdit.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <sys:Double x:Key="{dxet:SparklineEditThemeKey ResourceKey=MinHeight}">20</sys:Double>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = Brush}"
                     Color="#FF999999"
                     cs:Name="SolidColorBrush_0001"
                     cs:MetropolisLight.Color="#FF999999"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = MaxPointBrush}"
                     Color="#FFCC0000"
                     cs:Name="SolidColorBrush_0002"
                     cs:MetropolisLight.Color="#FFCC0000"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = MinPointBrush}"
                     Color="#FFF78A09"
                     cs:Name="SolidColorBrush_0003"
                     cs:MetropolisLight.Color="#FFF78A09"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = StartPointBrush}"
                     Color="#FF999999"
                     cs:Name="SolidColorBrush_0004"
                     cs:MetropolisLight.Color="#FF999999"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = EndPointBrush}"
                     Color="#FF999999"
                     cs:Name="SolidColorBrush_0005"
                     cs:MetropolisLight.Color="#FF999999"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = NegativePointBrush}"
                     Color="#FF12C6E6"
                     cs:Name="SolidColorBrush_0006"
                     cs:MetropolisLight.Color="#FF12C6E6"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = MarkerBrush}"
                     Color="#FF999999"
                     cs:Name="SolidColorBrush_0007"
                     cs:MetropolisLight.Color="#FF999999"/>

    <!--END WPF-->

</ResourceDictionary>