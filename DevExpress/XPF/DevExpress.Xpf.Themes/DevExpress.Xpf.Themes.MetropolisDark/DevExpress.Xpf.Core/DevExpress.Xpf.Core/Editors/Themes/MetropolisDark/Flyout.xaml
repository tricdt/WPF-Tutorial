<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.MetropolisDark.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/Flyout.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!--END WPF-->

    <Thickness x:Key="{dxet:FlyoutControlThemeKey ResourceKey=BorderThickness}">1</Thickness>

    <Thickness x:Key="{dxet:FlyoutControlThemeKey ResourceKey=Padding}">12</Thickness>

    <SolidColorBrush x:Key="{dxet:FlyoutControlThemeKey ResourceKey=Background}"
                     Color="#FF333333"
                     cs:Name="SolidColorBrush_0001"
                     cs:MetropolisLight.Color="#FFFFFFFF">
    </SolidColorBrush>

    <SolidColorBrush x:Key="{dxet:FlyoutControlThemeKey ResourceKey=BorderBrush}"
                     Color="#FF515151"
                     cs:Name="SolidColorBrush_0002"
                     cs:MetropolisLight.Color="#FFB9B9B9">
    </SolidColorBrush>

    <SolidColorBrush x:Key="{dxet:FlyoutControlThemeKey ResourceKey=Foreground}"
                     Color="#BFFFFFFF"
                     cs:Name="SolidColorBrush_0003"
                     cs:MetropolisLight.Color="#FF373A3D">
    </SolidColorBrush>

</ResourceDictionary>