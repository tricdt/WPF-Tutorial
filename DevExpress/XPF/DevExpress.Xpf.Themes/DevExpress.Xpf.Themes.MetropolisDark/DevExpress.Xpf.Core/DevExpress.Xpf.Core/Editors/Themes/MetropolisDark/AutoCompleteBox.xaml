<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxei="http://schemas.devexpress.com/winfx/2008/xaml/editors/internal"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.MetropolisDark.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/AutoCompleteBox.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="{dxet:TokenEditorThemeKey ResourceKey=TokenForeground}"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <ControlTemplate x:Key="{dxet:TokenEditorThemeKey ResourceKey=CellPresenterTemplate}" TargetType="{x:Type dxei:TokenEditorPresenter}">
        <Grid Background="Transparent"
              cs:Name="Grid_0001"
              cs:MetropolisLight.Background="Transparent"
             >
            <Border UseLayoutRounding="True">
                <ContentControl Focusable="False" Template="{TemplateBinding BorderTemplate}"/>
            </Border>
            <dxei:CellEditor x:Name="cell"
                             Focusable="False"
                             TextElement.Foreground="{TemplateBinding Foreground}"/>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsMouseOver, RelativeSource={RelativeSource Self}}" Value="True">
                <Setter cs:Name="Setter_0008"
                        Property="TextElement.Foreground"
                        TargetName="cell"
                        Value="#FFFFFFFF"
                        cs:MetropolisLight.Value="#FF373A3D"
                       />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsSelected, RelativeSource={RelativeSource Self}}" Value="True">
                <Setter cs:Name="Setter_0009"
                        Property="TextElement.Foreground"
                        TargetName="cell"
                        Value="#FF333333"
                        cs:MetropolisLight.Value="#FFFFFFFF"
                       />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsEditorActivated, RelativeSource={RelativeSource Self}}" Value="True">
                <Setter cs:Name="Setter_0010"
                        Property="TextElement.Foreground"
                        TargetName="cell"
                        Value="#FFFFFFFF"
                        cs:MetropolisLight.Value="#FF373A3D"
                       />
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:TokenEditorThemeKey ResourceKey=TokenContainerTemplate}" TargetType="{x:Type ContentControl}">
        <Grid Margin="0,1,1,1">
            <Border x:Name="selection"
                    Background="#FF515151"
                    cs:Name="Border_0001"
                    cs:MetropolisLight.Background="#FFC6C6C6"
                   />
            <Grid x:Name="FocusedState" Opacity="0">
                <Border Background="#FFF56C06"
                        BorderBrush="#FFF56C06"
                        BorderThickness="1"
                        cs:Name="Border_0004"
                        Opacity="1"
                        cs:MetropolisLight.Background="#FFF56C06"
                        cs:MetropolisLight.BorderBrush="#FFF56C06"
                       />
            </Grid>
            <Grid x:Name="FocusedUnselectedState" Opacity="0">
                <Border Background="#00000000"
                        BorderBrush="#FFF78A09"
                        BorderThickness="1"
                        cs:Name="Border_0006"
                        Opacity="1"
                        cs:MetropolisLight.Background="#00000000"
                        cs:MetropolisLight.BorderBrush="#FFF78A09"
                       />
            </Grid>
            <ContentPresenter/>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsMouseOver, RelativeSource={RelativeSource Self}}" Value="True">
                <Setter cs:Name="Setter_0005"
                        Property="Background"
                        TargetName="selection"
                        Value="#FF515151"
                        cs:MetropolisLight.Value="#FFD6D6D6"
                       />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsSelected, RelativeSource={RelativeSource Self}}" Value="True">
                <Setter cs:Name="Setter_0006"
                        Property="Background"
                        TargetName="selection"
                        Value="#FFF78A09"
                        cs:MetropolisLight.Value="#FFF78A09"
                       />
            </DataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsSelected, RelativeSource={RelativeSource Self}}" Value="True"/>
                    <Condition Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsTokenFocused, RelativeSource={RelativeSource Self}}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter Property="Opacity"
                        TargetName="FocusedUnselectedState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="FocusedState"
                        Value="1"/>
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsSelected, RelativeSource={RelativeSource Self}}" Value="False"/>
                    <Condition Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsTokenFocused, RelativeSource={RelativeSource Self}}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter Property="Opacity"
                        TargetName="FocusedUnselectedState"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="FocusedState"
                        Value="0"/>
            </MultiDataTrigger>
            <DataTrigger Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsEditorActivated, RelativeSource={RelativeSource Self}}" Value="True">
                <Setter cs:Name="Setter_0007"
                        Property="Background"
                        TargetName="selection"
                        Value="#00000000"
                        cs:MetropolisLight.Value="#00000000"
                       />
                <Setter Property="Opacity"
                        TargetName="FocusedState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="FocusedUnselectedState"
                        Value="1"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="{dxet:TokenEditorThemeKey ResourceKey=DeleteButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="Border"
                            Background="Transparent"
                            cs:Name="Border_0002"
                            Height="16"
                            HorizontalAlignment="Center"
                            Margin="0,0,2,0"
                            VerticalAlignment="Center"
                            Width="16"
                            cs:MetropolisLight.Background="Transparent"
                           >
                        <Path x:Name="Path"
                              cs:Name="RenderPath_0001"
                              Data="F1 M 6,0C 9.31371,0 12,2.68629 12,6C 12,9.31371 9.31371,12 6,12C 2.68629,12 -6.35783e-007,9.31371 -6.35783e-007,6C -6.35783e-007,2.68629 2.68629,0 6,0 Z M 3,4L 5,6L 3,8L 4,9L 6,7L 8,9L 9,8L 7,6L 9,4L 8,3L 6,5L 4,3L 3,4 Z "
                              Fill="#9FFFFFFF"
                              Height="12"
                              Stretch="Fill"
                              Width="12"
                              cs:MetropolisLight.Fill="#3F000000"
                             />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter cs:Name="Setter_0003"
                                    Property="Fill"
                                    TargetName="Path"
                                    Value="#BFFFFFFF"
                                    cs:MetropolisLight.Value="#7F000000"
                                   />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter cs:Name="Setter_0004"
                                    Property="Fill"
                                    TargetName="Path"
                                    Value="#BFFFFFFF"
                                    cs:MetropolisLight.Value="#7F000000"
                                   />
                        </Trigger>
                        <DataTrigger Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsSelected, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter cs:Name="Setter_0011"
                                    Property="Fill"
                                    TargetName="Path"
                                    Value="#FF333333"
                                    cs:MetropolisLight.Value="#FFFFFFFF"
                                   />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=(dxei:TokenEditorPresenter.OwnerPresenter).IsEditorActivated, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter cs:Name="Setter_0012"
                                    Property="Fill"
                                    TargetName="Path"
                                    Value="#FFFFFFFF"
                                    cs:MetropolisLight.Value="#FF666666"
                                   />
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>