<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- General -->

    <Color x:Key="DefaultBackgroundColor" cs:MetropolisLight.Color="#FFFFFFFF">#FF333333</Color>

    <Color x:Key="DefaultForegroundColor" cs:MetropolisLight.Color="#FF373A3D">#BFFFFFFF</Color>

    <SolidColorBrush x:Key="{dxdst:FloatingContainerThemeKey ResourceKey=FloatingContainerBackground}" Color="{StaticResource DefaultBackgroundColor}"/>

    <SolidColorBrush x:Key="{dxdst:ThemeColorsThemeKey ResourceKey=WhiteForegroundColor}"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="{dxdst:ThemeColorsThemeKey ResourceKey=BlackForegroundColor}"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="{dxdst:ThemeColorsThemeKey ResourceKey=DXMessageBoxForegroundColor}"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="Page.Background" Color="{StaticResource DefaultBackgroundColor}"/>

    <SolidColorBrush x:Key="Text.Foreground" Color="{StaticResource DefaultForegroundColor}"/>

    <SolidColorBrush x:Key="Text.DisabledState.Foreground"
                     Color="#3FE3E3E3"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="Shadow.Background"
                     Color="Transparent"
                     cs:MetropolisLight.Color="#00000000"/>

    <SolidColorBrush x:Key="Text.Selection.Background"
                     Color="#FFFF8800"
                     cs:MetropolisLight.Color="#FFF78A09"/>

    <SolidColorBrush x:Key="Text.CaretBrush"
                     Color="#FFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <!-- Button -->

    <SolidColorBrush x:Key="Button.DefaultState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="Button.DefaultState.Border2"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="Button.HoverState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="Button.HoverState.Border2"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="Button.PressedState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="Button.PressedState.Border2"
                     Color="#FF3C3C3C"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="Button.CheckedState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="Button.CheckedState.Border2"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <!-- CheckBox -->

    <SolidColorBrush x:Key="CheckBox.DefaultState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="CheckBox.DefaultState.Border2"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="CheckBox.HoverState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="CheckBox.HoverState.Border2"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="CheckBox.PressedState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="CheckBox.PressedState.Border2"
                     Color="#FF3C3C3C"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="CheckBox.IndeterminateState.Border"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <!-- TextBox -->

    <SolidColorBrush x:Key="TextBox.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="TextBox.BorderBrush"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="TextBox.ReadOnly.Background"
                     Color="#FF3C3C3C"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <!-- ListBox -->

    <SolidColorBrush x:Key="ListBox.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="ListBox.BorderBrush"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <!-- ListBoxItem -->

    <SolidColorBrush x:Key="ListBoxItem.Background"
                     Color="Transparent"
                     cs:MetropolisLight.Color="#00000000"/>

    <SolidColorBrush x:Key="ListBoxItem.BorderBrush"
                     Color="Transparent"
                     cs:MetropolisLight.Color="#00000000"/>

    <SolidColorBrush x:Key="ListBoxItem.Foreground"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="ListBoxItem.DisabledState.Foreground"
                     Color="#FF6D6D6D"
                     cs:MetropolisLight.Color="#7F373A3D"/>

    <SolidColorBrush x:Key="ListBoxItem.Selection.Border" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="ListBoxItem.HoverSelection.Border"
                     Color="#FF666666"
                     cs:MetropolisLight.Color="#FFE3E3E3"/>

    <SolidColorBrush x:Key="ListBoxItem.InactiveSelection.Border"
                     Color="#FF666666"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <!-- RadioButton -->

    <SolidColorBrush x:Key="RadioButton.DefaultState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="RadioButton.DefaultState.Border2"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="RadioButton.HoverState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="RadioButton.HoverState.Border2"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="RadioButton.PressedState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="RadioButton.PressedState.Border2"
                     Color="#FF3C3C3C"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="RadioButton.DisabledState.Border1"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="RadioButton.DisabledState.Border2"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="RadioButton.Icon.Background"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <!-- ComboBox -->

    <SolidColorBrush x:Key="ComboBox.Path.DefaultState"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF666666"/>

    <SolidColorBrush x:Key="ComboBox.Path.DisabledState"
                     Color="#7FE3E3E3"
                     cs:MetropolisLight.Color="#7F373A3D"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.DefaultState.Border"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.DefaultState.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.HoverState.Border"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.HoverState.Background"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.PressedState.Border"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.PressedState.Background"
                     Color="#FF3C3C3C"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.DisabledState.Border"
                     Color="Transparent"
                     cs:MetropolisLight.Color="#00000000"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.DisabledState.Background" Color="Transparent"/>

    <SolidColorBrush x:Key="ComboBox.DropDownBorder.BorderBrush"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ComboBox.DropDownBorder.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <!-- ComboBoxItem -->

    <!-- Label -->

    <SolidColorBrush x:Key="Label.Background"
                     Color="Transparent"
                     cs:MetropolisLight.Color="#00000000"/>

    <SolidColorBrush x:Key="Label.Foreground"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <!-- ProgressBar -->

    <SolidColorBrush x:Key="ProgressBar.BorderBrush"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ProgressBar.Background"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="ProgressBar.IndicatorBackground" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="ProgressBar.IndicatorAnimatedFill" Color="#FFF78A09"/>

    <!-- ScrollViewer -->

    <SolidColorBrush x:Key="ScrollViewer.BorderBrush"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ScrollViewer.Corner.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <!-- ScrollBar -->

    <SolidColorBrush x:Key="ScrollBar.Vertical.Background"
                     Color="#FF2F2F2F"
                     cs:MetropolisLight.Color="#FFF6F6F6"/>

    <SolidColorBrush x:Key="ScrollBar.Vertical.BorderBrush"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.Background"
                     Color="#FF2F2F2F"
                     cs:MetropolisLight.Color="#FFF6F6F6"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.BorderBrush"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ScrollBar.Vertical.PageButton.Background"
                     Color="Transparent"
                     cs:MetropolisLight.Color="#00000000"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.PageButton.Background"
                     Color="Transparent"
                     cs:MetropolisLight.Color="#00000000"/>

    <SolidColorBrush x:Key="ScrollBar.Thumb.Path.Background"
                     Color="#FF666666"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ScrollBar.Path.Background"
                     Color="#FF666666"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.DefaultState.Border"
                     Color="#FF666666"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.HoverState.Border"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.DraggingState.Border"
                     Color="#FF666666"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="ScrollBar.Vertical.DefaultState.Border"
                     Color="#FF666666"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ScrollBar.Vertical.HoverState.Border"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="ScrollBar.Vertical.DraggingState.Border"
                     Color="#FF666666"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <!-- Separator -->

    <SolidColorBrush x:Key="Separator.BorderBrush"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <!-- TabControl -->

    <SolidColorBrush x:Key="TabControl.BorderBrush"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="TabControl.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="TabControl.Foreground"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <!-- TabItem -->

    <SolidColorBrush x:Key="TabItem.Foreground"
                     Color="#FFB4B4B4"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="TabItem.HoverState.Foreground"
                     Color="#FFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="TabItem.SelectedState.Foreground" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="TabItem.BorderBrush"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="TabItem.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFF0F0F0"/>

    <SolidColorBrush x:Key="TabItem.HoverState.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="TabItem.SelectedState.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Top" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Bottom" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Left" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Right" Color="#FFF78A09"/>

    <!-- Slider -->

    <SolidColorBrush x:Key="Slider.Track.Background"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="Slider.Tick.Background"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="Slider.SelectionRange.Background" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="SliderThumb.DefaultState"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="SliderThumb.HoverState" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="SliderThumb.PressedState" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="SliderThumb.DisabledState"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <!-- GridSplitter -->

    <SolidColorBrush x:Key="GridSplitter.Background"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="GridSplitter.PreviewBrush" Color="#FFF78A09"/>

    <!-- ToolTip -->

    <SolidColorBrush x:Key="ToolTip.BorderBrush"
                     Color="#FF666666"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ToolTip.Background"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="ToolTip.Foreground" Color="{StaticResource DefaultForegroundColor}"/>

    <!-- Expander -->

    <SolidColorBrush x:Key="Expander.Background"
                     Color="Transparent"
                     cs:MetropolisLight.Color="#00000000"/>

    <SolidColorBrush x:Key="Expander.BorderBrush"
                     Color="Transparent"
                     cs:MetropolisLight.Color="#00000000"/>

</ResourceDictionary>