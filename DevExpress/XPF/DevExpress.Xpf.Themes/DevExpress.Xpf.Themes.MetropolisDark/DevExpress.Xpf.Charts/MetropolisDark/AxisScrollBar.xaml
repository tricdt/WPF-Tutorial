                                       cs:MetropolisLight.Background="#FFFFFFFF"
                                      />
                <Grid x:Name="PART_VerticalContainer">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <local:ScrollBarRepeatButton x:Name="PART_VerticalSmallIncrease"
                                                 AutomationProperties.Name="VerticalScrollbarSmallIncrement"
                                                 Grid.Row="0"
                                                 Height="20"
                                                 Interval="50"
                                                 IsTabStop="False"
