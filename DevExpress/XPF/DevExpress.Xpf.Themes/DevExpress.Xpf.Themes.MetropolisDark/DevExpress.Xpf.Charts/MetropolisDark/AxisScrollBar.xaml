<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/charts/themekeys"
                    xmlns:local="http://schemas.devexpress.com/winfx/2008/xaml/charts"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisScrollBarTemplate}" TargetType="local:AxisScrollBar">
        <Grid x:Name="Root">
            <Grid.Resources>
                <ControlTemplate x:Key="RepeatButtonTemplate" TargetType="RepeatButton">
                    <Grid x:Name="Root"
                          Background="Transparent"
                          cs:Name="Grid_0001"
                          cs:MetropolisLight.Background="#00000000"
                         >
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                </ControlTemplate>
                <ControlTemplate x:Key="HorizontalIncrementTemplate" TargetType="RepeatButton">
                    <Grid x:Name="Root"
                          Background="Transparent"
                          cs:Name="Grid_0002"
                          cs:MetropolisLight.Background="#00000000"
                         >
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Border"
                                                         Storyboard.TargetProperty="(Border.Opacity)"
                                                         To="1"/>
                                        <ColorAnimation cs:Name="ColorAnimation_0017"
                                                        Duration="0"
                                                        Storyboard.TargetName="Glyph"
                                                        Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                        To="#FF666666"
                                                        cs:MetropolisLight.To="#FFFFFFFF"
                                                       />
                                        <ColorAnimation cs:Name="ColorAnimation_0013"
                                                        Duration="0"
                                                        Storyboard.TargetName="Border"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#BFFFFFFF"
                                                        cs:MetropolisLight.To="#FF373A3D"
                                                       />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Border"
                                                         Storyboard.TargetProperty="(Border.Opacity)"
                                                         To="1"/>
                                        <ColorAnimation cs:Name="ColorAnimation_0018"
                                                        Duration="0"
                                                        Storyboard.TargetName="Border"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#FF666666"
                                                        cs:MetropolisLight.To="#FFB9B9B9"
                                                       />
                                        <ColorAnimation cs:Name="ColorAnimation_0025"
                                                        Duration="0"
                                                        Storyboard.TargetName="Glyph"
                                                        Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                        To="#FF222222"
                                                        cs:MetropolisLight.To="#FFFFFFFF"
                                                       />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Root"
                                                         Storyboard.TargetProperty="(UIElement.Opacity)"
                                                         To="0.35"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="Border"
                                Background="#FF666666"
                                cs:Name="Border_0007"
                                Margin="0,3,3,3"
                                Opacity="0"
                                cs:MetropolisLight.Background="#FFB9B9B9"
                               />
                        <Path x:Name="Glyph"
                              cs:Name="Path_0001"
                              Data="F1 M 1287,780L 1284,777L 1284,783L 1287,780 Z "
                              Fill="#FF666666"
                              Height="6"
                              HorizontalAlignment="Center"
                              Margin="-1,0,1,0"
                              Stretch="Fill"
                              VerticalAlignment="Center"
                              Width="3"
                              cs:MetropolisLight.Fill="#FF373A3D"
                             />
                    </Grid>
                </ControlTemplate>
                <ControlTemplate x:Key="HorizontalDecrementTemplate" TargetType="RepeatButton">
                    <Grid x:Name="Root"
                          Background="Transparent"
                          cs:Name="Grid_0003"
                          cs:MetropolisLight.Background="#00000000"
                         >
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Border"
                                                         Storyboard.TargetProperty="(Border.Opacity)"
                                                         To="1"/>
                                        <ColorAnimation cs:Name="ColorAnimation_0019"
                                                        Duration="0"
                                                        Storyboard.TargetName="Glyph"
                                                        Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                        To="#FF666666"
                                                        cs:MetropolisLight.To="#FFFFFFFF"
                                                       />
                                        <ColorAnimation cs:Name="ColorAnimation_0014"
                                                        Duration="0"
                                                        Storyboard.TargetName="Border"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#BFFFFFFF"
                                                        cs:MetropolisLight.To="#FF373A3D"
                                                       />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Border"
                                                         Storyboard.TargetProperty="(Border.Opacity)"
                                                         To="1"/>
                                        <ColorAnimation cs:Name="ColorAnimation_0020"
                                                        Duration="0"
                                                        Storyboard.TargetName="Border"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#FF666666"
                                                        cs:MetropolisLight.To="#FFB9B9B9"
                                                       />
                                        <ColorAnimation cs:Name="ColorAnimation_0026"
                                                        Duration="0"
                                                        Storyboard.TargetName="Glyph"
                                                        Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                        To="#FF222222"
                                                        cs:MetropolisLight.To="#FFFFFFFF"
                                                       />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Root"
                                                         Storyboard.TargetProperty="(UIElement.Opacity)"
                                                         To="0.35"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="Border"
                                Background="#FF666666"
                                cs:Name="Border_0008"
                                Margin="3,3,0,3"
                                Opacity="0"
                                cs:MetropolisLight.Background="#FFB9B9B9"
                               />
                        <Path x:Name="Glyph"
                              cs:Name="Path_0002"
                              Data="F1 M 1263,779L 1266,782L 1266,776L 1263,779 Z "
                              Fill="#FF666666"
                              Height="6"
                              HorizontalAlignment="Center"
                              Margin="1,0,-1,0"
                              Stretch="Fill"
                              VerticalAlignment="Center"
                              Width="3"
                              cs:MetropolisLight.Fill="#FF373A3D"
                             />
                    </Grid>
                </ControlTemplate>
                <ControlTemplate x:Key="VerticalDecrementTemplate" TargetType="RepeatButton">
                    <Grid x:Name="Root"
                          Background="Transparent"
                          cs:Name="Grid_0004"
                          cs:MetropolisLight.Background="#00000000"
                         >
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Border"
                                                         Storyboard.TargetProperty="(Border.Opacity)"
                                                         To="1"/>
                                        <ColorAnimation cs:Name="ColorAnimation_0021"
                                                        Duration="0"
                                                        Storyboard.TargetName="Glyph"
                                                        Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                        To="#FF666666"
                                                        cs:MetropolisLight.To="#FFFFFFFF"
                                                       />
                                        <ColorAnimation cs:Name="ColorAnimation_0015"
                                                        Duration="0"
                                                        Storyboard.TargetName="Border"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#BFFFFFFF"
                                                        cs:MetropolisLight.To="#FF373A3D"
                                                       />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Border"
                                                         Storyboard.TargetProperty="(Border.Opacity)"
                                                         To="1"/>
                                        <ColorAnimation cs:Name="ColorAnimation_0022"
                                                        Duration="0"
                                                        Storyboard.TargetName="Border"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#FF666666"
                                                        cs:MetropolisLight.To="#FFB9B9B9"
                                                       />
                                        <ColorAnimation cs:Name="ColorAnimation_0027"
                                                        Duration="0"
                                                        Storyboard.TargetName="Glyph"
                                                        Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                        To="#FF222222"
                                                        cs:MetropolisLight.To="#FFFFFFFF"
                                                       />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Root"
                                                         Storyboard.TargetProperty="(UIElement.Opacity)"
                                                         To="0.35"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="Border"
                                Background="#FF666666"
                                cs:Name="Border_0009"
                                Margin="3,0,3,3"
                                Opacity="0"
                                cs:MetropolisLight.Background="#FFB9B9B9"
                               />
                        <Path x:Name="Glyph"
                              cs:Name="Path_0003"
                              Data="F1 M 1281,774L 1284,771L 1278,771L 1281,774 Z "
                              Fill="#FF666666"
                              Height="3"
                              HorizontalAlignment="Center"
                              Margin="0,-1,0,1"
                              Stretch="Fill"
                              UseLayoutRounding="True"
                              VerticalAlignment="Center"
                              Width="6"
                              cs:MetropolisLight.Fill="#FF373A3D"
                             />
                    </Grid>
                </ControlTemplate>
                <ControlTemplate x:Key="VerticalIncrementTemplate" TargetType="RepeatButton">
                    <Grid x:Name="Root"
                          Background="Transparent"
                          cs:Name="Grid_0005"
                          cs:MetropolisLight.Background="#00000000"
                         >
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Border"
                                                         Storyboard.TargetProperty="(Border.Opacity)"
                                                         To="1"/>
                                        <ColorAnimation cs:Name="ColorAnimation_0023"
                                                        Duration="0"
                                                        Storyboard.TargetName="Glyph"
                                                        Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                        To="#FF666666"
                                                        cs:MetropolisLight.To="#FFFFFFFF"
                                                       />
                                        <ColorAnimation cs:Name="ColorAnimation_0016"
                                                        Duration="0"
                                                        Storyboard.TargetName="Border"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#BFFFFFFF"
                                                        cs:MetropolisLight.To="#FF373A3D"
                                                       />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Border"
                                                         Storyboard.TargetProperty="(Border.Opacity)"
                                                         To="1"/>
                                        <ColorAnimation cs:Name="ColorAnimation_0024"
                                                        Duration="0"
                                                        Storyboard.TargetName="Border"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#FF666666"
                                                        cs:MetropolisLight.To="#FFB9B9B9"
                                                       />
                                        <ColorAnimation cs:Name="ColorAnimation_0028"
                                                        Duration="0"
                                                        Storyboard.TargetName="Glyph"
                                                        Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                                        To="#FF222222"
                                                        cs:MetropolisLight.To="#FFFFFFFF"
                                                       />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0"
                                                         Storyboard.TargetName="Root"
                                                         Storyboard.TargetProperty="(UIElement.Opacity)"
                                                         To="0.35"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="Border"
                                Background="#FF666666"
                                cs:Name="Border_0010"
                                Margin="3,3,3,0"
                                Opacity="0"
                                cs:MetropolisLight.Background="#FFB9B9B9"
                               />
                        <Path x:Name="Glyph"
                              cs:Name="Path_0004"
                              Data="F1 M 1287,763L 1284,766L 1290,766L 1287,763 Z"
                              Fill="#FF666666"
                              Height="3"
                              HorizontalAlignment="Center"
                              Margin="0,1,0,-1"
                              Stretch="Fill"
                              UseLayoutRounding="True"
                              VerticalAlignment="Center"
                              Width="6"
                              cs:MetropolisLight.Fill="#FF373A3D"
                             />
                    </Grid>
                </ControlTemplate>
                <ControlTemplate x:Key="VerticalThumbTemplate" TargetType="local:AxisScrollBarThumb">
                    <Grid x:Name="Root">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimation cs:Name="ColorAnimation_0009"
                                                        Duration="0"
                                                        Storyboard.TargetName="ThumbVisual"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#BFFFFFFF"
                                                        cs:MetropolisLight.To="#FF373A3D"
                                                       />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed"/>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ColorAnimation cs:Name="ColorAnimation_0010"
                                                        Duration="0"
                                                        Storyboard.TargetName="ThumbVisual"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#FF666666"
                                                        cs:MetropolisLight.To="#FF373A3D"
                                                       />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="ThumbVisual"
                                Background="#FF666666"
                                cs:Name="Border_0001"
                                Margin="3,2,3,2"
                                cs:MetropolisLight.Background="#FFB9B9B9"
                               />
                    </Grid>
                </ControlTemplate>
                <ControlTemplate x:Key="HorizontalThumbTemplate" TargetType="local:AxisScrollBarThumb">
                    <Grid x:Name="Root">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimation cs:Name="ColorAnimation_0011"
                                                        Duration="0"
                                                        Storyboard.TargetName="ThumbVisual"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#BFFFFFFF"
                                                        cs:MetropolisLight.To="#FF373A3D"
                                                       />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed"/>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ColorAnimation cs:Name="ColorAnimation_0012"
                                                        Duration="0"
                                                        Storyboard.TargetName="ThumbVisual"
                                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                        To="#FF666666"
                                                        cs:MetropolisLight.To="#FF373A3D"
                                                       />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="ThumbVisual"
                                Background="#FF666666"
                                cs:Name="Border_0002"
                                Margin="2,3,2,3"
                                cs:MetropolisLight.Background="#FFB9B9B9"
                               />
                    </Grid>
                </ControlTemplate>
                <ControlTemplate x:Key="HorizontalThumbResizerTemplate" TargetType="local:AxisScrollBarThumbResizer">
                    <Grid Background="Transparent"
                          cs:Name="Grid_0006"
                          cs:MetropolisLight.Background="#00000000"
                         >
                        <Border Background="#FF333333"
                                cs:Name="Border_0003"
                                Margin="3,5,3,5"
                                Width="1"
                                cs:MetropolisLight.Background="#FFFFFFFF"
                               />
                    </Grid>
                </ControlTemplate>
                <ControlTemplate x:Key="VerticalThumbResizerTemplate" TargetType="local:AxisScrollBarThumbResizer">
                    <Grid Background="Transparent"
                          cs:Name="Grid_0007"
                          cs:MetropolisLight.Background="#00000000"
                         >
                        <Border Background="#FF333333"
                                cs:Name="Border_0004"
                                Height="1"
                                Margin="5,3,5,3"
                                cs:MetropolisLight.Background="#FFFFFFFF"
                               />
                    </Grid>
                </ControlTemplate>
            </Grid.Resources>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver"/>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Root"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.5"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="PART_HorizontalRoot">
                <local:ScrollBarBorder x:Name="PART_HorizontalBorder"
                                       Annotations="{TemplateBinding Annotations}"
                                       Background="#FF333333"
                                       BorderBrush="{TemplateBinding BorderBrush}"
                                       BorderThickness="{TemplateBinding BorderThickness}"
                                       cs:Name="Border_0005"
                                       cs:MetropolisLight.Background="#FFFFFFFF"
                                      />
                <Grid x:Name="PART_HorizontalContainer">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <local:ScrollBarRepeatButton x:Name="PART_HorizontalSmallDecrease"
                                                 AutomationProperties.Name="HorizontalScrollbarSmallDecrement"
                                                 Grid.Column="0"
                                                 Interval="50"
                                                 IsTabStop="False"
                                                 Margin="1"
                                                 Template="{StaticResource HorizontalDecrementTemplate}"
                                                 Width="20"/>
                    <!--BEGIN WPF-->
                    <local:ScrollBarRepeatButton x:Name="PART_HorizontalLargeDecrease"
                                                 AutomationProperties.Name="HorizontalScrollbarLargeDecrement"
                                                 Grid.Column="1"
                                                 Interval="50"
                                                 IsTabStop="False"
                                                 Margin="0,0,0,1"
                                                 Template="{StaticResource RepeatButtonTemplate}"/>
                    <!--END WPF-->
                    <!--ONLY SL <local:ScrollBarRepeatButton x:Name="PART_HorizontalLargeDecrease" Template="{StaticResource RepeatButtonTemplate}" Grid.Column="1" IsTabStop="False" Interval="50" /> ONLY SL-->
                    <local:AxisScrollBarThumb x:Name="PART_HorizontalThumb"
                                              Grid.Column="2"
                                              Margin="0,1,0,1"
                                              MinWidth="30"
                                              Template="{StaticResource HorizontalThumbTemplate}"/>
                    <local:AxisScrollBarThumbResizer x:Name="PART_HorizontalThumbDecrease"
                                                     Cursor="SizeWE"
                                                     Grid.Column="2"
                                                     HorizontalAlignment="Left"
                                                     MinWidth="3"
                                                     Template="{StaticResource HorizontalThumbResizerTemplate}"/>
                    <local:AxisScrollBarThumbResizer x:Name="PART_HorizontalThumbIncrease"
                                                     Cursor="SizeWE"
                                                     Grid.Column="2"
                                                     HorizontalAlignment="Right"
                                                     MinWidth="3"
                                                     Template="{StaticResource HorizontalThumbResizerTemplate}"/>
                    <!--BEGIN WPF-->
                    <local:ScrollBarRepeatButton x:Name="PART_HorizontalLargeIncrease"
                                                 AutomationProperties.Name="HorizontalScrollbarLargeIncrement"
                                                 Grid.Column="3"
                                                 Interval="50"
                                                 IsTabStop="False"
                                                 Margin="0,0,0,1"
                                                 Template="{StaticResource RepeatButtonTemplate}"/>
                    <!--END WPF-->
                    <!--ONLY SL <local:ScrollBarRepeatButton x:Name="PART_HorizontalLargeIncrease" Template="{StaticResource RepeatButtonTemplate}" Grid.Column="3" IsTabStop="False" Interval="50" /> ONLY SL-->
                    <local:ScrollBarRepeatButton x:Name="PART_HorizontalSmallIncrease"
                                                 AutomationProperties.Name="HorizontalScrollbarSmallIncrement"
                                                 Grid.Column="4"
                                                 Interval="50"
                                                 IsTabStop="False"
                                                 Margin="1"
                                                 Template="{StaticResource HorizontalIncrementTemplate}"
                                                 Width="20"/>
                </Grid>
            </Grid>
            <Grid x:Name="PART_VerticalRoot" Visibility="Collapsed">
                <local:ScrollBarBorder x:Name="PART_VerticalBorder"
                                       Annotations="{TemplateBinding Annotations}"
                                       Background="#FF333333"
                                       BorderBrush="{TemplateBinding BorderBrush}"
                                       BorderThickness="{TemplateBinding BorderThickness}"
