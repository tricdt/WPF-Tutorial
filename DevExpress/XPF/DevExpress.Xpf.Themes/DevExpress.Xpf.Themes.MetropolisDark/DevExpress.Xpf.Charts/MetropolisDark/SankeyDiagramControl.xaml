<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxsat="http://schemas.devexpress.com/winfx/2008/xaml/sankey/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=PaddingValue}">16</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=BorderThickness}">1</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=ToolTipContentMargin}">7</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TitleMargin}">0,0,0,10</Thickness>

    <sys:Double x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TitleFontSize}">21</sys:Double>

    <sys:Double x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TextElementFontSize}">16</sys:Double>

    <FontWeight x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=NodeLabelFontWeight}">Bold</FontWeight>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=BackgroundBrush}"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=BorderBrush}"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipForeground}"
                     Color="#FFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipBackground}"
                     Color="#FF333333"
                     cs:MetropolisLight.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipBorder}"
                     Color="#FF515151"
                     cs:MetropolisLight.Color="#CF000000"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=TitleForeground}" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=TextElementForeground}"
                     Color="#FFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=NodeLabelForeground}"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

</ResourceDictionary>