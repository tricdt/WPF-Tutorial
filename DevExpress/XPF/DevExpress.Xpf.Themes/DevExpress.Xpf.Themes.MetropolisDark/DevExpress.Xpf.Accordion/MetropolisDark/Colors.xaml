                     Color="#FFF78A09"
                     cs:Name="SolidColorBrush_0018"
                     cs:MetropolisLight.Color="#FF373A3D"/>

    <SolidColorBrush x:Key="{dxat:AccordionThemeKeys ResourceKey=HeaderForeground}"
                     Color="#BFFFFFFF"
                     cs:MetropolisLight.Color="#FF373A3D"/>

</ResourceDictionary>