<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
                    xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
                    xmlns:dxwuii="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/internal"
                    xmlns:dxwuit="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Colors -->

    <Color x:Key="colorDefault"
           cs:Name="Color_0001"
           cs:MetropolisLight.Color="#FFB9B9B9">
        #BFFFFFFF
    </Color>

    <Color x:Key="colorHover"
           cs:Name="Color_0002"
           cs:MetropolisLight.Color="#FFF78A09">
        #FFF78A09
    </Color>

    <Color x:Key="colorPressed"
           cs:Name="Color_0003"
           cs:MetropolisLight.Color="#FFF78A09">
        #FFF78A09
    </Color>

    <Color x:Key="colorDisabled"
           cs:Name="Color_0004"
           cs:MetropolisLight.Color="#FFFFFFFF">
        #FF333333
    </Color>

    <SolidColorBrush x:Key="ellipseFillDefault"
                     Color="#00333333"
                     cs:Name="SolidColorBrush_0001"
                     cs:MetropolisLight.Color="#00FFFFFF"/>

    <SolidColorBrush x:Key="ellipseFillHover"
                     Color="#FF333333"
                     cs:Name="SolidColorBrush_0002"
                     cs:MetropolisLight.Color="#1FF78A09"/>

    <SolidColorBrush x:Key="ellipseFillPressed"
                     Color="#FF222222"
                     cs:Name="SolidColorBrush_0003"
                     cs:MetropolisLight.Color="#3FF78A09"/>

    <SolidColorBrush x:Key="ellipseStrokeDefault"
                     Color="#BFFFFFFF"
                     cs:Name="SolidColorBrush_0004"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="ellipseStrokeHover"
                     Color="#FFF78A09"
                     cs:Name="SolidColorBrush_0005"
                     cs:MetropolisLight.Color="#FFF78A09"/>

    <SolidColorBrush x:Key="ellipseStrokePressed"
                     Color="#FFF78A09"
                     cs:Name="SolidColorBrush_0006"
                     cs:MetropolisLight.Color="#FFF78A09"/>

    <SolidColorBrush x:Key="pathColorDefault" Color="{StaticResource colorDefault}"/>

    <SolidColorBrush x:Key="pathColorHover" Color="{StaticResource colorHover}"/>

    <SolidColorBrush x:Key="pathColorPressed" Color="{StaticResource colorPressed}"/>

    <SolidColorBrush x:Key="brushDisabled" Color="{StaticResource colorDisabled}"/>

    <!-- Glyph -->

    <DataTemplate x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=BackGlyph}">
        <Path Data="F1 M 305,875L 305,874L 317,862L 321,862L 310,873L 332,873L 332,876L 310,876L 321,887L 317,887L 305,875 Z "
              Height="25"
              Stretch="Fill"
              Width="27"/>
    </DataTemplate>

    <!-- Glyph -->

    <DataTemplate x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=ForwardGlyph}">
        <Path Data="F1 M 305,875L 305,874L 317,862L 321,862L 310,873L 332,873L 332,876L 310,876L 321,887L 317,887L 305,875 Z "
              Height="25"
              RenderTransformOrigin="0.5,0.5"
              Stretch="Fill"
              Width="27">
            <Path.RenderTransform>
                <ScaleTransform ScaleX="-1"/>
            </Path.RenderTransform>
        </Path>
    </DataTemplate>

    <!-- Glyph -->

    <DataTemplate x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=ExitGlyph}">
        <Path Data="F1 M 0,2L 2,0L 8,6L 14,0L 16,2L 10,8L 16,14L 14,16L 8,10L 2,16L 0,14L 6,8L 0,2 Z "
              Fill="#FF000000"
              Height="16"
              Stretch="Fill"
              Width="16"
              cs:MetropolisLight.Fill="#FF000000"
             />
    </DataTemplate>

    <!-- Template -->

    <ControlTemplate x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=Template}" TargetType="dxwui:CommandButton">
        <Grid SnapsToDevicePixels="True">
            <Ellipse x:Name="border"
                     Fill="{StaticResource ellipseFillDefault}"
                     MinHeight="41"
                     MinWidth="41"
                     Stroke="{StaticResource ellipseStrokeDefault}"
                     StrokeThickness="3"
                     UseLayoutRounding="True">
                <dxi:Interaction.Behaviors>
                    <dxwuii:ConstrainedSizeBehavior/>
                </dxi:Interaction.Behaviors>
            </Ellipse>
            <dxwuii:ShapeContentPresenter x:Name="content"
                                          Foreground="{StaticResource pathColorDefault}"
                                          HorizontalAlignment="Center"
                                          VerticalAlignment="Center"/>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="Stroke">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource ellipseStrokeHover}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource ellipseFillHover}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="content" Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource pathColorHover}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="Stroke">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource ellipseStrokePressed}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource ellipseFillPressed}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="content" Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource pathColorPressed}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard></Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

    <Color x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=HoverForeColor}"
           cs:Name="Color_0005"
           cs:MetropolisLight.Color="#FFF78A09">
        #FFF78A09
    </Color>

    <SolidColorBrush x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=HoverBorderBrush}"
                     Color="#FFF78A09"
                     cs:Name="SolidColorBrush_0007"
                     cs:MetropolisLight.Color="#FFF78A09"/>

    <SolidColorBrush x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=HoverBackBrush}"
                     Color="#FF333333"
                     cs:Name="SolidColorBrush_0008"
                     cs:MetropolisLight.Color="#1FF78A09"/>

    <Color x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=PressedForeColor}"
           cs:Name="Color_0006"
           cs:MetropolisLight.Color="#FFF78A09">
        #FFF78A09
    </Color>

    <SolidColorBrush x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=PressedBorderBrush}"
                     Color="#FFF78A09"
                     cs:Name="SolidColorBrush_0009"
                     cs:MetropolisLight.Color="#FFF78A09"/>

    <SolidColorBrush x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=PressedBackBrush}"
                     Color="#FF222222"
                     cs:Name="SolidColorBrush_0010"
                     cs:MetropolisLight.Color="#3FF78A09"/>

    <Color x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=NormalForeColor}"
           cs:Name="Color_0007"
           cs:MetropolisLight.Color="#FFB9B9B9">
        #BFFFFFFF
    </Color>

    <SolidColorBrush x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=NormalBorderBrush}"
                     Color="#BFFFFFFF"
                     cs:Name="SolidColorBrush_0011"
                     cs:MetropolisLight.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="{dxwuit:CommandButtonElementsThemeKey ResourceKey=NormalBackBrush}"
                     Color="#00333333"
                     cs:Name="SolidColorBrush_0012"
                     cs:MetropolisLight.Color="#00FFFFFF"/>

</ResourceDictionary>