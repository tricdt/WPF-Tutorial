  <ResourceDictionary.MergedDictionaries>
    <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Win10Dark.v24.1;component/Core/Core/Editors/Themes/Win10Dark/CalcEdit/CalcEditButtons_Base.xaml" />
