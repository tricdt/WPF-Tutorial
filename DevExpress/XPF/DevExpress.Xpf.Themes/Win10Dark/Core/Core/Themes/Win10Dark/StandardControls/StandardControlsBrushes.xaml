<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- MENU BRUSHES -->
  <SolidColorBrush x:Key="Menu_Background" Color="#FF191919" x:Uid="@Color=Window.Background" />
  <SolidColorBrush x:Key="Menu_Border" Color="#FF2B2B2B" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="Menu_Foreground" Color="#FFF2F2F2" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="MenuItem_Foreground" Color="#FFF2F2F2" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="MenuItem_HoverForeground" Color="#FFF6F6F6" x:Uid="@Color=HoverForeground" />
  <SolidColorBrush x:Key="MenuItem_PressedForeground" Color="#FFFFFFFF" x:Uid="@Color=SelectionForeground" />
  <SolidColorBrush x:Key="MenuItem_HoverBackground" Color="#FF444444" x:Uid="@Color=HoverBackground" />
  <SolidColorBrush x:Key="MenuItem_HoverBorder" Color="#FF444444" x:Uid="@Color=HoverBorder" />
  <SolidColorBrush x:Key="MenuItem_SelectionBackground" Color="#FF777777" x:Uid="@Color=SelectionBackground" />
  <SolidColorBrush x:Key="MenuItem_SelectionBorder" Color="#FF777777" x:Uid="@Color=SelectionBorder" />
  <!-- CONTEXT MENU BRUSHES -->
  <SolidColorBrush x:Key="ContextMenu_Background" Color="#FF2B2B2B" x:Uid="@Color=Menu.Background" />
  <SolidColorBrush x:Key="ContextMenu_Border" Color="#FF535353" x:Uid="@Color=AltBorder" />
  <SolidColorBrush x:Key="ContextMenuItem_Foreground" Color="#FFF2F2F2" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverForeground" Color="#FFF6F6F6" x:Uid="@Color=HoverForeground" />
  <SolidColorBrush x:Key="ContextMenuItem_PressedForeground" Color="#FFFFFFFF" x:Uid="@Color=SelectionForeground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverBackground" Color="#FF444444" x:Uid="@Color=HoverBackground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverBorder" Color="#FF444444" x:Uid="@Color=HoverBorder" />
  <SolidColorBrush x:Key="ContextMenuItem_SelectionBackground" Color="#FF777777" x:Uid="@Color=SelectionBackground" />
  <SolidColorBrush x:Key="ContextMenuItem_SelectionBorder" Color="#FF777777" x:Uid="@Color=SelectionBorder" />
</ResourceDictionary>