  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=ConnectorCriticalPathStroke}" Color="#FFFF3838" x:Uid="@Color=Custom.Red" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=CriticalPathConnectorHighlightedShadowBrush}" Color="#FFFF3838" Opacity="0.32" x:Uid="@Color=Custom.Red" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=NonworkingTimeBrush}" Color="#FF535353" Opacity="0.25" x:Uid="@Color=AltBorder" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=GroupHighlightBrush}" Color="#FF0078D7" Opacity="0.16" x:Uid="@Color=Focused" />
  <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=StripLineBackground}" Color="#FF76A797" Opacity="0.25" />
  <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=CurrentDateTimeStripLineBackground}" Color="#FFD86344" />
</ResourceDictionary>