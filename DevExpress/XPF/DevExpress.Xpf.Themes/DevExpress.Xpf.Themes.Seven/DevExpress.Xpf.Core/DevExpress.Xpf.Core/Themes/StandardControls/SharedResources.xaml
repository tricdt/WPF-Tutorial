<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Commmon -->

    <Geometry x:Key="UpArrowGeometry">M 0 4 L 7 4 L 3.5 0 Z</Geometry>

    <Geometry x:Key="DownArrowGeometry">M 0 0 L 3.5 4 L 7 0 Z</Geometry>

    <Geometry x:Key="LeftArrowGeometry">M 0 4 L 4 8 L 4 0 Z</Geometry>

    <Geometry x:Key="RightArrowGeometry">M 0 0 L 0 8 L 4 4 Z</Geometry>

    <Geometry x:Key="SizeGripPart">F1 M 114,110L 115,110L 115,111L 114,111L 114,110 Z M 114,113L 115,113L 115,114L 114,114L 114,113 Z M 111,113L 112,113L 112,114L 111,114L 111,113 Z M 111,116L 112,116L 112,117L 111,117L 111,116 Z M 108,116L 109,116L 109,117L 108,117L 108,116 Z M 114,116L 115,116L 115,117L 114,117L 114,116 Z</Geometry>

    <!-- Button -->

    <Style x:Key="ButtonFocusTemplate" TargetType="{x:Type Rectangle}">
        <Setter Property="Stroke" Value="#FF66CCFF"/>
        <Setter Property="StrokeThickness" Value="1"/>
        <Setter Property="Margin" Value="1"/>
        <Setter Property="RadiusX" Value="2"/>
        <Setter Property="RadiusY" Value="2"/>
        <Setter Property="Opacity" Value="0.75"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
    </Style>

    <!-- CheckBox -->

    <Style x:Key="CheckBoxFocusTemplate" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="#FF66CCFF"/>
        <Setter Property="Opacity" Value="0.35"/>
        <Setter Property="CornerRadius" Value="1"/>
        <Setter Property="Margin" Value="-1"/>
    </Style>

    <Style x:Key="CheckBoxIconTemplate" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="M102.03442,598.79645 L105.22962,597.78918 L106.78825,600.42358 C106.78825,600.42358 108.51028,595.74304 110.21724,593.60419 C112.00967,591.35822 114.89314,591.42316 114.89314,591.42316 C114.89314,591.42316 112.67844,593.42645 111.93174,594.44464 C110.7449,596.06293 107.15683,604.13837 107.15683,604.13837 z"/>
        <Setter Property="FlowDirection" Value="LeftToRight"/>
        <Setter Property="Width" Value="10"/>
        <Setter Property="Height" Value="10"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Stretch" Value="Fill"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="UseLayoutRounding" Value="False"/>
    </Style>

    <!-- RadioButton -->

    <Style x:Key="RadioButtonFocusTemplate" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="#FF06CDFF"/>
        <Setter Property="Opacity" Value="0.35"/>
        <Setter Property="CornerRadius" Value="64"/>
        <Setter Property="Margin" Value="-1"/>
    </Style>

    <!-- Slider -->

    <Style x:Key="SliderFocusTemplate" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="#FF66CCFF"/>
        <Setter Property="CornerRadius" Value="1"/>
        <Setter Property="Opacity" Value="0.5"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
    </Style>

    <!-- TabItem -->

    <Thickness x:Key="TabItem.Padding">11,3,11,3</Thickness>

    <Thickness x:Key="TabItem.TopPadding">11,3,11,2</Thickness>

    <Thickness x:Key="TabItem.BottomPadding">11,2,11,3</Thickness>

    <Thickness x:Key="TabItem.DefaultState.Root.Margin">0</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Top.Root.Margin">0,-2,-1,0</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Top.Item.Padding">1,1,1,0</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Top.Item.Margin">0,0,0,-1</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Bottom.Root.Margin">0,0,-1,-2</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Bottom.Item.Padding">1,0,1,1</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Bottom.Item.Margin">0,-1,0,0</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Left.Root.Margin">-2,0,0,-1</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Left.Item.Padding">1,1,0,1</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Left.Item.Margin">0,0,-1,0</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Right.Root.Margin">0,0,-2,-1</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Right.Item.Padding">0,1,1,1</Thickness>

    <Thickness x:Key="TabItem.SelectedState.Right.Item.Margin">-1,0,0,0</Thickness>

</ResourceDictionary>