<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="RulerBackgroundBrush" Color="#FFE3EFFF"/>

    <SolidColorBrush x:Key="RulerBorderBrush" Color="#FFE3EFFF"/>

    <SolidColorBrush x:Key="RulerTabHotZoneBrush" Color="#FF637691"/>

    <LinearGradientBrush x:Key="RulerIndentHotZoneBackgroundBrush"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#FFC0CFE3" Offset="0"/>
        <GradientStop Color="#FFAEC1DA" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="RulerIndentHotZoneBorderBrush" Color="#FF637691"/>

    <SolidColorBrush x:Key="RulerTableHotZoneBorderBrush" Color="#7F637691"/>

    <SolidColorBrush x:Key="RulerTableHotZoneBackgroundBrush" Color="#7FFFFFFF"/>

    <SolidColorBrush x:Key="RulerTickMarkBrush" Color="#FF787878"/>

    <SolidColorBrush x:Key="RulerDefaultTabMarkBrush" Color="#592F3646"/>

    <SolidColorBrush x:Key="RulerSpaceAreaBrush" Color="#FFD8E5F6"/>

    <SolidColorBrush x:Key="TabTypeToggleHotZoneControlTemplateBackgroundBrush" Color="#FFAEC1DA"/>

</ResourceDictionary>