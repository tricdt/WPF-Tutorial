<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduling"
                    xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduling/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="{dx:SchedulingResource Generic/DefaultUICore.xaml}"/>
    </ResourceDictionary.MergedDictionaries>

    <Color x:Key="BrushSet.Resource1.Color">#F0F9E5</Color>

    <Color x:Key="BrushSet.Resource2.Color">#EDF7F9</Color>

    <Color x:Key="BrushSet.Resource3.Color">#FEF1E6</Color>

    <Color x:Key="BrushSet.Resource4.Color">#F7F1FF</Color>

    <Color x:Key="BrushSet.Resource5.Color">#E4FEEA</Color>

    <Color x:Key="BrushSet.Resource6.Color">#FFEFF7</Color>

    <Color x:Key="BrushSet.Resource7.Color">#F7F4EB</Color>

    <Color x:Key="BrushSet.Resource8.Color">#FFF1EF</Color>

    <Color x:Key="BrushSet.Resource9.Color">#E6FAFC</Color>

    <SolidColorBrush x:Key="BrushSet.LabelNone" Color="Transparent"/>

    <SolidColorBrush x:Key="BrushSet.LabelImportant" Color="#FFFFAFB3"/>

    <SolidColorBrush x:Key="BrushSet.LabelBusiness" Color="#FFAFCAFF"/>

    <SolidColorBrush x:Key="BrushSet.LabelPersonal" Color="#FFC4F7C1"/>

    <SolidColorBrush x:Key="BrushSet.LabelVacation" Color="#FFFFD5AF"/>

    <SolidColorBrush x:Key="BrushSet.LabelMustAttend" Color="#FFFFC5AF"/>

    <SolidColorBrush x:Key="BrushSet.LabelTravelRequired" Color="#FF8FE2FA"/>

    <SolidColorBrush x:Key="BrushSet.LabelNeedsPreparation" Color="#FFD2E186"/>

    <SolidColorBrush x:Key="BrushSet.LabelBirthday" Color="#FFEBB5D9"/>

    <SolidColorBrush x:Key="BrushSet.LabelAnniversary" Color="#FF8CE4C6"/>

    <SolidColorBrush x:Key="BrushSet.LabelPhoneCall" Color="#FFF7DD93"/>

    <SolidColorBrush x:Key="CellInplaceEditorControl.Background" Color="#FF6589BD"/>

    <SolidColorBrush x:Key="ResourceForeground" Color="Black"/>

    <SolidColorBrush x:Key="DefaultHeaderForeground" Color="Black"/>

    <SolidColorBrush x:Key="DefaultLabelColor" Color="White"/>

    <SolidColorBrush x:Key="LightCellBackground" Color="#ffffff"/>

    <SolidColorBrush x:Key="LightCellLightBorder" Color="#ececec"/>

    <SolidColorBrush x:Key="LightCellDarkBorder" Color="#cbcbcb"/>

    <SolidColorBrush x:Key="DarkCellBackground" Color="#f1f1f1"/>

    <SolidColorBrush x:Key="DarkCellLightBorder" Color="#e1e1e1"/>

    <SolidColorBrush x:Key="DarkCellDarkBorder" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="Transparent" Color="Transparent"/>

    <SolidColorBrush x:Key="AppointmentControl.ResizeGlyph.Background" Color="#ffffff"/>

    <SolidColorBrush x:Key="AppointmentControl.Foreground" Color="#FF000000"/>

    <SolidColorBrush x:Key="HeaderControl.Foreground" Color="#FF000000"/>

    <SolidColorBrush x:Key="HeaderControl.TodayForeground" Color="#FF000000"/>

    <SolidColorBrush x:Key="CellControl.Foreground" Color="#808080"/>

    <SolidColorBrush x:Key="TimeRulerCellControl.Foreground" Color="#6685ae"/>

    <SolidColorBrush x:Key="TimeRulerHeaderControl.Foreground" Color="#6685ae"/>

    <SolidColorBrush x:Key="MoreButton.Normal.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="MoreButton.Hover.Foreground" Color="#AF3E3E3E"/>

    <SolidColorBrush x:Key="MoreButton.Pressed.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="NavigationButton.Normal.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="NavigationButton.Hover.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="NavigationButton.Pressed.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.Normal.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.Hover.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.Pressed.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.Normal.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.Hover.Foreground" Color="#AF3E3E3E"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.Pressed.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="AllDayAreaCorner.Background" Color="#f1f1f1"/>

    <SolidColorBrush x:Key="AllDayAreaCorner.BorderBrush" Color="#979797"/>

    <SolidColorBrush x:Key="CellControl.SelectionBackground"
                     Color="#FFBAD8FC"
                     Opacity="0.5"/>

    <LinearGradientBrush x:Key="MonthCellControl.TodayBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE3F7FF" Offset="0"/>
        <GradientStop Color="#FFB7E7FB" Offset="1"/>
        <GradientStop Color="#FFE3F7FF" Offset="0.5"/>
        <GradientStop Color="#FFBDEDFF" Offset="0.5"/>
    </LinearGradientBrush>

    <!-- AppointmentControl -->

    <SolidColorBrush x:Key="AppointmentControl.StatusContainer.Background" Color="White"/>

    <SolidColorBrush x:Key="AppointmentControl.StatusContainer.BorderBrush" Color="#a0a0a0"/>

    <SolidColorBrush x:Key="AppointmentControl.BorderBrush" Color="#a0a0a0"/>

    <SolidColorBrush x:Key="AppointmentControl.SelectedBorderBrush" Color="#a0a0a0"/>

    <!-- Separators -->

    <SolidColorBrush x:Key="GroupSeparator.BorderBrush" Color="#cfcfcf"/>

    <SolidColorBrush x:Key="GroupSeparator.Background" Color="#FFFFFF"/>

    <SolidColorBrush x:Key="AllDaySeparator.BorderBrush" Color="#cfcfcf"/>

    <SolidColorBrush x:Key="AllDaySeparator.Background" Color="#FFFFFF"/>

    <!-- HeaderControl -->

    <SolidColorBrush x:Key="HeaderControl.BorderBrush" Color="#979797"/>

    <LinearGradientBrush x:Key="HeaderControl.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#ffffff" Offset="0"/>
        <GradientStop Color="#f1f1f1" Offset="0.5"/>
        <GradientStop Color="#e8e8e8" Offset="0.5"/>
        <GradientStop Color="#e9e9e9" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="HeaderControl.TodayBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE3F7FF" Offset="0"/>
        <GradientStop Color="#FFB7E7FB" Offset="1"/>
        <GradientStop Color="#FFE3F7FF" Offset="0.5"/>
        <GradientStop Color="#FFBDEDFF" Offset="0.5"/>
    </LinearGradientBrush>

    <!-- Buttons & ResourceNavigator -->

    <SolidColorBrush x:Key="MoreButton.NormalBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="MoreButton.NormalBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="MoreButton.HoverBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="MoreButton.HoverBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="MoreButton.PressedBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="MoreButton.PressedBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="MoreButton.DisabledBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="MoreButton.DisabledBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="NavigationButton.NormalBorderBrush" Color="#979797"/>

    <LinearGradientBrush x:Key="NavigationButton.NormalBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFFFFFF" Offset="0"/>
        <GradientStop Color="#FFE9E9E9" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="NavigationButton.HoverBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE3F7FF" Offset="0"/>
        <GradientStop Color="#FFB7E7FB" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="NavigationButton.HoverBorderBrush" Color="#979797"/>

    <SolidColorBrush x:Key="NavigationButton.PressedBackground" Color="#FFD5D5D5"/>

    <SolidColorBrush x:Key="NavigationButton.PressedBorderBrush" Color="#979797"/>

    <SolidColorBrush x:Key="NavigationButton.DisabledBorderBrush"
                     Color="#979797"
                     Opacity="0.35"/>

    <LinearGradientBrush x:Key="NavigationButton.DisabledBackground"
                         EndPoint="0.5,1"
                         Opacity="0.35"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFFFFFF" Offset="0"/>
        <GradientStop Color="#FFE9E9E9" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ResourceNavigatorButton.NormalBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.NormalBackground" Color="Transparent"/>

    <LinearGradientBrush x:Key="ResourceNavigatorButton.HoverBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFDCF1FC" Offset="0"/>
        <GradientStop Color="#FFACDCF4" Offset="1"/>
        <GradientStop Color="#FFC1E5F6" Offset="0.5"/>
        <GradientStop Color="#FF92CEE9" Offset="0.5"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ResourceNavigatorButton.HoverBorderBrush" Color="#efefef"/>

    <LinearGradientBrush x:Key="ResourceNavigatorButton.PressedBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF9AC3DB" Offset="0"/>
        <GradientStop Color="#FF629CBE" Offset="1"/>
        <GradientStop Color="#FF81B4CF" Offset="0.5"/>
        <GradientStop Color="#FF5A99B9" Offset="0.5"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ResourceNavigatorButton.PressedBorderBrush" Color="#FF000000"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.DisabledBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.DisabledBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="ResourceNavigator.BorderBrush" Color="#979797"/>

    <LinearGradientBrush x:Key="ResourceNavigator.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFFFFFF" Offset="0"/>
        <GradientStop Color="#FFE9E9E9" Offset="1"/>
        <GradientStop Color="#FFF1F1F1" Offset="0.5"/>
        <GradientStop Color="#FFE8E8E8" Offset="0.5"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="DateNavigationPanelButton.NormalBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.NormalBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.HoverBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.HoverBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.PressedBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.PressedBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.DisabledBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.DisabledBackground" Color="Transparent"/>

    <!-- TimeRuler -->

    <SolidColorBrush x:Key="TimeIndicatorControl.BorderBrush" Color="#26c4f1"/>

    <SolidColorBrush x:Key="TimeIndicatorControl.Background" Color="#26c4f1"/>

    <SolidColorBrush x:Key="TimeMarkerControl.Background" Color="#26c4f1"/>

    <SolidColorBrush x:Key="TimeMarkerControl.BorderBrush" Color="#26c4f1"/>

    <LinearGradientBrush x:Key="TimeRulerCellControl.Delimiter.Long.Background"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#efefef" Offset="0"/>
        <GradientStop Color="#d0d0d0" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="TimeRulerCellControl.Delimiter.Short.Background"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#efefef" Offset="0"/>
        <GradientStop Color="#d0d0d0" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="TimeRulerHeaderSeparator.BorderBrush" Color="#979797"/>

    <!-- Scheduler -->

    <SolidColorBrush x:Key="Scheduler.Background" Color="#f0f0f0"/>

    <SolidColorBrush x:Key="Scheduler.Foreground" Color="#000000"/>

    <SolidColorBrush x:Key="Scheduler.Background.Legacy" Color="#f0f0f0"/>

    <SolidColorBrush x:Key="Scheduler.BorderBrush" Color="#979797"/>

    <SolidColorBrush x:Key="Scheduler.Corner.Background" Color="#efefef"/>

</ResourceDictionary>