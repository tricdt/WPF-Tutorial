                BorderThickness="1,1,0,0"
                Margin="-7,-1,-7,0"
                SnapsToDevicePixels="True"
                cs:id="Border_0000">
            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1"
                                     StartPoint="0.5,0"
                                     cs:id="LinearGradientBrush_0000">
                    <GradientStop Color="#FF98B4D0"
                                  Offset="0"
