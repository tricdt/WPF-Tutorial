                                  Width="19"/>
                            <Path Data="F1 M 285.5,240C 286.328,240 287,240.672 287,241.5C 287,242.328 286.328,243 285.5,243C 284.672,243 284,242.328 284,241.5C 284,240.672 284.672,240 285.5,240 Z M 293.5,240C 294.328,240 295,240.672 295,241.5C 295,242.329 294.328,243 293.5,243C 292.671,243 292,242.329 292,241.5C 292,240.672 292.671,240 293.5,240 Z M 289.5,240C 290.328,240 291,240.672 291,241.5C 291,242.328 290.328,243 289.5,243C 288.672,243 288,242.328 288,241.5C 288,240.672 288.671,240 289.5,240 Z M 297.5,240C 298.328,240 299,240.672 299,241.5C 299,242.328 298.328,243 297.5,243C 296.672,243 296,242.328 296,241.5C 296,240.672 296.672,240 297.5,240 Z M 301.5,240C 302.328,240 303,240.672 303,241.5C 303,242.328 302.328,243 301.5,243C 300.672,243 300,242.328 300,241.5C 300,240.672 300.672,240 301.5,240 Z"
