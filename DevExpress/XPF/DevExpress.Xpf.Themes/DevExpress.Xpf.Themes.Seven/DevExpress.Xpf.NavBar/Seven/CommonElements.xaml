                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0,0,0,0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="SideBarSingle">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0,0,0,0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="SideBarSingleScrollModeNormal">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0,0,0,0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="SideBarLast">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0,0,0,0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.CornerRadius="0,0,3,3"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="CornerRadius"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0,0,0,-1"
                                                           Storyboard.TargetName="scrollControl"
                                                           Storyboard.TargetProperty="Margin"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.CornerRadius="0,0,2,2"
                                                           Storyboard.TargetName="Background"
                                                           Storyboard.TargetProperty="CornerRadius"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="SideBarLastScrollModeNormal">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0,0,0,0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="SideBarLast0">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0,0,0,0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="ExplorerBarNoBorder">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           dxcn:ValueSetter.CornerRadius="0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="CornerRadius"/>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           dxcn:ValueSetter.Thickness="0,0,0,0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0"
                                                           Storyboard.TargetName="scrollControl"
                                                           Storyboard.TargetProperty="Margin"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.CornerRadius="0"
                                                           Storyboard.TargetName="Background"
                                                           Storyboard.TargetProperty="CornerRadius"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="SideBarNoBorder">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           dxcn:ValueSetter.CornerRadius="0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="CornerRadius"/>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           dxcn:ValueSetter.Thickness="0,0,0,0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0"
                                                           Storyboard.TargetName="scrollControl"
                                                           Storyboard.TargetProperty="Margin"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.CornerRadius="0"
                                                           Storyboard.TargetName="Background"
                                                           Storyboard.TargetProperty="CornerRadius"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="NavigationPaneNoBorder">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           dxcn:ValueSetter.CornerRadius="0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="CornerRadius"/>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           dxcn:ValueSetter.Thickness="0,0,0,0"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0"
                                                           Storyboard.TargetName="scrollControl"
                                                           Storyboard.TargetProperty="Margin"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.CornerRadius="0"
                                                           Storyboard.TargetName="Background"
                                                           Storyboard.TargetProperty="CornerRadius"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <dx:LayoutTransformPanel Orientation="{Binding Path=NavBar.View.Orientation}">
                <Border x:Name="Border"
                        Background="#FF979797"
                        Padding="1,0,1,1">
                    <Border x:Name="Background" Background="#FFFFFFFF"/>
                </Border>
            </dx:LayoutTransformPanel>
            <dxn:ScrollControl x:Name="scrollControl"
                               dx:FocusHelper2.Focusable="False"
                               ScrollBarControlTemplate="{DynamicResource {dxnt:CommonElementsThemeKey ResourceKey=NormalScrollTemplate}}"
                               ScrollButtonsControlTemplate="{DynamicResource {dxnt:CommonElementsThemeKey ResourceKey=SmoothScrollTemplate}}"
                               ScrollViewer.HorizontalScrollBarVisibility="{Binding RelativeSource={RelativeSource TemplatedParent}, 
                Path=NavBarViewKind, Converter={StaticResource NavBarViewKindToScrollBarVisibilityConverter}}">
                <dx:LayoutTransformPanel Orientation="{Binding Path=NavBar.View.Orientation}">
                    <!--ONLY SL<dx:BackgroundPanel.PatchForeground>True</dx:BackgroundPanel.PatchForeground>ONLY SL-->
                    <ContentPresenter x:Name="Content"
                                      Content="{TemplateBinding Content}"
                                      ContentTemplate="{TemplateBinding ContentTemplate}"/>
                </dx:LayoutTransformPanel>
            </dxn:ScrollControl>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxnt:CommonElementsThemeKey ResourceKey=NavBarGroupItemsControlTemplate}" TargetType="{x:Type dxn:NavBarGroupItemsControl}">
        <Border x:Name="Border" Background="#FF979797">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="BorderStates">
                    <VisualState x:Name="Hidden">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Border"
                                            Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                            To="Transparent"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="ExplorerBar">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Border"
                                            Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                            To="Transparent"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="NavigationPane">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           dxcn:ValueSetter.CornerRadius="3"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="CornerRadius"/>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           dxcn:ValueSetter.Thickness="1"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="SideBar">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           dxcn:ValueSetter.CornerRadius="3"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="CornerRadius"/>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           dxcn:ValueSetter.Thickness="1"
                                                           Storyboard.TargetName="Border"
                                                           Storyboard.TargetProperty="Padding"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <ItemsPresenter/>
        </Border>
    </ControlTemplate>

    <Thickness x:Key="{dxnt:CommonElementsThemeKey ResourceKey=GroupContentMargin}">0</Thickness>

    <Thickness x:Key="{dxnt:NavBarGroupControlThemeKey ResourceKey=NavBarItemsControlMargin}">1</Thickness>

    <dxn:FontSettings x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewItemFontSettings}"/>

    <dxn:FontSettings x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewGroupFontSettings}"/>

    <dxn:ImageSettings x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewItemImageSettings}"
                       Height="16"
                       Stretch="Uniform"
                       StretchDirection="Both"
                       Width="16"/>

    <dxn:ImageSettings x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewGroupImageSettings}"
                       Height="24"
                       Stretch="Uniform"
                       StretchDirection="Both"
                       Width="24"/>

    <dxn:LayoutSettings x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewItemLayoutSettings}"
                        ImageDocking="Left"
                        ImageHorizontalAlignment="Left"
                        ImageVerticalAlignment="Center"
                        TextHorizontalAlignment="Stretch"
                        TextVerticalAlignment="Center"/>

    <dxn:LayoutSettings x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewGroupLayoutSettings}"
                        ImageDocking="Left"
                        ImageHorizontalAlignment="Left"
                        ImageVerticalAlignment="Center"
                        TextHorizontalAlignment="Stretch"
                        TextVerticalAlignment="Center"/>

    <dxn:DisplayMode x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewItemDisplayMode}">ImageAndText</dxn:DisplayMode>

    <dxn:DisplayMode x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ViewGroupDisplayMode}">ImageAndText</dxn:DisplayMode>

    <SolidColorBrush x:Key="{dxnt:CommonElementsThemeKey ResourceKey=GroupForegroundBrush}" Color="#FF000000"/>

    <SolidColorBrush x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ItemForegroundBrush}" Color="#FF000000"/>

    <SolidColorBrush x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ItemForegroundBrushInPopup}" Color="Black"/>

    <SolidColorBrush x:Key="{dxnt:NavigationPaneButtonPanelThemeKey ResourceKey=GroupForegroundBrush}" Color="#FF4C535C"/>

    <SolidColorBrush x:Key="{dxnt:NavigationPaneGroupClientThemeKey ResourceKey=CollapsedGroupForegroundBrush}" Color="#FF000000"/>

</ResourceDictionary>