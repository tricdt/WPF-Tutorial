<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxsat="http://schemas.devexpress.com/winfx/2008/xaml/sankey/themekeys"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=PaddingValue}">16</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=BorderThickness}">1</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=ToolTipContentMargin}">7</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TitleMargin}">0,0,0,10</Thickness>

    <sys:Double x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TitleFontSize}">21</sys:Double>

    <sys:Double x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TextElementFontSize}">16</sys:Double>

    <FontWeight x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=NodeLabelFontWeight}">Bold</FontWeight>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=BackgroundBrush}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=BorderBrush}" Color="#FF979797"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipForeground}" Color="#FF000000"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipBackground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipBorder}" Color="#5AFFFFFF"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=TitleForeground}" Color="Black"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=TextElementForeground}" Color="Black"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=NodeLabelForeground}" Color="#FF000000"/>

</ResourceDictionary>