<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
                    xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=SizeGripContainerTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FFD6D6D6"
                BorderThickness="0,1,0,0"
                cs:Name="Border_0002"
                Height="16"
                cs:Office2013DarkGray.Background="#FFE3E3E3"
                cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                cs:Office2013LightGray.Background="#FFF6F6F6"
                cs:Office2013LightGray.BorderBrush="#FFABABAB"
                cs:Office2016Black.Background="#FFB2B2B2"
                cs:Office2016Black.BorderBrush="#FF444444"
                cs:Office2016Colorful.Background="#FFF0F0F0"
                cs:Office2016Colorful.BorderBrush="#FFA6A6A6"
                cs:Office2016White.Background="#FFFFFFFF"
                cs:Office2016White.BorderBrush="#FFD6D6D6"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <Style x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryScrollBarContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Margin" Value="1,0,0,0"/>
    </Style>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=MenuBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FFD6D6D6"
                BorderThickness="0,1,0,0"
                cs:Name="Border_0004"
                cs:Office2013DarkGray.Background="#FFE3E3E3"
                cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                cs:Office2013LightGray.Background="#FFF6F6F6"
                cs:Office2013LightGray.BorderBrush="#FFABABAB"
                cs:Office2016Black.Background="#FFD4D4D4"
                cs:Office2016Black.BorderBrush="#FF444444"
                cs:Office2016Colorful.Background="#FFFFFFFF"
                cs:Office2016Colorful.BorderBrush="#FFA6A6A6"
                cs:Office2016White.Background="#FFFFFFFF"
                cs:Office2016White.BorderBrush="#FFD6D6D6"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=MenuGlyphSideBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                cs:Name="Border_0005"
                cs:Office2013DarkGray.Background="#FFE3E3E3"
                cs:Office2013LightGray.Background="#FFF6F6F6"
                cs:Office2016Black.Background="#FFD4D4D4"
                cs:Office2016Colorful.Background="#FFFFFFFF"
                cs:Office2016White.Background="#FFFFFFFF"
               />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=MenuContentSideBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                cs:Name="Border_0006"
                cs:Office2013DarkGray.Background="#FFE3E3E3"
                cs:Office2013LightGray.Background="#FFF6F6F6"
                cs:Office2016Black.Background="#FFD4D4D4"
                cs:Office2016Colorful.Background="#FFFFFFFF"
                cs:Office2016White.Background="#FFFFFFFF"
               />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=ThumbForBothResizeModeTemplate}" TargetType="{x:Type Thumb}">
        <Border Background="Transparent"
                cs:Name="Border_0015"
                HorizontalAlignment="Stretch"
                Margin="0,1,2,1"
                SnapsToDevicePixels="True"
                cs:Office2013DarkGray.Background="Transparent"
                cs:Office2013LightGray.Background="Transparent"
                cs:Office2016Black.Background="Transparent"
                cs:Office2016Colorful.Background="Transparent"
                cs:Office2016White.Background="Transparent"
               >
            <Grid Height="7"
                  HorizontalAlignment="Right"
                  VerticalAlignment="Bottom"
                  Width="6">
                <Rectangle cs:Name="Rectangle_0001"
                           Fill="#FFD6D6D6"
                           Height="2"
                           HorizontalAlignment="Left"
                           Margin="0,0,0,1"
                           Stretch="Fill"
                           VerticalAlignment="Bottom"
                           Width="2"
                           cs:Office2013DarkGray.Fill="#FFD6D6D6"
                           cs:Office2013LightGray.Fill="#FFD6D6D6"
                           cs:Office2016Black.Fill="#87000000"
                           cs:Office2016Colorful.Fill="#87000000"
                           cs:Office2016White.Fill="#FFD6D6D6"
                          />
                <Rectangle cs:Name="Rectangle_0002"
                           Fill="#FFD6D6D6"
                           Height="2"
                           HorizontalAlignment="Right"
                           Margin="0,0,0,1"
                           Stretch="Fill"
                           VerticalAlignment="Bottom"
                           Width="2"
                           cs:Office2013DarkGray.Fill="#FFD6D6D6"
                           cs:Office2013LightGray.Fill="#FFD6D6D6"
                           cs:Office2016Black.Fill="#87000000"
                           cs:Office2016Colorful.Fill="#87000000"
                           cs:Office2016White.Fill="#FFD6D6D6"
                          />
                <Rectangle cs:Name="Rectangle_0003"
                           Fill="#FFD6D6D6"
                           Height="2"
                           HorizontalAlignment="Right"
                           Stretch="Fill"
                           VerticalAlignment="Top"
                           Width="2"
                           cs:Office2013DarkGray.Fill="#FFD6D6D6"
                           cs:Office2013LightGray.Fill="#FFD6D6D6"
                           cs:Office2016Black.Fill="#87000000"
                           cs:Office2016Colorful.Fill="#87000000"
                           cs:Office2016White.Fill="#FFD6D6D6"
                          />
                <ContentPresenter/>
            </Grid>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=ThumbForVertResizeModeTemplate}" TargetType="{x:Type Thumb}">
        <Border Background="#00000000"
                cs:Name="Border_0007"
                Cursor="SizeNS"
                HorizontalAlignment="Center"
                SnapsToDevicePixels="True"
                VerticalAlignment="Center"
                cs:Office2013DarkGray.Background="#00000000"
                cs:Office2013LightGray.Background="#00000000"
                cs:Office2016Black.Background="#00000000"
                cs:Office2016Colorful.Background="#00000000"
                cs:Office2016White.Background="#00000000"
               >
            <Grid Margin="2">
                <Path cs:Name="Path_0002"
                      Data="F1 M 285.5,240C 286.328,240 287,240.672 287,241.5C 287,242.328 286.328,243 285.5,243C 284.672,243 284,242.328 284,241.5C 284,240.672 284.672,240 285.5,240 Z M 293.5,240C 294.328,240 295,240.672 295,241.5C 295,242.329 294.328,243 293.5,243C 292.671,243 292,242.329 292,241.5C 292,240.672 292.671,240 293.5,240 Z M 289.5,240C 290.328,240 291,240.672 291,241.5C 291,242.328 290.328,243 289.5,243C 288.672,243 288,242.328 288,241.5C 288,240.672 288.671,240 289.5,240 Z M 297.5,240C 298.328,240 299,240.672 299,241.5C 299,242.328 298.328,243 297.5,243C 296.672,243 296,242.328 296,241.5C 296,240.672 296.672,240 297.5,240 Z M 301.5,240C 302.328,240 303,240.672 303,241.5C 303,242.328 302.328,243 301.5,243C 300.672,243 300,242.328 300,241.5C 300,240.672 300.672,240 301.5,240 Z"
                      Fill="#FFD6D6D6"
                      Height="3"
                      Margin="0,0,1,1"
                      Stretch="Fill"
                      UseLayoutRounding="False"
                      Width="19"
                      cs:Office2013DarkGray.Fill="#FFABABAB"
                      cs:Office2013LightGray.Fill="#FFABABAB"
                      cs:Office2016Black.Fill="#BA000000"
                      cs:Office2016Colorful.Fill="#87000000"
                      cs:Office2016White.Fill="#FFD6D6D6"
                     />
            </Grid>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                cs:Name="Border_0008"
                cs:Office2013DarkGray.Background="#FFFFFFFF"
                cs:Office2013LightGray.Background="#FFFFFFFF"
                cs:Office2016Black.Background="#FFD4D4D4"
                cs:Office2016Colorful.Background="#FFFFFFFF"
                cs:Office2016White.Background="#FFFFFFFF"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryGroupsBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <ContentPresenter/>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryCaptionTemplate}" TargetType="{x:Type ToggleButton}">
        <Border Background="#FFB1D6F0"
                cs:Name="Border_0009"
                Margin="1"
                cs:Office2013DarkGray.Background="#5F2592DA"
                cs:Office2013LightGray.Background="#5F2592DA"
                cs:Office2016Black.Background="#FF969696"
                cs:Office2016Colorful.Background="#FFA6A6A6"
                cs:Office2016White.Background="#FFB1D6F0"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryItemBorderTemplate}" TargetType="{x:Type dxb:ItemBorderControl}">
        <Grid Background="#00000000"
              cs:Name="Grid_0001"
              SnapsToDevicePixels="True"
              cs:Office2013DarkGray.Background="#00000000"
              cs:Office2013LightGray.Background="#00000000"
              cs:Office2016Black.Background="#00000000"
              cs:Office2016Colorful.Background="#00000000"
              cs:Office2016White.Background="#00000000"
             >
            <Border x:Name="HoverState"
                    Background="#FFCDE6F7"
                    cs:Name="Border_0010"
                    Opacity="0"
                    cs:Office2013DarkGray.Background="#3F009AFF"
                    cs:Office2013LightGray.Background="#3F009AFF"
                    cs:Office2016Black.Background="#30000000"
                    cs:Office2016Colorful.Background="#30000000"
                    cs:Office2016White.Background="#FFCDE6F7"
                   />
            <Border x:Name="PressedState"
                    Background="#FF92C0E0"
                    cs:Name="Border_0011"
                    Opacity="0"
                    cs:Office2013DarkGray.Background="#5F2592DA"
                    cs:Office2013LightGray.Background="#5F2592DA"
                    cs:Office2016Black.Background="#60000000"
                    cs:Office2016Colorful.Background="#47000000"
                    cs:Office2016White.Background="#FF92C0E0"
                   />
            <Border x:Name="CheckedState"
                    Background="#FFB1D6F0"
                    cs:Name="Border_0012"
                    Opacity="0"
                    cs:Office2013DarkGray.Background="#5F2592DA"
                    cs:Office2013LightGray.Background="#5F2592DA"
                    cs:Office2016Black.Background="#60000000"
                    cs:Office2016Colorful.Background="#47000000"
                    cs:Office2016White.Background="#FFB1D6F0"
                   />
            <ContentPresenter/>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="State">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed"/>
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="CheckedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="HoverChecked">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=GalleryItemGlyphBorderTemplate}" TargetType="{x:Type dxb:ItemBorderControl}">
        <Border Name="Root"
                BorderBrush="#00000000"
                cs:Name="Border_0013"
                cs:Office2013DarkGray.BorderBrush="#00000000"
                cs:Office2013LightGray.BorderBrush="#00000000"
                cs:Office2016Black.BorderBrush="#00000000"
                cs:Office2016Colorful.BorderBrush="#00000000"
                cs:Office2016White.BorderBrush="#00000000"
               >
            <ContentPresenter/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="State" Value="Checked">
                <Setter cs:Name="Setter_0001"
                        Property="BorderBrush"
                        TargetName="Root"
                        Value="#FF0072C6"
                        cs:Office2013DarkGray.Value="#FF333333"
                        cs:Office2013LightGray.Value="#FF333333"
                        cs:Office2016Black.Value="#FF444444"
                        cs:Office2016Colorful.Value="#FF0173C7"
                        cs:Office2016White.Value="#FF0072C6"
                       />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:GalleryDropDownControlThemeKey ResourceKey=PopupBorderTemplate}" TargetType="{x:Type dxr:GalleryDropDownPopupBorderControl}">
        <Grid x:Name="Root" SnapsToDevicePixels="True">
            <Border x:Name="Border"
                    Background="#FFD6D6D6"
                    cs:Name="Border_0014"
                    Padding="1"
                    cs:Office2013DarkGray.Background="#FFABABAB"
                    cs:Office2013LightGray.Background="#FFABABAB"
                    cs:Office2016Black.Background="#FF444444"
                    cs:Office2016Colorful.Background="#FFA6A6A6"
                    cs:Office2016White.Background="#FFD6D6D6"
                   >
                <ContentPresenter Content="{TemplateBinding Content}"/>
            </Border>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="AppType">
                    <VisualState x:Name="Standalone">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0,0,3,3</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border" Storyboard.TargetProperty="CornerRadius">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <CornerRadius>0</CornerRadius>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Root" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0,0,3,3</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="BrowserHosted"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

</ResourceDictionary>