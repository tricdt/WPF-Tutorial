<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=LeftPartTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FF0072C6"
                BorderBrush="#FF0072C6"
                BorderThickness="0,1,0,0"
                cs:Name="Border_0001"
                cs:Office2013DarkGray.Background="#FF333333"
                cs:Office2013DarkGray.BorderBrush="#FF333333"
                cs:Office2013LightGray.Background="#FF19478A"
                cs:Office2013LightGray.BorderBrush="#FF19478A"
                cs:Office2016Black.Background="#FF595959"
                cs:Office2016Black.BorderBrush="#FF444444"
                cs:Office2016Colorful.Background="#FFF0F0F0"
                cs:Office2016Colorful.BorderBrush="#FFC5C5C5"
                cs:Office2016White.Background="#FF0072C6"
                cs:Office2016White.BorderBrush="#FF0072C6"
               >
            <ContentPresenter Margin="1,0,0,0"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=RightPartTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FF0072C6"
                BorderBrush="#FF0072C6"
                BorderThickness="0,1,0,0"
                cs:Name="Border_0002"
                cs:Office2013DarkGray.Background="#FF333333"
                cs:Office2013DarkGray.BorderBrush="#FF333333"
                cs:Office2013LightGray.Background="#FF19478A"
                cs:Office2013LightGray.BorderBrush="#FF19478A"
                cs:Office2016Black.Background="#FF595959"
                cs:Office2016Black.BorderBrush="#FF444444"
                cs:Office2016Colorful.Background="#FFF0F0F0"
                cs:Office2016Colorful.BorderBrush="#FFC5C5C5"
                cs:Office2016White.Background="#FF0072C6"
                cs:Office2016White.BorderBrush="#FF0072C6"
               >
            <ContentPresenter Margin="1,0,0,0"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=SizeGripTemplate}" TargetType="{x:Type Thumb}">
        <Grid Background="#00000000"
              cs:Name="Grid_0001"
              HorizontalAlignment="Right"
              Margin="1"
              SnapsToDevicePixels="True"
              VerticalAlignment="Bottom"
              cs:Office2013DarkGray.Background="#00000000"
              cs:Office2013LightGray.Background="#00000000"
              cs:Office2016Black.Background="#00000000"
              cs:Office2016Colorful.Background="#00000000"
              cs:Office2016White.Background="#00000000"
             >
            <Path cs:Name="Path_0001"
                  Data="F1 M 671,130L 673,130L 673,132L 671,132L 671,130 Z M 674,130L 676,130L 676,132L 674,132L 674,130 Z M 677,130L 679,130L 679,132L 677,132L 677,130 Z M 677,127L 679,127L 679,129L 677,129L 677,127 Z M 677,124L 679,124L 679,126L 677,126L 677,124 Z M 674,127L 676,127L 676,129L 674,129L 674,127 Z "
                  Fill="#FFFFFFFF"
                  Height="8"
                  Stretch="Fill"
                  Width="8"
                  cs:Office2013DarkGray.Fill="#FFE3E3E3"
                  cs:Office2013LightGray.Fill="#FFF6F6F6"
                  cs:Office2016Black.Fill="#E3FFFFFF"
                  cs:Office2016Colorful.Fill="#87000000"
                  cs:Office2016White.Fill="#FFFFFFFF"
                 />
        </Grid>
    </ControlTemplate>

    <sys:Double x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=MinHeight}">26</sys:Double>

</ResourceDictionary>