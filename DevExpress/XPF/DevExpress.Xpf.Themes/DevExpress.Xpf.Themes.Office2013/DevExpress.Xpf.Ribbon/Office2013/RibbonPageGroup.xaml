<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
                    xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys"
                    xmlns:interop="clr-namespace:System.Windows.Interop;assembly=PresentationFramework"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:syscm="clr-namespace:System.ComponentModel;assembly=PresentationFramework"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <sys:Double x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupVerticalOffset}">0</sys:Double>

    <sys:Double x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupHorizontalOffset}">0</sys:Double>

    <Thickness x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupBorderMargin}">0,20,0,20</Thickness>

    <Thickness x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=ContentMargin}">2,2,2,0</Thickness>

    <Style x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=ContentStyle}" TargetType="{x:Type ItemsPresenter}">
        <Setter Property="Margin" Value="{StaticResource {dxrt:RibbonPageGroupThemeKey ResourceKey=ContentMargin}}"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding Path=IsTabletMode, RelativeSource={RelativeSource AncestorType={x:Type dxr:RibbonPageGroupControl}}}" Value="True">
                <Setter Property="Margin" Value="2"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=CaptionButtonStyle}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
        <Setter Property="VerticalAlignment" Value="Bottom"/>
        <Setter Property="Margin" Value="0,0,-1,-1"/>
    </Style>

    <Style x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=CaptionPanelStyle}" TargetType="{x:Type dxb:SplitLayoutPanel}">
        <Style.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="LeftContent1Margin" Value="12,0,12,0"/>
                <Setter Property="SingleContent1Margin" Value="12,0,12,0"/>
            </Trigger>
        </Style.Triggers>
        <Setter Property="MaximizeContent1" Value="True"/>
        <Setter Property="LeftContent1Margin" Value="4,0,0,2"/>
        <Setter Property="RightContent2Margin" Value="0,0,1,2"/>
        <Setter Property="SingleContent1Margin" Value="4,0,4,2"/>
    </Style>

    <dxb:FontSettings x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupButtonFontSettings}"/>

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
        <Grid SnapsToDevicePixels="True">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <ContentPresenter Grid.Column="0"/>
            <TextBlock Name="PART_DesignTimeCaption"
                       Grid.Column="1"
                       Margin="2,0"
                       Text="[PageGroup]"
                       VerticalAlignment="Center"
                       Visibility="Collapsed"/>
            <Border x:Name="Separator"
                    Background="#FFD6D6D6"
                    cs:Name="Border_0001"
                    Grid.Column="2"
                    Margin="0,4,0,5"
                    Width="1"
                    cs:Office2013DarkGray.Background="#FFABABAB"
                    cs:Office2013LightGray.Background="#FFABABAB"
                    cs:Office2016Black.Background="#36000000"
                    cs:Office2016Colorful.Background="#FFC4C4C4"
                    cs:Office2016White.Background="#FFD6D6D6"
                   />
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=IsDesignTimeCaptionVisible, RelativeSource={RelativeSource AncestorType=dxr:RibbonPageGroupControl}}" Value="True">
                <Setter Property="Visibility"
                        TargetName="PART_DesignTimeCaption"
                        Value="Visible"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=IsSeparatorVisible, RelativeSource={RelativeSource AncestorType=dxr:RibbonPageGroupControl}}" Value="False">
                <Setter Property="Visibility"
                        TargetName="Separator"
                        Value="Collapsed"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=BorderTemplateInPopup}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FFD6D6D6"
                BorderThickness="1"
                cs:Name="Border_0003"
                cs:Office2013DarkGray.Background="#FFF3F3F3"
                cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                cs:Office2013LightGray.Background="#FFFAFAFA"
                cs:Office2013LightGray.BorderBrush="#FFABABAB"
                cs:Office2016Black.Background="#FFD4D4D4"
                cs:Office2016Black.BorderBrush="#FF444444"
                cs:Office2016Colorful.Background="#FFFFFFFF"
                cs:Office2016Colorful.BorderBrush="#FFA6A6A6"
                cs:Office2016White.Background="#FFFFFFFF"
                cs:Office2016White.BorderBrush="#FFD6D6D6"
               >
            <ContentPresenter/>
        </Border>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(interop:BrowserInteropHelper.IsBrowserHosted)}" Value="True"/>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=CaptionButtonTemplate}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
        <Grid x:Name="Grid1"
              Background="#00000000"
              cs:Name="Grid_0001"
              Height="15"
              SnapsToDevicePixels="True"
              Width="15"
              cs:Office2013DarkGray.Background="#00000000"
              cs:Office2013LightGray.Background="#00000000"
              cs:Office2016Black.Background="#00000000"
              cs:Office2016Colorful.Background="#00000000"
              cs:Office2016White.Background="#00000000"
             >
            <Border x:Name="HoverState"
                    Background="#FFCDE6F7"
                    cs:Name="Border_0004"
                    Opacity="0"
                    cs:Office2013DarkGray.Background="#3F009AFF"
                    cs:Office2013LightGray.Background="#3F009AFF"
                    cs:Office2016Black.Background="#30000000"
                    cs:Office2016Colorful.Background="#30000000"
                    cs:Office2016White.Background="#FFCDE6F7"
                   />
            <Border x:Name="PressedState"
                    Background="#FF92C0E0"
                    cs:Name="Border_0005"
                    Opacity="0"
                    cs:Office2013DarkGray.Background="#5F2592DA"
                    cs:Office2013LightGray.Background="#5F2592DA"
                    cs:Office2016Black.Background="#60000000"
                    cs:Office2016Colorful.Background="#47000000"
                    cs:Office2016White.Background="#FF92C0E0"
                   />
            <Path x:Name="Glyph"
                  cs:Name="Path_0001"
                  Data="F1 M 279,86L 281,86L 281,85L 282,85L 282,86L 282,88L 282,89L 279,89L 278,89L 278,88L 279,88L 279,86L 278,86L 278,85L 279,85L 279,86 Z M 275,88.0002L 275,82.0002L 276,82.0002L 276,82.0001L 281,82.0001L 281,83.0001L 276,83.0001L 276,88.0002L 275,88.0002 Z "
                  Fill="#FF666666"
                  Height="7"
                  Margin="2"
                  Stretch="Fill"
                  Width="7"
                  cs:Office2013DarkGray.Fill="#FF666666"
                  cs:Office2013LightGray.Fill="#FF666666"
                  cs:Office2016Black.Fill="#BA000000"
                  cs:Office2016Colorful.Fill="#87000000"
                  cs:Office2016White.Fill="#FF666666"
                 />
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF2A8DD4"
                                            cs:Office2013DarkGray.To="#FF2B579A"
                                            cs:Office2013LightGray.To="#FF19478A"
                                            cs:Office2016Black.To="#BA000000"
                                            cs:Office2016Colorful.To="#BA000000"
                                            cs:Office2016White.To="#FF2A8DD4"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF0072C6"
                                            cs:Office2013DarkGray.To="#FF2B579A"
                                            cs:Office2013LightGray.To="#FF19478A"
                                            cs:Office2016Black.To="#BA000000"
                                            cs:Office2016Colorful.To="#BA000000"
                                            cs:Office2016White.To="#FF0072C6"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0003"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF0072C6"
                                            cs:Office2013DarkGray.To="#FF2B579A"
                                            cs:Office2013LightGray.To="#FF19478A"
                                            cs:Office2016Black.To="#BA000000"
                                            cs:Office2016Colorful.To="#BA000000"
                                            cs:Office2016White.To="#FF0072C6"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0004"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF2A8DD4"
                                            cs:Office2013DarkGray.To="#FF2B579A"
                                            cs:Office2013LightGray.To="#FF19478A"
                                            cs:Office2016Black.To="#BA000000"
                                            cs:Office2016Colorful.To="#BA000000"
                                            cs:Office2016White.To="#FF2A8DD4"
                                           />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers></ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=CaptionBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Grid Margin="-1,0,-1,0">
            <Border x:Name="Separator"
                    Background="#FFFFFFFF"
                    cs:Name="Border_0017"
                    Opacity="0"
                    cs:Office2013DarkGray.Background="#FFFFFFFF"
                    cs:Office2013LightGray.Background="#FFFFFFFF"
                    cs:Office2016Black.Background="#FFD4D4D4"
                    cs:Office2016Colorful.Background="#FFFFFFFF"
                    cs:Office2016White.Background="#FFFFFFFF"
                   >
                <Border BorderBrush="#FFD6D6D6"
                        BorderThickness="0,1,0,0"
                        cs:Name="Border_0018"
                        Margin="2,0,2,0"
                        cs:Office2013DarkGray.BorderBrush="#FFD6D6D6"
                        cs:Office2013LightGray.BorderBrush="#FFD6D6D6"
                        cs:Office2016Black.BorderBrush="#FF444444"
                        cs:Office2016Colorful.BorderBrush="#FFA6A6A6"
                        cs:Office2016White.BorderBrush="#FFD6D6D6"
                       />
            </Border>
            <ContentPresenter Content="{TemplateBinding Content}" Margin="1"/>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(dxr:RibbonControl.Ribbon), RelativeSource={RelativeSource Self}}" Value="OfficeSlim">
                <Setter Property="Opacity"
                        TargetName="Separator"
                        Value="1"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupButtonBorderTemplate}" TargetType="{x:Type dxb:ItemBorderControl}">
        <Grid SnapsToDevicePixels="True">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Grid Grid.Column="0" Grid.ColumnSpan="1">
                <Grid x:Name="CustomizationState"
                      Opacity="0.0"
                      Visibility="Visible">
                    <Border BorderBrush="#FF0072C6"
                            BorderThickness="2"
                            CornerRadius="3"
                            cs:Name="Border_0006"
                            cs:Office2013DarkGray.BorderBrush="#FF333333"
                            cs:Office2013LightGray.BorderBrush="#FF333333"
                            cs:Office2016Black.BorderBrush="#FF444444"
                            cs:Office2016Colorful.BorderBrush="#FF0173C7"
                            cs:Office2016White.BorderBrush="#FF0072C6"
                           />
                </Grid>
                <Grid x:Name="NormalState" Opacity="0">
                    <Border BorderBrush="#FFD6D6D6"
                            BorderThickness="0,0,1,0"
                            cs:Name="Border_0007"
                            Margin="0,4,0,4"
                            cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                            cs:Office2013LightGray.BorderBrush="#FFABABAB"
                            cs:Office2016Black.BorderBrush="#00000000"
                            cs:Office2016Colorful.BorderBrush="#00000000"
                            cs:Office2016White.BorderBrush="#FFD6D6D6"
                           />
                </Grid>
                <Grid x:Name="HoverState" Opacity="0">
                    <Border Background="#FFCDE6F7"
                            cs:Name="Border_0008"
                            cs:Office2013DarkGray.Background="#3F009AFF"
                            cs:Office2013LightGray.Background="#3F009AFF"
                            cs:Office2016Black.Background="#30000000"
                            cs:Office2016Colorful.Background="#30000000"
                            cs:Office2016White.Background="#FFCDE6F7"
                           />
                </Grid>
                <Grid x:Name="PressedState" Opacity="0">
                    <Border Background="#FF92C0E0"
                            cs:Name="Border_0009"
                            cs:Office2013DarkGray.Background="#5F2592DA"
                            cs:Office2013LightGray.Background="#5F2592DA"
                            cs:Office2016Black.Background="#60000000"
                            cs:Office2016Colorful.Background="#47000000"
                            cs:Office2016White.Background="#FF92C0E0"
                           />
                </Grid>
                <ContentPresenter Content="{TemplateBinding Content}"/>
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="State">
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="NormalState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="HoverChecked">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Focused"/>
                    <VisualState x:Name="Customization">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="CustomizationState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupControlTemplate}" TargetType="{x:Type ContentControl}">
        <ContentPresenter/>
    </ControlTemplate>

    <Style x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=TextSplitterStyle}" TargetType="{x:Type dxb:TextSplitterControl}">
