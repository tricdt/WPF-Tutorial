<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:interop="clr-namespace:System.Windows.Interop;assembly=PresentationFramework"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                cs:Name="Border_0001"
                cs:Office2013DarkGray.Background="#FFE3E3E3"
                cs:Office2013LightGray.Background="#FFF6F6F6"
                cs:Office2016Black.Background="#FFB2B2B2"
                cs:Office2016Colorful.Background="#FFFFFFFF"
                cs:Office2016White.Background="#FFFFFFFF"
               />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBarControlBorderTemplate2007}" TargetType="{x:Type ContentControl}">
        <Grid SnapsToDevicePixels="True">
            <Border x:Name="Border"
                    Background="#FF0072C6"
                    BorderBrush="#FF0072C6"
                    BorderThickness="2"
                    cs:Name="Border_0003"
                    cs:Office2013DarkGray.Background="#FF333333"
                    cs:Office2013DarkGray.BorderBrush="#FF333333"
                    cs:Office2013LightGray.Background="#FF19478A"
                    cs:Office2013LightGray.BorderBrush="#FF19478A"
                    cs:Office2016Black.Background="#FF262626"
                    cs:Office2016Black.BorderBrush="#FF262626"
                    cs:Office2016Colorful.Background="#FF0173C7"
                    cs:Office2016Colorful.BorderBrush="#FF0173C7"
                    cs:Office2016White.Background="#FF0072C6"
                    cs:Office2016White.BorderBrush="#FF0072C6"
                   >
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" MinHeight="23"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Border Background="#FFFFFFFF"
                            cs:Name="Border_0005"
                            Grid.Row="1"
                            cs:Office2013DarkGray.Background="#FFE3E3E3"
                            cs:Office2013LightGray.Background="#FFF6F6F6"
                            cs:Office2016Black.Background="#FFB2B2B2"
                            cs:Office2016Colorful.Background="#FFFFFFFF"
                            cs:Office2016White.Background="#FFFFFFFF"
                           >
                        <ContentPresenter/>
                    </Border>
                </Grid>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(interop:BrowserInteropHelper.IsBrowserHosted)}" Value="True"></DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBottomPaneTemplate}" TargetType="{x:Type ContentControl}">
        <Border x:Name="Footer"
                Background="#FF0072C6"
                BorderBrush="#FF0072C6"
                BorderThickness="0,1,0,0"
                cs:Name="Border_0007"
                cs:Office2013DarkGray.Background="#FF333333"
                cs:Office2013DarkGray.BorderBrush="#FF333333"
                cs:Office2013LightGray.Background="#FF19478A"
                cs:Office2013LightGray.BorderBrush="#FF19478A"
                cs:Office2016Black.Background="#FF262626"
                cs:Office2016Black.BorderBrush="#FF262626"
                cs:Office2016Colorful.Background="#FF0173C7"
                cs:Office2016Colorful.BorderBrush="#FF0173C7"
                cs:Office2016White.Background="#FF0072C6"
                cs:Office2016White.BorderBrush="#FF0072C6"
               >
            <ContentPresenter HorizontalAlignment="Right"
                              Margin="4"
                              SnapsToDevicePixels="True"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuRightPaneTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FFD6D6D6"
                BorderThickness="1,0,0,0"
                cs:Name="Border_0009"
                HorizontalAlignment="Stretch"
                cs:Office2013DarkGray.Background="#FFE3E3E3"
                cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                cs:Office2013LightGray.Background="#FFF6F6F6"
                cs:Office2013LightGray.BorderBrush="#FFABABAB"
                cs:Office2016Black.Background="#FFB2B2B2"
                cs:Office2016Black.BorderBrush="#36000000"
                cs:Office2016Colorful.Background="#FFFFFFFF"
                cs:Office2016Colorful.BorderBrush="#FFC4C4C4"
                cs:Office2016White.Background="#FFFFFFFF"
                cs:Office2016White.BorderBrush="#FFD6D6D6"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <Thickness x:Key="{dxrt:BackstageThemeKey ResourceKey=BorderThickness}">1,1,1,1</Thickness>

    <Thickness x:Key="{dxrt:BackstageThemeKey ResourceKey=BorderThicknessNoFullscreen}">0,1,0,0</Thickness>

    <ControlTemplate x:Key="{dxrt:BackstageThemeKey ResourceKey=BorderTemplate}">
        <Border Background="{DynamicResource {dxt:ThemedWindowThemeKey ResourceKey=WindowActiveBorderBrush}}" SnapsToDevicePixels="True"/>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuContentControlBorderTemplate}" TargetType="{x:Type ContentControl}">
        <ContentPresenter Content="{TemplateBinding Content}"/>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBarControlBorderTemplate2010}" TargetType="{x:Type ContentControl}">
        <ContentControl Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBarControlBorderTemplate2007}}">
            <ContentPresenter/>
        </ContentControl>
    </ControlTemplate>

    <SolidColorBrush x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuRightPaneForeground}"
                     Color="#FF444444"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2013DarkGray.Color="#FF444444"
                     cs:Office2013LightGray.Color="#FF444444"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E3000000"
                     cs:Office2016White.Color="#FF444444"/>

</ResourceDictionary>