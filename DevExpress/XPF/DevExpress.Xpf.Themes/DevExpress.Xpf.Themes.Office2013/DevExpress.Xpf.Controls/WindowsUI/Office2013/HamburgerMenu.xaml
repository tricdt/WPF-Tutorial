                Value="#FF3E3E3E"
                cs:Office2013DarkGray.Value="#FF3E3E3E"
                cs:Office2013LightGray.Value="#FF3E3E3E"
                cs:Office2016Black.Value="#FF3E3E3E"
                cs:Office2016Colorful.Value="#FF3E3E3E"
                cs:Office2016White.Value="#FF3E3E3E"
               />
        <Setter Property="Height" Value="14"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
