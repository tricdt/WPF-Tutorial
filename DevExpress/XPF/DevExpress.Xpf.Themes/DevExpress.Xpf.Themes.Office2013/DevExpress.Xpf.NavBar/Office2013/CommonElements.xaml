    <Storyboard x:Key="{dxnt:CommonElementsThemeKey ResourceKey=DefaultCollapsingStoryboard}">
        <DoubleAnimation Duration="0:0:0.5"
                         Storyboard.TargetName="PART_DXExpander"
                         Storyboard.TargetProperty="AnimationProgress"
                         To="0"/>
    </Storyboard>

    <Storyboard x:Key="{dxnt:CommonElementsThemeKey ResourceKey=DefaultExpandingStoryboard}">
        <DoubleAnimation Duration="0:0:0.5"
                         Storyboard.TargetName="PART_DXExpander"
                         Storyboard.TargetProperty="AnimationProgress"
                         To="1"/>
    </Storyboard>

    <!--END WPF-->

    <dxn:NavBarViewKindToScrollBarVisibilityConverter x:Key="NavBarViewKindToScrollBarVisibilityConverter"/>

    <dx:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>

    <dxn:ScrollUpCommandProvider x:Key="ScrollUpCommandProvider"/>

    <dxn:ScrollDownCommandProvider x:Key="ScrollDownCommandProvider"/>

    <ControlTemplate x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ScrollUpButtonTemplate}" TargetType="dxn:NavBarRepeatButton">
        <dx:LayoutTransformPanel Orientation="{TemplateBinding Orientation}">
            <Grid x:Name="Root"
                  Background="#00000000"
                  cs:Name="Grid_0001"
                  MinWidth="12"
                  cs:Office2013DarkGray.Background="#00000000"
                  cs:Office2013LightGray.Background="#00000000"
                  cs:Office2016Black.Background="#00000000"
                  cs:Office2016Colorful.Background="#00000000"
                  cs:Office2016White.Background="#00000000"
                 >
                <Border x:Name="HoverState"
                        Background="#FFCDE6F7"
                        cs:Name="Border_0001"
                        Opacity="0"
                        cs:Office2013DarkGray.Background="#3F009AFF"
                        cs:Office2013LightGray.Background="#3F009AFF"
                        cs:Office2016Black.Background="#30000000"
                        cs:Office2016Colorful.Background="#30000000"
                        cs:Office2016White.Background="#FFCDE6F7"
                       />
                <Border x:Name="PressedState"
                        Background="#FF92C0E0"
                        cs:Name="Border_0002"
                        Opacity="0"
                        cs:Office2013DarkGray.Background="#5F2592DA"
                        cs:Office2013LightGray.Background="#5F2592DA"
                        cs:Office2016Black.Background="#60000000"
                        cs:Office2016Colorful.Background="#47000000"
                        cs:Office2016White.Background="#FF92C0E0"
                       />
                <Path x:Name="Glyph"
                      cs:Name="Path_0001"
                      Data="F1 M 0,3L 1,3L 1,2L 2,2L 2,1L 3,1L 3,0L 4,0L 4,1L 5,1L 5,2L 6,2L 6,3L 7,3L 7,6L 6,6L 6,5L 5,5L 5,4L 4,4L 4,3L 3,3L 3,4L 2,4L 2,5L 1,5L 1,6L 0,6L 0,3 Z "
                      Fill="#FF666666"
                      Height="6"
                      HorizontalAlignment="Center"
                      Margin="4"
                      Stretch="Fill"
                      VerticalAlignment="Center"
                      Width="7"
                      cs:Office2013DarkGray.Fill="#FF666666"
                      cs:Office2013LightGray.Fill="#FF666666"
                      cs:Office2016Black.Fill="#E3FFFFFF"
                      cs:Office2016Colorful.Fill="#87000000"
                      cs:Office2016White.Fill="#FF666666"
                     />
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF2A8DD4"
                                            cs:Office2013DarkGray.To="#FF2B579A"
                                            cs:Office2013LightGray.To="#FF19478A"
                                            cs:Office2016Black.To="#FFFFFFFF"
                                            cs:Office2016Colorful.To="#BA000000"
                                            cs:Office2016White.To="#FF2A8DD4"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF0072C6"
                                            cs:Office2013DarkGray.To="#FF2B579A"
                                            cs:Office2013LightGray.To="#FF19478A"
                                            cs:Office2016Black.To="#FFFFFFFF"
                                            cs:Office2016Colorful.To="#BA000000"
                                            cs:Office2016White.To="#FF0072C6"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Root"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.5"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </dx:LayoutTransformPanel>
        <ControlTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinWidth"
                        TargetName="Root"
                        Value="44"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

