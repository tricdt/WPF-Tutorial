<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys"
                    xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxpdf="http://schemas.devexpress.com/winfx/2008/xaml/pdf"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:themes="http://schemas.devexpress.com/winfx/2008/xaml/pdf/themekeys"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/DevExpress.Xpf.Themes.Office2013.v24.1;component/DevExpress.Xpf.PdfViewer/Generic/Templates.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfCommentViewerSelectionBrush}"
                     Color="#FFCDE6F7"
                     cs:Office2013DarkGray.Color="#3F3EA9F1"
                     cs:Office2013LightGray.Color="#3F3EA9F1"
                     cs:Office2016Black.Color="#30000000"
                     cs:Office2016Colorful.Color="#30000000"/>

    <SolidColorBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfCommentViewerCommentDateForegroundBrush}"
                     Color="#8A000000"
                     cs:Office2016Colorful.Color="#97000000"
                     cs:Office2016White.Color="#72000000"/>

    <SolidColorBrush x:Key="CurrentDateButtonForeground"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0005"
                     cs:Office2013DarkGray.Color="#FF3C3C3C"
                     cs:Office2013LightGray.Color="#FF3C3C3C"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E3000000"
                     cs:Office2016White.Color="#FF3C3C3C"/>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=HighlightSearchTextColor}"
           cs:Name="Color_0001"
           cs:Office2013DarkGray.Color="#596098C0"
           cs:Office2013LightGray.Color="#596098C0"
           cs:Office2016Black.Color="#596098C0"
           cs:Office2016Colorful.Color="#596098C0"
           cs:Office2016White.Color="#7F0072C6">
        #7F0072C6
    </Color>

    <SolidColorBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=HighlightSelectionColor}"
                     Color="#0072C6"
                     Opacity="0.5"
                     cs:Office2013DarkGray.Color="#0072C6"
                     cs:Office2013LightGray.Color="#0072C6"
                     cs:Office2016Black.Color="#0072C6"
                     cs:Office2016Colorful.Color="#0072C6"
                     cs:Office2016White.Color="#0072C6"/>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=CaretColor}"
           cs:Name="Color_0007"
           cs:Office2013DarkGray.Color="#FF000000"
           cs:Office2013LightGray.Color="#FF000000"
           cs:Office2016Black.Color="#FF000000"
           cs:Office2016Colorful.Color="#FF000000"
           cs:Office2016White.Color="#FF000000">
        #FF000000
    </Color>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenCaptionForegroundColor}"
           cs:Name="Color_0002"
           cs:Office2013DarkGray.Color="#FF3C3C3C"
           cs:Office2013LightGray.Color="#FF3C3C3C"
           cs:Office2016Black.Color="#E3000000"
           cs:Office2016Colorful.Color="#E3000000"
           cs:Office2016White.Color="#FF3C3C3C">
        #FF3C3C3C
    </Color>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenDocumentLinkForegroundColor}"
           cs:Name="Color_0003"
           cs:Office2013DarkGray.Color="#FF0072C6"
           cs:Office2013LightGray.Color="#FF0072C6"
           cs:Office2016Black.Color="#E3000000"
           cs:Office2016Colorful.Color="#E0000000"
           cs:Office2016White.Color="#FF0072C6">
        #FF0072C6
    </Color>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenOpenDocumentForegroundColor}"
           cs:Name="Color_0004"
           cs:Office2013DarkGray.Color="#FF3C3C3C"
           cs:Office2013LightGray.Color="#FF3C3C3C"
           cs:Office2016Black.Color="#E3000000"
           cs:Office2016Colorful.Color="#E3000000"
           cs:Office2016White.Color="#FF3C3C3C">
        #FF3C3C3C
    </Color>

    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=LoadingDocumentLinkForegroundColor}"
           cs:Name="Color_0005"
           cs:Office2013DarkGray.Color="#FF0072C6"
           cs:Office2013LightGray.Color="#FF0072C6"
           cs:Office2016Black.Color="#E3000000"
           cs:Office2016Colorful.Color="#E0000000"
           cs:Office2016White.Color="#FF0072C6">
        #FF0072C6
    </Color>

    <sys:Double x:Key="{themes:PdfViewerThemeKey ResourceKey=PrintDialogRightColumnWidth}">120</sys:Double>

    <GridLength x:Key="{themes:PdfViewerThemeKey ResourceKey=PrintDialogRightColumnGridLength}">120</GridLength>

    <LinearGradientBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPresenterControlBackgroundBrush}"
                         cs:Name="LinearGradientBrush_0107"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFFFFFF"
                      cs:Name="GradientStop_0001"
                      Offset="0"
                      cs:Office2013DarkGray.Color="#FFE3E3E3"
                      cs:Office2013LightGray.Color="#FFF6F6F6"
                      cs:Office2016Black.Color="#FF696969"
                      cs:Office2016Colorful.Color="#FFE6E6E6"
                      cs:Office2016White.Color="#FFFFFFFF"
                     />
        <GradientStop Color="#FFF0F0F0"
                      cs:Name="GradientStop_0002"
                      Offset="1"
                      cs:Office2013DarkGray.Color="#FFC6C6C6"
                      cs:Office2013LightGray.Color="#FFE6E6E6"
                      cs:Office2016Black.Color="#FF696969"
                      cs:Office2016Colorful.Color="#FFE6E6E6"
                      cs:Office2016White.Color="#FFF0F0F0"
                     />
    </LinearGradientBrush>

    <ControlTemplate x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPageControlItemTemplate}" TargetType="{x:Type dxpdf:PdfPageControlItem}">
        <Grid>
            <Border x:Name="Selection"
                    BorderBrush="#FFC6C6C6"
                    BorderThickness="1"
                    cs:Name="Border_0001"
                    Margin="-1"
                    cs:Office2013DarkGray.BorderBrush="#26000000"
                    cs:Office2013LightGray.BorderBrush="#26000000"
                    cs:Office2016Black.BorderBrush="#FFC3C3C3"
                    cs:Office2016Colorful.BorderBrush="#FFC6C6C6"
                    cs:Office2016White.BorderBrush="#FFC6C6C6"
                   />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="true">
                <Setter cs:Name="Setter_0001"
                        Property="BorderBrush"
                        TargetName="Selection"
                        Value="#FF0072C6"
                        cs:Office2013DarkGray.Value="#FF333333"
                        cs:Office2013LightGray.Value="#FF19478A"
                        cs:Office2016Black.Value="#FF444444"
                        cs:Office2016Colorful.Value="#FF0173C7"
                        cs:Office2016White.Value="#FF0072C6"
                       />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfThumbnailControlItemTemplate}" TargetType="{x:Type dxpdf:PdfThumbnailPageControlItem}">
        <Grid>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid x:Name="Shadow" Grid.Row="0">
                    <Border BorderBrush="#FF000000"
                            BorderThickness="5"
                            CornerRadius="5"
                            cs:Name="Border_0004"
                            Margin="-6"
                            Opacity="0.01"
                            cs:Office2013DarkGray.BorderBrush="#FF000000"
                            cs:Office2013LightGray.BorderBrush="#FF000000"
                            cs:Office2016Black.BorderBrush="#FF000000"
                            cs:Office2016Colorful.BorderBrush="#FF000000"
                            cs:Office2016White.BorderBrush="#FF000000"
                           />
                    <Border BorderBrush="#FF000000"
                            BorderThickness="4"
                            CornerRadius="4"
                            cs:Name="Border_0005"
                            Margin="-5"
                            Opacity="0.01"
                            cs:Office2013DarkGray.BorderBrush="#FF000000"
                            cs:Office2013LightGray.BorderBrush="#FF000000"
                            cs:Office2016Black.BorderBrush="#FF000000"
                            cs:Office2016Colorful.BorderBrush="#FF000000"
                            cs:Office2016White.BorderBrush="#FF000000"
                           />
                    <Border BorderBrush="#FF000000"
                            BorderThickness="3"
                            CornerRadius="3"
                            cs:Name="Border_0006"
                            Margin="-4"
                            Opacity="0.02"
                            cs:Office2013DarkGray.BorderBrush="#FF000000"
                            cs:Office2013LightGray.BorderBrush="#FF000000"
                            cs:Office2016Black.BorderBrush="#FF000000"
                            cs:Office2016Colorful.BorderBrush="#FF000000"
                            cs:Office2016White.BorderBrush="#FF000000"
                           />
                    <Border BorderBrush="#FF000000"
                            BorderThickness="2"
                            CornerRadius="2"
                            cs:Name="Border_0007"
                            Margin="-3"
                            Opacity="0.03"
                            cs:Office2013DarkGray.BorderBrush="#FF000000"
                            cs:Office2013LightGray.BorderBrush="#FF000000"
                            cs:Office2016Black.BorderBrush="#FF000000"
                            cs:Office2016Colorful.BorderBrush="#FF000000"
                            cs:Office2016White.BorderBrush="#FF000000"
                           />
                    <Border BorderBrush="#FF000000"
                            BorderThickness="1"
                            CornerRadius="1"
                            cs:Name="Border_0008"
                            Margin="-2"
                            Opacity="0.05"
                            cs:Office2013DarkGray.BorderBrush="#FF000000"
                            cs:Office2013LightGray.BorderBrush="#FF000000"
                            cs:Office2016Black.BorderBrush="#FF000000"
                            cs:Office2016Colorful.BorderBrush="#FF000000"
                            cs:Office2016White.BorderBrush="#FF000000"
                           />
                    <Border BorderBrush="#FFFFFFFF"
                            BorderThickness="1"
                            cs:Name="Border_0009"
                            Margin="-1"
                            cs:Office2013DarkGray.BorderBrush="#FFFFFFFF"
                            cs:Office2013LightGray.BorderBrush="#FFFFFFFF"
                            cs:Office2016Black.BorderBrush="#FFFFFFFF"
                            cs:Office2016Colorful.BorderBrush="#FFFFFFFF"
                            cs:Office2016White.BorderBrush="#FFFFFFFF"
                           />
                </Grid>
                <Border x:Name="Highlight"
                        BorderBrush="#7F0072C6"
                        BorderThickness="6"
                        cs:Name="Border_0011"
                        Grid.Row="0"
                        Margin="-7"
                        Opacity="0"
                        cs:Office2013DarkGray.BorderBrush="#7F0072C6"
                        cs:Office2013LightGray.BorderBrush="#7F0072C6"
                        cs:Office2016Black.BorderBrush="#7F0072C6"
                        cs:Office2016Colorful.BorderBrush="#7F0072C6"
                        cs:Office2016White.BorderBrush="#7F0072C6"
                       />
                <Border x:Name="Selection"
                        BorderBrush="#FF0072C6"
                        BorderThickness="2"
                        cs:Name="Border_0010"
                        Grid.Row="0"
                        Margin="-9"
                        Opacity="0"
                        cs:Office2013DarkGray.BorderBrush="#FF0072C6"
                        cs:Office2013LightGray.BorderBrush="#FF19478A"
                        cs:Office2016Black.BorderBrush="#FF444444"
                        cs:Office2016Colorful.BorderBrush="#FF0173C7"
                        cs:Office2016White.BorderBrush="#FF0072C6"
                       />
                <TextBlock Grid.Row="1"
                           Margin="0,12,0,10"
                           Text="{Binding Path=PageNumber}"
                           TextAlignment="Center"/>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="true">
                <Setter Property="Opacity"
                        TargetName="Selection"
                        Value="1"/>
            </Trigger>
            <Trigger Property="IsHighlighted" Value="true">
                <Setter Property="Opacity"
                        TargetName="Highlight"
                        Value="1"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <DataTemplate x:Key="{themes:PdfViewerThemeKey ResourceKey=SelectionCanvasContentTemplate}">
        <Border BorderBrush="#FF007FFF"
                BorderThickness="1"
                cs:Name="Border_0003"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                cs:Office2013DarkGray.BorderBrush="#FF007FFF"
                cs:Office2013LightGray.BorderBrush="#FF007FFF"
                cs:Office2016Black.BorderBrush="#FF444444"
                cs:Office2016Colorful.BorderBrush="#FF0173C7"
                cs:Office2016White.BorderBrush="#FF007FFF"
               >
            <Border.Background>
                <SolidColorBrush Color="#007FFF"
                                 Opacity="0.3"
                                 cs:Office2013DarkGray.Color="#007FFF"
                                 cs:Office2013LightGray.Color="#007FFF"
                                 cs:Office2016Black.Color="#007FFF"
                                 cs:Office2016Colorful.Color="#007FFF"
                                 cs:Office2016White.Color="#007FFF"
                                />
            </Border.Background>
        </Border>
    </DataTemplate>

</ResourceDictionary>