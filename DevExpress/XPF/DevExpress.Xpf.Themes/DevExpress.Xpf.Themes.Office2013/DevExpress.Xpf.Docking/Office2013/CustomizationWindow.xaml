<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/docking/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:ve="http://schemas.devexpress.com/winfx/2008/xaml/docking/visualelements"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=CutBrushVertical}"
                         cs:Name="LinearGradientBrush_2052"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#FFFFFFFF"
                      cs:Name="GradientStop_0001"
                      Offset="1"
                      cs:Office2013DarkGray.Color="#FFFFFFFF"
                      cs:Office2013LightGray.Color="#FFFFFFFF"
                      cs:Office2016Black.Color="#FFFFFFFF"
                      cs:Office2016Colorful.Color="#FFFFFFFF"
                      cs:Office2016White.Color="#FFFFFFFF"
                     />
        <GradientStop Color="#F0FFFFFF"
                      cs:Name="GradientStop_0002"
                      Offset="0.5"
                      cs:Office2013DarkGray.Color="#F0FFFFFF"
                      cs:Office2013LightGray.Color="#F0FFFFFF"
                      cs:Office2016Black.Color="#F0FFFFFF"
                      cs:Office2016Colorful.Color="#F0FFFFFF"
                      cs:Office2016White.Color="#F0FFFFFF"
                     />
        <GradientStop Color="#00FFFFFF"
                      cs:Name="GradientStop_0003"
                      Offset="0"
                      cs:Office2013DarkGray.Color="#00FFFFFF"
                      cs:Office2013LightGray.Color="#00FFFFFF"
                      cs:Office2016Black.Color="#00000000"
                      cs:Office2016Colorful.Color="#00000000"
                      cs:Office2016White.Color="#00FFFFFF"
                     />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=CutBrushHorizontal}"
                         cs:Name="LinearGradientBrush_2053"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFFFFFF"
                      cs:Name="GradientStop_0004"
                      Offset="1"
                      cs:Office2013DarkGray.Color="#FFFFFFFF"
                      cs:Office2013LightGray.Color="#FFFFFFFF"
                      cs:Office2016Black.Color="#FFFFFFFF"
                      cs:Office2016Colorful.Color="#FFFFFFFF"
                      cs:Office2016White.Color="#FFFFFFFF"
                     />
        <GradientStop Color="#F0FFFFFF"
                      cs:Name="GradientStop_0005"
                      Offset="0.5"
                      cs:Office2013DarkGray.Color="#F0FFFFFF"
                      cs:Office2013LightGray.Color="#F0FFFFFF"
                      cs:Office2016Black.Color="#F0FFFFFF"
                      cs:Office2016Colorful.Color="#F0FFFFFF"
                      cs:Office2016White.Color="#F0FFFFFF"
                     />
        <GradientStop Color="#00FFFFFF"
                      cs:Name="GradientStop_0006"
                      Offset="0"
                      cs:Office2013DarkGray.Color="#00FFFFFF"
                      cs:Office2013LightGray.Color="#00FFFFFF"
                      cs:Office2016Black.Color="#00000000"
                      cs:Office2016Colorful.Color="#00000000"
                      cs:Office2016White.Color="#00FFFFFF"
                     />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxt:BrushElementsThemeKey ResourceKey=HiddenItem}"
                     Color="#FFD6D6D6"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#FFC3C3C3"
                     cs:Office2016Colorful.Color="#FFC4C4C4"
                     cs:Office2016White.Color="#FFD6D6D6"/>

    <SolidColorBrush x:Key="{dxt:BrushElementsThemeKey ResourceKey=HiddenItemCaption}"
                     Color="#FFFFFFFF"
                     cs:Name="SolidColorBrush_0002"
                     cs:Office2013DarkGray.Color="#FFE3E3E3"
                     cs:Office2013LightGray.Color="#FFF6F6F6"
                     cs:Office2016Black.Color="#FFFEFEFE"
                     cs:Office2016Colorful.Color="#FFFEFEFE"
                     cs:Office2016White.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}"
                     Color="#FFD6D6D6"
                     cs:Name="SolidColorBrush_0003"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#FFC3C3C3"
                     cs:Office2016Colorful.Color="#FFC4C4C4"
                     cs:Office2016White.Color="#FFD6D6D6"/>

    <ControlTemplate x:Key="{dxt:OtherResourcesThemeKey ResourceKey=HiddenItemTemplate}" TargetType="ve:HiddenItem">
        <Grid>
            <Border x:Name="Item"
                    Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItem}}"
                    BorderBrush="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}}"
                    BorderThickness="1"/>
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                <Image x:Name="PART_Image"
                       Height="16"
                       Margin="8,6,0,6"
                       Width="16"/>
                <ContentPresenter x:Name="TextBlock1" Margin="8,6"/>
            </StackPanel>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Margin"
                        TargetName="PART_Image"
                        Value="12,12,0,12"/>
                <Setter Property="Margin"
                        TargetName="TextBlock1"
                        Value="12"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:DockElementsThemeKey ResourceKey=DragCursor}" TargetType="ve:DragCursorControl">
        <ControlTemplate.Resources>
            <ControlTemplate x:Key="ItemTemplate" TargetType="ContentControl">
                <Grid>
                    <Border x:Name="Item"
                            Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItem}}"
                            BorderBrush="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}}"
                            BorderThickness="1"
                            Margin="0,0,2,2"
                            MinHeight="28"
                            VerticalAlignment="Top"/>
                    <Border x:Name="CaptionContainer"
                            HorizontalAlignment="Center"
                            Margin="0,0,2,2"
                            MinHeight="28"
                            VerticalAlignment="Top">
                        <ContentPresenter Margin="8,2"/>
                    </Border>
                </Grid>
                <ControlTemplate.Triggers>
                    <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                        <Setter Property="Margin"
                                TargetName="CaptionContainer"
                                Value="0,44,2,2"/>
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>
            <ControlTemplate x:Key="PanelTemplate" TargetType="ContentControl">
                <Grid>
                    <Border x:Name="Panel"
                            Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}}"
                            Margin="0,8,2,2">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition x:Name="RowDefinition1" Height="16"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemCaption}}"
                                    Grid.Row="0"
                                    Margin="1"/>
                            <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItem}}"
                                    Grid.Row="1"
                                    Margin="1,0,1,1"/>
                        </Grid>
                    </Border>
                    <Border x:Name="CaptionContainer"
                            HorizontalAlignment="Center"
                            Margin="0,0,2,2"
                            MinHeight="28"
                            VerticalAlignment="Center">
                        <ContentPresenter Margin="8,6"/>
                    </Border>
                </Grid>
                <ControlTemplate.Triggers>
                    <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                        <Setter Property="Height"
                                TargetName="RowDefinition1"
                                Value="44"/>
                        <Setter Property="Margin"
                                TargetName="CaptionContainer"
                                Value="0,44,2,2"/>
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>
            <ControlTemplate x:Key="DragCursorTemplate" TargetType="ContentControl">
                <Border Background="#3F0072C6"
                        BorderBrush="#FF0072C6"
                        BorderThickness="1"
                        cs:Office2013DarkGray.Background="#3F333333"
                        cs:Office2013DarkGray.BorderBrush="#FF333333"
                        cs:Office2013LightGray.Background="#3F0072C6"
                        cs:Office2013LightGray.BorderBrush="#FF0072C6"
                        cs:Office2016Black.Background="#30000000"
                        cs:Office2016Black.BorderBrush="#FF444444"
                        cs:Office2016Colorful.Background="#30000000"
                        cs:Office2016Colorful.BorderBrush="#FF444444"
                        cs:Office2016White.Background="#3F0072C6"
                        cs:Office2016White.BorderBrush="#FF0072C6"/>
            </ControlTemplate>
        </ControlTemplate.Resources>
        <ContentControl x:Name="content" Template="{StaticResource PanelTemplate}">
            <ve:TemplatedCaptionControl x:Name="PART_CaptionControl"
                                        cs:Name="CaptionControl_0001"
                                        Foreground="#FF3C3C3C"
                                        LayoutItem="{Binding}"
                                        cs:Office2013DarkGray.Foreground="#FF262626"
                                        cs:Office2013LightGray.Foreground="#FF444444"
                                        cs:Office2016Black.Foreground="#E3000000"
                                        cs:Office2016Colorful.Foreground="#E3000000"
                                        cs:Office2016White.Foreground="#FF3C3C3C"
                                       />
        </ContentControl>
        <ControlTemplate.Triggers>
            <Trigger Property="CursorType" Value="Item">
                <Setter Property="Template"
                        TargetName="content"
                        Value="{StaticResource ItemTemplate}"/>
            </Trigger>
            <Trigger Property="CursorType" Value="Window">
                <Setter Property="Template"
                        TargetName="content"
                        Value="{StaticResource DragCursorTemplate}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!--Resources imported from ONLY SL section -->

    <dx:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>

    <dx:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverterInverted" Invert="True"/>

    <ControlTemplate x:Key="ExpanderTemplate" TargetType="ToggleButton">
        <Grid Background="#00000000"
              cs:Name="Grid_0001"
              SnapsToDevicePixels="True"
              cs:Office2013DarkGray.Background="#00000000"
              cs:Office2013LightGray.Background="#00000000"
              cs:Office2016Black.Background="#00000000"
              cs:Office2016Colorful.Background="#00000000"
              cs:Office2016White.Background="#00000000"
             >
            <Path x:Name="GlyphCollapsed"
                  cs:Name="Path_0001"
                  Data="F1 M 5,4L 5,5L 4,5L 4,6L 3,6L 3,7L 2,7L 2,8L 1,8L 1,9L 0,9L 0,8L 0,7L 0,6L 0,5L 0,4L 0,3L 0,2L 0,1L 0,0L 1,0L 1,1L 2,1L 2,2L 3,2L 3,3L 4,3L 4,4L 5,4 Z "
                  Fill="#FF666666"
                  Height="9"
                  SnapsToDevicePixels="True"
                  Stretch="Fill"
                  Visibility="{Binding IsChecked, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverterInverted}}"
                  Width="5"
                  cs:Office2013DarkGray.Fill="#FF666666"
                  cs:Office2013LightGray.Fill="#FF666666"
                  cs:Office2016Black.Fill="#BA000000"
                  cs:Office2016Colorful.Fill="#87000000"
                  cs:Office2016White.Fill="#FF666666"
                 />
            <Path x:Name="GlyphExpanded"
                  cs:Name="Path_0002"
                  Data="F1 M 0,5L 1,5L 1,4L 2,4L 2,3L 3,3L 3,2L 4,2L 4,1L 5,1L 5,0L 6,0L 6,1L 6,2L 6,3L 6,5L 6,6L 5,6L 4,6L 3,6L 2,6L 1,6L 0,6L 0,5 Z "
                  Fill="#FF666666"
                  Height="6"
                  SnapsToDevicePixels="True"
                  Stretch="Fill"
                  Visibility="{Binding IsChecked, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"
                  Width="6"
                  cs:Office2013DarkGray.Fill="#FF666666"
                  cs:Office2013LightGray.Fill="#FF666666"
                  cs:Office2016Black.Fill="#BA000000"
                  cs:Office2016Colorful.Fill="#87000000"
                  cs:Office2016White.Fill="#FF666666"
                 />
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="treeviewtemplate" TargetType="ve:psvTreeView">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FFD6D6D6"
                BorderThickness="1"
                cs:Name="Border_0002"
                cs:Office2013DarkGray.Background="#FFE3E3E3"
                cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                cs:Office2013LightGray.Background="#FFF6F6F6"
                cs:Office2013LightGray.BorderBrush="#FFABABAB"
                cs:Office2016Black.Background="#FFD4D4D4"
                cs:Office2016Black.BorderBrush="#FFC3C3C3"
                cs:Office2016Colorful.Background="#FFFFFFFF"
                cs:Office2016Colorful.BorderBrush="#FFC4C4C4"
                cs:Office2016White.Background="#FFFFFFFF"
                cs:Office2016White.BorderBrush="#FFD6D6D6"
               >
            <ScrollViewer x:Name="PART_ScrollViewer"
                          HorizontalAlignment="Stretch"
                          HorizontalContentAlignment="Stretch"
                          VerticalAlignment="Stretch"
                          VerticalContentAlignment="Stretch">
                <!--ONLY SL 
                <ve:MouseEventsPanel>ONLY SL-->
                <ItemsPresenter/>
                <!--ONLY SL
                </ve:MouseEventsPanel>ONLY SL-->
            </ScrollViewer>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="psvTreeViewBorderControlTemplate" TargetType="ContentControl">
        <Grid SnapsToDevicePixels="True">
            <Border x:Name="PART_Hover"
                    Background="#FFD6D6D6"
                    cs:Name="Border_0003"
                    Opacity="0"
                    cs:Office2013DarkGray.Background="#FFABABAB"
                    cs:Office2013LightGray.Background="#FFABABAB"
                    cs:Office2016Black.Background="#FFC3C3C3"
                    cs:Office2016Colorful.Background="#FFC4C4C4"
                    cs:Office2016White.Background="#FFD6D6D6"
                   />
            <Border x:Name="PART_Pressed"
                    Background="#FF0072C6"
                    cs:Name="Border_0004"
                    Opacity="0"
                    cs:Office2013DarkGray.Background="#FFABABAB"
                    cs:Office2013LightGray.Background="#FFABABAB"
                    cs:Office2016Black.Background="#FF444444"
                    cs:Office2016Colorful.Background="#FF0173C7"
                    cs:Office2016White.Background="#FF0072C6"
                   />
            <ContentControl Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            Foreground="{TemplateBinding Foreground}"
                            HorizontalAlignment="Stretch"
                            HorizontalContentAlignment="Stretch"/>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PART_Hover"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PART_Pressed"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PART_Hover"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PART_Pressed"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

    <!-- -->

</ResourceDictionary>