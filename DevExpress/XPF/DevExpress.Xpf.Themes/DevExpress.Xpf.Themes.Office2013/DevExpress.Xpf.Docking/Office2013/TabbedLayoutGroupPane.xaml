    <!-- TabbedLayoutGroupPane Theme Elements -->

    <ControlTemplate x:Key="{dxt:TabbedLayoutGroupPaneElementsThemeKey ResourceKey=PageHeader}" TargetType="{x:Type ve:TabbedLayoutGroupItem}">
        <ve:UnclippableGrid x:Name="Grid"
                            Margin="0,0,-1,0"
                            ve:DockPane.HitTestType="Header">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabItemTopHover"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabItemBottomHover"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabItemLeftHover"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabItemRightHover"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Normal"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="SelectionStates">
                    <VisualState x:Name="EmptySelectionState"/>
                    <VisualState x:Name="Selected">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabItemTopSelected"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabItemBottomSelected"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabItemLeftSelected"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabItemRightSelected"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Unselected">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_CaptionControlPresenter" Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FF3C3C3C"
                                                         cs:Name="SolidColorBrush_0006"
                                                         cs:Office2013DarkGray.Color="#FF333333"
                                                         cs:Office2013LightGray.Color="#FF333333"
                                                         cs:Office2016Black.Color="#E3000000"
                                                         cs:Office2016Colorful.Color="#FF3C3C3C"
                                                         cs:Office2016White.Color="#FF3C3C3C"
                                                        />
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="LocationStates">
                    <VisualState x:Name="Top">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabTop"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Bottom">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabBottom"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0,0,-1,0"
                                                           Storyboard.TargetName="Grid"
                                                           Storyboard.TargetProperty="Margin"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="10,3,10,4"
                                                           Storyboard.TargetName="PART_CaptionControlPresenter"
                                                           Storyboard.TargetProperty="Padding"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Left">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabLeft"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0,0,0,-1"
                                                           Storyboard.TargetName="Grid"
                                                           Storyboard.TargetProperty="Margin"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="10,3"
                                                           Storyboard.TargetName="PART_CaptionControlPresenter"
                                                           Storyboard.TargetProperty="Padding"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Right">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="TabRight"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0,0,0,-1"
                                                           Storyboard.TargetName="Grid"
                                                           Storyboard.TargetProperty="Margin"/>
                            <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="10,3"
                                                           Storyboard.TargetName="PART_CaptionControlPresenter"
                                                           Storyboard.TargetProperty="Padding"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <ve:TabHeaderContainer BottomMargin="0"
                                   BottomSelectedMargin="0,-1,0,-2"
                                   CaptionLocation="{TemplateBinding CaptionLocation}"
                                   IsSelected="{TemplateBinding IsSelected}"
                                   LeftMargin="0"
                                   LeftSelectedMargin="-2,0,-1,0"
                                   RightMargin="0"
                                   RightSelectedMargin="-1,0,-2,0"
                                   TopMargin="0"
                                   TopSelectedMargin="0,-2,0,-1">
                <Grid>
                    <Grid x:Name="TabTop">
                        <Border x:Name="TabItemTop"
                                Background="{StaticResource TabItem.Background.Top}"
                                BorderBrush="{StaticResource TabItem.BorderBrush}"
                                BorderThickness="1,1,1,0"/>
                        <Border x:Name="TabItemTopHover"
                                Background="{StaticResource TabItem.Background.Hover.Top}"
                                BorderBrush="{StaticResource TabItem.BorderBrush}"
                                BorderThickness="1,1,1,0"
                                Opacity="0"/>
                        <Border x:Name="TabItemTopSelected"
                                Background="{StaticResource TabItem.SelectedState.Background}"
                                BorderBrush="{StaticResource TabItem.SelectedState.BorderBrush}"
                                BorderThickness="1,1,1,0"
                                Opacity="0"/>
                    </Grid>
                    <Grid x:Name="TabBottom" Opacity="0">
                        <Border x:Name="TabItemBottom"
                                Background="{StaticResource TabItem.Background.Bottom}"
                                BorderBrush="{StaticResource TabItem.BorderBrush}"
                                BorderThickness="1,0,1,1"/>
                        <Border x:Name="TabItemBottomHover"
                                Background="{StaticResource TabItem.Background.Hover.Bottom}"
                                BorderBrush="{StaticResource TabItem.BorderBrush}"
                                BorderThickness="1,0,1,1"
                                Opacity="0"/>
                        <Border x:Name="TabItemBottomSelected"
                                Background="{StaticResource TabItem.SelectedState.Background}"
                                BorderBrush="{StaticResource TabItem.SelectedState.BorderBrush}"
                                BorderThickness="1,0,1,1"
                                Opacity="0"/>
                    </Grid>
                    <Grid x:Name="TabLeft" Opacity="0">
                        <Border x:Name="TabItemLeft"
                                Background="{StaticResource TabItem.Background.Left}"
                                BorderBrush="{StaticResource TabItem.BorderBrush}"
                                BorderThickness="1,1,0,1"/>
                        <Border x:Name="TabItemLeftHover"
                                Background="{StaticResource TabItem.Background.Hover.Left}"
                                BorderBrush="{StaticResource TabItem.BorderBrush}"
                                BorderThickness="1,1,0,1"
                                Opacity="0"/>
                        <Border x:Name="TabItemLeftSelected"
                                Background="{StaticResource TabItem.SelectedState.Background}"
                                BorderBrush="{StaticResource TabItem.SelectedState.BorderBrush}"
                                BorderThickness="1,1,0,1"
                                Opacity="0"/>
                    </Grid>
                    <Grid x:Name="TabRight" Opacity="0">
                        <Border x:Name="TabItemRight"
                                Background="{StaticResource TabItem.Background.Right}"
                                BorderBrush="{StaticResource TabItem.BorderBrush}"
                                BorderThickness="0,1,1,1"/>
                        <Border x:Name="TabItemRightHover"
                                Background="{StaticResource TabItem.Background.Hover.Right}"
                                BorderBrush="{StaticResource TabItem.BorderBrush}"
                                BorderThickness="0,1,1,1"
                                Opacity="0"/>
                        <Border x:Name="TabItemRightSelected"
                                Background="{StaticResource TabItem.SelectedState.Background}"
                                BorderBrush="{StaticResource TabItem.SelectedState.BorderBrush}"
                                BorderThickness="0,1,1,1"
                                Opacity="0"/>
                    </Grid>
                </Grid>
            </ve:TabHeaderContainer>
            <ve:TabHeaderTransformPanel CaptionLocation="{TemplateBinding CaptionLocation}" CaptionOrientation="{TemplateBinding CaptionOrientation}">
                <Grid x:Name="Content">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <ve:TemplatedTabCaptionControl x:Name="PART_CaptionControlPresenter"
                                                   cs:Name="TemplatedTabCaptionControl_0001"
                                                   Foreground="#FF3C3C3C"
                                                   LayoutItem="{Binding}"
                                                   Padding="10,4,10,2"
                                                   cs:Office2013DarkGray.Foreground="#FF262626"
                                                   cs:Office2013LightGray.Foreground="#FF444444"
                                                   cs:Office2016Black.Foreground="#E3000000"
                                                   cs:Office2016Colorful.Foreground="#E3000000"
                                                   cs:Office2016White.Foreground="#FF3C3C3C"
                                                  />
                    <ve:BaseControlBoxControl x:Name="PART_ControlBox"
                                              ButtonHeight="16"
                                              ButtonWidth="16"
                                              Grid.Column="1"
                                              LayoutItem="{Binding}"
                                              Margin="-8,4,10,2"/>
                    <Border x:Name="PART_TouchPlaceHolder"
                            MinHeight="44"
                            Visibility="Collapsed"/>
                </Grid>
            </ve:TabHeaderTransformPanel>
        </ve:UnclippableGrid>
        <ControlTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="ButtonWidth"
                        TargetName="PART_ControlBox"
                        Value="44"/>
                <Setter Property="ButtonHeight"
                        TargetName="PART_ControlBox"
                        Value="44"/>
                <Setter Property="Padding"
                        TargetName="PART_CaptionControlPresenter"
                        Value="12"/>
                <Setter Property="Margin"
                        TargetName="PART_ControlBox"
                        Value="-12,12,12,12"/>
                <Setter Property="Visibility"
                        TargetName="PART_TouchPlaceHolder"
                        Value="Visible"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:TabbedLayoutGroupPaneElementsThemeKey ResourceKey=Template}" TargetType="{x:Type ve:TabbedLayoutGroupPane}">
        <ve:DockingSplitLayoutPanel BottomContent1Margin="0,0,0,2"
                                    DataContext="{Binding}"
                                    LeftContent1Margin="2,0,0,0"
                                    RightContent1Margin="0,0,2,0"
                                    TopContent1Margin="0,2,0,0"
                                    ve:DockPane.HitTestType="Bounds">
            <ve:DockingSplitLayoutPanel.Content1Location>
                <Binding Path="CaptionLocation">
                    <Binding.Converter>
                        <ve:GroupCaptionLocationToDockConverter/>
                    </Binding.Converter>
                </Binding>
            </ve:DockingSplitLayoutPanel.Content1Location>
            <ve:DockingSplitLayoutPanel.Content1>
                <ve:DockingSplitLayoutPanel x:Name="PART_Header"
                                            ClipMargin="{TemplateBinding ActualClipMargin}"
                                            Margin="{TemplateBinding ActualTabPanelMargin}"
                                            MaximizeContent1="True"
                                            Panel.ZIndex="1">
                    <ve:DockingSplitLayoutPanel.Content1Location>
                        <Binding Path="CaptionLocation">
                            <Binding.Converter>
                                <ve:TabHeaderCaptionLocationToDockConverter/>
                            </Binding.Converter>
                        </Binding>
                    </ve:DockingSplitLayoutPanel.Content1Location>
                    <ve:DockingSplitLayoutPanel.Content1>
                        <ItemsPresenter x:Name="PART_HeadersPanelPresenter"/>
                    </ve:DockingSplitLayoutPanel.Content1>
                    <ve:DockingSplitLayoutPanel.Content2>
                        <ve:TabHeaderControlBoxControl x:Name="PART_ControlBox"
                                                       ButtonHeight="16"
                                                       ButtonStyle="{DynamicResource {dxt:TabbedLayoutGroupPaneElementsThemeKey ResourceKey=TabbedLayoutGroupPaneButtonStyle}}"
                                                       ButtonWidth="16"
                                                       LayoutItem="{Binding}"
                                                       ScrollNextButtonTemplate="{DynamicResource {dxt:DocumentPaneElementsThemeKey ResourceKey=ScrollNextButton}}"
                                                       ScrollPrevButtonTemplate="{DynamicResource {dxt:DocumentPaneElementsThemeKey ResourceKey=ScrollPrevButton}}"
                                                       ve:DockPane.HitTestType="ControlBox"
                                                       ve:TabHeaderControlBoxControl.Location="{Binding CaptionLocation}"/>
                    </ve:DockingSplitLayoutPanel.Content2>
                </ve:DockingSplitLayoutPanel>
            </ve:DockingSplitLayoutPanel.Content1>
            <ve:DockingSplitLayoutPanel.Content2>
                <Border x:Name="PART_SelectedPage"
                        Background="#FFFFFFFF"
                        BorderBrush="#FFACACAC"
                        BorderThickness="1"
                        cs:Name="Border_0002"
                        Padding="11"
                        cs:Office2013DarkGray.Background="#FFE3E3E3"
                        cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                        cs:Office2013LightGray.Background="#FFF6F6F6"
                        cs:Office2013LightGray.BorderBrush="#FFABABAB"
                        cs:Office2016Black.Background="#FFB2B2B2"
                        cs:Office2016Black.BorderBrush="#FF414141"
                        cs:Office2016Colorful.Background="#FFF0F0F0"
                        cs:Office2016Colorful.BorderBrush="#FFA6A6A6"
                        cs:Office2016White.Background="#FFFFFFFF"
