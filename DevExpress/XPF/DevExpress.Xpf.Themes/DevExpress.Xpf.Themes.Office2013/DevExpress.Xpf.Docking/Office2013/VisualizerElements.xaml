    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Docking.VisualElements.DocumentSelectorContainer}" TargetType="{x:Type ve:DocumentSelectorContainer}">
        <Style.Setters>
            <Setter cs:Name="Setter_0001"
                    Property="Foreground"
                    Value="#FF3C3C3C"
                    cs:Office2013DarkGray.Value="#FF262626"
                    cs:Office2013LightGray.Value="#FF444444"
                    cs:Office2016Black.Value="#E3000000"
                    cs:Office2016Colorful.Value="#E3000000"
                    cs:Office2016White.Value="#FF3C3C3C"
                   />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ve:DocumentSelectorContainer}">
                        <Grid>
                            <Border x:Name="Window"
                                    Background="#FFD6D6D6"
                                    cs:Name="Border_0026"
                                    cs:Office2013DarkGray.Background="#FFABABAB"
                                    cs:Office2013LightGray.Background="#FFABABAB"
                                    cs:Office2016Black.Background="#60000000"
                                    cs:Office2016Colorful.Background="#FFC4C4C4"
                                    cs:Office2016White.Background="#FFD6D6D6"
                                   >
                                <Border Background="#FFFFFFFF"
                                        cs:Name="Border_0027"
                                        Margin="1"
                                        cs:Office2013DarkGray.Background="#FFE3E3E3"
                                        cs:Office2013LightGray.Background="#FFF6F6F6"
                                        cs:Office2016Black.Background="#FFB2B2B2"
                                        cs:Office2016Colorful.Background="#FFFFFFFF"
                                        cs:Office2016White.Background="#FFFFFFFF"
                                       >
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <Border x:Name="Header"
                                                Background="#FFF0F0F0"
                                                cs:Name="Border_0028"
                                                Margin="1,1,1,0"
                                                cs:Office2013DarkGray.Background="#FFE3E3E3"
                                                cs:Office2013LightGray.Background="#FFF6F6F6"
                                                cs:Office2016Black.Background="#FFB2B2B2"
                                                cs:Office2016Colorful.Background="#FFF0F0F0"
                                                cs:Office2016White.Background="#FFF0F0F0"
                                               >
                                            <StackPanel HorizontalAlignment="Left"
                                                        Margin="12,8"
                                                        Orientation="Horizontal"
                                                        VerticalAlignment="Center">
                                                <Image HorizontalAlignment="Left"
                                                       Margin="0,0,6,0"
                                                       Source="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionImage}"
                                                       Stretch="None"
                                                       VerticalAlignment="Top"/>
                                                <StackPanel>
                                                    <ContentControl Content="{Binding Path=(local:DockLayoutManager.LayoutItem).Caption}"
                                                                    ContentTemplate="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionTemplate}"
                                                                    ContentTemplateSelector="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionTemplateSelector}"
                                                                    cs:Name="ContentControl_0001"
                                                                    Focusable="False"
                                                                    FontSize="18"
                                                                    FontWeight="Light"
                                                                    Foreground="#FF0072C6"
                                                                    HorizontalAlignment="Left"
                                                                    VerticalAlignment="Bottom"
                                                                    cs:Office2013DarkGray.Foreground="#FF262626"
                                                                    cs:Office2013LightGray.Foreground="#FF444444"
                                                                    cs:Office2016Black.Foreground="#E3000000"
                                                                    cs:Office2016Colorful.Foreground="#E3000000"
                                                                    cs:Office2016White.Foreground="#FF0072C6"
                                                                   />
                                                    <TextBlock HorizontalAlignment="Left"
                                                               Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Description}"
                                                               TextWrapping="Wrap"
                                                               VerticalAlignment="Top"/>
                                                </StackPanel>
                                            </StackPanel>
                                        </Border>
                                        <Border x:Name="Content"
                                                Background="#FFFFFFFF"
                                                cs:Name="Border_0029"
                                                Grid.Row="1"
                                                cs:Office2013DarkGray.Background="#FFE3E3E3"
                                                cs:Office2013LightGray.Background="#FFF6F6F6"
                                                cs:Office2016Black.Background="#FFD4D4D4"
                                                cs:Office2016Colorful.Background="#FFFFFFFF"
                                                cs:Office2016White.Background="#FFFFFFFF"
                                               >
                                            <ContentPresenter x:Name="PART_ContainerContent"
                                                              Content="{Binding}"
                                                              Margin="12"/>
                                        </Border>
                                        <Border x:Name="Footer"
                                                Background="#FFF0F0F0"
                                                cs:Name="Border_0030"
                                                Grid.Row="2"
                                                Margin="1,0,1,1"
                                                MinHeight="24"
                                                cs:Office2013DarkGray.Background="#FFE3E3E3"
                                                cs:Office2013LightGray.Background="#FFF6F6F6"
                                                cs:Office2016Black.Background="#FFB2B2B2"
                                                cs:Office2016Colorful.Background="#FFF0F0F0"
                                                cs:Office2016White.Background="#FFF0F0F0"
                                               >
                                            <ContentPresenter Content="{Binding Path=(local:DockLayoutManager.LayoutItem).FooterDescription}"
                                                              HorizontalAlignment="Left"
                                                              Margin="12,0,0,0"
                                                              VerticalAlignment="Center"/>
                                        </Border>
                                    </Grid>
                                </Border>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <ControlTemplate x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=ListBoxItem}" TargetType="{x:Type ListBoxItem}">
        <Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Border Background="#FFCDE6F7"
                        cs:Name="Border_0031"
                        Margin="0,0,0,1"
                        cs:Office2013DarkGray.Background="#3F009AFF"
                        cs:Office2013LightGray.Background="#3F009AFF"
                        cs:Office2016Black.Background="#FF696969"
                        cs:Office2016Colorful.Background="#FFAEAEAE"
                        cs:Office2016White.Background="#FFCDE6F7"
                       />
            </Grid>
            <Grid x:Name="SelectedState" Opacity="0">
                <Border Background="#FF92C0E0"
                        cs:Name="Border_0032"
                        Margin="0,0,0,1"
                        cs:Office2013DarkGray.Background="#5F2592DA"
                        cs:Office2013LightGray.Background="#5F2592DA"
                        cs:Office2016Black.Background="#FFC3C3C3"
                        cs:Office2016Colorful.Background="#FFC4C4C4"
                        cs:Office2016White.Background="#FF92C0E0"
                       />
            </Grid>
            <Border Padding="{TemplateBinding Padding}">
                <ve:TemplatedCaptionControl x:Name="caption"
                                            LayoutItem="{Binding}"
                                            Margin="3,2"/>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Opacity"
                        TargetName="SelectedState"
                        Value="1"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <DataTemplate x:Key="{dxt:DockPaneElementsThemeKey ResourceKey=ContentPreview}">
