<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxfuit="http://schemas.devexpress.com/winfx/2008/xaml/core/filteringui/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region FilterEditor-->

    <!-- Control -->

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=BorderBrush}"
                     Color="#FFD6D6D6"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#6A000000"
                     cs:Office2016Colorful.Color="#47000000"
                     cs:Office2016White.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=Background}"
                     Color="#FFFFFFFF"
                     cs:Office2013DarkGray.Color="#FFE3E3E3"
                     cs:Office2013LightGray.Color="#FFF6F6F6"
                     cs:Office2016Black.Color="#FFD4D4D4"/>

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=Foreground}"
                     Color="#FF000000"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF444444"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E3000000"/>

    <!-- Property Selector -->

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=PropertySelectorBackground}"
                     Color="#FFF9D3B0"
                     cs:Office2013DarkGray.Color="#FFF1CEAD"
                     cs:Office2013LightGray.Color="#FFF1CEAD"
                     cs:Office2016Black.Color="#FFE8BC94"
                     cs:Office2016Colorful.Color="#FFF1CEAD"/>

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=PropertySelectorForeground}" Color="#FF000000"/>

    <!-- Operator Selector -->

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=OperatorSelectorBackground}"
                     Color="#FFBCD7E7"
                     cs:Office2013DarkGray.Color="#FFB5D6EA"
                     cs:Office2013LightGray.Color="#FFB5D6EA"
                     cs:Office2016Black.Color="#FF95BDD4"
                     cs:Office2016Colorful.Color="#FFB5D6EA"/>

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=OperatorSelectorForeground}" Color="#FF000000"/>

    <!-- Group Selector -->

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=GroupOperatorSelectorBackground}" Color="#FFD36E80"/>

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=GroupOperatorSelectorForeground}" Color="#FF000000"/>

    <!-- Inplace Active -->

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=InplaceEditorActiveBackground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=InplaceEditorActiveBorderBrush}"
                     Color="#FF0072C6"
                     cs:Office2013DarkGray.Color="#FF2B579A"
                     cs:Office2013LightGray.Color="#FF19478A"
                     cs:Office2016Black.Color="#FFFFFFFF"
                     cs:Office2016Colorful.Color="#FF0173C7"/>

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=InplaceEditorActiveForeground}"
                     Color="#FF000000"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF444444"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E3000000"/>

    <!-- Property Inactive -->

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=InplaceEditorInactiveBackground}"
                     Color="#FFBBE2C5"
                     cs:Office2013DarkGray.Color="#FFA7DAB4"
                     cs:Office2013LightGray.Color="#FFA7DAB4"
                     cs:Office2016Black.Color="#FF86C796"
                     cs:Office2016Colorful.Color="#FFA7DAB4"/>

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=InplaceEditorInactiveBorderBrush}"
                     Color="#FFB9B9B9"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#60000000"
                     cs:Office2016Colorful.Color="#47000000"/>

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=InplaceEditorInactiveForeground}" Color="#FF000000"/>

    <!-- Options -->

    <CornerRadius x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=ItemCornerRadius}">0</CornerRadius>

    <CornerRadius x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=FormatConditionCornerRadius}">0</CornerRadius>

    <sys:Double x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=RowMinHeight}">22</sys:Double>

    <sys:Double x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=InplaceEditorCellMinWidth}">38</sys:Double>

    <Thickness x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=PropertySelectorContentMargin}">4,2,4,2</Thickness>

    <SolidColorBrush x:Key="{dxfuit:FilterEditorThemeKey ResourceKey=AddChildSplitButtonMouseOverFill}"
                     Color="#FF68A490"
                     cs:Office2016Black.Color="#FF69A891"
                    />

    <!--#endregion-->

    <!--#region FilterElements-->

    <Thickness x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=HeaderPadding}">12,3,12,6</Thickness>

    <SolidColorBrush x:Key="{dxfuit:CheckedTreeListFilterElementThemeKey ResourceKey=DataPresenterBackground}"
                     Color="#00000000"
                     cs:Office2013DarkGray.Color="#FFFFFFFF"
                     cs:Office2013LightGray.Color="#FFFFFFFF"
                     cs:Office2016Black.Color="#FFd4d4d4"
                     cs:Office2016Colorful.Color="#FFFFFFFF"/>

    <sys:Double x:Key="{dxfuit:FilteringUIThemeKey ResourceKey=DatePeriodsFilterElementHeight}">145</sys:Double>

    <sys:Double x:Key="{dxfuit:FilteringUIThemeKey ResourceKey=DatePeriodsFilterElementHeightTouch}">320</sys:Double>

    <SolidColorBrush x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=HeaderPanelBottomBorderColor}"
                     Color="#FFD6D6D6"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#6A000000"
                     cs:Office2016Colorful.Color="#47000000"
                     cs:Office2016White.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=HeaderPanelSeparatorColor}"
                     Color="#FFB9B9B9"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#60000000"
                     cs:Office2016Colorful.Color="#47000000"/>

    <SolidColorBrush x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=HeaderPanelSelectionMarkerBackground}"
                     Color="#FFFFFFFF"
                     cs:Office2013DarkGray.Color="#FFE3E3E3"
                     cs:Office2013LightGray.Color="#FFF6F6F6"
                     cs:Office2016Black.Color="#FFB2B2B2"
                     cs:Office2016Colorful.Color="#FFF0F0F0"/>

    <SolidColorBrush x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=BorderBrush}"
                     Color="#FFD6D6D6"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#6A000000"
                     cs:Office2016Colorful.Color="#47000000"
                     cs:Office2016White.Color="#FFB9B9B9"/>

    <SolidColorBrush x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=Background}"
                     Color="#FFFFFFFF"
                     cs:Office2013DarkGray.Color="#FFE3E3E3"
                     cs:Office2013LightGray.Color="#FFF6F6F6"
                     cs:Office2016Black.Color="#FFB2B2B2"
                     cs:Office2016Colorful.Color="#FFF0F0F0"/>

    <SolidColorBrush x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=FilterValueContainerTopAreaBackground}"
                     Color="#FFFFFFFF"
                     cs:Office2013DarkGray.Color="#FFE3E3E3"
                     cs:Office2013LightGray.Color="#FFF6F6F6"
                     cs:Office2016Black.Color="#FF696969"
                     cs:Office2016Colorful.Color="#FFE6E6E6"/>

    <SolidColorBrush x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=FilterValueContainerTopAreaBorderColor}"
                     Color="#FFABABAB"
                     cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                     cs:Office2013LightGray.BorderBrush="#FFABABAB"
                     cs:Office2016Black.BorderBrush="#47000000"
                     cs:Office2016Colorful.BorderBrush="#FF777777"
                     cs:Office2016White.BorderBrush="#FF828790"/>

    <Thickness x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=FilterValueContainerTopAreaMargin}">2,1</Thickness>

    <Thickness x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=FilterValueContainerTopAreaMarginTouch}">11,9</Thickness>

    <SolidColorBrush x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=HeaderPanelBackground}" Color="Transparent"/>

    <!--#endregion-->

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlBackground}"
                     Color="#FFFFFFFF"
                     cs:Office2013DarkGray.Color="#FFE3E3E3"
                     cs:Office2013LightGray.Color="#FFF6F6F6"
                     cs:Office2016Black.Color="#FF696969"
                     cs:Office2016Colorful.Color="#FFE6E6E6"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlButtonsBackgroundPressed}"
                     Color="#FF90C0E3"
                     cs:Office2016Black.Color="#FF414141"
                     cs:Office2016Colorful.Color="#FFA6A6A6"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlButtonsBorderBrushPressed}"
                     Color="#FF90C0E3"
                     cs:Office2016Black.Color="#FF414141"
                     cs:Office2016Colorful.Color="#FFA6A6A6"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlButtonsBackgroundHover}"
                     Color="#FFCFE7F8"
                     cs:Office2016Black.Color="#FF555555"
                     cs:Office2016Colorful.Color="#FFBBBBBB"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlButtonsBorderBrushHover}"
                     Color="#FFCFE7F8"
                     cs:Office2016Black.Color="#FF555555"
                     cs:Office2016Colorful.Color="#FFBBBBBB"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlBorderBrush}"
                     Color="#FFD6D6D6"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#FF444444"
                     cs:Office2016Colorful.Color="#30000000"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=PropertyBackground}"
                     Color="#FFF9D3B0"
                     cs:Office2013DarkGray.Color="#FFF1CEAD"
                     cs:Office2013LightGray.Color="#FFF1CEAD"
                     cs:Office2016Black.Color="#FFE8BC94"
                     cs:Office2016Colorful.Color="#FFF1CEAD"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ValueBackground}"
                     Color="#FFBBE2C5"
                     cs:Office2013DarkGray.Color="#FFA7DAB4"
                     cs:Office2013LightGray.Color="#FFA7DAB4"
                     cs:Office2016Black.Color="#FF86C796"
                     cs:Office2016Colorful.Color="#FFA7DAB4"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=HighlightedBackground}"
                     Color="#FFBCD7E7"
                     cs:Office2013DarkGray.Color="#FFB5D6EA"
                     cs:Office2013LightGray.Color="#FFB5D6EA"
                     cs:Office2016Black.Color="#FF95BDD4"
                     cs:Office2016Colorful.Color="#FFB5D6EA"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=Foreground}"
                     Color="#FF000000"
                     cs:Office2016Black.Color="#E3FFFFFF"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ElementForeground}" Color="#FF000000"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=BracketBorderBrush}"
                     Color="#FFD6D6D6"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#E3FFFFFF"
                     cs:Office2016Colorful.Color="#30000000"/>

    <dx:WpfSvgPalette x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=SvgPalette}">
        <SolidColorBrush x:Key="BlackColor"
                         Color="#FF7F7F7F"
                         cs:Office2013DarkGray.Color="#FF262626"
                         cs:Office2013LightGray.Color="#FF444444"
                         cs:Office2016Black.Color="#E3FFFFFF"
                         cs:Office2016Colorful.Color="#E3000000"
                         cs:Office2016White.Color="#FF3C3C3C"
                        />
    </dx:WpfSvgPalette>

    <dx:WpfSvgPalette x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ButtonSvgPalette}">
        <SolidColorBrush x:Key="BlackColor"
                         Color="#FF3C3C3C"
                         cs:Office2016Black.Color="#FFEFEFEF"
                         cs:Office2016Colorful.Color="#FF191919"
                        />
        <dx:WpfSvgPalette.States>
            <dx:WpfSvgPalette x:Key="Hover">
                <SolidColorBrush x:Key="BlackColor"
                                 Color="#FF3C3C3C"
                                 cs:Office2016Black.Color="#FFEFEFEF"
                                 cs:Office2016Colorful.Color="#FF191919"
                                />
            </dx:WpfSvgPalette>
            <dx:WpfSvgPalette x:Key="Pressed">
                <SolidColorBrush x:Key="BlackColor"
                                 Color="Black"
                                 cs:Office2016Black.Color="White"
                                />
            </dx:WpfSvgPalette>
        </dx:WpfSvgPalette.States>
    </dx:WpfSvgPalette>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=PopupElementPressedBackground}"
                     Color="#FF90C0E3"
                     Opacity="0.5"
                     cs:Office2016Black.Color="#FF414141"
                     cs:Office2016Colorful.Color="#FFA6A6A6"/>

    <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=PopupElementHoverBackground}"
                     Color="#FF90C0E3"
                     Opacity="0.3"
                     cs:Office2016Black.Color="#FF414141"
                     cs:Office2016Colorful.Color="#FFA6A6A6"/>

</ResourceDictionary>
