<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                    xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2013.v24.1;component/DevExpress.Xpf.Grid/Office2013/Constants.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="ScrollBar.Background"
                     Color="#FFFFFFFF"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2013DarkGray.Color="#FFE3E3E3"
                     cs:Office2013LightGray.Color="#FFF6F6F6"
                     cs:Office2016Black.Color="#FF696969"
                     cs:Office2016Colorful.Color="#FFE6E6E6"
                     cs:Office2016White.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="ScrollBar.BorderBrush"
                     Color="#FFD6D6D6"
                     cs:Name="SolidColorBrush_0002"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#FF444444"
                     cs:Office2016Colorful.Color="#FFA6A6A6"
                     cs:Office2016White.Color="#FFD6D6D6"/>

    <!--END WPF-->

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingCornerStyle}" TargetType="Border">
        <Setter cs:Name="Setter_0001"
                Property="Background"
                Value="#FFF0F0F0"
                cs:Office2013DarkGray.Value="#00000000"
                cs:Office2013LightGray.Value="#00000000"
                cs:Office2016Black.Value="#00000000"
                cs:Office2016Colorful.Value="#FFE6E6E6"
                cs:Office2016White.Value="#FFF0F0F0"
               />
        <!--BEGIN WPF-->
        <Style.Triggers>
            <Trigger Property="dxg:GridBorderHelper.ShowBorder" Value="True">
                <Setter Property="Background" Value="{StaticResource ScrollBar.Background}"/>
                <Setter Property="BorderBrush" Value="{StaticResource ScrollBar.BorderBrush}"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="dxg:GridBorderHelper.ShowBorder" Value="False"/>
                    <Condition Property="dxg:GridScrollBarHelper.HasFixedRightColumns" Value="True"/>
                    <Condition Property="dxg:GridScrollBarHelper.ExtendScrollBarToFixedColumns" Value="False"/>
                </MultiTrigger.Conditions>
                <MultiTrigger.Setters>
                    <Setter cs:Name="Setter_0012"
                            Property="Background"
                            Value="Transparent"
                            cs:Office2013DarkGray.Value="Transparent"
                            cs:Office2013LightGray.Value="Transparent"
                            cs:Office2016Black.Value="#00000000"
                            cs:Office2016Colorful.Value="Transparent"
                            cs:Office2016White.Value="Transparent"
                           />
                    <Setter Property="BorderBrush" Value="{StaticResource ScrollBar.BorderBrush}"/>
                    <Setter Property="BorderThickness" Value="0,1,0,0"/>
                </MultiTrigger.Setters>
            </MultiTrigger>
        </Style.Triggers>
        <!--END WPF-->
    </Style>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingCornerLeftStyle}" TargetType="Border">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="Background" Value="{StaticResource ScrollBar.Background}"/>
        <Setter Property="BorderBrush" Value="{StaticResource ScrollBar.BorderBrush}"/>
        <Setter Property="BorderThickness" Value="0,1,1,0"/>
        <!--BEGIN WPF-->
        <Style.Triggers>
            <Trigger Property="dx:ScrollBarExtensions.ScrollBarMode" Value="TouchOverlap">
                <Setter cs:Name="Setter_0002"
                        Property="Background"
                        Value="Transparent"
                        cs:Office2013DarkGray.Value="Transparent"
                        cs:Office2013LightGray.Value="Transparent"
                        cs:Office2016Black.Value="#00000000"
                        cs:Office2016Colorful.Value="Transparent"
                        cs:Office2016White.Value="Transparent"
                       />
                <Setter cs:Name="Setter_0003"
                        Property="BorderBrush"
                        Value="Transparent"
                        cs:Office2013DarkGray.Value="Transparent"
                        cs:Office2013LightGray.Value="Transparent"
                        cs:Office2016Black.Value="#FF444444"
                        cs:Office2016Colorful.Value="Transparent"
                        cs:Office2016White.Value="Transparent"
                       />
            </Trigger>
        </Style.Triggers>
        <!--END WPF-->
    </Style>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedLeftThumbStyle}" TargetType="Border">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="Background" Value="{StaticResource ScrollBar.Background}"/>
        <Setter Property="BorderBrush" Value="{StaticResource ScrollBar.BorderBrush}"/>
        <Setter Property="BorderThickness" Value="0,1,0,0"/>
        <!--BEGIN WPF-->
        <Style.Triggers>
            <Trigger Property="dx:ScrollBarExtensions.ScrollBarMode" Value="TouchOverlap">
                <Setter cs:Name="Setter_0004"
                        Property="Background"
                        Value="Transparent"
                        cs:Office2013DarkGray.Value="Transparent"
                        cs:Office2013LightGray.Value="Transparent"
                        cs:Office2016Black.Value="#00000000"
                        cs:Office2016Colorful.Value="Transparent"
                        cs:Office2016White.Value="Transparent"
                       />
                <Setter cs:Name="Setter_0005"
                        Property="BorderBrush"
                        Value="Transparent"
                        cs:Office2013DarkGray.Value="Transparent"
                        cs:Office2013LightGray.Value="Transparent"
                        cs:Office2016Black.Value="#FF444444"
                        cs:Office2016Colorful.Value="Transparent"
                        cs:Office2016White.Value="Transparent"
                       />
            </Trigger>
            <Trigger Property="dxg:GridBorderHelper.ShowBorder" Value="True">
                <Setter Property="BorderThickness" Value="0"/>
            </Trigger>
        </Style.Triggers>
        <!--END WPF-->
    </Style>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedRightThumbStyle}" TargetType="Border">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="Background" Value="{StaticResource ScrollBar.Background}"/>
        <Setter Property="BorderBrush" Value="{StaticResource ScrollBar.BorderBrush}"/>
        <Setter Property="BorderThickness" Value="0,1,0,0"/>
        <!--BEGIN WPF-->
        <Style.Triggers>
            <Trigger Property="dx:ScrollBarExtensions.ScrollBarMode" Value="TouchOverlap">
                <Setter cs:Name="Setter_0006"
                        Property="Background"
                        Value="Transparent"
                        cs:Office2013DarkGray.Value="Transparent"
                        cs:Office2013LightGray.Value="Transparent"
                        cs:Office2016Black.Value="#00000000"
                        cs:Office2016Colorful.Value="Transparent"
                        cs:Office2016White.Value="Transparent"
                       />
                <Setter cs:Name="Setter_0007"
                        Property="BorderBrush"
                        Value="Transparent"
                        cs:Office2013DarkGray.Value="Transparent"
                        cs:Office2013LightGray.Value="Transparent"
                        cs:Office2016Black.Value="#FF444444"
                        cs:Office2016Colorful.Value="Transparent"
                        cs:Office2016White.Value="Transparent"
                       />
            </Trigger>
            <Trigger Property="dxg:GridBorderHelper.ShowBorder" Value="True">
                <Setter Property="BorderThickness" Value="0"/>
            </Trigger>
        </Style.Triggers>
        <!--END WPF-->
    </Style>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedLineStyle}" TargetType="Border">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter cs:Name="Setter_0008"
                Property="Background"
                Value="Transparent"
                cs:Office2013DarkGray.Value="Transparent"
                cs:Office2013LightGray.Value="Transparent"
                cs:Office2016Black.Value="#00000000"
                cs:Office2016Colorful.Value="Transparent"
                cs:Office2016White.Value="Transparent"
               />
        <Setter Property="BorderBrush" Value="{StaticResource ScrollBar.BorderBrush}"/>
        <Setter Property="BorderThickness" Value="1,1,1,0"/>
        <!--BEGIN WPF-->
        <Style.Triggers>
            <Trigger Property="dx:ScrollBarExtensions.ScrollBarMode" Value="TouchOverlap">
                <Setter cs:Name="Setter_0009"
                        Property="Background"
                        Value="Transparent"
                        cs:Office2013DarkGray.Value="Transparent"
                        cs:Office2013LightGray.Value="Transparent"
                        cs:Office2016Black.Value="#00000000"
                        cs:Office2016Colorful.Value="Transparent"
                        cs:Office2016White.Value="Transparent"
                       />
                <Setter cs:Name="Setter_0010"
                        Property="BorderBrush"
                        Value="Transparent"
                        cs:Office2013DarkGray.Value="Transparent"
                        cs:Office2013LightGray.Value="Transparent"
                        cs:Office2016Black.Value="#FF444444"
                        cs:Office2016Colorful.Value="Transparent"
                        cs:Office2016White.Value="Transparent"
                       />
            </Trigger>
            <Trigger Property="dxg:GridBorderHelper.ShowBorder" Value="True">
                <Setter Property="BorderThickness" Value="1,0"/>
            </Trigger>
        </Style.Triggers>
        <!--END WPF-->
    </Style>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedLineContentTotalSummaryStyle}" TargetType="Border">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter cs:Name="Setter_0011"
                Property="Background"
                Value="Transparent"
                cs:Office2013DarkGray.Value="Transparent"
                cs:Office2013LightGray.Value="Transparent"
                cs:Office2016Black.Value="#00000000"
                cs:Office2016Colorful.Value="Transparent"
                cs:Office2016White.Value="Transparent"
               />
        <Setter cs:Name="Setter_0013"
                Property="BorderBrush"
                Value="#FFD6D6D6"
                cs:Office2013DarkGray.Value="#FFABABAB"
                cs:Office2013LightGray.Value="#FFABABAB"
                cs:Office2016Black.Value="#FF444444"
                cs:Office2016Colorful.Value="#FFA6A6A6"
                cs:Office2016White.Value="#FFD6D6D6"
               />
        <Setter Property="BorderThickness" Value="1,1,1,0"/>
    </Style>

    <DataTemplate x:Key="{dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedLineTotalSummaryTemplate}">
        <Border Style="{DynamicResource ResourceKey={dxgt:ScrollBarThemeKey ResourceKey=ScrollingFixedLineContentTotalSummaryStyle}}"/>
    </DataTemplate>

    <Style x:Key="{dxgt:ScrollBarThemeKey ResourceKey=FixedLeftLinePlaceHolderTotalSummaryStyle}" TargetType="ContentPresenter">
        <Setter Property="Margin" Value="0,0,0,-3"/>
    </Style>

</ResourceDictionary>