<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxgt:TreeListViewThemeKey ResourceKey=IndentItemTemplate}">
        <Path cs:Name="Path_0001"
              Stroke="#FFD6D6D6"
              StrokeThickness="1"
              Width="{DynamicResource {dxgt:TreeListViewThemeKey ResourceKey=RowIndent}}"
              cs:Office2013DarkGray.Stroke="#FFABABAB"
              cs:Office2013LightGray.Stroke="#FFABABAB"
              cs:Office2016Black.Stroke="#BA000000"
              cs:Office2016Colorful.Stroke="#87000000"
              cs:Office2016White.Stroke="#FFD6D6D6"
             >
            <Path.Data>
                <GeometryGroup>
                    <LineGeometry x:Name="PART_Line1"/>
                    <LineGeometry x:Name="PART_Line2"/>
                    <LineGeometry x:Name="PART_Line3"/>
                </GeometryGroup>
            </Path.Data>
        </Path>
    </ControlTemplate>

</ResourceDictionary>