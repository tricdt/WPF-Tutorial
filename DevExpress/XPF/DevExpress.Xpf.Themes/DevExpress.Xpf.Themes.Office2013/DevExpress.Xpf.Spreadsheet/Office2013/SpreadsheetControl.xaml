                                Padding="0,1,1,0"/>
                        <Grid Margin="0,1,0,0">
                            <Path cs:Name="Path_0005"
                                  Data="F1 M 119,100L 122,100L 122,97L 124,97L 124,100L 127,100L 127,102L 124,102L 124,105L 122,105L 122,102L 119,102L 119,100 Z "
                                  Fill="#3FFFFFFF"
                                  Height="8"
                                  Margin="0,1,0,-1"
                                  Stretch="Fill"
                                  Width="8"
                                  cs:Office2013DarkGray.Fill="#3FFFFFFF"
                                  cs:Office2013LightGray.Fill="#3FFFFFFF"
                                  cs:Office2016Black.Fill="#00000000"
                                  cs:Office2016Colorful.Fill="#3FFFFFFF"
                                  cs:Office2016White.Fill="#3FFFFFFF"
                                 />
                            <Path Data="F1 M 119,100L 122,100L 122,97L 124,97L 124,100L 127,100L 127,102L 124,102L 124,105L 122,105L 122,102L 119,102L 119,100 Z "
                                  Fill="{StaticResource GlyphBrush}"
                                  Height="8"
                                  Stretch="Fill"
                                  Width="8"/>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Opacity"
                                    TargetName="DefaultState"
                                    Value="0"/>
                            <Setter Property="Opacity"
                                    TargetName="HoverState"
                                    Value="1"/>
                            <Setter Property="Opacity"
                                    TargetName="PressedState"
                                    Value="0"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Opacity"
                                    TargetName="DefaultState"
                                    Value="0"/>
                            <Setter Property="Opacity"
                                    TargetName="HoverState"
                                    Value="0"/>
                            <Setter Property="Opacity"
                                    TargetName="PressedState"
                                    Value="1"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity"
                                    TargetName="Root"
                                    Value="0.5"/>
                        </Trigger>
                        <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                            <Setter Property="MinHeight"
                                    TargetName="Root"
                                    Value="43"/>
                        </Trigger>
