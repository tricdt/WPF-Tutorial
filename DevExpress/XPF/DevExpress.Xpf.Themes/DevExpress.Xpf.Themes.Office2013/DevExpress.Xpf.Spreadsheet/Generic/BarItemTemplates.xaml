<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:dxspsui="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet" xmlns:themes="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet/themekeys" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <DataTemplate x:Key="{themes:BarItemTemplate ResourceKey=ChartLayoutGalleryGroup}" DataType="dxb:GalleryItemGroup">
    <ContentControl>
      <dxb:GalleryItemGroup Caption="{Binding Caption}" ItemsSource="{Binding Items}">
        <dxb:GalleryItemGroup.ItemTemplate>
          <DataTemplate>
            <dxb:GalleryItem Glyph="{Binding Image}" Hint="{Binding Hint}" AutomationProperties.Name="{Binding Hint}" />
          </DataTemplate>
        </dxb:GalleryItemGroup.ItemTemplate>
      </dxb:GalleryItemGroup>
    </ContentControl>
  </DataTemplate>
  <DataTemplate x:Key="{themes:BarItemTemplate ResourceKey=ChartStylesGalleryGroup}" DataType="dxb:GalleryItemGroup">
    <ContentControl>
      <dxb:GalleryItemGroup Caption="{Binding Caption}" ItemsSource="{Binding Items}">
        <dxb:GalleryItemGroup.ItemTemplate>
          <DataTemplate>
            <dxb:GalleryItem Glyph="{Binding Image}" Hint="{Binding Hint}" AutomationProperties.Name="{Binding Hint}" />
          </DataTemplate>
        </dxb:GalleryItemGroup.ItemTemplate>
      </dxb:GalleryItemGroup>
    </ContentControl>
  </DataTemplate>
  <DataTemplate x:Key="{themes:BarItemTemplate ResourceKey=StyleGalleryGroup}">
    <dxb:GalleryItemGroup Caption="{Binding Caption}" ItemsSource="{Binding Items}">
      <dxb:GalleryItemGroup.ItemTemplate>
        <DataTemplate>
          <dxb:GalleryItem Caption="{Binding}" AutomationProperties.Name="{Binding Text}" />
        </DataTemplate>
      </dxb:GalleryItemGroup.ItemTemplate>
    </dxb:GalleryItemGroup>
  </DataTemplate>
  <DataTemplate x:Key="{themes:BarItemTemplate ResourceKey=CellStyleGalleryItemCaption}">
    <Grid Width="100" Height="22" Background="{Binding Background}">
      <TextBlock Text="{Binding Text}" FontFamily="{Binding TextSettings.FontFamily}" FontSize="{Binding TextSettings.FontSize}" FontStyle="{Binding TextSettings.FontStyle}" FontWeight="{Binding TextSettings.FontWeight}" Foreground="{Binding TextSettings.Foreground}" TextDecorations="{Binding TextSettings.TextDecorations}" TextWrapping="{Binding TextSettings.TextWrapping}" TextTrimming="{Binding TextSettings.TextTrimming}" HorizontalAlignment="{Binding TextSettings.HorizontalAlignment}" Padding="4" VerticalAlignment="Center" />
    </Grid>
  </DataTemplate>
  <DataTemplate x:Key="{themes:BarItemTemplate ResourceKey=StyleGalleryHintCaption}">
    <TextBlock Text="{Binding Text}" />
  </DataTemplate>
  <DataTemplate x:Key="{themes:BarItemTemplate ResourceKey=TableStyleGalleryItemCaption}">
    <Grid Width="61" Height="46">
      <dxspsui:TableStyleGalleryItem Spreadsheet="{Binding Spreadsheet}" StyleName="{Binding StyleName}" />
    </Grid>
  </DataTemplate>
  <DataTemplate x:Key="{themes:BarItemTemplate ResourceKey=PivotTableStyleGalleryItemCaption}">
    <Grid Width="61" Height="46">
      <dxspsui:PivotTableStyleGalleryItem Spreadsheet="{Binding Spreadsheet}" StyleName="{Binding StyleName}" />
    </Grid>
  </DataTemplate>
</ResourceDictionary>