<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/treemap/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- General -->

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ControlBorderBrush}"
                     Color="#FFACACAC"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#FF555555"
                     cs:Office2016Colorful.Color="#FFA6A6A6"
                     cs:Office2016White.Color="#FFACACAC"/>

    <!-- Group -->

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupBorderBrush}"
                     Color="#FFACACAC"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#FF555555"
                     cs:Office2016Colorful.Color="#FFA6A6A6"
                     cs:Office2016White.Color="#FFACACAC"/>

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupBackground}"
                     Color="#FFFFFFFF"
                     cs:Office2013DarkGray.Color="#FFE3E3E3"
                     cs:Office2013LightGray.Color="#FFF6F6F6"
                     cs:Office2016Black.Color="#FF969696"
                     cs:Office2016Colorful.Color="#FFE6E6E6"
                     cs:Office2016White.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupForeground}"
                     Color="#FF0072C6"
                     cs:Office2013DarkGray.Color="#FF2B579A"
                     cs:Office2013LightGray.Color="#FF19478A"
                     cs:Office2016Black.Color="#FF1F1F1F"
                     cs:Office2016Colorful.Color="#FF0173C7"
                     cs:Office2016White.Color="#FF0072C6"/>

    <sys:Double x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupFontSize}">18</sys:Double>

    <FontWeight x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=GroupFontWeight}">Light</FontWeight>

    <!-- SubMenu -->

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupBackground}"
                     Color="#FFF0F0F0"
                     cs:Office2013DarkGray.Color="#FFF3F3F3"
                     cs:Office2013LightGray.Color="#FFFFFFFF"
                     cs:Office2016Black.Color="#FFB2B2B2"
                     cs:Office2016Colorful.Color="#FFF0F0F0"
                     cs:Office2016White.Color="#FFF0F0F0"/>

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupForeground}"
                     Color="#FF444444"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF444444"
                     cs:Office2016Black.Color="#FF1F1F1F"
                     cs:Office2016Colorful.Color="#FF444444"
                     cs:Office2016White.Color="#FF444444"/>

    <sys:Double x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupFontSize}">12</sys:Double>

    <FontWeight x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=SubGroupFontWeight}">Regular</FontWeight>

    <!-- ToolTip -->

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipBackground}"
                     Color="#FF0072C6"
                     cs:Office2013DarkGray.Color="#FF333333"
                     cs:Office2013LightGray.Color="#FF19478A"
                     cs:Office2016Black.Color="#FF444444"
                     cs:Office2016Colorful.Color="#FF0173C7"
                     cs:Office2016White.Color="#FF0072C6"/>

    <SolidColorBrush x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipForeground}"
                     Color="#FFFFFFFF"
                     cs:Office2013DarkGray.Color="#FFFFFFFF"
                     cs:Office2013LightGray.Color="#FFFFFFFF"
                     cs:Office2016Black.Color="#FFFFFFFF"
                     cs:Office2016Colorful.Color="#FFFFFFFF"
                     cs:Office2016White.Color="#FFFFFFFF"/>

    <sys:Double x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipFontSize}">18</sys:Double>

    <FontWeight x:Key="{dxt:TreeMapBrushesThemeKey ResourceKey=ToolTipFontWeight}">Light</FontWeight>

</ResourceDictionary>