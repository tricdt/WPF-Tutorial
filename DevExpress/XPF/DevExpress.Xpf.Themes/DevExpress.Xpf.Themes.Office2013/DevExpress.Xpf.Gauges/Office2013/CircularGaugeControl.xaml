<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxga="http://schemas.devexpress.com/winfx/2008/xaml/gauges"
                    xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/gauges/themekeys"
                    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- ArcScaleNeedle -->

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=NeedleFill}"
                     Color="#FF0072C6"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF19478A"
                     cs:Office2016Black.Color="#FFFFFFFF"
                     cs:Office2016Colorful.Color="#FF0173C7"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=ForegroundBrush}"
                     Color="#FF3C3C3C"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF444444"
                     cs:Office2016Black.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=ScaleElementsBrush}"
                     Color="#FF969696"
                     cs:Office2013DarkGray.Color="#FF666666"
                     cs:Office2013LightGray.Color="#FF666666"
                     cs:Office2016Black.Color="#FF414141"
                     cs:Office2016Colorful.Color="#FF717171"
                     cs:Office2016White.Color="#FF666666"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=ScaleBackgroundBrush}"
                     Color="#FFE5E5E5"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFB1B1B1"
                     cs:Office2016Black.Color="#FF555555"
                     cs:Office2016Colorful.Color="#FFB8B8B8"
                     cs:Office2016White.Color="#FFACACAC"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=ScaleInnerBackgroundBrush}"
                     Color="#FFFFFFFF"
                     Opacity="0.62"
                     cs:Office2013DarkGray.Color="#FFE3E3E3"
                     cs:Office2013LightGray.Color="#FFF6F6F6"
                     cs:Office2016Black.Color="#FF696969"
                     cs:Office2016Colorful.Color="#FFE6E6E6"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=SpindleCapBackgroundBrush}"
                     Color="#FF0072C6"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF19478A"
                     cs:Office2016Black.Color="#FFFFFFFF"
                     cs:Office2016Colorful.Color="#FF0173C7"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=SpindleCapForegroundBrush}"
                     Color="#FFFFFFFF"
                     cs:Office2013DarkGray.Color="#FFE3E3E3"
                     cs:Office2013LightGray.Color="#FFDCDCDC"
                     cs:Office2016Black.Color="#FF616161"
                     cs:Office2016Colorful.Color="#FFD5D5D5"
                     cs:Office2016White.Color="#FFDFDFDF"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=MarkerBackgroundBrush}" Color="Transparent"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=MarkerForegroundBrush}"
                     Color="#FF0072C6"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF19478A"
                     cs:Office2016Black.Color="#FFFFFFFF"
                     cs:Office2016Colorful.Color="#FF0173C7"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=RangeGreenBrush}"
                     Color="#FF7DD68C"
                     cs:Office2013DarkGray.Color="#FF6C9D87"
                     cs:Office2013LightGray.Color="#FF6C9D87"
                     cs:Office2016Black.Color="#FF448844"
                     cs:Office2016Colorful.Color="#FF66CC66"
                     cs:Office2016White.Color="#FF66CC66"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=RangeYellowBrush}"
                     Color="#FFF4C68C"
                     cs:Office2013DarkGray.Color="#FFB3A383"
                     cs:Office2013LightGray.Color="#FFB3A383"
                     cs:Office2016Black.Color="#FF888844"
                     cs:Office2016Colorful.Color="#FFFFCC66"
                     cs:Office2016White.Color="#FFFF9966"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=RangeRedBrush}"
                     Color="#FFF48A8C"
                     cs:Office2013DarkGray.Color="#FFBA7975"
                     cs:Office2013LightGray.Color="#FFBA7975"
                     cs:Office2016Black.Color="#FF884444"
                     cs:Office2016Colorful.Color="#FFCC6666"
                     cs:Office2016White.Color="#FFCC6666"/>

    <SolidColorBrush x:Key="{dxgt:GaugesBrushesThemeKey ResourceKey=GaugeBackgroundBrush}" Color="Transparent"/>

</ResourceDictionary>