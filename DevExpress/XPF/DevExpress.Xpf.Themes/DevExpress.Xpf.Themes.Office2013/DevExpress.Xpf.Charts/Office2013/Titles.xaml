<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/charts/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <sys:Double x:Key="{dxt:ChartControlThemeKey ResourceKey=TitleFontSize}">21</sys:Double>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=TitleForeground}"
                     Color="#FF0072C6"
                     cs:Office2013DarkGray.Color="#FF2B579A"
                     cs:Office2013LightGray.Color="#FF2B579A"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E0000000"/>

    <Thickness x:Key="{dxt:ChartControlThemeKey ResourceKey=TitleMarginValue}">0,0,0,10</Thickness>

    <Thickness x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisTitlePadding}">5</Thickness>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=PaneTitleForeground}"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF2B579A"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E0000000">
        #FF444444
    </SolidColorBrush>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=PaneTitleForegroundHot}"
                     cs:Office2013DarkGray.Color="#FF2B579A"
                     cs:Office2013LightGray.Color="#FF2B579A"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E0000000">
        #FF0072C6
    </SolidColorBrush>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=PaneTitleForegroundDisabled}"
                     cs:Office2013DarkGray.Color="#2F2B579A"
                     cs:Office2013LightGray.Color="#2F2B579A"
                     cs:Office2016Black.Color="#23000000"
                     cs:Office2016Colorful.Color="#20000000">
        #2F444444
    </SolidColorBrush>

    <sys:Double x:Key="{dxt:ChartControlThemeKey ResourceKey=PaneTitleFontSize}">16</sys:Double>

    <Thickness x:Key="{dxt:ChartControlThemeKey ResourceKey=PaneTitleMarginValue}">0,10,0,10</Thickness>

    <FontWeight x:Key="{dxt:ChartControlThemeKey ResourceKey=PaneTitleFontWeight}">Normal</FontWeight>

</ResourceDictionary>