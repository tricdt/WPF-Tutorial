<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/charts/themekeys"
                    xmlns:local="http://schemas.devexpress.com/winfx/2008/xaml/charts"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <sys:Double x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisLabelFontSize}">12</sys:Double>

    <sys:Double x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisTitleFontSize}">16</sys:Double>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=AxisBrush}"
                     Color="#FFD6D6D6"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#FF696969"
                     cs:Office2016Colorful.Color="#FFAEAEAE"
                     cs:Office2016White.Color="#FFD6D6D6"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=Axis2DInterlacedBrush}"
                     Color="#07000000"
                     cs:Name="SolidColorBrush_0002"
                     cs:Office2013DarkGray.Color="#07000000"
                     cs:Office2013LightGray.Color="#07000000"
                     cs:Office2016Black.Color="#07000000"
                     cs:Office2016Colorful.Color="#07000000"
                     cs:Office2016White.Color="#07000000"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=StripBrush}"
                     Color="#3FFF0000"
                     cs:Name="SolidColorBrush_0003"
                     cs:Office2013DarkGray.Color="#3FFF0000"
                     cs:Office2013LightGray.Color="#3FFF0000"
                     cs:Office2016Black.Color="#3FFF0000"
                     cs:Office2016Colorful.Color="#3FFF0000"
                     cs:Office2016White.Color="#3FFF0000"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=ConstantLineBrush}"
                     Color="#7FFF0000"
                     cs:Name="SolidColorBrush_0004"
                     cs:Office2013DarkGray.Color="#7FFF0000"
                     cs:Office2013LightGray.Color="#7FFF0000"
                     cs:Office2016Black.Color="#7FFF0000"
                     cs:Office2016Colorful.Color="#7FFF0000"
                     cs:Office2016White.Color="#7FFF0000"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=ConstantLineTitleForeground}"
                     Color="#FFCC0000"
                     cs:Name="SolidColorBrush_0005"
                     cs:Office2013DarkGray.Color="#FFCC0000"
                     cs:Office2013LightGray.Color="#FFCC0000"
                     cs:Office2016Black.Color="#FFCC3333"
                     cs:Office2016Colorful.Color="#FFCC3333"
                     cs:Office2016White.Color="#FFCC0000"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=GridLineBrush}"
                     Color="#26000000"
                     cs:Name="SolidColorBrush_0006"
                     cs:Office2013DarkGray.Color="#26000000"
                     cs:Office2013LightGray.Color="#26000000"
                     cs:Office2016Black.Color="#30000000"
                     cs:Office2016Colorful.Color="#26000000"
                     cs:Office2016White.Color="#26000000"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=MinorGridLineBrush}"
                     Color="#0F000000"
                     cs:Name="SolidColorBrush_0007"
                     cs:Office2013DarkGray.Color="#0F000000"
                     cs:Office2013LightGray.Color="#0F000000"
                     cs:Office2016Black.Color="#10000000"
                     cs:Office2016Colorful.Color="#10000000"
                     cs:Office2016White.Color="#0F000000"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=AxisLabelForeground}"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0008"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF444444"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E3000000"
                     cs:Office2016White.Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=AxisTitleForeground}"
                     Color="#FF0072C6"
                     cs:Name="SolidColorBrush_0009"
                     cs:Office2013DarkGray.Color="#FF2B579A"
                     cs:Office2013LightGray.Color="#FF2B579A"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E0000000"
                     cs:Office2016White.Color="#FF0072C6"/>

    <Color x:Key="{dxt:ChartBrushesThemeKey ResourceKey=StripBorderColor}"
           cs:Name="Color_0001"
           cs:Office2013DarkGray.Color="#7F2F8BC9"
           cs:Office2013LightGray.Color="#7F2F8BC9"
           cs:Office2016Black.Color="#FF696969"
           cs:Office2016Colorful.Color="#FFAEAEAE"
           cs:Office2016White.Color="#7F0072C6">
        #7F0072C6
    </Color>

    <Thickness x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisLabelPadding}">5,1</Thickness>

    <DataTemplate x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisLabelTemplate}">
        <ContentPresenter Content="{Binding Path=Content}"/>
    </DataTemplate>

    <!--BEGIN WPF-->

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=Axis3DInterlacedBrush}"
                     Color="#07000000"
                     cs:Name="SolidColorBrush_0010"
                     cs:Office2013DarkGray.Color="#07000000"
                     cs:Office2013LightGray.Color="#07000000"
                     cs:Office2016Black.Color="#07000000"
                     cs:Office2016Colorful.Color="#07000000"
                     cs:Office2016White.Color="#07000000"/>

    <!--END WPF-->

</ResourceDictionary>