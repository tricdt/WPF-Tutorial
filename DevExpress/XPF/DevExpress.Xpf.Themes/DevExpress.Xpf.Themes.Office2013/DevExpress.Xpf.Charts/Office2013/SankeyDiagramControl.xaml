<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxsat="http://schemas.devexpress.com/winfx/2008/xaml/sankey/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=PaddingValue}">16</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=BorderThickness}">1</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=ToolTipContentMargin}">7</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TitleMargin}">0,0,0,10</Thickness>

    <sys:Double x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TitleFontSize}">21</sys:Double>

    <sys:Double x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TextElementFontSize}">16</sys:Double>

    <FontWeight x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=NodeLabelFontWeight}">Bold</FontWeight>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=BackgroundBrush}"
                     Color="#FFFFFFFF"
                     cs:Office2013DarkGray.Color="#FFFFFFFF"
                     cs:Office2013LightGray.Color="#FFFFFFFF"
                     cs:Office2016Black.Color="#FFD4D4D4"
                     cs:Office2016Colorful.Color="#FFFFFFFF"
                     cs:Office2016White.Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=BorderBrush}"
                     Color="#FFD6D6D6"
                     cs:Office2013DarkGray.Color="#FFABABAB"
                     cs:Office2013LightGray.Color="#FFABABAB"
                     cs:Office2016Black.Color="#60000000"
                     cs:Office2016Colorful.Color="#FFAEAEAE"
                     cs:Office2016White.Color="#FFD6D6D6"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipForeground}"
                     Color="#FF3C3C3C"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF444444"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E3000000"
                     cs:Office2016White.Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipBackground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipBorder}" Color="#7F999999"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=TitleForeground}"
                     Color="#FF0072C6"
                     cs:Office2013DarkGray.Color="#FF2B579A"
                     cs:Office2013LightGray.Color="#FF2B579A"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E0000000"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=TextElementForeground}" Color="Black"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=NodeLabelForeground}"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0008"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF444444"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E3000000"
                     cs:Office2016White.Color="#FF3C3C3C"/>

</ResourceDictionary>