<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=CurrentTimeIndicatorStyle}" TargetType="{x:Type ContentPresenter}">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Border Background="#FF2A8DD4"
                            BorderBrush="#7F2A8DD4"
                            BorderThickness="0,1,0,1"
                            cs:Name="Border_0002"
                            Height="3"
                            Margin="1,0,1,0"
                            Width="32"
                            cs:Office2013DarkGray.Background="#FF2B579A"
                            cs:Office2013DarkGray.BorderBrush="#7F2A8DD4"
                            cs:Office2013LightGray.Background="#FF19478A"
                            cs:Office2013LightGray.BorderBrush="#7F2A8DD4"
                            cs:Office2016Black.Background="#FF444444"
                            cs:Office2016Black.BorderBrush="#30000000"
                            cs:Office2016Colorful.Background="#FF0173C7"
                            cs:Office2016Colorful.BorderBrush="#3F0173C7"
                            cs:Office2016White.Background="#FF2A8DD4"
                            cs:Office2016White.BorderBrush="#7F2A8DD4"
                           />
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>