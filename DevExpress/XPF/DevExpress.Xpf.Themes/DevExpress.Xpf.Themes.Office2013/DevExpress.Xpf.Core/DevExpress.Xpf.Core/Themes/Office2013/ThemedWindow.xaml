                     cs:Office2013DarkGray.Color="#FF999999"
                     cs:Office2013LightGray.Color="#FF999999"
                     cs:Office2016Black.Color="#FF969696"
                     cs:Office2016Colorful.Color="#FF0072C6"
                     cs:Office2016White.Color="#FF0072C6"/>

