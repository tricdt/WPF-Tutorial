<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=TextEditPadding}">4,2,4,2</Thickness>

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=ValidationErrorPadding}">3,0,1,0</Thickness>

    <Thickness x:Key="leftButtonsMargin">0,-1,0,-1</Thickness>

    <Thickness x:Key="rightButtonsMargin">0,-1,0,-1</Thickness>

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=CheckEditPadding}">0,3,0,3</Thickness>

    <Thickness x:Key="{dxet:ComboBoxEditThemeKey ResourceKey=SelectedItemImagePadding}">3,0,0,0</Thickness>

    <Thickness x:Key="{dxet:ComboBoxEditThemeKey ResourceKey=SelectedItemContentPadding}">3,2,3,2</Thickness>

    <Color x:Key="{dxet:SpinEditThemeKey ResourceKey=ArrowBrushColor}"
           cs:Name="Color_0001"
           cs:Office2013DarkGray.Color="#FF666666"
           cs:Office2013LightGray.Color="#FF666666"
           cs:Office2016Black.Color="#87000000"
           cs:Office2016Colorful.Color="#87000000"
           cs:Office2016White.Color="#FF666666">
        #FF666666
    </Color>

    <SolidColorBrush x:Key="{dxet:ButtonsThemeKey ResourceKey=Foreground}"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF444444"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E3000000"
                     cs:Office2016White.Color="#FF3C3C3C"/>

</ResourceDictionary>