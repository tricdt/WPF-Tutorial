    <!-- Calendar -->

    <ControlTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarGroupItemTemplate}" TargetType="{x:Type dxer:CalendarGroupItem}">
        <Border Background="#FFACACAC"
                cs:Name="Border_0002"
                cs:Office2013DarkGray.Background="#FFABABAB"
                cs:Office2013LightGray.Background="#FFABABAB"
                cs:Office2016Black.Background="#60000000"
                cs:Office2016Colorful.Background="#FFD5D5D5"
                cs:Office2016White.Background="#FFACACAC"
               >
            <TextBlock cs:Name="TextBlock_0003"
                       Foreground="#BFFFFFFF"
                       Margin="20,0"
                       Text="{TemplateBinding Text}"
                       TextAlignment="Center"
                       VerticalAlignment="Center"
                       cs:Office2013DarkGray.Foreground="#BFFFFFFF"
                       cs:Office2013LightGray.Foreground="#BFFFFFFF"
                       cs:Office2016Black.Foreground="#E3FFFFFF"
                       cs:Office2016Colorful.Foreground="#BA000000"
                       cs:Office2016White.Foreground="#BFFFFFFF"
                      />
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarItemTemplate}" TargetType="{x:Type dxer:CalendarItem}">
        <Border x:Name="Border"
                Background="#FFACACAC"
                BorderThickness="0"
                cs:Name="Border_0003"
                cs:Office2013DarkGray.Background="#FFABABAB"
                cs:Office2013LightGray.Background="#FFABABAB"
                cs:Office2016Black.Background="#60000000"
                cs:Office2016Colorful.Background="#FFD5D5D5"
                cs:Office2016White.Background="#FFACACAC"
               >
            <TextBlock x:Name="Text"
                       cs:Name="TextBlock_0002"
                       Foreground="#BFFFFFFF"
                       HorizontalAlignment="Center"
                       Margin="0,10,0,10"
                       Text="{TemplateBinding Text}"
                       TextAlignment="Center"
                       cs:Office2013DarkGray.Foreground="#BFFFFFFF"
                       cs:Office2013LightGray.Foreground="#BFFFFFFF"
                       cs:Office2016Black.Foreground="#E3FFFFFF"
                       cs:Office2016Colorful.Foreground="#BA000000"
                       cs:Office2016White.Foreground="#BFFFFFFF"
                      />
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter cs:Name="Setter_0001"
                        Property="Background"
                        TargetName="Border"
                        Value="#FF92C0E0"
                        cs:Office2013DarkGray.Value="#FF333333"
                        cs:Office2013LightGray.Value="#FF19478A"
                        cs:Office2016Black.Value="#FF444444"
                        cs:Office2016Colorful.Value="#6A000000"
                        cs:Office2016White.Value="#FF92C0E0"
                       />
                <Setter cs:Name="Setter_0002"
                        Property="Foreground"
                        TargetName="Text"
                        Value="#BFFFFFFF"
                        cs:Office2013DarkGray.Value="#BFFFFFFF"
                        cs:Office2013LightGray.Value="#BFFFFFFF"
                        cs:Office2016Black.Value="#FFFFFFFF"
                        cs:Office2016Colorful.Value="#BAFFFFFF"
                        cs:Office2016White.Value="#BFFFFFFF"
                       />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!-- Range Bar -->

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FFACACAC"
                BorderThickness="{Binding Path=(dxer:RangeControl.OwnerRangeControl).BorderThickness, RelativeSource={RelativeSource Self}}"
                cs:Name="Border_0005"
                cs:Office2013DarkGray.Background="#FFFFFFFF"
                cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                cs:Office2013LightGray.Background="#FFFFFFFF"
                cs:Office2013LightGray.BorderBrush="#FFABABAB"
                cs:Office2016Black.Background="#FFD4D4D4"
                cs:Office2016Black.BorderBrush="#60000000"
                cs:Office2016Colorful.Background="#FFFFFFFF"
                cs:Office2016Colorful.BorderBrush="#47000000"
                cs:Office2016White.Background="#FFFFFFFF"
                cs:Office2016White.BorderBrush="#FFACACAC"
               >
            <ContentPresenter Margin="1"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarRootBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FFACACAC"
