                       >
                    <Border x:Name="border1"
                            BorderBrush="#FFFFFFFF"
                            BorderThickness="1,0,1,0"
                            cs:Name="Border_0016"
                            Margin="2,3,2,3"
                            Width="3"
                            cs:Office2013DarkGray.BorderBrush="#FFE3E3E3"
