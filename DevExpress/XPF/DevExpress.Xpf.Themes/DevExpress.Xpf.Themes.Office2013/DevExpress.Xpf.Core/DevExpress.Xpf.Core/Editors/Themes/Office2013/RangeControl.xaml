                     Fill="#FFFFFFFF"
                     Height="34"
                     Stretch="Fill"
                     Width="34"
                     cs:Office2013DarkGray.Fill="#FFE3E3E3"
                     cs:Office2013LightGray.Fill="#FFF6F6F6"
                     cs:Office2016Black.Fill="#30000000"
                     cs:Office2016Colorful.Fill="#FFFFFFFF"
                     cs:Office2016White.Fill="#FFFFFFFF"
                    />
            <Path x:Name="Glyph"
                  cs:Name="Path_0001"
                  Data="F1 M 210,314L 210,312L 218,304L 221,304L 213,312L 230,312L 230,314L 213,314L 221,322L 218,322L 210,314 Z M 220,297C 228.837,297 236,304.164 236,313C 236,321.837 228.836,329 220,329C 211.163,329 204,321.837 204,313C 204,304.164 211.163,297 220,297 Z M 220,299C 212.268,299 206,305.268 206,313C 206,320.732 212.268,327 220,327C 227.732,327 234,320.732 234,313C 234,305.268 227.732,299 220,299 Z "
                  Fill="#FFB9B9B9"
                  Height="32"
                  Stretch="Fill"
                  Width="32"
                  cs:Office2013DarkGray.Fill="#FFABABAB"
                  cs:Office2013LightGray.Fill="#FFABABAB"
                  cs:Office2016Black.Fill="#BA000000"
                  cs:Office2016Colorful.Fill="#FFC3C3C3"
                  cs:Office2016White.Fill="#FFB9B9B9"
                 />
        </Grid>
    </ControlTemplate>

