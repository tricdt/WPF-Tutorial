                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2013.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/RangeControl.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarClientForegroundBrush}"
                     Color="#FFFFFFFF"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2013DarkGray.Color="#BFFFFFFF"
                     cs:Office2013LightGray.Color="#BFFFFFFF"
                     cs:Office2016Black.Color="#E3FFFFFF"
                     cs:Office2016Colorful.Color="#BFFFFFFF"
                     cs:Office2016White.Color="#FFFFFFFF"/>

    <Thickness x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftLabelMargin}"
               Bottom="1"
               Left="0"
               Right="5"
               Top="1"/>

    <Thickness x:Key="{dxt:RangeControlThemeKey ResourceKey=RightLabelMargin}"
               Bottom="1"
               Left="5"
               Right="0"
               Top="1"/>

    <!-- CalendarSelectionMarker -->

    <DataTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarSelectionMarkerTemplate}">
        <Ellipse Fill="{DynamicResource {dxt:CalendarClientThemeKey ResourceKey=CalendarClientForegroundBrush}}"
                 Height="20"
                 HorizontalAlignment="Center"
                 VerticalAlignment="Center"
                 Width="20"/>
    </DataTemplate>

    <!-- ToolTips -->

    <DataTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LabelTemplate}">
        <Border x:Name="border"
                Background="#FFB9B9B9"
                cs:Name="Border_0001"
                Margin="0,-1,0,1"
                MinHeight="22"
                MinWidth="80"
                VerticalAlignment="Top"
                cs:Office2013DarkGray.Background="#FFABABAB"
                cs:Office2013LightGray.Background="#FFABABAB"
                cs:Office2016Black.Background="#60000000"
                cs:Office2016Colorful.Background="#30000000"
                cs:Office2016White.Background="#FFB9B9B9"
               >
            <TextBlock cs:Name="TextBlock_0001"
                       Foreground="#FFFFFFFF"
                       HorizontalAlignment="Center"
                       Margin="12,2,12,2"
                       Text="{Binding}"
                       VerticalAlignment="Center"
                       cs:Office2013DarkGray.Foreground="#FFFFFFFF"
                       cs:Office2013LightGray.Foreground="#FFFFFFFF"
                       cs:Office2016Black.Foreground="#E3FFFFFF"
                       cs:Office2016Colorful.Foreground="#FFFFFFFF"
                       cs:Office2016White.Foreground="#FFFFFFFF"
                      />
        </Border>
        <DataTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Margin"
                        TargetName="border"
                        Value="2,-1,2,1"/>
            </Trigger>
        </DataTemplate.Triggers>
    </DataTemplate>

    <!-- Left & Right Navigation Buttons -->

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftNavigationButtonTemplate}" TargetType="{x:Type Button}">
        <Grid Background="Transparent"
              cs:Name="Grid_0001"
              Margin="12,0,0,0"
              Opacity="0.9"
              cs:Office2013DarkGray.Background="Transparent"
              cs:Office2013LightGray.Background="Transparent"
              cs:Office2016Black.Background="Transparent"
              cs:Office2016Colorful.Background="Transparent"
              cs:Office2016White.Background="Transparent"
             >
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FFB9B9B9"
                                            cs:Office2013DarkGray.To="#FFABABAB"
                                            cs:Office2013LightGray.To="#FFABABAB"
                                            cs:Office2016Black.To="#BA000000"
                                            cs:Office2016Colorful.To="#FFC3C3C3"
                                            cs:Office2016White.To="#FFB9B9B9"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF92C0E0"
                                            cs:Office2013DarkGray.To="#FF2B579A"
                                            cs:Office2013LightGray.To="#FF19478A"
                                            cs:Office2016Black.To="#FF000000"
                                            cs:Office2016Colorful.To="#87000000"
                                            cs:Office2016White.To="#FF92C0E0"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0003"
                                            Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF92C0E0"
                                            cs:Office2013DarkGray.To="#FF2B579A"
                                            cs:Office2013LightGray.To="#FF19478A"
                                            cs:Office2016Black.To="#FF000000"
                                            cs:Office2016Colorful.To="#87000000"
                                            cs:Office2016White.To="#FF92C0E0"
                                           />
                        </Storyboard>
                    </VisualState>
