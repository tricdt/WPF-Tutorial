                                cs:Office2016Colorful.BorderBrush="#47000000"
                                cs:Office2016White.BorderBrush="#FF7EABEA"
                               >
                            <Border.Background>
                                <LinearGradientBrush cs:Name="LinearGradientBrush_0050"
                                                     EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                    >
                                    <GradientStop Color="#FFECF3FC"
                                                  cs:Name="GradientStop_0003"
                                                  Offset="0"
                                                  cs:Office2013DarkGray.Color="#FFECF4FC"
                                                  cs:Office2013LightGray.Color="#FFECF4FC"
                                                  cs:Office2016Black.Color="#FF969696"
