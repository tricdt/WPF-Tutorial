                                cs:Office2013LightGray.BorderBrush="#FF569DE5"
                                cs:Office2016Black.BorderBrush="#7B000000"
                                cs:Office2016Colorful.BorderBrush="#68000000"
                                cs:Office2016White.BorderBrush="#FF5691E5"
                               >
