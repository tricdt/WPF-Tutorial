                                                  cs:Office2013LightGray.Color="#FFC4DBFC"
                                                  cs:Office2016Black.Color="#FF696969"
                                                  cs:Office2016Colorful.Color="#FFAEAEAE"
                                                  cs:Office2016White.Color="#FFC4DBFC"
                                                 />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                        <ContentPresenter x:Name="contentPresenter"
                                          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                            <Setter Property="MinWidth"
                                    TargetName="Control"
                                    Value="44"/>
                            <Setter Property="MinHeight"
                                    TargetName="Control"
                                    Value="44"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <ControlTemplate x:Key="{dxet:CalculatorThemeKey ResourceKey=DisplayTemplate}" TargetType="{x:Type ContentControl}">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver"/>
                    <VisualState x:Name="Pressed"/>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DefaultState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DisabledState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="Content"
                                            Storyboard.TargetProperty="(ContentControl.Foreground).(SolidColorBrush.Color)"
                                            To="#FFACACAC"
                                            cs:Office2013DarkGray.To="#FFABABAB"
                                            cs:Office2013LightGray.To="#FFABABAB"
                                            cs:Office2016Black.To="#E3000000"
                                            cs:Office2016Colorful.To="#E3000000"
                                            cs:Office2016White.To="#FFACACAC"
                                           />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
                <Border x:Name="DisabledState"
                        Background="#FFFFFFFF"
                        BorderBrush="#FFC6C6C6"
                        BorderThickness="1"
                        cs:Name="Border_0017"
                        Opacity="0"
                        cs:Office2013DarkGray.Background="#FFE3E3E3"
                        cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                        cs:Office2013LightGray.Background="#FFF6F6F6"
                        cs:Office2013LightGray.BorderBrush="#FFABABAB"
                        cs:Office2016Black.Background="#FFC3C3C3"
                        cs:Office2016Black.BorderBrush="#60000000"
                        cs:Office2016Colorful.Background="#FFFFFFFF"
                        cs:Office2016Colorful.BorderBrush="#47000000"
                        cs:Office2016White.Background="#FFFFFFFF"
                        cs:Office2016White.BorderBrush="#FFC6C6C6"
                       />
                <Border x:Name="DefaultState"
                        Background="#FFFFFFFF"
                        BorderBrush="#FFC6C6C6"
                        BorderThickness="1"
                        cs:Name="Border_0019"
                        cs:Office2013DarkGray.Background="#FFFFFFFF"
                        cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                        cs:Office2013LightGray.Background="#FFFFFFFF"
                        cs:Office2013LightGray.BorderBrush="#FFABABAB"
                        cs:Office2016Black.Background="#FFD4D4D4"
                        cs:Office2016Black.BorderBrush="#60000000"
                        cs:Office2016Colorful.Background="#FFFFFFFF"
                        cs:Office2016Colorful.BorderBrush="#68000000"
                        cs:Office2016White.Background="#FFFFFFFF"
                        cs:Office2016White.BorderBrush="#FFC6C6C6"
                       />
                <ContentControl x:Name="Content"
                                dx:FocusHelper2.Focusable="False"
                                Margin="0,0,2,2"
                                Template="{DynamicResource {dxet:CalculatorThemeKey ResourceKey=DisplayContentTemplate}}">
                    <ContentControl.Foreground>
                        <SolidColorBrush Color="#FF0072C6"
                                         cs:Name="SolidColorBrush_0001"
                                         cs:Office2013DarkGray.Color="#FF333333"
                                         cs:Office2013LightGray.Color="#FF333333"
                                         cs:Office2016Black.Color="#FF444444"
                                         cs:Office2016Colorful.Color="#FF444444"
                                         cs:Office2016White.Color="#FF0072C6"
                                        />
                    </ContentControl.Foreground>
                </ContentControl>
            </Grid>
        </Grid>
    </ControlTemplate>

    <Style x:Key="{dxet:CalculatorThemeKey ResourceKey=DisplayStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="dx:FocusHelper2.Focusable" Value="False"/>
        <Setter Property="FontSize" Value="21"/>
        <Setter Property="Margin" Value="0,0,0,10"/>
        <Setter Property="Template" Value="{DynamicResource {dxet:CalculatorThemeKey ResourceKey=DisplayTemplate}}"/>
    </Style>

    <ControlTemplate x:Key="{dxet:CalculatorThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
        <Grid>
            <Grid.Resources>
                <dx:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
                <dx:BoolToVisibilityInverseConverter x:Key="BoolToVisibilityInverseConverter"/>
                <dx:BoolToThicknessConverter x:Key="BoolToThicknessConverter"
                                             ThicknessFalse="11"
                                             ThicknessTrue="13"/>
            </Grid.Resources>
            <Border Background="#FFFFFFFF"
                    BorderBrush="#FFC6C6C6"
                    BorderThickness="1"
                    cs:Name="Border_0021"
                    Visibility="{Binding ShowBorder, Converter={StaticResource BoolToVisibilityConverter}}"
                    cs:Office2013DarkGray.Background="#FFE3E3E3"
                    cs:Office2013DarkGray.BorderBrush="#FFABABAB"
                    cs:Office2013LightGray.Background="#FFF6F6F6"
                    cs:Office2013LightGray.BorderBrush="#FFABABAB"
                    cs:Office2016Black.Background="#FFB2B2B2"
                    cs:Office2016Black.BorderBrush="#60000000"
                    cs:Office2016Colorful.Background="#FFFFFFFF"
                    cs:Office2016Colorful.BorderBrush="#47000000"
                    cs:Office2016White.Background="#FFFFFFFF"
                    cs:Office2016White.BorderBrush="#FFC6C6C6"
                   />
            <Grid Background="#FFFFFFFF"
                  cs:Name="Grid_0001"
                  Visibility="{Binding ShowBorder, Converter={StaticResource BoolToVisibilityInverseConverter}}"
                  cs:Office2013DarkGray.Background="#FFE3E3E3"
                  cs:Office2013LightGray.Background="#FFF6F6F6"
                  cs:Office2016Black.Background="#FFB2B2B2"
                  cs:Office2016Colorful.Background="#FFFFFFFF"
                  cs:Office2016White.Background="#FFFFFFFF"
                 />
            <ContentPresenter Margin="{Binding ShowBorder, Converter={StaticResource BoolToThicknessConverter}}"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:CalculatorThemeKey ResourceKey=PopupContentContainerTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                cs:Name="Border_0022"
                Padding="11"
                cs:Office2013DarkGray.Background="#FFE3E3E3"
                cs:Office2013LightGray.Background="#FFF6F6F6"
                cs:Office2016Black.Background="#FFB2B2B2"
                cs:Office2016Colorful.Background="#FFFFFFFF"
                cs:Office2016White.Background="#FFFFFFFF"
               >
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

</ResourceDictionary>