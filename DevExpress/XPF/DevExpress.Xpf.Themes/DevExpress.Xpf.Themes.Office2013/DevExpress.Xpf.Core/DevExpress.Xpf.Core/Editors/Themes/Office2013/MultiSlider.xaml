<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxei="http://schemas.devexpress.com/winfx/2008/xaml/editors/internal"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2013.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/GradientMultiSlider.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=NextThumbButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=NextThumbButtonTemplate}}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>

    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=PreviousThumbButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=PreviousThumbButtonTemplate}}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>

    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=FlipThumbsButtonStyle}" TargetType="{x:Type Button}">
        <Style.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Width" Value="44"/>
                <Setter Property="Height" Value="44"/>
            </Trigger>
        </Style.Triggers>
        <Setter Property="Template" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=FlipThumbsButtonTemplate}}"/>
        <Setter Property="Width" Value="22"/>
        <Setter Property="Height" Value="22"/>
    </Style>

    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=SelectedThumbContentPresenterStyle}" TargetType="{x:Type ContentPresenter}">
        <Setter Property="Width" Value="14"/>
        <Setter Property="Height" Value="14"/>
        <Setter Property="ContentTemplate" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=SelectedThumbContentPresenterContentTemplate}}"/>
    </Style>

    <DataTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=SelectedThumbContentPresenterContentTemplate}">
        <Border BorderBrush="#FF000000"
                BorderThickness="1"
                cs:Name="Border_0001"
                cs:Office2013DarkGray.BorderBrush="#FF808080"
                cs:Office2013LightGray.BorderBrush="#FF808080"
                cs:Office2016Black.BorderBrush="#FF444444"
                cs:Office2016Colorful.BorderBrush="#FF3C3C3C"
                cs:Office2016White.BorderBrush="#FF000000"
               >
            <Border.Background>
                <SolidColorBrush Color="{Binding Path=Color}"/>
            </Border.Background>
        </Border>
    </DataTemplate>

    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=PreviousThumbButtonTemplate}">
        <Grid x:Name="Root"
              Background="#00000000"
              cs:Name="Grid_0001"
              Height="22"
              Width="22"
              cs:Office2013DarkGray.Background="#00000000"
              cs:Office2013LightGray.Background="#00000000"
              cs:Office2016Black.Background="#00000000"
              cs:Office2016Colorful.Background="#00000000"
              cs:Office2016White.Background="#00000000"
             >
            <Path x:Name="Path"
                  cs:Name="Path_0001"
                  Data="M5,0L0,6L5,12Z"
                  Fill="#FF333333"
                  Height="10"
                  Stretch="Fill"
                  Width="5"
                  cs:Office2013DarkGray.Fill="#FF595959"
                  cs:Office2013LightGray.Fill="#FF595959"
                  cs:Office2016Black.Fill="#BA000000"
                  cs:Office2016Colorful.Fill="#87000000"
                  cs:Office2016White.Fill="#FF333333"
                 />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter cs:Name="Setter_0001"
                        Property="Fill"
                        TargetName="Path"
                        Value="#FF0072C6"
                        cs:Office2013DarkGray.Value="#FF0072C6"
                        cs:Office2013LightGray.Value="#FF0072C6"
                        cs:Office2016Black.Value="#BA000000"
                        cs:Office2016Colorful.Value="#BA000000"
                        cs:Office2016White.Value="#FF0072C6"
                       />
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter cs:Name="Setter_0002"
                        Property="Fill"
                        TargetName="Path"
                        Value="#FF0072C6"
                        cs:Office2013DarkGray.Value="#FF0072C6"
                        cs:Office2013LightGray.Value="#FF0072C6"
                        cs:Office2016Black.Value="#BA000000"
                        cs:Office2016Colorful.Value="#BA000000"
                        cs:Office2016White.Value="#FF0072C6"
                       />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity"
                        TargetName="Root"
                        Value="0.35"/>
            </Trigger>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Width"
                        TargetName="Root"
                        Value="44"/>
                <Setter Property="Height"
                        TargetName="Root"
                        Value="44"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=NextThumbButtonTemplate}">
        <Grid x:Name="Root"
              Background="#00000000"
              cs:Name="Grid_0002"
              Height="22"
              Width="22"
              cs:Office2013DarkGray.Background="#00000000"
              cs:Office2013LightGray.Background="#00000000"
              cs:Office2016Black.Background="#00000000"
              cs:Office2016Colorful.Background="#00000000"
              cs:Office2016White.Background="#00000000"
             >
            <Path x:Name="Path"
                  cs:Name="Path_0002"
                  Data="M0,0L5,6L0,12Z"
                  Fill="#FF333333"
                  Height="10"
                  Stretch="Fill"
                  Width="5"
                  cs:Office2013DarkGray.Fill="#FF595959"
                  cs:Office2013LightGray.Fill="#FF595959"
                  cs:Office2016Black.Fill="#BA000000"
                  cs:Office2016Colorful.Fill="#87000000"
                  cs:Office2016White.Fill="#FF333333"
                 />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter cs:Name="Setter_0003"
                        Property="Fill"
                        TargetName="Path"
                        Value="#FF0072C6"
                        cs:Office2013DarkGray.Value="#FF0072C6"
                        cs:Office2013LightGray.Value="#FF0072C6"
                        cs:Office2016Black.Value="#BA000000"
                        cs:Office2016Colorful.Value="#BA000000"
                        cs:Office2016White.Value="#FF0072C6"
                       />
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter cs:Name="Setter_0004"
                        Property="Fill"
                        TargetName="Path"
                        Value="#FF0072C6"
                        cs:Office2013DarkGray.Value="#FF0072C6"
                        cs:Office2013LightGray.Value="#FF0072C6"
                        cs:Office2016Black.Value="#BA000000"
                        cs:Office2016Colorful.Value="#BA000000"
                        cs:Office2016White.Value="#FF0072C6"
                       />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity"
                        TargetName="Root"
                        Value="0.35"/>
            </Trigger>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Width"
                        TargetName="Root"
                        Value="44"/>
                <Setter Property="Height"
                        TargetName="Root"
                        Value="44"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=FlipThumbsButtonTemplate}">
        <Grid x:Name="Root"
              Background="Transparent"
              cs:Name="Grid_0003"
              cs:Office2013DarkGray.Background="Transparent"
              cs:Office2013LightGray.Background="Transparent"
              cs:Office2016Black.Background="Transparent"
              cs:Office2016Colorful.Background="Transparent"
              cs:Office2016White.Background="Transparent"
             >
            <Border x:Name="HoverState"
                    Background="#FFCDE6F7"
                    cs:Name="Border_0002"
                    Opacity="0"
                    cs:Office2013DarkGray.Background="#FFCDE6F7"
                    cs:Office2013LightGray.Background="#FFCDE6F7"
                    cs:Office2016Black.Background="#FF969696"
                    cs:Office2016Colorful.Background="#FFC5C5C5"
                    cs:Office2016White.Background="#FFCDE6F7"
                   />
            <Border x:Name="PressedState"
                    Background="#FF92C0E0"
                    cs:Name="Border_0003"
                    Opacity="0"
                    cs:Office2013DarkGray.Background="#FF92C0E0"
                    cs:Office2013LightGray.Background="#FF92C0E0"
                    cs:Office2016Black.Background="#FF696969"
                    cs:Office2016Colorful.Background="#FFAEAEAE"
                    cs:Office2016White.Background="#FF92C0E0"
                   />
            <Path x:Name="Path"
                  cs:Name="Path_0003"
                  Data="F1 M 1,4L 2,4L 2,3L 3,3L 3,2L 4,2L 4,1L 5,1L 5,2L 5,4L 13,4L 13,4.99991L 5,4.99999L 5,7L 5,8L 4,8L 4,7L 3,7L 3,6L 2,6L 2,5L 1,5L 1,4 Z M 13,10L 12,10L 12,11L 11,11L 11,12L 10,12L 10,13L 9,13L 9,12L 9,10L 1,10L 1,9.00009L 9,9.00001L 9,7L 9,6L 10,6L 10,7.00001L 11,7.00001L 11,8.00001L 12,8.00001L 12,9L 13,9L 13,10 Z "
                  Fill="#FF333333"
                  Height="12"
                  Margin="5"
                  Stretch="Fill"
                  Width="12"
                  cs:Office2013DarkGray.Fill="#FF595959"
                  cs:Office2013LightGray.Fill="#FF595959"
                  cs:Office2016Black.Fill="#BA000000"
                  cs:Office2016Colorful.Fill="#87000000"
                  cs:Office2016White.Fill="#FF333333"
                 />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="0"/>
            </Trigger>
            <!-- <Trigger Property="IsSelected" Value="True">
                <Setter TargetName="HoverState"   Property="Opacity" Value="0" />
                <Setter TargetName="PressedState" Property="Opacity" Value="1" />
            </Trigger> -->
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="0"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity"
                        TargetName="Root"
                        Value="0.35"/>
            </Trigger>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Margin"
                        TargetName="Path"
                        Value="12"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=ThumbTemplate}" TargetType="{x:Type dxei:GradientMultiSliderThumb}">
        <Grid>
            <Path x:Name="PART_DarkPath"
                  cs:Name="Path_0004"
                  Data="F1 M 15,14L 21,7L 27,14L 27,25L 15,25L 15,14 Z "
                  Fill="#FF808080"
                  Height="17"
                  Stretch="Fill"
                  Width="12"
                  cs:Office2013DarkGray.Fill="#FFFFFFFF"
                  cs:Office2013LightGray.Fill="#FFFFFFFF"
                  cs:Office2016Black.Fill="#BA000000"
                  cs:Office2016Colorful.Fill="#87000000"
                  cs:Office2016White.Fill="#FF808080"
                 />
            <Path x:Name="PART_WhitePath"
                  cs:Name="Path_0005"
                  Data="F1 M 16,14L 21,9L 26,14L 26,24L 16,24L 16,14 Z "
                  Fill="#FFFFFFFF"
                  Height="15"
                  Stretch="Fill"
                  Width="10"
                  cs:Office2013DarkGray.Fill="#FFFFFFFF"
                  cs:Office2013LightGray.Fill="#FFFFFFFF"
                  cs:Office2016Black.Fill="#E3FFFFFF"
                  cs:Office2016Colorful.Fill="#FFFFFFFF"
                  cs:Office2016White.Fill="#FFFFFFFF"
                 />
            <Border x:Name="PART_ColorPath"
                    Height="8"
                    Margin="0,5,0,0"
                    Width="8">
                <Border.Background>
                    <SolidColorBrush Color="{TemplateBinding Color}"/>
                </Border.Background>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True"></Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!--END WPF-->

</ResourceDictionary>