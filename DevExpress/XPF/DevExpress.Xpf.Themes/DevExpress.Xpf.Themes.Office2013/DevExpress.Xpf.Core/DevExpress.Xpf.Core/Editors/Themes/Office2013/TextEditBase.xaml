<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2013.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/TextEditBase.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!--END WPF-->

    <SolidColorBrush x:Key="{dxet:TextEditThemeKey ResourceKey=CaretBrush}"
                     Color="#FF3C3C3C"
                     cs:Name="SolidColorBrush_0001"
                     cs:Office2013DarkGray.Color="#FF262626"
                     cs:Office2013LightGray.Color="#FF444444"
                     cs:Office2016Black.Color="#FF444444"
                     cs:Office2016Colorful.Color="#FF444444"
                     cs:Office2016White.Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="TextEditSelectionBackground"
                     Color="#5F0072C6"
                     cs:Name="SolidColorBrush_0002"
                     cs:Office2013DarkGray.Color="#5F0072C6"
                     cs:Office2013LightGray.Color="#5F0072C6"
                     cs:Office2016Black.Color="#7F0173C7"
                     cs:Office2016Colorful.Color="#7F0173C7"
                     cs:Office2016White.Color="#5F0072C6"/>

    <SolidColorBrush x:Key="TextEditSelectionForeground"
                     Color="#8F000000"
                     cs:Name="SolidColorBrush_0003"
                     cs:Office2013DarkGray.Color="#8F000000"
                     cs:Office2013LightGray.Color="#7F262626"
                     cs:Office2016Black.Color="#E3000000"
                     cs:Office2016Colorful.Color="#E3000000"
                     cs:Office2016White.Color="#8F000000"/>

</ResourceDictionary>