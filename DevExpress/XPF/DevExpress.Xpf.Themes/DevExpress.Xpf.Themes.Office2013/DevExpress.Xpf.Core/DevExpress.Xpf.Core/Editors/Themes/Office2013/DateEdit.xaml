    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=ClearButtonTemplate}" TargetType="{x:Type Button}">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DefaultState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="DefaultState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="DefaultState"
                    Background="{StaticResource Button.DefaultState.Border2}"
                    BorderBrush="{StaticResource Button.DefaultState.Border1}"
                    BorderThickness="1"
                    Padding="1"/>
            <Border x:Name="HoverState"
                    Background="{StaticResource Button.HoverState.Border2}"
                    BorderBrush="{StaticResource Button.HoverState.Border1}"
                    BorderThickness="1"
                    Opacity="0"
                    Padding="1"/>
            <Border x:Name="PressedState"
                    Background="{StaticResource Button.PressedState.Border2}"
                    BorderBrush="{StaticResource Button.PressedState.Border1}"
                    BorderThickness="1"
                    Opacity="0"
                    Padding="1"/>
            <dx:ContentContainer x:Name="Content"
                                 cs:Name="ContentContainer_0001"
                                 ForegroundSolidColor5="#FF3C3C3C"
                                 cs:Office2013DarkGray.ForegroundSolidColor5="#FF262626"
                                 cs:Office2013LightGray.ForegroundSolidColor5="#FF444444"
                                 cs:Office2016Black.ForegroundSolidColor5="#E3000000"
                                 cs:Office2016Colorful.ForegroundSolidColor5="#E3000000"
                                 cs:Office2016White.ForegroundSolidColor5="#FF3C3C3C"
                                >
                <ContentPresenter x:Name="ContentPresenter1"
                                  Content="{TemplateBinding Content}"
                                  ContentTemplate="{TemplateBinding ContentTemplate}"
                                  HorizontalAlignment="Center"
                                  Margin="16,2,16,3"
                                  VerticalAlignment="Center"/>
            </dx:ContentContainer>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Margin"
                        TargetName="ContentPresenter1"
                        Value="12"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=LeftArrowTemplate}" TargetType="{x:Type RepeatButton}">
        <Grid x:Name="Root"
              Background="#00000000"
              cs:Name="Grid_0001"
              Height="15"
              SnapsToDevicePixels="True"
              Width="15"
              cs:Office2013DarkGray.Background="#00000000"
              cs:Office2013LightGray.Background="#00000000"
              cs:Office2016Black.Background="#00000000"
              cs:Office2016Colorful.Background="#00000000"
              cs:Office2016White.Background="#00000000"
             >
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0001"
                                            Duration="0"
                                            Storyboard.TargetName="Path"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF0072C6"
                                            cs:Office2013DarkGray.To="#FF2B579A"
                                            cs:Office2013LightGray.To="#FF19478A"
                                            cs:Office2016Black.To="#E3000000"
                                            cs:Office2016Colorful.To="#BA000000"
                                            cs:Office2016White.To="#FF0072C6"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation cs:Name="ColorAnimation_0002"
                                            Duration="0"
                                            Storyboard.TargetName="Path"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF0072C6"
                                            cs:Office2013DarkGray.To="#FF2B579A"
                                            cs:Office2013LightGray.To="#FF19478A"
                                            cs:Office2016Black.To="#E3000000"
                                            cs:Office2016Colorful.To="#BA000000"
                                            cs:Office2016White.To="#FF0072C6"
                                           />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Path x:Name="Path"
                  cs:Name="Path_0001"
                  Data="F1 M 0,5L 5,0L 5,10L 0,5 Z  "
                  Fill="#FF666666"
                  Height="8"
                  Stretch="Fill"
                  Width="4"
                  cs:Office2013DarkGray.Fill="#FF666666"
                  cs:Office2013LightGray.Fill="#FF666666"
                  cs:Office2016Black.Fill="#BA000000"
                  cs:Office2016Colorful.Fill="#87000000"
                  cs:Office2016White.Fill="#FF666666"
                 />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Height"
                        TargetName="Root"
                        Value="44"/>
                <Setter Property="Width"
                        TargetName="Root"
                        Value="44"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

