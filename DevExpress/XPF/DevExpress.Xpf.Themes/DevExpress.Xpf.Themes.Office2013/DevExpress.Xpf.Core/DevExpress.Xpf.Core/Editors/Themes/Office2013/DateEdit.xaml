                    cs:Office2016Colorful.BorderBrush="#FF444444"
                    cs:Office2016White.BorderBrush="#FF0072C6"
                   />
            <dx:ContentContainer x:Name="ButtonContent"
                                 cs:Name="ContentContainer_0003"
                                 Foreground="#FF3C3C3C"
                                 cs:Office2013DarkGray.Foreground="#FF262626"
                                 cs:Office2013LightGray.Foreground="#FF444444"
                                 cs:Office2016Black.Foreground="#E3000000"
                                 cs:Office2016Colorful.Foreground="#E3000000"
                                 cs:Office2016White.Foreground="#FF3C3C3C"
                                >
                <ContentPresenter x:Name="ContentPresenter1"
                                  Content="{TemplateBinding Content}"
                                  ContentTemplate="{TemplateBinding ContentTemplate}"
                                  HorizontalAlignment="Center"
                                  Margin="4,0,4,0"
                                  VerticalAlignment="Center"/>
            </dx:ContentContainer>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup>
                    <VisualState x:Name="CellNormalState"/>
                    <VisualState x:Name="CellTodayState">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="Today"
                                                           Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="CellFocusedState">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="Focused"
                                                           Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="ButtonContent"
                                                           Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FF3C3C3C"
                                                         cs:Name="SolidColorBrush_0006"
                                                         cs:Office2013DarkGray.Color="#FF333333"
                                                         cs:Office2013LightGray.Color="#FF333333"
                                                         cs:Office2016Black.Color="#FFFFFFFF"
                                                         cs:Office2016Colorful.Color="#FFFFFFFF"
                                                         cs:Office2016White.Color="#FF3C3C3C"
                                                        />
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="CellInactiveState">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0"
                                                           Storyboard.TargetName="ButtonContent"
                                                           Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FFD6D6D6"
                                                         cs:Name="SolidColorBrush_0007"
                                                         cs:Office2013DarkGray.Color="#FFABABAB"
                                                         cs:Office2013LightGray.Color="#FFABABAB"
                                                         cs:Office2016Black.Color="#47000000"
                                                         cs:Office2016Colorful.Color="#47000000"
                                                         cs:Office2016White.Color="#FFD6D6D6"
                                                        />
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Margin"
                        TargetName="ContentPresenter1"
                        Value="12"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="{dxet:DateEditThemeKey ResourceKey=WeekdayNameStyle}" TargetType="{x:Type TextBlock}"/>

    <Style x:Key="{dxet:DateEditThemeKey ResourceKey=WeekNumbersStyle}" TargetType="{x:Type TextBlock}">
        <Style.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Margin" Value="12"/>
            </Trigger>
        </Style.Triggers>
        <Setter cs:Name="Setter_0001"
                Property="Foreground"
                Value="#FF3C3C3C"
                cs:Office2013DarkGray.Value="#FF262626"
                cs:Office2013LightGray.Value="#FF444444"
                cs:Office2016Black.Value="#E3000000"
                cs:Office2016Colorful.Value="#E3000000"
                cs:Office2016White.Value="#FF3C3C3C"
               />
        <Setter Property="TextElement.FontSize" Value="9"/>
        <Setter Property="Margin" Value="0,3,12,0"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

    <Style x:Key="{dxet:DateEditThemeKey ResourceKey=WeekdayAbbreviationStyle}" TargetType="{x:Type TextBlock}">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter cs:Name="Setter_0002"
                Property="Foreground"
                Value="#FF3C3C3C"
                cs:Office2013DarkGray.Value="#FF262626"
                cs:Office2013LightGray.Value="#FF444444"
                cs:Office2016Black.Value="#E3000000"
                cs:Office2016Colorful.Value="#E3000000"
                cs:Office2016White.Value="#FF3C3C3C"
               />
        <Setter Property="MinHeight" Value="0"/>
        <Setter Property="MinWidth" Value="0"/>
    </Style>

    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=WeekNumberDelimeter}">
        <Border Background="#FF3C3C3C"
                cs:Name="Border_0006"
                Height="1"
                cs:Office2013DarkGray.Background="#FF333333"
                cs:Office2013LightGray.Background="#FF333333"
                cs:Office2016Black.Background="#30000000"
                cs:Office2016Colorful.Background="#30000000"
                cs:Office2016White.Background="#FF3C3C3C"
               />
    </ControlTemplate>

    <Style x:Key="{dxet:DateEditThemeKey ResourceKey=CellButtonStyle}" TargetType="{x:Type Button}">
        <Setter cs:Name="Setter_0003"
                Property="Background"
                Value="#FF3C3C3C"
                cs:Office2013DarkGray.Value="#FF333333"
                cs:Office2013LightGray.Value="#FF333333"
                cs:Office2016Black.Value="#30000000"
                cs:Office2016Colorful.Value="#30000000"
                cs:Office2016White.Value="#FF3C3C3C"
               />
        <Setter Property="Template" Value="{DynamicResource {dxet:DateEditThemeKey ResourceKey=CellButtonTemplate}}"/>
    </Style>

    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=YearInfoTemplate}">
        <Grid x:Name="PART_ContentGrid">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=MonthInfoTemplate}">
        <Grid x:Name="PART_ContentGrid">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition Height="Auto"/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <ContentControl Grid.Column="2"
                            Grid.ColumnSpan="7"
                            Grid.Row="1"
                            Template="{DynamicResource {dxet:DateEditThemeKey ResourceKey=MonthInfoDelimeter}}"/>
            <ContentControl x:Name="WeekNumberDelimeter"
                            Grid.Column="1"
                            Grid.Row="2"
                            Grid.RowSpan="6"
                            Template="{DynamicResource {dxet:DateEditThemeKey ResourceKey=WeekNumberDelimeter}}"
                            Visibility="Collapsed"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateEditThemeKey ResourceKey=PopupContentTemplate}" TargetType="{x:Type ContentControl}">
        <dxepc:DateEditCalendar dx:FocusHelper2.Focusable="False" SnapsToDevicePixels="True"/>
    </ControlTemplate>

