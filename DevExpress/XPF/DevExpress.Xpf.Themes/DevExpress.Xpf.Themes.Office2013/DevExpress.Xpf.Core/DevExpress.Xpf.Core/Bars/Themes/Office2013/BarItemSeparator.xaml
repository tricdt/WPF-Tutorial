                    cs:Office2016White.Background="#FFFFFFFF"
                   />
        </dxc:PixelSnapper>
        <ControlTemplate.Triggers>
            <Trigger Property="dxc:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Margin"
                        TargetName="border"
