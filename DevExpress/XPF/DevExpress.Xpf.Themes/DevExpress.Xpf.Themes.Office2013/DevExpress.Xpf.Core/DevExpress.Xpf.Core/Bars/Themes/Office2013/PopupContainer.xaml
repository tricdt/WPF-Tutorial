<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys"
                    xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxbt:PopupControlContainerThemeKey ResourceKey=ControlTemplate}" TargetType="{x:Type dxb:PopupContainer}">
        <dx:DXContentPresenter Background="{TemplateBinding Background}" Content="{TemplateBinding Content}"/>
    </ControlTemplate>

    <!--BEGIN WPF-->

    <Style x:Key="{dxbt:OfficeTabletItemsControlThemeKey ResourceKey=BackButtonStyle}" TargetType="{x:Type ButtonBase}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="MinHeight" Value="25"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ButtonBase}">
                    <Grid Background="Transparent"
                          cs:Name="Grid_0001"
                          cs:Office2013DarkGray.Background="Transparent"
                          cs:Office2013LightGray.Background="Transparent"
                          cs:Office2016Black.Background="Transparent"
                          cs:Office2016Colorful.Background="Transparent"
                          cs:Office2016White.Background="Transparent"
                         >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Path x:Name="path"
                              cs:Name="Path_0001"
                              Data="F1 M 6,2L 7.46699,3.47572L 2.88984,8.05286L 7.41782,12.5808L 6,14L -1.32341e-007,8L 6,2 Z "
                              Fill="#FF666666"
                              Grid.Column="0"
                              Height="12"
                              HorizontalAlignment="Center"
                              Margin="8,3,8,3"
                              Stretch="Fill"
                              VerticalAlignment="Center"
                              Width="8"
                              cs:Office2013DarkGray.Fill="#FF666666"
                              cs:Office2013LightGray.Fill="#FF666666"
                              cs:Office2016Black.Fill="#BA000000"
                              cs:Office2016Colorful.Fill="#87000000"
                              cs:Office2016White.Fill="#FF666666"
                             />
                        <TextBlock x:Name="backText"
                                   cs:Name="TextBlock_0001"
                                   Foreground="#FF666666"
                                   Grid.Column="1"
                                   MaxWidth="60"
                                   Text="{TemplateBinding Content}"
                                   TextAlignment="Center"
                                   TextTrimming="CharacterEllipsis"
                                   VerticalAlignment="Center"
                                   cs:Office2013DarkGray.Foreground="#FF484848"
                                   cs:Office2013LightGray.Foreground="#FF484848"
                                   cs:Office2016Black.Foreground="#E3000000"
                                   cs:Office2016Colorful.Foreground="#E3000000"
                                   cs:Office2016White.Foreground="#FF666666"
                                  />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter cs:Name="Setter_0001"
                                    Property="Foreground"
                                    TargetName="backText"
                                    Value="#FF0072C6"
                                    cs:Office2013DarkGray.Value="#FF000000"
                                    cs:Office2013LightGray.Value="#FF000000"
                                    cs:Office2016Black.Value="#E3000000"
                                    cs:Office2016Colorful.Value="#E0000000"
                                    cs:Office2016White.Value="#FF0072C6"
                                   />
                            <Setter cs:Name="Setter_0002"
                                    Property="Fill"
                                    TargetName="path"
                                    Value="#FF0072C6"
                                    cs:Office2013DarkGray.Value="#FF222222"
                                    cs:Office2013LightGray.Value="#FF222222"
                                    cs:Office2016Black.Value="#BA000000"
                                    cs:Office2016Colorful.Value="#BA000000"
                                    cs:Office2016White.Value="#FF0072C6"
                                   />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxbt:OfficeTabletItemsControlThemeKey ResourceKey=CaptionStyle}" TargetType="{x:Type TextBlock}">
        <Setter Property="Margin" Value="8,-1,8,0"/>
        <Setter cs:Name="Setter_0003"
                Property="Foreground"
                Value="#FF666666"
                cs:Office2013DarkGray.Value="#FF484848"
                cs:Office2013LightGray.Value="#FF484848"
                cs:Office2016Black.Value="#E3000000"
                cs:Office2016Colorful.Value="#E3000000"
                cs:Office2016White.Value="#FF666666"
               />
    </Style>

    <Style x:Key="{dxbt:OfficeTabletItemsControlThemeKey ResourceKey=BackgroundStyle}" TargetType="{x:Type Border}">
        <Setter Property="MinHeight" Value="25"/>
        <Setter cs:Name="Setter_0004"
                Property="Background"
                Value="#FFDDDDDD"
                cs:Office2013DarkGray.Value="#FFDDDDDD"
                cs:Office2013LightGray.Value="#FFDDDDDD"
                cs:Office2016Black.Value="#60000000"
                cs:Office2016Colorful.Value="#FFC5C5C5"
                cs:Office2016White.Value="#FFDDDDDD"
               />
        <Setter cs:Name="Setter_0005"
                Property="BorderBrush"
                Value="#FFFFFFFF"
                cs:Office2013DarkGray.Value="#FFFFFFFF"
                cs:Office2013LightGray.Value="#FFFFFFFF"
                cs:Office2016Black.Value="#FFF0F0F0"
                cs:Office2016Colorful.Value="#FFFFFFFF"
                cs:Office2016White.Value="#FFFFFFFF"
               />
        <Setter Property="BorderThickness" Value="1"/>
    </Style>

    <!--END WPF-->

</ResourceDictionary>