<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxret="http://schemas.devexpress.com/winfx/2008/xaml/richedit/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="{dxret:RichEditControlThemeKey ResourceKey=ResizePicturePlaceholderRectangleStyle}" TargetType="{x:Type Rectangle}">
        <Setter cs:Name="Setter_0001"
                Property="Stroke"
                Value="#7FD6D6D6"
                cs:Office2013DarkGray.Value="#FFABABAB"
                cs:Office2013LightGray.Value="#FFABABAB"
                cs:Office2016Black.Value="#7FD6D6D6"
                cs:Office2016Colorful.Value="#7FD6D6D6"
                cs:Office2016White.Value="#7FD6D6D6"
               />
        <Setter Property="StrokeThickness" Value="2"/>
        <Setter Property="StrokeDashArray" Value="2,2"/>
        <Setter Property="Fill">
            <Setter.Value>
                <LinearGradientBrush cs:Name="LinearGradientBrush_0085"
                                     EndPoint="0.5,0"
                                     StartPoint="0.5,1"
                                    >
                    <GradientStop Color="#1FD6D6D6"
                                  cs:Name="GradientStop_0001"
                                  Offset="0"
                                  cs:Office2013DarkGray.Color="#FFC6C6C6"
                                  cs:Office2013LightGray.Color="#1FC6C6C6"
                                  cs:Office2016Black.Color="#1FD6D6D6"
                                  cs:Office2016Colorful.Color="#1FD6D6D6"
                                  cs:Office2016White.Color="#1FD6D6D6"
                                 />
                    <GradientStop Color="#1FD6D6D6"
                                  cs:Name="GradientStop_0002"
                                  Offset="1"
                                  cs:Office2013DarkGray.Color="#FFC6C6C6"
                                  cs:Office2013LightGray.Color="#1FC6C6C6"
                                  cs:Office2016Black.Color="#1FD6D6D6"
                                  cs:Office2016Colorful.Color="#1FD6D6D6"
                                  cs:Office2016White.Color="#1FD6D6D6"
                                 />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>