<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxret="http://schemas.devexpress.com/winfx/2008/xaml/richedit/themekeys"
                    xmlns:dxreui="http://schemas.devexpress.com/winfx/2008/xaml/richedit"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="{dxret:RichEditControlThemeKey ResourceKey=PageNumberWindowControlTemplate}" TargetType="{x:Type dxreui:PageNumberWindow}">
        <Border Background="#FFD6D6D6"
                cs:Name="Border_0001"
                Padding="12,5,12,5"
                SnapsToDevicePixels="True"
                UseLayoutRounding="True"
                cs:Office2013DarkGray.Background="#FFABABAB"
                cs:Office2013LightGray.Background="#FFABABAB"
                cs:Office2016Black.Background="#FFC3C3C3"
                cs:Office2016Colorful.Background="#FFC4C4C4"
                cs:Office2016White.Background="#FFD6D6D6"
               >
            <TextBlock cs:Name="TextBlock_0001"
                       FontSize="13"
                       Foreground="#FF3C3C3C"
                       Text="{TemplateBinding Text}"
                       UseLayoutRounding="True"
                       cs:Office2013DarkGray.Foreground="#FF262626"
                       cs:Office2013LightGray.Foreground="#FF444444"
                       cs:Office2016Black.Foreground="#E3000000"
                       cs:Office2016Colorful.Foreground="#E3000000"
                       cs:Office2016White.Foreground="#FF3C3C3C"
                      />
        </Border>
    </ControlTemplate>

</ResourceDictionary>