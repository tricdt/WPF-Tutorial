                            </DataTemplate>
                          </Setter.Value>
                        </Setter>
                      </DataTrigger>
                    </Style.Triggers>
                  </Style>
                </dxpg:PropertyDefinition.Style>
              </dxpg:PropertyDefinition>
              <dxpg:PropertyDefinition Path="Shape.TailLength" IsReadOnly="False">
