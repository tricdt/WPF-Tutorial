<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxtEx="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" xmlns:dxpg="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid" xmlns:dxprgt="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid/themekeys" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:utils="clr-namespace:DevExpress.Utils;assembly=DevExpress.Data.v24.1" xmlns:designer="http://schemas.devexpress.com/winfx/2008/xaml/reports/userdesigner" xmlns:dxrud="http://schemas.devexpress.com/winfx/2008/xaml/reports/userdesigner" xmlns:designernative="http://schemas.devexpress.com/winfx/2008/xaml/reports/userdesigner/internal" xmlns:propertygrid="http://schemas.devexpress.com/winfx/2008/xaml/reports/userdesigner/internal" xmlns:reportexplorernative="http://schemas.devexpress.com/winfx/2008/xaml/reports/userdesigner/internal" xmlns:designerthemes="http://schemas.devexpress.com/winfx/2008/xaml/reports/userdesigner/themekeys" xmlns:reportModel="http://schemas.devexpress.com/winfx/2008/xaml/reports/userdesigner/internal" xmlns:cm="clr-namespace:System.ComponentModel;assembly=WindowsBase" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <ResourceDictionary.MergedDictionaries>
    <dxdst:ResourceDictionaryEx Source="PropertyGrid/PropertyGridCustomizations.xaml" />
