  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Reports.UserDesigner.PropertyGrid.ReportDesignerPropertyGridControl}" TargetType="propertygrid:ReportDesignerPropertyGridControl">
    <Setter Property="Items" Value="{Binding Path=(dxrud:ReportDesigner.Document).ReportModel, RelativeSource={RelativeSource Self}, Converter={designernative:YieldConverter UseEnumerableConverter=True}}" />
    <Setter Property="SelectedItem" Value="{Binding Path=(dxrud:ReportDesigner.Document).ReportModel.SelectedModel, RelativeSource={RelativeSource Self}, Mode=TwoWay}" />
    <Setter Property="SelectedObjectModel" Value="{Binding Path=(dxrud:ReportDesigner.Document).Diagram.SelectionModel, RelativeSource={RelativeSource Self}}" />
    <Setter Property="reportModel:XtraReportDiagramItem.ReportModel" Value="{Binding Path=(dxrud:ReportDesigner.Document).ReportModel, RelativeSource={RelativeSource Self}}" />
    <Setter Property="MessageBoxServiceTemplate" Value="{DynamicResource {designerthemes:ReportDesignerThemeKey ResourceKey=DefaultMessageBoxServiceTemplate}}" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="propertygrid:ReportDesignerPropertyGridControl">
          <Grid>
            <Grid.RowDefinitions>
              <RowDefinition Height="Auto" />
              <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.Resources>
              <reportModel:HierarchyCollectionViewSource x:Key="ItemsSource" ItemsSource="{TemplateBinding Items}">
                <dxmvvm:Interaction.Behaviors>
                  <reportModel:SetFakeContainerFilterBehavior />
                </dxmvvm:Interaction.Behaviors>
                <reportModel:HierarchyCollectionViewSource.ItemTemplateSelector>
                  <reportModel:ReportExplorerItemTemplateSelector>
                    <reportModel:ReportExplorerItemTemplateSelector.ContainerTemplate>
                      <DataTemplate>
                        <reportModel:HierarchyCollectionViewSourceNode ItemsSource="{Binding Components}" />
                      </DataTemplate>
                    </reportModel:ReportExplorerItemTemplateSelector.ContainerTemplate>
                    <reportModel:ReportExplorerItemTemplateSelector.ItemTemplate>
                      <DataTemplate>
                        <reportModel:HierarchyCollectionViewSourceNode />
                      </DataTemplate>
                    </reportModel:ReportExplorerItemTemplateSelector.ItemTemplate>
                  </reportModel:ReportExplorerItemTemplateSelector>
                </reportModel:HierarchyCollectionViewSource.ItemTemplateSelector>
                <reportModel:HierarchyCollectionViewSource.SortDescriptions>
                  <cm:SortDescription PropertyName="ControlName" Direction="Ascending" />
                </reportModel:HierarchyCollectionViewSource.SortDescriptions>
              </reportModel:HierarchyCollectionViewSource>
            </Grid.Resources>
            <dxe:ComboBoxEdit Grid.IsSharedSizeScope="True" Margin="4" Name="controlsList" ItemsSource="{Binding Source={StaticResource ItemsSource}}" IsTextEditable="False" EditValue="{Binding SelectedItem, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
              <dxe:ComboBoxEdit.ItemTemplate>
                <DataTemplate>
                  <Grid>
                    <Grid.ColumnDefinitions>
                      <ColumnDefinition Width="auto" />
                      <ColumnDefinition Width="auto" SharedSizeGroup="controlName" />
                      <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <dx:DXImage Source="{Binding Icon}" Width="16" Margin="0,0,4,0" />
                    <TextBlock Text="{Binding ControlName}" Grid.Column="1" Margin="0,0,18,0" />
                    <TextBlock Text="{Binding Converter={reportexplorernative:ReportExplorerItemToTypeStringConverter}}" Grid.Column="2" />
                  </Grid>
                </DataTemplate>
              </dxe:ComboBoxEdit.ItemTemplate>
              <dxe:ComboBoxEdit.EditNonEditableTemplate>
                <ControlTemplate>
                  <Grid Margin="4,2">
                    <Grid.ColumnDefinitions>
                      <ColumnDefinition Width="auto" />
                      <ColumnDefinition Width="auto" SharedSizeGroup="controlName" />
                      <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <dx:DXImage Source="{Binding EditValue.Icon}" Width="16" Margin="0,0,4,0" />
                    <TextBlock Name="PART_Name" Text="{Binding EditValue.ControlName}" Grid.Column="1" Margin="0,0,18,0" />
                    <TextBlock Text="{Binding EditValue, Converter={reportexplorernative:ReportExplorerItemToTypeStringConverter}}" Grid.Column="2" />
                  </Grid>
                </ControlTemplate>
              </dxe:ComboBoxEdit.EditNonEditableTemplate>
            </dxe:ComboBoxEdit>
            <reportModel:ReportDesignerPropertyGrid Grid.Row="1" x:Name="propertyGrid" SelectedObject="{TemplateBinding SelectedObjectModel}" reportModel:PropertyGridHelper.SelectedObject="{Binding SelectedObject, RelativeSource={RelativeSource Self}}" ShowFavoriteProperties="{Binding Path=(designer:ReportDesigner.Designer).PropertyGridOptions.ShowFavoriteProperties, Mode=TwoWay, RelativeSource={RelativeSource Self}}" UseTabbedView="{Binding Path=(dxrud:ReportDesigner.Designer).PropertyGridOptions.UseTabbedView, RelativeSource={RelativeSource Self}}" ShowExpressionsInSeparateTab="{Binding Path=(dxrud:ReportDesigner.Designer).PropertyGridOptions.ShowExpressionsInSeparateTab, RelativeSource={RelativeSource Self}}" Style="{StaticResource propertyGridStyle}" IsEnabled="{Binding SelectedObjectModel, Converter={designernative:SelectedItemsLockedInEUDToBooleanConverter Inverse=True}, RelativeSource={RelativeSource TemplatedParent}}">
              <reportModel:ReportDesignerPropertyGrid.Resources>
                <ResourceDictionary>
                  <ResourceDictionary.MergedDictionaries>
                    <dxdst:ResourceDictionaryEx Source="PropertyGrid/RowControlRenderTemplate.xaml" />
                  </ResourceDictionary.MergedDictionaries>
                  <ControlTemplate x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=ControlTemplate}" TargetType="{x:Type dxpg:PropertyGridView}">
                    <Grid>
                      <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                      </Grid.RowDefinitions>
                      <ScrollViewer x:Name="PART_ScrollViewer" Grid.Row="1" Focusable="false" HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}" VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}" CanContentScroll="True">
                        <dxmvvm:Interaction.Behaviors>
                          <dx:ScrollViewerTouchBehavior Orientation="Vertical" />
                        </dxmvvm:Interaction.Behaviors>
                        <ItemsPresenter x:Name="PART_ItemsPresenter" />
                      </ScrollViewer>
                    </Grid>
                  </ControlTemplate>
                  <ItemsPanelTemplate x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=ItemsPanelTemplate}">
                    <reportModel:ReportDesignerVirtualizingStackPanel />
                  </ItemsPanelTemplate>
                </ResourceDictionary>
              </reportModel:ReportDesignerPropertyGrid.Resources>
              <dxmvvm:Interaction.Behaviors>
                <reportModel:CustomSortAndExpandBehavior />
              </dxmvvm:Interaction.Behaviors>
              <reportModel:PropertyGridHelper.SelectedObjectDelayedBinding>
                <MultiBinding Converter="{propertygrid:SelectedObjectToPropertiesSelectorConverter}">
                  <Binding Path="SelectedObjectModel" RelativeSource="{RelativeSource TemplatedParent}" />
                  <Binding Path="ActualPropertiesShowMode" RelativeSource="{RelativeSource Self}" />
                </MultiBinding>
              </reportModel:PropertyGridHelper.SelectedObjectDelayedBinding>
              <reportModel:CustomPropertyDefinition Type="{x:Type sys:Boolean}" HeaderShowMode="OnlyExpander">
                <reportModel:CustomPropertyDefinition.CellTemplate>
                  <DataTemplate>
                    <dxe:CheckEdit EditValue="{Binding Value, Mode=TwoWay}" Name="PART_Editor" HorizontalAlignment="Left" Content="{Binding Header}" IsEnabled="{Binding IsReadOnly, Converter={dxmvvm:BooleanNegationConverter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Path=(dxpg:PropertyGridHelper.PropertyGrid).Foreground, RelativeSource={RelativeSource Self}}" ContentTemplate="{StaticResource simpleHeaderTemplate}" />
                  </DataTemplate>
                </reportModel:CustomPropertyDefinition.CellTemplate>
              </reportModel:CustomPropertyDefinition>
              <dxpg:PropertyDefinition Type="{x:Type utils:DefaultBoolean}" HeaderShowMode="OnlyExpander">
                <dxpg:PropertyDefinition.CellTemplate>
                  <DataTemplate>
                    <dxe:CheckEdit EditValue="{Binding Value, Mode=TwoWay, Converter={dx:DefaultBooleanToNullableBooleanConverter}}" IsThreeState="True" Content="{Binding Header}" Foreground="{Binding Path=(dxpg:PropertyGridHelper.PropertyGrid).Foreground, RelativeSource={RelativeSource Self}}" ContentTemplate="{StaticResource simpleHeaderTemplate}" />
                  </DataTemplate>
                </dxpg:PropertyDefinition.CellTemplate>
              </dxpg:PropertyDefinition>
              <reportModel:CustomPropertyDefinition Path="Shape.ArrowWidth" IsReadOnly="False">
                <reportModel:CustomPropertyDefinition.Style>
                  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=http://schemas.devexpress.com/winfx/2008/xaml/propertygrid.PropertyDefinition}" TargetType="dxpg:PropertyDefinition">
                    <Style.Triggers>
                      <DataTrigger Binding="{Binding Path=(designer:ReportDesigner.Designer).PropertyGridOptions.UseOfficeInspiredPropertyGrid, RelativeSource={RelativeSource Self}}" Value="True">
                        <Setter Property="EditSettings" Value="{x:Null}" />
                        <Setter Property="CellTemplate">
                          <Setter.Value>
                            <DataTemplate>
                              <dxe:TrackBarEdit Grid.Column="1" EditValue="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource TrackBarEditItemStyle}" />
                            </DataTemplate>
                          </Setter.Value>
                        </Setter>
                      </DataTrigger>
                    </Style.Triggers>
                  </Style>
                </reportModel:CustomPropertyDefinition.Style>
              </reportModel:CustomPropertyDefinition>
              <dxpg:PropertyDefinition Path="Shape.ArrowHeight" IsReadOnly="False">
                <dxpg:PropertyDefinition.Style>
                  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=http://schemas.devexpress.com/winfx/2008/xaml/propertygrid.PropertyDefinition}" TargetType="dxpg:PropertyDefinition">
                    <Style.Triggers>
                      <DataTrigger Binding="{Binding Path=(designer:ReportDesigner.Designer).PropertyGridOptions.UseOfficeInspiredPropertyGrid, RelativeSource={RelativeSource Self}}" Value="True">
