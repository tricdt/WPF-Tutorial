                        <Setter Property="EditSettings" Value="{x:Null}" />
                        <Setter Property="CellTemplate">
                          <Setter.Value>
                            <DataTemplate>
                              <dxe:TrackBarEdit Grid.Column="1" EditValue="{Binding Value, Mode=TwoWay}" Minimum="0" Maximum="100" EditValueType="{x:Type sys:Int32}" />
                            </DataTemplate>
                          </Setter.Value>
                        </Setter>
                      </DataTrigger>
                    </Style.Triggers>
                  </Style>
                </dxpg:PropertyDefinition.Style>
              </dxpg:PropertyDefinition>
              <dxpg:PropertyDefinition Path="Shape.StarPointCount" IsReadOnly="False">
                <dxpg:PropertyDefinition.Resources>
                  <Style x:Key="ShapeBaseEditItem" TargetType="{x:Type dxe:SpinEdit}">
                    <Setter Property="MinValue" Value="0" />
                    <Setter Property="MaxValue" Value="100" />
                    <Setter Property="IsFloatValue" Value="False" />
                    <Setter Property="EditValueType" Value="{x:Type sys:Int32}" />
                  </Style>
                </dxpg:PropertyDefinition.Resources>
                <dxpg:PropertyDefinition.Style>
                  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=http://schemas.devexpress.com/winfx/2008/xaml/propertygrid.PropertyDefinition}" TargetType="dxpg:PropertyDefinition">
                    <Style.Triggers>
                      <DataTrigger Binding="{Binding Path=(designer:ReportDesigner.Designer).PropertyGridOptions.UseOfficeInspiredPropertyGrid, RelativeSource={RelativeSource Self}}" Value="True">
                        <Setter Property="EditSettings" Value="{x:Null}" />
                        <Setter Property="CellTemplate">
                          <Setter.Value>
                            <DataTemplate>
                              <dxe:SpinEdit Grid.Column="1" EditValue="{Binding Value}" NullValue="3" MinValue="3" MaxValue="9999" Style="{StaticResource ShapeBaseEditItem}" />
