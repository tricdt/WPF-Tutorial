<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BEGIN WPF-->

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2007Black.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Office2007Generic/Brushes.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=TextEditPadding}">4,2,4,2</Thickness>

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=ValidationErrorPadding}">3,0,1,0</Thickness>

    <Thickness x:Key="leftButtonsMargin">0,-1,0,-1</Thickness>

    <Thickness x:Key="rightButtonsMargin">0,-1,0,-1</Thickness>

    <!--END WPF-->

    <Color x:Key="{dxet:SpinEditThemeKey ResourceKey=ArrowBrushColor}">#FF7C7C7C</Color>

    <SolidColorBrush x:Key="{dxet:ButtonsThemeKey ResourceKey=Foreground}" Color="#FF7C7C7C"/>

    <!--CHECKEDIT-->

    <!--normal-->

    <SolidColorBrush x:Key="CheckEdit.Normal.FocusBorder.Background">#FFFFC200</SolidColorBrush>

    <SolidColorBrush x:Key="CheckEdit.Normal.OuterBorder.Background">#FFF4F4F4</SolidColorBrush>

    <SolidColorBrush x:Key="CheckEdit.Normal.OuterBorder.BorderBrush">#FF8E8F8F</SolidColorBrush>

    <LinearGradientBrush x:Key="CheckEdit.Normal.InnerBorder.Background"
                         EndPoint="0.806,0.899"
                         MappingMode="RelativeToBoundingBox"
                         StartPoint="0.194,0.101">
        <GradientStop Color="#FFCBCFD5" Offset="0"/>
        <GradientStop Color="#FFF6F6F6" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="CheckEdit.Normal.InnerBorder.BorderBrush"
                         EndPoint="0.795,0.904"
                         StartPoint="0.205,0.096">
        <GradientStop Color="#FFAEB3B9" Offset="0"/>
        <GradientStop Color="#FFE9E9EA" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="CheckEdit.Normal.TriState.BorderBrush">#FF18354A</SolidColorBrush>

    <LinearGradientBrush x:Key="CheckEdit.Normal.TriState.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFA0CDF2" Offset="0"/>
        <GradientStop Color="#FF5AB6EA" Offset="1"/>
        <GradientStop Color="#FF177FC7" Offset="0.514"/>
    </LinearGradientBrush>

    <!--hover-->

    <SolidColorBrush x:Key="CheckEdit.Hover.OuterBorder.Background">#FFFCF1C2</SolidColorBrush>

    <SolidColorBrush x:Key="CheckEdit.Hover.OuterBorder.BorderBrush">#FFCF9037</SolidColorBrush>

    <LinearGradientBrush x:Key="CheckEdit.Hover.InnerBorder.Background"
                         EndPoint="0.856,0.856"
                         MappingMode="RelativeToBoundingBox"
                         StartPoint="0.144,0.144">
        <GradientStop Color="#FFFAECC8" Offset="0"/>
        <GradientStop Color="#FFFDF6E4" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="CheckEdit.Hover.InnerBorder.BorderBrush"
                         EndPoint="0.795,0.904"
                         StartPoint="0.205,0.096">
        <GradientStop Color="#FFFCAD5D" Offset="0"/>
        <GradientStop Color="#FFFEDEBE" Offset="1"/>
    </LinearGradientBrush>

    <!--fill-->

    <SolidColorBrush x:Key="CheckEdit.CheckElement.Fill">#FF4C4E53</SolidColorBrush>

    <!--BUTTONEDIT-->

    <!--NORMAL-->

    <SolidColorBrush x:Key="Button.Normal.Outer.BorderBrush" Color="#00000000"/>

    <SolidColorBrush x:Key="Button.Normal.Outer.Background" Color="#00000000"/>

    <SolidColorBrush x:Key="Button.Normal.Inner.Background" Color="#00000000"/>

    <!--MOUSEOVER-->

    <LinearGradientBrush x:Key="Button.MouseOver.Outer.BorderBrush"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFA19460 " Offset="0"/>
        <GradientStop Color="#FFC6BA87 " Offset="1"/>
        <GradientStop Color="#FFA99D6C " Offset="0.3"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="Button.MouseOver.Outer.Background" Color="#FFFFFCF3"/>

    <LinearGradientBrush x:Key="Button.MouseOver.Inner.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFFFCE2 " Offset="0"/>
        <GradientStop Color="#FFFFE7A5 " Offset="1"/>
        <GradientStop Color="#FFFFE896 " Offset="0.5"/>
        <GradientStop Color="#FFFFD975 " Offset="0.5"/>
    </LinearGradientBrush>

    <!--PRESSED\CHECKED-->

    <LinearGradientBrush x:Key="Button.Pressed.Outer.BorderBrush"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF9A8F63 " Offset="0"/>
        <GradientStop Color="#FFB1A572 " Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="Button.Pressed.Outer.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFC0B9A5 " Offset="0"/>
        <GradientStop Color="#FFFFE694 " Offset="1"/>
        <GradientStop Color="#FFF5D4A6 " Offset="0.5"/>
        <GradientStop Color="#FFFFC67D " Offset="0.5"/>
        <GradientStop Color="#FFF1EBD5 " Offset="0.1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="Button.Pressed.Inner.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE0D4B2 " Offset="0"/>
        <GradientStop Color="#FFFFD456 " Offset="1"/>
        <GradientStop Color="#FFEFBD77 " Offset="0.5"/>
        <GradientStop Color="#FFFFA838 " Offset="0.5"/>
        <GradientStop Color="#FFEAE0BF " Offset="0.1"/>
    </LinearGradientBrush>

</ResourceDictionary>