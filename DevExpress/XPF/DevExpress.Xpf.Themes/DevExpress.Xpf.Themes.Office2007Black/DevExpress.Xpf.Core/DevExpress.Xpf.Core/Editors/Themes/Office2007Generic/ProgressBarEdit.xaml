<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxtEx="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <!--BEGIN WPF-->
  <ResourceDictionary.MergedDictionaries>
    <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2007Black.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/ProgressBarEdit.xaml" />
  </ResourceDictionary.MergedDictionaries>
  <!--END WPF-->
  <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideHorizontalTemplate}" TargetType="{x:Type ContentControl}">
    <Rectangle Fill="Transparent" />
  </ControlTemplate>
  <LinearGradientBrush x:Key="fillBrush" EndPoint="1,0.5" StartPoint="0,0.5">
    <GradientStop Color="#95FFFFFF" Offset="0.5" />
    <GradientStop Offset="1" />
    <GradientStop />
  </LinearGradientBrush>
  <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideVerticalTemplate}" TargetType="{x:Type ContentControl}">
    <Grid>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>
      <Border Grid.ColumnSpan="2" CornerRadius="1">
        <Border.Background>
          <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
            <GradientStop Color="#FF07B01F" Offset="0" />
            <GradientStop Color="#FF07AD1E" Offset="1" />
            <GradientStop Color="#FF1BDA38" Offset="0.5" />
            <GradientStop Color="#FF03D023" Offset="0.10" />
            <GradientStop Color="#FF03D023" Offset="0.90" />
          </LinearGradientBrush>
        </Border.Background>
      </Border>
      <Border Grid.Column="0" CornerRadius="1,1,0,0">
        <Border.Background>
          <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
            <GradientStop Color="#66FFFFFF" Offset="1" />
            <GradientStop Color="#E5FFFFFF" />
          </LinearGradientBrush>
        </Border.Background>
      </Border>
      <Rectangle RadiusX="1" RadiusY="1" Grid.RowSpan="2" x:Name="Background" Grid.ColumnSpan="2" Opacity="1">
        <!--BEGIN WPF-->
        <Rectangle.Fill>
          <MultiBinding>
            <MultiBinding.Converter>
              <dxe:ProgressBarHighlightConverter />
            </MultiBinding.Converter>
            <Binding Source="{StaticResource fillBrush}" />
            <Binding Path="ActualWidth" ElementName="Background" />
            <Binding Path="ActualHeight" ElementName="Background" />
            <Binding Path="(dxe:BaseEdit.OwnerEdit).StyleSettings.AccelerateRatio" />
          </MultiBinding>
        </Rectangle.Fill>
        <Rectangle.LayoutTransform>
          <RotateTransform Angle="-90" />
        </Rectangle.LayoutTransform>
        <!--END WPF-->
      </Rectangle>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideVerticalTemplate}" TargetType="{x:Type ContentControl}">
    <Rectangle Fill="Transparent" />
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelForegroundTemplate}" TargetType="{x:Type ContentControl}">
    <Rectangle Fill="Transparent" />
  </ControlTemplate>
  <Style x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideHorizontalStyle}" TargetType="{x:Type ContentControl}">
    <Setter Property="Focusable" Value="false" />
    <Setter Property="Template" Value="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideHorizontalTemplate}}" />
  </Style>
  <Style x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideVerticalStyle}" TargetType="{x:Type ContentControl}">
    <Setter Property="Focusable" Value="false" />
    <Setter Property="Template" Value="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideVerticalTemplate}}" />
  </Style>
  <Style x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideVerticalStyle}" TargetType="{x:Type ContentControl}">
    <Setter Property="Focusable" Value="false" />
    <Setter Property="Template" Value="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideVerticalTemplate}}" />
  </Style>
  <Style x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelForegroundStyle}" TargetType="{x:Type ContentControl}">
    <Setter Property="Focusable" Value="false" />
    <Setter Property="Template" Value="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelForegroundTemplate}}" />
  </Style>
  <ControlTemplate x:Key="{dxet:ProgressBarEditStyleThemeKey ResourceKey=ProgressBarMarqueeHorizontalAnimationElementTemplate}">
    <Grid>
      <Grid.OpacityMask>
        <LinearGradientBrush EndPoint="0,0.5" StartPoint="1,0.5">
          <GradientStop Offset="0" />
          <GradientStop Offset="1" />
          <GradientStop Color="White" Offset="0.5" />
        </LinearGradientBrush>
      </Grid.OpacityMask>
      <Grid.RowDefinitions>
        <RowDefinition Height="*" />
        <RowDefinition Height="*" />
      </Grid.RowDefinitions>
      <Border Grid.RowSpan="2">
        <Border.Background>
          <LinearGradientBrush EndPoint="0,0.5" StartPoint="1,0.5">
            <GradientStop Color="#FF07B01F" Offset="0" />
            <GradientStop Color="#FF07AD1E" Offset="1" />
            <GradientStop Color="#FF1BDA38" Offset="0.5" />
            <GradientStop Color="#FF03D023" Offset="0.25" />
            <GradientStop Color="#FF03D023" Offset="0.75" />
          </LinearGradientBrush>
        </Border.Background>
      </Border>
      <Border>
        <Border.Background>
          <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#7FFFFFFF" Offset="1" />
            <GradientStop Color="#BFFFFFFF" />
          </LinearGradientBrush>
        </Border.Background>
      </Border>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditStyleThemeKey ResourceKey=ProgressBarMarqueeVerticalAnimationElementTemplate}">
    <Grid>
      <Grid.OpacityMask>
        <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
          <GradientStop Offset="0" />
          <GradientStop Offset="1" />
          <GradientStop Color="White" Offset="0.5" />
        </LinearGradientBrush>
      </Grid.OpacityMask>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>
      <Border Grid.ColumnSpan="2">
        <Border.Background>
          <LinearGradientBrush EndPoint="0,0.5" StartPoint="1,0.5">
            <GradientStop Color="#FF07B01F" Offset="0" />
            <GradientStop Color="#FF07AD1E" Offset="1" />
            <GradientStop Color="#FF1BDA38" Offset="0.5" />
            <GradientStop Color="#FF03D023" Offset="0.25" />
            <GradientStop Color="#FF03D023" Offset="0.75" />
          </LinearGradientBrush>
        </Border.Background>
      </Border>
      <Border>
        <Border.Background>
          <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
            <GradientStop Color="#7FFFFFFF" Offset="1" />
            <GradientStop Color="#BFFFFFFF" />
          </LinearGradientBrush>
        </Border.Background>
      </Border>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditStyleThemeKey ResourceKey=MarqueeHorizontalAnimationControlTemplate}" TargetType="Control">
    <dxe:HorizontalAnimationElement AccelerateRatio="{Binding Path=(dxe:BaseEdit.OwnerEdit).StyleSettings.AccelerateRatio}">
      <ContentControl MinWidth="100" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" dx:FocusHelper2.Focusable="False" Template="{DynamicResource {dxet:ProgressBarEditStyleThemeKey ResourceKey=ProgressBarMarqueeHorizontalAnimationElementTemplate}}" />
    </dxe:HorizontalAnimationElement>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditStyleThemeKey ResourceKey=MarqueeVerticalAnimationControlTemplate}" TargetType="Control">
    <dxe:VerticalAnimationElement AccelerateRatio="{Binding Path=(dxe:BaseEdit.OwnerEdit).StyleSettings.AccelerateRatio}">
      <ContentControl MinHeight="100" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" dx:FocusHelper2.Focusable="False" Template="{DynamicResource {dxet:ProgressBarEditStyleThemeKey ResourceKey=ProgressBarMarqueeVerticalAnimationElementTemplate}}" />
    </dxe:VerticalAnimationElement>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=EmptyBorderTemplate}" TargetType="{x:Type ContentControl}">
    <Border Background="Transparent" Name="TextEditEmptyBorder">
      <ContentPresenter />
    </Border>
    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).EditMode}" Value="Standalone">
        <Setter Property="Padding" Value="1,2,1,2" />
      </DataTrigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
</ResourceDictionary>