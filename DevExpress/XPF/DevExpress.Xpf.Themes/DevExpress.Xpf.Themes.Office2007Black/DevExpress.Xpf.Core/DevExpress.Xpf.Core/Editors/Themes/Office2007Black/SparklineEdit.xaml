<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2007Black.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Office2007Generic/SparklineEdit.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = Brush}" Color="#FF222222"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = MaxPointBrush}" Color="#FFCC0000"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = MinPointBrush}" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = StartPointBrush}" Color="#FF222222"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = EndPointBrush}" Color="#FF222222"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = NegativePointBrush}" Color="#FF12C6E6"/>

    <SolidColorBrush x:Key="{dxet:SparklineEditThemeKey ResourceKey = MarkerBrush}" Color="#FF222222"/>

</ResourceDictionary>