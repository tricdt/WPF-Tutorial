                              Fill="{DynamicResource {dxscht:Office2007ThemeKey ResourceKey=ResourceNavigatorGlyphBrush}}"
                              Height="8"
                              Margin="0,0,0,1"
                              Stretch="Fill"
                              UseLayoutRounding="False"
                              Width="8"/>
                    </Grid>
                </Button>
                <Button x:Name="left"
