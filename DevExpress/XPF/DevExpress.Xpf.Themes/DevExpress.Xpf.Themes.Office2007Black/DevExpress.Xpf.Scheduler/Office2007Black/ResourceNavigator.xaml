                                            <GradientStop Color="#FFFFE894" Offset="1"/>
                                            <GradientStop Color="#FFFFEA9A" Offset="0.5"/>
                                            <GradientStop Color="#FFFFDC58" Offset="0.5"/>
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                        </Border>
                        <Border x:Name="Pressed1"
                                Background="#FF9A8F63"
                                Opacity="0">
