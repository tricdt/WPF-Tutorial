                        CommandParameter="{TemplateBinding SchedulerControl}"
                        MinHeight="20"
                        Padding="0"
                        Style="{TemplateBinding Property=ButtonNextStyle}"
                        Visibility="{Binding SchedulerControl.ResourceNavigator.ButtonNext.Visible, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent} }">
