                    <Grid x:Name="last_glyph"
                          Height="9"
                          HorizontalAlignment="Center"
                          Margin="1,0,0,0"
                          VerticalAlignment="Center"
                          Width="9">
                        <Path Data="F1 M 1386,918L 1390,922L 1390,918L 1391,918L 1391,926L 1390,926L 1390,922L 1386,926L 1386,922L 1382,926L 1382,918L 1386,922L 1386,918 Z "
                              Fill="{DynamicResource {dxscht:Office2007ThemeKey ResourceKey=ResourceNavigatorGlyphBrush}}"
                              Height="8"
                              Margin="0,0,0,1"
                              Stretch="Fill"
                              UseLayoutRounding="False"
                              Width="9"/>
                    </Grid>
                </Button>
                <Button x:Name="plus"
                        Command="{TemplateBinding CommandIncCount}"
                        CommandParameter="{TemplateBinding SchedulerControl}"
                        MinWidth="20"
                        Padding="0"
                        Style="{TemplateBinding Property=ButtonIncCountStyle}"
                        Visibility="{Binding SchedulerControl.ResourceNavigator.ButtonIncCount.Visible, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent} }">
                    <ToolTipService.ToolTip>
                        <ToolTip Content="{Binding Source={StaticResource SchedulerStringIdConverter}, ConverterParameter=Caption_IncreaseVisibleResourcesCount, Converter={StaticResource SchedulerStringIdConverter}}"/>
                    </ToolTipService.ToolTip>
                    <Grid x:Name="plus_glyph"
                          Height="9"
                          HorizontalAlignment="Center"
                          Margin="0"
                          VerticalAlignment="Center"
                          Width="8">
                        <Path Data="F1 M 1328,939L 1330,939L 1330,937C 1330,936.448 1330.45,936 1331,936C 1331.55,936 1332,936.448 1332,937L 1332,939L 1334,939C 1334.55,939 1335,939.448 1335,940C 1335,940.552 1334.55,941 1334,941L 1332,941L 1332,942.999C 1332,943.552 1331.55,943.999 1331,943.999C 1330.45,943.999 1330,943.552 1330,942.999L 1330,941L 1328,941C 1327.45,941 1327,940.552 1327,940C 1327,939.448 1327.45,939 1328,939 Z "
                              Fill="{DynamicResource {dxscht:Office2007ThemeKey ResourceKey=ResourceNavigatorGlyphBrush}}"
                              Height="8"
                              Margin="0,0,0,1"
                              Stretch="Fill"
                              UseLayoutRounding="False"
                              Width="8"/>
                    </Grid>
                </Button>
                <Button x:Name="minus"
                        Command="{TemplateBinding CommandDecCount}"
                        CommandParameter="{TemplateBinding SchedulerControl}"
                        MinWidth="20"
                        Padding="0"
                        Style="{TemplateBinding Property=ButtonDecCountStyle}"
                        Visibility="{Binding SchedulerControl.ResourceNavigator.ButtonDecCount.Visible, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent} }">
                    <ToolTipService.ToolTip>
                        <ToolTip Content="{Binding Source={StaticResource SchedulerStringIdConverter}, ConverterParameter=Caption_DecreaseVisibleResourcesCount, Converter={StaticResource SchedulerStringIdConverter}}"/>
                    </ToolTipService.ToolTip>
                    <Grid x:Name="minus_glyph"
                          Height="3"
