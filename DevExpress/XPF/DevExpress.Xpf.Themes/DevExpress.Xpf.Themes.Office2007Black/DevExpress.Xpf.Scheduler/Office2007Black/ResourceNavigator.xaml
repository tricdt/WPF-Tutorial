                                <Border.Background>
                                    <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                                        <GradientStop Color="#FFE7CAA6" Offset="0"/>
                                        <GradientStop Color="#FFFCE190" Offset="1"/>
                                    </LinearGradientBrush>
                                </Border.Background>
                                <Border Margin="1,0">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                                            <GradientStop Color="#FFF8D6AD" Offset="0"/>
                                            <GradientStop Color="#FFFBD681" Offset="1"/>
                                            <GradientStop Color="#FFF8C37E" Offset="0.5"/>
                                            <GradientStop Color="#FFF9B555" Offset="0.5"/>
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                        </Border>
                        <Grid Height="9"
                              HorizontalAlignment="Center"
                              VerticalAlignment="Center"
                              Width="9">
                            <ContentPresenter x:Name="contentPresenter"
                                              HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                              VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <ControlTemplate x:Key="{dxscht:ResourceNavigatorThemeKey ResourceKey=HorizontalResourceNavigatorTemplate}" TargetType="{x:Type dxschd:ResourceNavigatorControl}">
