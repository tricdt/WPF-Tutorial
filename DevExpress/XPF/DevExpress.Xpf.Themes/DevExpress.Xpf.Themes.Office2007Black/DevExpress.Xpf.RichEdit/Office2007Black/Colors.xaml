<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxret="http://schemas.devexpress.com/winfx/2008/xaml/richedit/themekeys"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--RichEditControlBackgroundBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RichEditControlBackgroundBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF4E4E4E"/>
        <GradientStop Color="#FF0B0B0B" Offset="1"/>
        <GradientStop Color="#FF373737" Offset="0.75"/>
    </LinearGradientBrush>

    <!--RichEditControlBackgroundPatternBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RichEditControlBackgroundPatternBrush}" Color="#27FFFFFF"/>

    <!--RichEditControlBorderBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RichEditControlBorderBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF3B3B3B" Offset="0"/>
        <GradientStop Color="#FF0B0B0B" Offset="1"/>
    </LinearGradientBrush>

    <!--PageShadowBackgroundBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=PageShadowBackgroundBrush}" Color="#3F000000"/>

    <!--PageBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=PageBorderBrush}" Color="#7F000000"/>

    <!--RulerBackgroundBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBackgroundBrush}" Color="#7F000000"/>

    <!--RulerBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBorderBrush}" Color="#BF000000"/>

    <!--RulerTabHotZoneBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTabHotZoneBrush}" Color="#FF353535"/>

    <!--RulerIndentHotZoneBackgroundBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerIndentHotZoneBackgroundBrush}"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#FFEDEDED" Offset="0"/>
        <GradientStop Color="#FFC1C1C1" Offset="1"/>
    </LinearGradientBrush>

    <!--RulerIndentHotZoneBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerIndentHotZoneBorderBrush}" Color="#FF353535"/>

    <!--RulerTableHotZoneBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTableHotZoneBorderBrush}" Color="#8F000000"/>

    <!--RulerTableHotZoneBackgroundBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTableHotZoneBackgroundBrush}" Color="#FFF8F8F8"/>

    <!--RulerTickMarkBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTickMarkBrush}" Color="#FF545454"/>

    <!--RulerDefaultTabMarkBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerDefaultTabMarkBrush}" Color="#59FFFFFF"/>

    <!--RulerSpaceAreaBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerSpaceAreaBrush}" Color="#FFC28F36"/>

    <!--TabTypeToggleHotZoneControlTemplateBackgroundBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=TabTypeToggleHotZoneControlTemplateBackgroundBrush}"
                         EndPoint="0.774,0.918"
                         StartPoint="0.226,0.082">
        <GradientStop Color="#FFE9E9E9"/>
        <GradientStop Color="#FF9D9D9D" Offset="1"/>
    </LinearGradientBrush>

</ResourceDictionary>