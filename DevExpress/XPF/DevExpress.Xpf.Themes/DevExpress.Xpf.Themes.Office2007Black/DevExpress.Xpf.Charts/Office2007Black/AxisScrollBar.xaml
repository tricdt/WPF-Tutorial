                    <!--END WPF-->
                    <!--ONLY SL <local:ScrollBarRepeatButton x:Name="PART_HorizontalLargeDecrease" Grid.Column="1" IsTabStop="False" Interval="50" Template="{StaticResource RepeatButtonTemplate}"/> ONLY SL-->
                    <local:AxisScrollBarThumb x:Name="PART_HorizontalThumb"
                                              Grid.Column="2"
                                              Margin="0,1,0,1"
                                              MinWidth="30"
                                              Template="{StaticResource HorizontalThumbTemplate}"/>
                    <local:AxisScrollBarThumbResizer x:Name="PART_HorizontalThumbDecrease"
                                                     Cursor="SizeWE"
                                                     Grid.Column="2"
                                                     HorizontalAlignment="Left"
                                                     MinWidth="3"
                                                     Template="{StaticResource HorizontalThumbResizerTemplate}"/>
                    <local:AxisScrollBarThumbResizer x:Name="PART_HorizontalThumbIncrease"
                                                     Cursor="SizeWE"
                                                     Grid.Column="2"
                                                     HorizontalAlignment="Right"
                                                     MinWidth="3"
                                                     Template="{StaticResource HorizontalThumbResizerTemplate}"/>
                    <!--BEGIN WPF-->
                    <local:ScrollBarRepeatButton x:Name="PART_HorizontalLargeIncrease"
                                                 AutomationProperties.Name="HorizontalScrollbarLargeIncrement"
                                                 Grid.Column="3"
                                                 Interval="50"
                                                 IsTabStop="False"
                                                 Margin="0,0,0,1"
                                                 Template="{StaticResource RepeatButtonTemplate}"/>
                    <!--END WPF-->
                    <!--ONLY SL <local:ScrollBarRepeatButton x:Name="PART_HorizontalLargeIncrease" Grid.Column="3" IsTabStop="False" Interval="50" Template="{StaticResource RepeatButtonTemplate}"/> ONLY SL-->
                    <local:ScrollBarRepeatButton x:Name="PART_HorizontalSmallIncrease"
                                                 AutomationProperties.Name="HorizontalScrollbarSmallIncrement"
                                                 Grid.Column="4"
                                                 Interval="50"
                                                 IsTabStop="False"
                                                 Margin="1"
                                                 Template=                                  "{StaticResource HorizontalIncrementTemplate}"
                                                 Width="19"/>
                </Grid>
            </Grid>
            <Grid x:Name="PART_VerticalRoot">
                <local:ScrollBarBorder x:Name="PART_VerticalBorder"
                                       Annotations="{TemplateBinding Annotations}"
                                       BorderBrush="{TemplateBinding BorderBrush}"
                                       BorderThickness="{TemplateBinding BorderThickness}"/>
                <Grid x:Name="PART_VerticalContainer">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <local:ScrollBarRepeatButton x:Name="PART_VerticalSmallIncrease"
                                                 AutomationProperties.Name="VerticalScrollbarSmallIncrement"
                                                 Grid.Row="0"
                                                 Height="19"
                                                 Interval="50"
                                                 IsTabStop="False"
                                                 Margin="1"
                                                 Template="{StaticResource VerticalIncrementTemplate}"/>
                    <!--BEGIN WPF-->
                    <local:ScrollBarRepeatButton x:Name="PART_VerticalLargeIncrease"
                                                 AutomationProperties.Name="VerticalScrollbarLargeIncrement"
                                                 Grid.Row="1"
                                                 Interval="50"
                                                 IsTabStop="False"
                                                 Margin="0,0,1,0"
                                                 Template="{StaticResource RepeatButtonTemplate}"/>
                    <!--END WPF-->
                    <!--ONLY SL <local:ScrollBarRepeatButton x:Name="PART_VerticalLargeIncrease" IsTabStop="False" Interval="50" Grid.Row="1" Template="{StaticResource RepeatButtonTemplate}"/> ONLY SL-->
                    <local:AxisScrollBarThumb x:Name="PART_VerticalThumb"
                                              Grid.Row="2"
                                              Margin="1,0,1,0"
                                              MinHeight="30"
                                              Template="{StaticResource VerticalThumbTemplate}"/>
                    <local:AxisScrollBarThumbResizer x:Name="PART_VerticalThumbIncrease"
                                                     Cursor="SizeNS"
                                                     Grid.Row="2"
                                                     MinHeight="3"
                                                     Template="{StaticResource VerticalThumbResizerTemplate}"
                                                     VerticalAlignment="Top"/>
                    <local:AxisScrollBarThumbResizer x:Name="PART_VerticalThumbDecrease"
                                                     Cursor="SizeNS"
                                                     Grid.Row="2"
                                                     MinHeight="3"
                                                     Template="{StaticResource VerticalThumbResizerTemplate}"
                                                     VerticalAlignment="Bottom"/>
                    <!--BEGIN WPF-->
                    <local:ScrollBarRepeatButton x:Name="PART_VerticalLargeDecrease"
                                                 AutomationProperties.Name="VerticalScrollbarLargeDecrement"
                                                 Grid.Row="3"
                                                 Height="0"
                                                 Interval="50"
                                                 IsTabStop="False"
                                                 Margin="0,0,1,0"
                                                 Template="{StaticResource RepeatButtonTemplate}"/>
                    <!--END WPF-->
                    <!--ONLY SL <local:ScrollBarRepeatButton x:Name="PART_VerticalLargeDecrease" Height="0" IsTabStop="False" Interval="50" Grid.Row="3" Template="{StaticResource RepeatButtonTemplate}"/> ONLY SL-->
                    <local:ScrollBarRepeatButton x:Name="PART_VerticalSmallDecrease"
                                                 AutomationProperties.Name="VerticalScrollbarSmallDecrement"
                                                 Grid.Row="4"
                                                 Height="19"
                                                 Interval="50"
                                                 IsTabStop="False"
                                                 Margin="1"
                                                 Template="{StaticResource VerticalDecrementTemplate}"/>
                </Grid>
            </Grid>
        </Grid>
    </ControlTemplate>

</ResourceDictionary>