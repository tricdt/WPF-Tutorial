<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <ResourceDictionary.MergedDictionaries>
    <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2016ColorfulSE.v24.1;component/Core/Core/Editors/Themes/Generic/TextEditBase.xaml" />
  </ResourceDictionary.MergedDictionaries>
  <ControlTemplate x:Key="{dxet:HyperLinkEditThemeKey ResourceKey=Template}" TargetType="{x:Type dxe:HyperlinkEdit}">
    <dxe:EditorControl x:Name="PART_Content" Template="{TemplateBinding ActualEditorControlTemplate}" />
  </ControlTemplate>
  <SolidColorBrush x:Key="{dxet:HyperLinkEditThemeKey ResourceKey=Foreground}">Blue</SolidColorBrush>
  <SolidColorBrush x:Key="{dxet:HyperLinkEditThemeKey ResourceKey=InplaceForeground}">Blue</SolidColorBrush>
  <Style x:Key="{dxet:HyperLinkEditThemeKey ResourceKey=TextBlockStyle}" TargetType="{x:Type TextBlock}" BasedOn="{StaticResource BaseTextBlockStyleBase}">
    <Setter Property="Margin" Value="2,3,2,3" />
    <Style.Triggers>
      <Trigger Property="IsMouseOver" Value="True">
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="TextDecorations" Value="Underline" />
      </Trigger>
    </Style.Triggers>
  </Style>
  <Style x:Key="{dxet:HyperLinkEditThemeKey ResourceKey=TextBlockInplaceStyle}" TargetType="{x:Type TextBlock}" BasedOn="{StaticResource BaseTextBlockInplaceStyle}">
    <Setter Property="Margin" Value="2" />
    <Style.Triggers>
      <Trigger Property="IsMouseOver" Value="True">
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="TextDecorations" Value="Underline" />
      </Trigger>
    </Style.Triggers>
  </Style>
  <ControlTemplate x:Key="{dxet:HyperLinkEditThemeKey ResourceKey=DisplayTemplate}">
    <TextBlock x:Name="PART_Editor" TextWrapping="{Binding Path=(dxe:BaseEdit.OwnerEdit).TextWrapping, RelativeSource={RelativeSource Self}}" TextTrimming="{Binding Path=(dxe:BaseEdit.OwnerEdit).TextTrimming, RelativeSource={RelativeSource Self}}" HorizontalAlignment="{Binding Path=(dxe:BaseEdit.OwnerEdit).HorizontalContentAlignment, RelativeSource={RelativeSource Self}}" Style="{DynamicResource {dxet:HyperLinkEditThemeKey ResourceKey=TextBlockInplaceStyle}}" dx:FocusHelper2.Focusable="False" />
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:HyperLinkEditThemeKey ResourceKey=EditTemplate}">
    <TextBlock x:Name="PART_Editor" TextWrapping="{Binding Path=(dxe:BaseEdit.OwnerEdit).TextWrapping, RelativeSource={RelativeSource Self}}" TextTrimming="{Binding Path=(dxe:BaseEdit.OwnerEdit).TextTrimming, RelativeSource={RelativeSource Self}}" HorizontalAlignment="{Binding Path=(dxe:BaseEdit.OwnerEdit).HorizontalContentAlignment, RelativeSource={RelativeSource Self}}" Style="{DynamicResource {dxet:HyperLinkEditThemeKey ResourceKey=TextBlockStyle}}" dx:FocusHelper2.Focusable="False" />
  </ControlTemplate>
</ResourceDictionary>