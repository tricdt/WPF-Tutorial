<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dxprg="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid" xmlns:dxprgt="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid/themekeys" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <ResourceDictionary.MergedDictionaries>
    <dxdst:ResourceDictionaryEx Source="{dxprgt:DynamicPropertyGridResource RenderTemplates.xaml}" />
    <dxdst:ResourceDictionaryEx Source="{dxprgt:DynamicPropertyGridResource DataRowControl.xaml}" />
  </ResourceDictionary.MergedDictionaries>
  <dxn:RenderTemplate x:Key="trimmedTextTemplate">
    <dxn:RenderTextBlock Name="textBlock" TextTrimming="CharacterEllipsis" />
    <dxn:RenderTemplate.Triggers>
      <dxn:RenderBinding TargetName="textBlock" DependencyProperty="{x:Static dxprg:RowData.HeaderProperty}" TargetProperty="Text" />
    </dxn:RenderTemplate.Triggers>
  </dxn:RenderTemplate>
  <dxn:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=HeaderHost}">
    <dxn:RenderPanel LayoutProvider="{dxn:DockPanelLayoutProvider LastChildFill=True}">
      <dxn:RenderBorder Name="PART_Indent" Dock="Left" />
      <dxprg:RenderExpander Name="PART_CheckBox" RenderTemplate="{StaticResource {dxprgt:VSViewRenderTemplateThemeKey ResourceKey=Expander}}" RenderTemplateSelector="{x:Null}" MinHeight="20" Width="{StaticResource {dxprgt:DataRowThemeKey ResourceKey=ExpanderWidth}}" HorizontalAlignment="Center" VerticalAlignment="Center" Dock="Left" Visibility="Hidden" UsePropagatedIsMouseOver="True" />
      <dxn:RenderContentControl Dock="Right" Name="PART_ContentPresenter" RenderContentTemplate="{StaticResource trimmedTextTemplate}" PreferRenderTemplate="False" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" />
    </dxn:RenderPanel>
    <dxn:RenderTemplate.Triggers>
      <dxn:RenderBinding TargetName="PART_Indent" DependencyProperty="{x:Static dxprg:RowControlBase.ActualIndentProperty}" TargetProperty="Width" />
      <dxn:RenderBinding TargetName="PART_CheckBox" DependencyProperty="{x:Static dxprg:RowControlBase.ExpandButtonWidthProperty}" TargetProperty="Width" />
      <dxn:RenderBinding TargetName="PART_ContentPresenter" DependencyProperty="{x:Static dxprg:RowControl.RowDataProperty}" TargetProperty="Content" />
      <dxn:RenderBinding TargetName="PART_ContentPresenter" DependencyProperty="{x:Static dxprg:RowControl.HeaderTemplateSelectorProperty}" TargetProperty="ContentTemplateSelector" />
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.CanExpandProperty}" Value="True">
        <dxn:RenderSetter TargetName="PART_CheckBox" Property="Visibility" Value="Visible" />
      </dxn:RenderTrigger>
      <dxn:MultiRenderTrigger>
        <dxn:MultiRenderTrigger.Conditions>
          <dxn:RenderCondition DependencyProperty="{x:Static dxprg:RowControl.CanExpandProperty}" Value="False" />
          <dxn:RenderCondition DependencyProperty="{x:Static dxprg:RowControl.CategoriesShowModeProperty}" Value="Tabbed" />
          <dxn:RenderCondition DependencyProperty="{x:Static dxprg:RowControl.ExpandButtonVisibilityProperty}" Value="Visible" />
        </dxn:MultiRenderTrigger.Conditions>
        <dxn:RenderSetter TargetName="PART_CheckBox" Property="Visibility" Value="Hidden" />
      </dxn:MultiRenderTrigger>
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.ExpandButtonVisibilityProperty}" Value="Hidden">
        <dxn:RenderSetter TargetName="PART_CheckBox" Property="Visibility" Value="Hidden" />
      </dxn:RenderTrigger>
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.ExpandButtonVisibilityProperty}" Value="Collapsed">
        <dxn:RenderSetter TargetName="PART_CheckBox" Property="Visibility" Value="Collapsed" />
      </dxn:RenderTrigger>
    </dxn:RenderTemplate.Triggers>
  </dxn:RenderTemplate>
  <dxn:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=Separator}">
    <dxn:RenderBorder Background="{StaticResource {dxprgt:VSViewBrushThemeKey ResourceKey=BorderBrush}}" MinWidth="1" MinHeight="1" />
  </dxn:RenderTemplate>
  <dxn:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=RowControl}">
    <dxn:RenderBorder Name="PART_Root" BorderThickness="0,0,0,1" BorderBrush="{StaticResource {dxprgt:VSViewBrushThemeKey ResourceKey=BorderBrush}}">
      <dxprg:RenderResizingPanel Dock="Left">
        <dxn:RenderBorder Name="PART_HeaderBackground">
          <dxn:RenderControl Name="PART_HeaderHost" RenderTemplate="{StaticResource {dxprgt:VSViewRenderTemplateThemeKey ResourceKey=HeaderHost}}" />
        </dxn:RenderBorder>
        <dxn:RenderBorder Name="PART_EditorBackground">
          <dxn:RenderPanel Dock="Right" LayoutProvider="{dxn:DockPanelLayoutProvider LastChildFill=True}">
            <dxprg:RenderCommandButton Name="PART_CommandButton" Dock="Right" RenderTemplate="{StaticResource {dxprgt:VSViewRenderTemplateThemeKey ResourceKey=RowCommandButton}}" RenderTemplateSelector="{x:Null}" UsePropagatedIsMouseOver="True" VerticalAlignment="Center" PreferRenderTemplate="False" />
            <dxprg:RenderCollectionButton Name="PART_CollectionButton" Dock="Right" RenderTemplate="{StaticResource {dxprgt:VSViewRenderTemplateThemeKey ResourceKey=RowCollectionButton}}" RenderTemplateSelector="{x:Null}" UsePropagatedIsMouseOver="True" Visibility="Collapsed" />
            <dxn:RenderPanel Dock="Left" LayoutProvider="{dxn:DockPanelLayoutProvider LastChildFill=True}">
              <dxn:RenderPanel Dock="Left" Name="PART_OnlyExpanderModePanel" Visibility="Collapsed" LayoutProvider="{dxn:DockPanelLayoutProvider LastChildFill=True}">
                <dxn:RenderBorder Name="PART_Indent" Dock="Left" />
                <dxprg:RenderExpander Name="PART_CheckBox" RenderTemplate="{StaticResource {dxprgt:VSViewRenderTemplateThemeKey ResourceKey=Expander}}" RenderTemplateSelector="{x:Null}" Width="{StaticResource {dxprgt:DataRowThemeKey ResourceKey=ExpanderWidth}}" HorizontalAlignment="Center" VerticalAlignment="Center" Dock="Left" Visibility="Collapsed" UsePropagatedIsMouseOver="True" />
              </dxn:RenderPanel>
              <dxn:RenderContentControl Dock="Right" Name="PART_ContentHost" PreferRenderTemplate="False" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" />
            </dxn:RenderPanel>
          </dxn:RenderPanel>
        </dxn:RenderBorder>
        <dxn:RenderControl Name="PART_Separator" RenderTemplate="{StaticResource {dxprgt:VSViewRenderTemplateThemeKey ResourceKey=Separator}}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
      </dxprg:RenderResizingPanel>
    </dxn:RenderBorder>
    <dxn:RenderTemplate.Triggers>
      <dxn:RenderBinding DependencyProperty="{x:Static dxprg:RowControl.HeaderForegroundProperty}" TargetProperty="Foreground" TargetName="PART_HeaderBackground" />
      <dxn:RenderBinding DependencyProperty="{x:Static dxprg:RowControl.HeaderBackgroundProperty}" TargetProperty="Background" TargetName="PART_HeaderBackground" />
      <dxn:RenderBinding DependencyProperty="{x:Static dxprg:RowControl.HeaderPaddingProperty}" TargetProperty="Padding" TargetName="PART_HeaderBackground" />
      <dxn:RenderBinding DependencyProperty="{x:Static dxprg:RowControl.ContentForegroundProperty}" TargetProperty="Foreground" TargetName="PART_EditorBackground" />
      <dxn:RenderBinding DependencyProperty="{x:Static dxprg:RowControl.ContentBackgroundProperty}" TargetProperty="Background" TargetName="PART_EditorBackground" />
      <dxn:RenderBinding DependencyProperty="{x:Static dxprg:RowControl.ContentPaddingProperty}" TargetProperty="Padding" TargetName="PART_EditorBackground" />
      <dxn:RenderBinding TargetName="PART_CheckBox" DependencyProperty="{x:Static dxprg:RowControlBase.ExpandButtonWidthProperty}" TargetProperty="Width" />
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.ShowGridLinesProperty}" Value="False">
        <dxn:RenderSetter TargetName="PART_Root" Property="BorderThickness" Value="0" />
        <dxn:RenderSetter TargetName="PART_Separator" Property="Visibility" Value="Collapsed" />
      </dxn:RenderTrigger>
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.HeaderShowModeProperty}" Value="OnlyHeader">
        <dxn:RenderSetter TargetName="PART_EditorBackground" Property="Visibility" Value="Collapsed" />
      </dxn:RenderTrigger>
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.HeaderShowModeProperty}" Value="OnlyExpander">
        <dxn:RenderSetter TargetName="PART_OnlyExpanderModePanel" Property="Visibility" Value="Visible" />
      </dxn:RenderTrigger>
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.CanExpandProperty}" Value="True">
        <dxn:RenderSetter TargetName="PART_CheckBox" Property="Visibility" Value="Visible" />
      </dxn:RenderTrigger>
      <dxn:MultiRenderTrigger>
        <dxn:MultiRenderTrigger.Conditions>
          <dxn:RenderCondition DependencyProperty="{x:Static dxprg:RowControl.CanExpandProperty}" Value="False" />
          <dxn:RenderCondition DependencyProperty="{x:Static dxprg:RowControl.CategoriesShowModeProperty}" Value="Tabbed" />
          <dxn:RenderCondition DependencyProperty="{x:Static dxprg:RowControl.ExpandButtonVisibilityProperty}" Value="Visible" />
        </dxn:MultiRenderTrigger.Conditions>
        <dxn:RenderSetter TargetName="PART_CheckBox" Property="Visibility" Value="Hidden" />
      </dxn:MultiRenderTrigger>
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.ExpandButtonVisibilityProperty}" Value="Hidden">
        <dxn:RenderSetter TargetName="PART_CheckBox" Property="Visibility" Value="Hidden" />
      </dxn:RenderTrigger>
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.ExpandButtonVisibilityProperty}" Value="Collapsed">
        <dxn:RenderSetter TargetName="PART_CheckBox" Property="Visibility" Value="Collapsed" />
      </dxn:RenderTrigger>
      <dxn:RenderBinding TargetName="PART_Indent" DependencyProperty="{x:Static dxprg:RowControlBase.ActualIndentProperty}" TargetProperty="Width" />
      <dxn:RenderBinding TargetName="PART_ContentHost" DependencyProperty="{x:Static dxprg:RowControl.RowDataProperty}" TargetProperty="Content" />
      <dxn:RenderBinding TargetName="PART_ContentHost" DependencyProperty="{x:Static dxprg:RowControl.EditorHorizontalAlignmentProperty}" TargetProperty="HorizontalContentAlignment" />
      <dxn:RenderBinding TargetName="PART_ContentHost" DependencyProperty="{x:Static dxprg:RowControl.ContentTemplateSelectorProperty}" TargetProperty="ContentTemplateSelector" />
      <dxn:RenderBinding TargetName="PART_CommandButton" DependencyProperty="{x:Static dxprg:RowControl.MenuButtonTemplateProperty}" TargetProperty="ContentTemplate" />
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControlBase.ActualShowCommandButtonProperty}" Value="False">
        <dxn:RenderSetter TargetName="PART_CommandButton" Property="Visibility" Value="Collapsed" />
      </dxn:RenderTrigger>
      <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControlBase.ShowCollectionButtonProperty}" Value="True">
        <dxn:RenderSetter TargetName="PART_CollectionButton" Property="Visibility" Value="Visible" />
      </dxn:RenderTrigger>
    </dxn:RenderTemplate.Triggers>
  </dxn:RenderTemplate>
</ResourceDictionary>