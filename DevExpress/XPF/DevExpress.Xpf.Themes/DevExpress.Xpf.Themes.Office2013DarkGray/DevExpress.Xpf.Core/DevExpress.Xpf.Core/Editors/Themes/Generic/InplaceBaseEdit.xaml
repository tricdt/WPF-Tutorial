    <dxn:RenderPanel>
      <dxe:EditorRenderButton Name="PART_SpinUpButton" Dock="Top" RenderTemplateSelector="{dxei:RenderSpinButtonUpTemplateSelector}" RenderContentTemplateSelector="{dxei:RenderSpinButtonUpGlyphTemplateSelector}" UsePropagatedIsMouseOver="True" ButtonKind="Repeat" AllowMouseCapturing="true" Content="{StaticResource ArrowBrush}" />
      <dxe:EditorRenderButton Name="PART_SpinDownButton" Dock="Bottom" RenderTemplateSelector="{dxei:RenderSpinButtonDownTemplateSelector}" RenderContentTemplateSelector="{dxei:RenderSpinButtonDownGlyphTemplateSelector}" UsePropagatedIsMouseOver="True" ButtonKind="Repeat" AllowMouseCapturing="true" Content="{StaticResource ArrowBrush}" />
