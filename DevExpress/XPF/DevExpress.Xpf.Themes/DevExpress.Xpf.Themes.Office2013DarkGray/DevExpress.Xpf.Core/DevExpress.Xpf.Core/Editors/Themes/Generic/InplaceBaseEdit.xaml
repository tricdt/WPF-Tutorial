      <dxn:RenderBinding TargetName="PART_ErrorProvider" TargetProperty="ContentTemplate" DependencyProperty="{x:Static dxe:BaseEdit.ValidationErrorTemplateProperty}" ValueSource="DataContext" />
      <dxn:RenderTrigger DependencyProperty="{x:Static FrameworkElement.IsKeyboardFocusWithinProperty}" ValueSource="DataContext" Value="True">
