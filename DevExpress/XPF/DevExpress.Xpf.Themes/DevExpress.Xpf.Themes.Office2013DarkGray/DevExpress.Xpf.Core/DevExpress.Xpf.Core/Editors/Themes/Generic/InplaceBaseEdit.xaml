      <dxn:RenderBinding TargetName="PART_ButtonContainer" TargetProperty="Content" ValueSource="DataContext" />
    </dxn:RenderTemplate.Triggers>
  </dxn:RenderTemplate>
  <dxn:RenderTemplate x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=TextEditInplaceInactiveTemplate}">
    <dxn:RenderTextBlock Name="PART_Editor" ClipToBounds="True" Margin="{StaticResource {dxet:InplaceBaseEditThemeKey ResourceKey=TextEditPadding}}" AllowGlyphRunRendering="{x:Static core:CompatibilitySettings.AllowGlyphRunRenderingInInplaceEditors}" />
    <dxn:RenderTemplate.Triggers>
      <dxn:RenderStateGroup Name="TouchStates">
        <dxn:RenderStateTrigger Name="NonTouch" />
        <dxn:RenderStateTrigger Name="Touch">
          <dxn:RenderSetter TargetName="PART_Editor" Property="Margin" Value="13" />
        </dxn:RenderStateTrigger>
      </dxn:RenderStateGroup>
    </dxn:RenderTemplate.Triggers>
  </dxn:RenderTemplate>
  <SolidColorBrush x:Key="ArrowBrush" Color="{StaticResource {dxet:SpinEditThemeKey ResourceKey=ArrowBrushColor}}" />
