      <dxn:RenderContentControl Name="PART_Border" RenderTemplate="{StaticResource {dxet:InplaceBaseEditThemeKey ResourceKey=TextEditBorderTemplate}}" />
      <dxn:RenderPanel Name="PART_Root" LayoutProvider="{dxn:DockPanelLayoutProvider LastChildFill=True}" Margin="{StaticResource ResourceKey={dxet:InplaceBaseEditThemeKey ResourceKey=TextEditBorderThickness}}">
        <dxn:RenderContentControl VerticalAlignment="Center" HorizontalAlignment="Center" Dock="Left" Name="PART_ErrorProvider" PreferRenderTemplate="false" RenderContentTemplateSelector="{StaticResource {dxet:InplaceBaseEditThemeKey ResourceKey=ValidationErrorTemplateSelector}}" Visibility="Collapsed" Margin="{StaticResource {dxet:InplaceBaseEditThemeKey ResourceKey=ValidationErrorPadding}}" />
        <dxn:RenderItemsControl Dock="Left" Name="PART_LeftButtons" ItemTemplateSelector="{StaticResource {dxet:InplaceBaseEditThemeKey ResourceKey=RenderButtonContainerTemplateSelector}}" Margin="{StaticResource {dxet:ButtonEditThemeKey ResourceKey=ButtonEditPadding}}" Visibility="Collapsed">
          <dxn:RenderItemsControl.ItemPanelTemplate>
            <dxn:RenderPanel LayoutProvider="{dxe:ButtonsPanelLayoutProvider}" />
          </dxn:RenderItemsControl.ItemPanelTemplate>
        </dxn:RenderItemsControl>
        <dxn:RenderItemsControl Dock="Right" Name="PART_RightButtons" ItemTemplateSelector="{StaticResource {dxet:InplaceBaseEditThemeKey ResourceKey=RenderButtonContainerTemplateSelector}}" Margin="{StaticResource {dxet:ButtonEditThemeKey ResourceKey=ButtonEditPadding}}" Visibility="Collapsed">
          <dxn:RenderItemsControl.ItemPanelTemplate>
            <dxn:RenderPanel LayoutProvider="{dxe:ButtonsPanelLayoutProvider}" />
          </dxn:RenderItemsControl.ItemPanelTemplate>
        </dxn:RenderItemsControl>
        <dxei:RenderStandaloneEditorControl Name="PART_Editor" />
      </dxn:RenderPanel>
      <dxn:RenderContentControl Name="PART_Focus" RenderTemplate="{StaticResource {dxet:InplaceBaseEditThemeKey ResourceKey=HoverBorderTemplate}}" Visibility="Collapsed" />
    </dxn:RenderPanel>
    <dxn:RenderTemplate.Triggers>
      <dxn:ConditionalRenderBinding DependencyProperty="{x:Static dxn:AppearanceOptions.BorderThicknessProperty}" TargetName="PART_Root" TargetProperty="Margin" ValueSource="TemplatedParent">
        <dxn:RenderCondition DependencyProperty="{x:Static dxn:AppearanceOptions.HasBorderThicknessProperty}" Value="True" ValueSource="TemplatedParent" />
      </dxn:ConditionalRenderBinding>
      <dxn:RenderBinding TargetName="PART_LeftButtons" TargetProperty="Visibility" DependencyProperty="{x:Static dxe:ButtonEdit.ActualShowLeftButtonsProperty}" Converter="{StaticResource BooleanToVisibilityConverter}" ValueSource="DataContext" />
      <dxn:RenderBinding TargetName="PART_LeftButtons" TargetProperty="ItemsSource" DependencyProperty="{x:Static dxe:ButtonEdit.LeftButtonsProperty}" ValueSource="DataContext" />
      <dxn:RenderBinding TargetName="PART_RightButtons" TargetProperty="Visibility" DependencyProperty="{x:Static dxe:ButtonEdit.ActualShowRightButtonsProperty}" Converter="{StaticResource BooleanToVisibilityConverter}" ValueSource="DataContext" />
      <dxn:RenderBinding TargetName="PART_RightButtons" TargetProperty="ItemsSource" DependencyProperty="{x:Static dxe:ButtonEdit.RightButtonsProperty}" ValueSource="DataContext" />
