        </dxn:RenderItemsControl>
        <dxn:RenderItemsControl Dock="Right" Name="PART_RightButtons" ItemTemplateSelector="{StaticResource {dxet:InplaceBaseEditThemeKey ResourceKey=RenderButtonContainerTemplateSelector}}" Margin="{StaticResource {dxet:ButtonEditThemeKey ResourceKey=ButtonEditPadding}}" Visibility="Collapsed">
          <dxn:RenderItemsControl.ItemPanelTemplate>
