      <dx:WpfSvgPalette x:Key="Hover">
        <SolidColorBrush x:Key="BlackColor" Color="#FF1E1E1E" cs:id="SolidColorBrush_0002" x:Uid="@Color=Foreground" />
      </dx:WpfSvgPalette>
      <dx:WpfSvgPalette x:Key="Pressed">
        <SolidColorBrush x:Key="BlackColor" Color="#FF1E1E1E" cs:id="SolidColorBrush_0003" x:Uid="@Color=Foreground" />
      </dx:WpfSvgPalette>
    </dx:WpfSvgPalette.States>
  </dx:WpfSvgPalette>
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=PopupElementHoverBackground}" Color="#FF5B7199" Opacity="0.13" x:Uid="@Color=Backstage.Delimiter" />
