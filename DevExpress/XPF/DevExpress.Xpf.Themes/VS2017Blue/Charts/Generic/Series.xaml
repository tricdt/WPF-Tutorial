    <dxdst:ResourceDictionaryEx Source="{dx:ChartsResource themes/Generic/ToolTip.xaml}" />
    <dxdst:ResourceDictionaryEx Source="{dx:ChartsResource themes/Generic/LegendMarker.xaml}" />
  </ResourceDictionary.MergedDictionaries>
  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Charts.SeriesPresentation}" TargetType="{x:Type local:SeriesPresentation}">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type local:SeriesPresentation}">
          <Grid>
            <local:ChartContentPresenter x:Name="PART_AdditionalGeometryHolder" Content="{Binding Path=Series, RelativeSource={RelativeSource TemplatedParent}, Converter={local:XYSeries2DToAdditionalGeometryConverter}}" />
            <local:PointsContainer x:Name="PART_PointsContainer" ItemsSource="{Binding Path=SeriesItem.DrawingPointItems, RelativeSource={RelativeSource TemplatedParent}}" DataContext="{Binding Path=Series, RelativeSource={RelativeSource TemplatedParent}}">
              <local:ChartItemsControl.ItemsPanel>
                <ItemsPanelTemplate>
                  <local:SimplePanel x:Name="PART_PointsPanel" />
                </ItemsPanelTemplate>
              </local:ChartItemsControl.ItemsPanel>
            </local:PointsContainer>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Style.Triggers>
      <DataTrigger Binding="{Binding SeriesItem.IsDirectXSurfaceRendering, RelativeSource={RelativeSource Self}}" Value="True">
        <Setter Property="Template">
          <Setter.Value>
            <ControlTemplate TargetType="{x:Type local:SeriesPresentation}">
              <local:ChartContentPresenter x:Name="PART_AdditionalGeometryHolder" Content="{Binding Path=Series, RelativeSource={RelativeSource TemplatedParent}, Converter={local:XYSeries2DToAdditionalGeometryConverter}}" />
            </ControlTemplate>
          </Setter.Value>
        </Setter>
      </DataTrigger>
    </Style.Triggers>
  </Style>
