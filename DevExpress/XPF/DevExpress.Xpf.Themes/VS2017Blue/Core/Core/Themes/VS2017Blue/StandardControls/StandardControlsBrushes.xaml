<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- MENU BRUSHES -->
  <SolidColorBrush x:Key="Menu_Background" Color="#FFD6DBE9" x:Uid="@Color=Window.Background" />
  <SolidColorBrush x:Key="Menu_Border" Color="#FF8E9BBC" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="Menu_Foreground" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="MenuItem_Foreground" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="MenuItem_HoverForeground" Color="#FF1E1E1E" x:Uid="@Color=HoverForeground" />
  <SolidColorBrush x:Key="MenuItem_PressedForeground" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="MenuItem_HoverBackground" Color="#FFFDF4BF" x:Uid="@Color=HoverBackground" />
  <SolidColorBrush x:Key="MenuItem_HoverBorder" Color="#FFE5C365" x:Uid="@Color=HoverBorder" />
  <SolidColorBrush x:Key="MenuItem_SelectionBackground" Color="#FFFFF29D" x:Uid="@Color=SelectionBackground" />
  <SolidColorBrush x:Key="MenuItem_SelectionBorder" Color="#FFE5C365" x:Uid="@Color=SelectionBorder" />
  <!-- CONTEXT MENU BRUSHES -->
  <SolidColorBrush x:Key="ContextMenu_Background" Color="#FFEAF0FF" x:Uid="@Color=Popup.Background" />
  <SolidColorBrush x:Key="ContextMenu_Border" Color="#FF8E9BBC" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="ContextMenuItem_Foreground" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverForeground" Color="#FF1E1E1E" x:Uid="@Color=HoverForeground" />
  <SolidColorBrush x:Key="ContextMenuItem_PressedForeground" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverBackground" Color="#FFFDF4BF" x:Uid="@Color=HoverBackground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverBorder" Color="#FFE5C365" x:Uid="@Color=HoverBorder" />
  <SolidColorBrush x:Key="ContextMenuItem_SelectionBackground" Color="#FFFFF29D" x:Uid="@Color=SelectionBackground" />
  <SolidColorBrush x:Key="ContextMenuItem_SelectionBorder" Color="#FFE5C365" x:Uid="@Color=SelectionBorder" />
</ResourceDictionary>