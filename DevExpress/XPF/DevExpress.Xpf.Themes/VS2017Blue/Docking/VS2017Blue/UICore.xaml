    <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.VS2017Blue.v24.1;component/Docking/VS2017Blue/UICustom.xaml" />
    <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.VS2017Blue.v24.1;component/Docking/VS2017Blue/UIPreset.xaml" />
  </ResourceDictionary.MergedDictionaries>
  <!-- Panel -->
  <SolidColorBrush x:Key="{dxdot:BrushElementsThemeKey ResourceKey=PanelForeground}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="{dxdot:BrushElementsThemeKey ResourceKey=PanelBackground}" Color="#FF293955" x:Uid="@Color=Backstage.Window.Background" />
  <!-- TabbedPaneElements -->
  <SolidColorBrush x:Key="{dxdot:TabbedPaneElementsThemeKey ResourceKey=TabHeadersBackground}" Color="Transparent" />
  <!-- DocumentElements -->
  <SolidColorBrush x:Key="{dxdot:DocumentElementsThemeKey ResourceKey=Background}" Color="#FFD6DBE9" x:Uid="@Color=Window.Background" />
  <SolidColorBrush x:Key="{dxdot:DocumentPaneElementsThemeKey ResourceKey=TabHeadersBackground}" Color="Transparent" />
  <!-- DockPane -->
  <SolidColorBrush x:Key="{dxdot:DockPaneElementsThemeKey ResourceKey=BorderBrush}" Color="#FF8E9BBC" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="{dxdot:DockPaneElementsThemeKey ResourceKey=FloatingActiveBorderBrush}" Color="#FF1E1E1E" x:Uid="@Color=HoverForeground" />
  <SolidColorBrush x:Key="{dxdot:DockPaneElementsThemeKey ResourceKey=Background}" Color="#FFD6DBE9" x:Uid="@Color=Window.Background" />
  <SolidColorBrush x:Key="{dxdot:DockPaneElementsThemeKey ResourceKey=CaptionActiveBackground}" Color="#FFFFF29D" x:Uid="@Color=SelectionBackground" />
  <SolidColorBrush x:Key="{dxdot:DockPaneElementsThemeKey ResourceKey=CaptionNormalBackground}" Color="#FF4D6082" x:Uid="@Color=Backstage.Button.Background" />
