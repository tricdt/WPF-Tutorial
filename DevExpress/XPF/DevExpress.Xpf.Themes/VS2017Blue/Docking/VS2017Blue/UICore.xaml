  <SolidColorBrush x:Key="{dxdot:DockPaneElementsThemeKey ResourceKey=CaptionNormalForeground}" Color="#FFFFFFFF" Opacity="0.62" x:Uid="@Color=Backstage.Foreground" />
  <SolidColorBrush x:Key="{dxdot:DocumentPaneElementsThemeKey ResourceKey=TabbedBackground}" Color="#FF293955" x:Uid="@Color=Backstage.Window.Background" />
  <SolidColorBrush x:Key="{dxdot:DocumentPaneElementsThemeKey ResourceKey=MDIBackground}" Color="#FF293955" x:Uid="@Color=Backstage.Window.Background" />
