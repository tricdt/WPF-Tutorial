  <SolidColorBrush x:Key="{dxdot:BrushElementsThemeKey ResourceKey=MoveTargetBackground}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="{dxdot:BrushElementsThemeKey ResourceKey=HiddenItem}" Color="#FFDCE0EC" x:Uid="@Color=Button.Background" />
  <SolidColorBrush x:Key="{dxdot:BrushElementsThemeKey ResourceKey=HiddenItemCaption}" Color="#FFDCE0EC" x:Uid="@Color=Button.Background" />
  <SolidColorBrush x:Key="{dxdot:BrushElementsThemeKey ResourceKey=HiddenItemBorder}" Color="#FF8E9BBC" x:Uid="@Color=Border" />
</ResourceDictionary>