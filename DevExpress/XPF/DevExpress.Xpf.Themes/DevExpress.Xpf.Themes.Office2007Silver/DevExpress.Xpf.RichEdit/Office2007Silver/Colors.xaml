<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxret="http://schemas.devexpress.com/winfx/2008/xaml/richedit/themekeys"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--RichEditControlBackgroundBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RichEditControlBackgroundBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFCBCED7"/>
        <GradientStop Color="#FF9B9FA6" Offset="1"/>
        <GradientStop Color="#FFB3B6BE" Offset="0.75"/>
    </LinearGradientBrush>

    <!--RichEditControlBackgroundPatternBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RichEditControlBackgroundPatternBrush}" Color="#27FFFFFF"/>

    <!--RichEditControlBorderBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RichEditControlBorderBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF6F7074"/>
        <GradientStop Color="#FF6F7074" Offset="1"/>
    </LinearGradientBrush>

    <!--PageShadowBackgroundBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=PageShadowBackgroundBrush}" Color="#26000000"/>

    <!--PageBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=PageBorderBrush}" Color="#7F5C5E66"/>

    <!--RulerBackgroundBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBackgroundBrush}" Color="#7F6F7074"/>

    <!--RulerBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBorderBrush}" Color="#BF5A5C62"/>

    <!--RulerTabHotZoneBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTabHotZoneBrush}" Color="#FF7D818F"/>

    <!--RulerIndentHotZoneBackgroundBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerIndentHotZoneBackgroundBrush}"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#FFE8EAEF" Offset="0"/>
        <GradientStop Color="#FFBFC2CC" Offset="1"/>
    </LinearGradientBrush>

    <!--RulerIndentHotZoneBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerIndentHotZoneBorderBrush}" Color="#FF7D818F"/>

    <!--RulerTableHotZoneBorderBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTableHotZoneBorderBrush}" Color="#64000000"/>

    <!--RulerTableHotZoneBackgroundBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTableHotZoneBackgroundBrush}" Color="#BDF8F8F8"/>

    <!--RulerTickMarkBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerTickMarkBrush}" Color="#FF2F3646"/>

    <!--RulerDefaultTabMarkBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerDefaultTabMarkBrush}" Color="#592F3646"/>

    <!--RulerSpaceAreaBrush-->

    <SolidColorBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerSpaceAreaBrush}" Color="#FFD7DDE8"/>

    <!--TabTypeToggleHotZoneControlTemplateBackgroundBrush-->

    <LinearGradientBrush x:Key="{dxret:RichEditOffice2007ColorThemeKey ResourceKey=TabTypeToggleHotZoneControlTemplateBackgroundBrush}"
                         EndPoint="0.774,0.918"
                         StartPoint="0.226,0.082">
        <GradientStop Color="#FFE9EBEF"/>
        <GradientStop Color="#FFC7CAD4" Offset="1"/>
    </LinearGradientBrush>

</ResourceDictionary>