    <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=LeftIndentHotZoneControlTemplate}" TargetType="dxrec:HorizontalHotZoneControl">
        <Grid HorizontalAlignment="Left" VerticalAlignment="Top">
            <Grid Height="7"
                  Visibility="{TemplateBinding EnabledVisibility}"
                  Width="9">
                <Path Data="F1 M 547,359L 551,355L 552,355L 556,359L 556,362L 547,362L 547,359 Z "
                      Fill="{DynamicResource {dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerIndentHotZoneBorderBrush}}"
                      Stretch="Fill"
                      UseLayoutRounding="False"/>
                <Path Data="F1 M 548,359L 551,356L 552,356L 555,359L 555,361L 548,361L 548,359 Z "
                      Fill="{DynamicResource {dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerIndentHotZoneBackgroundBrush}}"
                      Margin="1"
                      Stretch="Fill"
                      UseLayoutRounding="False"/>
            </Grid>
            <Grid Height="7"
                  Opacity="0.5"
                  Visibility="{TemplateBinding DisabledVisibility}"
                  Width="9">
                <Path Data="F1 M 547,359L 551,355L 552,355L 556,359L 556,362L 547,362L 547,359 Z "
                      Fill="{DynamicResource {dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerIndentHotZoneBorderBrush}}"
                      Stretch="Fill"
                      UseLayoutRounding="False"/>
