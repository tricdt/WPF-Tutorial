            <Border Background="{DynamicResource {dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBackgroundBrush}}"
                    BorderBrush="{DynamicResource {dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBorderBrush}}"
                    BorderThickness="0"
                    Opacity="0.35"/>
            <Border Background="{DynamicResource {dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBackgroundBrush}}"
                    BorderBrush="{DynamicResource {dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBorderBrush}}"
                    BorderThickness="0,0,1,0"
                    Opacity="0.35"/>
        </Grid>
    </ControlTemplate>

