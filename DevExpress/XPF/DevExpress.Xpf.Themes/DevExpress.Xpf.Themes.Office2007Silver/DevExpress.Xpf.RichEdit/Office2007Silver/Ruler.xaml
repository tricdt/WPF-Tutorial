                BorderBrush="{DynamicResource {dxret:RichEditOffice2007ColorThemeKey ResourceKey=RulerBorderBrush}}"
                BorderThickness="0,0,1,0"
                Opacity="0.35"/>
    </ControlTemplate>

    <Style x:Key="{dxret:RichEditRulerThemeKey ResourceKey=HorizontalRulerContentStyle}" TargetType="{x:Type Canvas}">
        <Setter Property="Margin" Value="0,5,0,6"/>
    </Style>

    <Style x:Key="{dxret:RichEditRulerThemeKey ResourceKey=VerticalRulerContentStyle}" TargetType="{x:Type Canvas}">
        <Setter Property="Margin" Value="5,0,6,0"/>
    </Style>

    <Style x:Key="{dxret:RichEditRulerThemeKey ResourceKey=HorizontalRulerShadowStyle}" TargetType="{x:Type Border}">
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Height" Value="6"/>
        <Setter Property="Background" Value="Transparent"/>
