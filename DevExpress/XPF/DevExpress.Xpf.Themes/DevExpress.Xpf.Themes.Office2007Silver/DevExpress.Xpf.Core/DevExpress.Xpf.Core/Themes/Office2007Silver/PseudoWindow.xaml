        <GradientStop Color="#FF8899B0 " Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxt:FloatingContainerThemeKey ResourceKey=FloatingContainerDefaultButtonForegroundMouseOverState}"
