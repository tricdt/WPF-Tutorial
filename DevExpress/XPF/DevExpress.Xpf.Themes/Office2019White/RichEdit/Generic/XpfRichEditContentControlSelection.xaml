<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxrec="http://schemas.devexpress.com/winfx/2008/xaml/richedit/internal" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.RichEdit.Controls.Internal.XpfRichEditContentControlSelection}" TargetType="{x:Type dxrec:XpfRichEditContentControlSelection}">
    <Setter Property="RenderOptions.EdgeMode" Value="Aliased" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="dxrec:XpfRichEditContentControlSelection">
          <Grid IsHitTestVisible="False" SnapsToDevicePixels="True" UseLayoutRounding="True">
            <Path x:Name="Path" SnapsToDevicePixels="True" UseLayoutRounding="True">
              <Path.Data>
                <PathGeometry x:Name="Geometry" FillRule="Nonzero" />
              </Path.Data>
            </Path>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <!--BEGIN WPF-->
    <Setter Property="Focusable" Value="False" />
    <!--END WPF-->
  </Style>
</ResourceDictionary>