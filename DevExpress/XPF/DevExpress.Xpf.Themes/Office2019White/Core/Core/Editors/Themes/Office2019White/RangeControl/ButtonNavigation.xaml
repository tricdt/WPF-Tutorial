          <VisualState x:Name="Pressed">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)" To="#FFDEECF9" cs:id="ColorAnimation_0001" x:Uid="@To=SelectionBackground" />
              <ColorAnimation Duration="0" Storyboard.TargetName="Path" Storyboard.TargetProperty="Fill.(SolidColorBrush.Color)" To="#FF444444" cs:id="ColorAnimation_0004" x:Uid="@To=Foreground" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
</ResourceDictionary>