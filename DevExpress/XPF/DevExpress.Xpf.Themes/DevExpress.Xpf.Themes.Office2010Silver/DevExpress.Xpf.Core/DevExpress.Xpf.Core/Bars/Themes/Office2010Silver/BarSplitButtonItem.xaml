          <VisualState x:Name="Pressed">
            <Storyboard>
              <DoubleAnimation Duration="0" Storyboard.TargetName="PressedState" Storyboard.TargetProperty="Opacity" To="1" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Checked" />
          <VisualState x:Name="HoverChecked" />
          <VisualState x:Name="Focused" />
          <VisualState x:Name="Customization">
            <Storyboard>
              <DoubleAnimation Duration="0" Storyboard.TargetName="PART_Customization" Storyboard.TargetProperty="Opacity" To="1" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
