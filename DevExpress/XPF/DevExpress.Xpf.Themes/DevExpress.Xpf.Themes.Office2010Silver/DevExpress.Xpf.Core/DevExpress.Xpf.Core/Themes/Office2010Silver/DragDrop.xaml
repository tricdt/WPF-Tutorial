<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <SolidColorBrush x:Key="{dxt:DragDropThemeKey ResourceKey=DropMarkerBackground}" Color="White" />
  <SolidColorBrush x:Key="{dxt:DragDropThemeKey ResourceKey=DropMarkerBorderBrush}" Color="#FFDDAA22" />
  <SolidColorBrush x:Key="{dxt:DragDropThemeKey ResourceKey=DragHintBorderBrush}" Color="#FFA5ACB5" />
  <CornerRadius x:Key="{dxt:DragDropThemeKey ResourceKey=DragHintCornerRadius}">0</CornerRadius>
  <LinearGradientBrush x:Key="{dxt:DragDropThemeKey ResourceKey=DragHintBackground}" EndPoint="0.5,1" StartPoint="0.5,0">
    <GradientStop Color="#FFE3E6E8" Offset="0" />
    <GradientStop Color="#FFF3F4F5" Offset="1" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="{dxt:DragDropThemeKey ResourceKey=DragHintCaptionForeground}" Color="#FF3C3C3C" />
  <FontWeight x:Key="{dxt:DragDropThemeKey ResourceKey=DragHintCaptionFontWeight}">Bold</FontWeight>
  <sys:Double x:Key="{dxt:DragDropThemeKey ResourceKey=DragHintCaptionFontSize}">12</sys:Double>
  <SolidColorBrush x:Key="{dxt:DragDropThemeKey ResourceKey=DragHintDescriptionForeground}" Color="#FF80838F" />
</ResourceDictionary>