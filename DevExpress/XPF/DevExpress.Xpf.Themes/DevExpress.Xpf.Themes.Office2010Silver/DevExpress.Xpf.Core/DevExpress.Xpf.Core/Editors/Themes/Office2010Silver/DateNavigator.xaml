                <GradientStop Color="#FFF8EBB2" Offset="1" />
                <GradientStop Color="#FFFFD86C" Offset="0.5" />
              </LinearGradientBrush>
            </Border.Background>
          </Border>
        </Border>
      </Border>
      <Border x:Name="BorderFocused" Visibility="Collapsed">
        <Border CornerRadius="2" Opacity="0.35">
          <Border.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
