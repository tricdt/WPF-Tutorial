        <DropShadowEffect x:Name="Shadow" BlurRadius="3" Direction="270" Opacity="0.35" ShadowDepth="1" />
      </Grid.Effect>
      <Border Background="#FF585B5E" CornerRadius="2">
        <Grid Margin="3,7,3,3">
          <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
          </Grid.RowDefinitions>
          <ve:DockHintButton x:Name="PART_Hide" Grid.Row="0" Style="{StaticResource button-tophide}" />
          <ve:DockHintButton x:Name="PART_Dock" Grid.Row="1" Style="{StaticResource button-top}" />
