  <ControlTemplate x:Key="{dxt:DockVisualizerThemeKey ResourceKey=Center}" TargetType="dxp:DockHintElement">
    <Grid x:Name="Root" HorizontalAlignment="Center" VerticalAlignment="Center">
      <Grid.Effect>
        <DropShadowEffect x:Name="Shadow" BlurRadius="3" Direction="270" Opacity="0.35" ShadowDepth="1" />
      </Grid.Effect>
      <Grid.RowDefinitions>
        <RowDefinition Height="Auto" />
        <RowDefinition Height="34" />
        <RowDefinition Height="Auto" />
      </Grid.RowDefinitions>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition Width="34" />
        <ColumnDefinition Width="Auto" />
      </Grid.ColumnDefinitions>
      <!-- Cross -->
      <Ellipse Fill="#FF585B5E" Grid.Column="1" Grid.Row="1" Margin="-22" StrokeThickness="0" />
      <!-- Content -->
      <Border x:Name="gridTop" Background="#FF585B5E" CornerRadius="2" Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Bottom">
        <Grid Margin="3,3,3,0">
          <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
          </Grid.RowDefinitions>
          <ve:DockHintButton x:Name="PART_Top" dxp:DockHintElement.DockType="Top" Grid.Row="0" Height="28" Margin="0,0,0,3" Style="{StaticResource button-top-3}" Width="28" />
          <ve:DockHintButton x:Name="PART_TabTop" dxp:DockHintElement.DockType="Top" Grid.Row="1" Height="28" Style="{StaticResource button-top-2}" Width="28" />
        </Grid>
      </Border>
      <Border x:Name="gridBottom" Background="#FF585B5E" CornerRadius="2" Grid.Column="1" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Bottom">
        <Grid Margin="3,0,3,3">
          <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
          </Grid.RowDefinitions>
          <ve:DockHintButton x:Name="PART_TabBottom" dxp:DockHintElement.DockType="Bottom" Grid.Row="0" Height="28" Style="{StaticResource button-bottom-2}" Width="28" />
          <ve:DockHintButton x:Name="PART_Bottom" dxp:DockHintElement.DockType="Bottom" Grid.Row="1" Height="28" Margin="0,3,0,0" Style="{StaticResource button-bottom-3}" Width="28" />
        </Grid>
      </Border>
      <Border x:Name="gridLeft" Background="#FF585B5E" CornerRadius="2" Grid.Column="0" Grid.Row="1" HorizontalAlignment="Right" VerticalAlignment="Center">
        <Grid Margin="3,3,0,3">
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="Auto" />
          </Grid.ColumnDefinitions>
          <ve:DockHintButton x:Name="PART_Left" dxp:DockHintElement.DockType="Left" Grid.Column="0" Height="28" Margin="0,0,3,0" Style="{StaticResource button-left-3}" Width="28" />
          <ve:DockHintButton x:Name="PART_TabLeft" dxp:DockHintElement.DockType="Left" Grid.Column="1" Height="28" Style="{StaticResource button-left-2}" Width="28" />
        </Grid>
      </Border>
      <Border x:Name="gridRight" Background="#FF585B5E" CornerRadius="2" Grid.Column="2" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center">
        <Grid Margin="0,3,3,3">
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="Auto" />
          </Grid.ColumnDefinitions>
          <ve:DockHintButton x:Name="PART_TabRight" dxp:DockHintElement.DockType="Right" Grid.Column="0" Height="28" Style="{StaticResource button-right-2}" Width="28" />
          <ve:DockHintButton x:Name="PART_Right" dxp:DockHintElement.DockType="Right" Grid.Column="1" Height="28" Margin="3,0,0,0" Style="{StaticResource button-right-3}" Width="28" />
        </Grid>
      </Border>
      <ve:DockHintButton x:Name="PART_Fill" dxp:DockHintElement.DockType="Fill" Grid.Column="1" Grid.Row="1" Height="28" HorizontalAlignment="Center" Style="{StaticResource button-center}" VerticalAlignment="Center" Width="28" />
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="ShowShadow" Value="False">
        <Setter Property="Effect" TargetName="Root" Value="{x:Null}" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <!--BEGIN WPF-->
  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Docking.VisualElements.DocumentSelectorContainer}" TargetType="{x:Type ve:DocumentSelectorContainer}">
    <Style.Setters>
      <Setter Property="Foreground" Value="#FF585B5E" />
      <Setter Property="Template">
        <Setter.Value>
          <ControlTemplate TargetType="{x:Type ve:DocumentSelectorContainer}">
            <Grid>
              <Grid x:Name="Shadow" Margin="2,2,-3,-3">
                <Border Background="#FF000000" CornerRadius="8" Opacity="0.01" />
                <Border Background="#FF000000" CornerRadius="7" Margin="1" Opacity="0.02" />
                <Border Background="#FF000000" CornerRadius="6" Margin="2" Opacity="0.03" />
              </Grid>
              <Border x:Name="Window" Background="#FFA5ACB5" CornerRadius="5" Margin="1">
                <Border Background="#FFFFFFFF" CornerRadius="4" Margin="1">
                  <Grid Margin="1">
                    <Grid.RowDefinitions>
                      <RowDefinition Height="Auto" />
                      <RowDefinition Height="*" />
                      <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Border x:Name="Header" CornerRadius="3,3,0,0">
                      <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                          <GradientStop Color="#FFE9EDF1" Offset="0" />
                          <GradientStop Color="#FFE3E6E8" Offset="1" />
                        </LinearGradientBrush>
                      </Border.Background>
                      <StackPanel HorizontalAlignment="Left" Margin="12,8" Orientation="Horizontal" VerticalAlignment="Center">
                        <Image HorizontalAlignment="Left" Margin="0,0,6,0" Source="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionImage}" Stretch="None" VerticalAlignment="Top" />
                        <StackPanel>
                          <ContentControl Content="{Binding Path=(local:DockLayoutManager.LayoutItem).Caption}" ContentTemplate="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionTemplate}" ContentTemplateSelector="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionTemplateSelector}" Focusable="False" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Bottom" />
                          <TextBlock HorizontalAlignment="Left" Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Description}" TextWrapping="Wrap" VerticalAlignment="Top" />
                        </StackPanel>
                      </StackPanel>
                    </Border>
                    <Border x:Name="Content" Background="#FFE9EDF1" Grid.Row="1">
                      <ContentPresenter x:Name="PART_ContainerContent" Content="{Binding}" Margin="12" />
                    </Border>
                    <Border x:Name="Footer" Background="#FFE9EDF1" CornerRadius="0,0,3,3" Grid.Row="2" MinHeight="24">
                      <ContentPresenter Content="{Binding Path=(local:DockLayoutManager.LayoutItem).FooterDescription}" HorizontalAlignment="Left" Margin="12,0,0,0" VerticalAlignment="Center" />
                    </Border>
                  </Grid>
                </Border>
              </Border>
            </Grid>
          </ControlTemplate>
        </Setter.Value>
      </Setter>
    </Style.Setters>
  </Style>
  <ControlTemplate x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=ListBoxItem}" TargetType="{x:Type ListBoxItem}">
    <Grid>
      <Grid x:Name="HoverState" Opacity="0">
        <Border CornerRadius="2">
          <Border.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
              <GradientStop Color="#FFECC757" Offset="0" />
              <GradientStop Color="#FFF1D449" Offset="1" />
            </LinearGradientBrush>
          </Border.Background>
          <Border CornerRadius="1" Margin="1">
            <Border.Background>
              <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FFFCF7E4" Offset="0" />
                <GradientStop Color="#FFFCF8E7" Offset="1" />
              </LinearGradientBrush>
            </Border.Background>
            <Border CornerRadius="1" Margin="1">
              <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                  <GradientStop Color="#FFF8EBB2" Offset="0" />
                  <GradientStop Color="#FFFCF7E4" Offset="1" />
                  <GradientStop Color="#FFFFE575" Offset="0.5" />
                </LinearGradientBrush>
              </Border.Background>
            </Border>
          </Border>
        </Border>
      </Grid>
      <Grid x:Name="SelectedState" Opacity="0">
        <Border CornerRadius="2">
          <Border.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
              <GradientStop Color="#FFC28E35" Offset="0" />
              <GradientStop Color="#FFC2A44D" Offset="1" />
            </LinearGradientBrush>
          </Border.Background>
          <Border CornerRadius="1" Margin="1">
            <Border.Background>
              <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FFFFE575" Offset="0" />
                <GradientStop Color="#FFF8EBB2" Offset="1" />
                <GradientStop Color="#FFFFD86C" Offset="0.5" />
              </LinearGradientBrush>
            </Border.Background>
          </Border>
        </Border>
      </Grid>
      <Border Padding="{TemplateBinding Padding}">
        <ve:TemplatedCaptionControl x:Name="caption" LayoutItem="{Binding}" Margin="3,2" />
      </Border>
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="IsSelected" Value="True">
        <Setter Property="Opacity" TargetName="SelectedState" Value="1" />
        <Setter Property="Foreground" TargetName="caption" Value="#FF585B5E" />
      </Trigger>
      <Trigger Property="IsMouseOver" Value="True">
        <Setter Property="Opacity" TargetName="HoverState" Value="1" />
        <Setter Property="Foreground" TargetName="caption" Value="#FF585B5E" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <DataTemplate x:Key="{dxt:DockPaneElementsThemeKey ResourceKey=ContentPreview}">
    <Grid Margin="0,0,-2,0" SnapsToDevicePixels="True" VerticalAlignment="Top">
      <Border x:Name="Shadow" Background="#FF000000" Margin="2,2,0,0" Opacity="0.15" />
      <Border Background="#FFFFFFFF" BorderBrush="#BF80838F" BorderThickness="1" Margin="0,0,2,2" Padding="12">
        <ContentPresenter Content="{TemplateBinding Content}" />
      </Border>
    </Grid>
  </DataTemplate>
  <DataTemplate x:Key="{dxt:DocumentElementsThemeKey ResourceKey=ContentPreview}">
    <Grid Margin="0,0,-2,0" SnapsToDevicePixels="True" VerticalAlignment="Top">
      <Border x:Name="Shadow" Background="#FF000000" Margin="2,2,0,0" Opacity="0.15" />
      <Border Background="#FFFFFFFF" BorderBrush="#BF80838F" BorderThickness="1" Margin="0,0,2,2" Padding="1">
        <ContentPresenter Content="{TemplateBinding Content}" />
      </Border>
    </Grid>
  </DataTemplate>
  <!--END WPF-->
</ResourceDictionary>