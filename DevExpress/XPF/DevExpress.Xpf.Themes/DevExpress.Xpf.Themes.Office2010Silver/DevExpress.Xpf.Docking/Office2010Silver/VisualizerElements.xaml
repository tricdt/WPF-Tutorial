  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Docking.Platform.AutoHideResizePointer}" TargetType="dxp:AutoHideResizePointer">
    <Setter Property="Background" Value="{StaticResource BrushNormal}" />
    <Setter Property="BorderBrush" Value="{StaticResource BrushHot}" />
    <Setter Property="Opacity" Value="0.5" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="dxp:AutoHideResizePointer">
          <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}">
            <Border.BorderThickness>
              <Binding Path="Dock" RelativeSource="{RelativeSource Mode=TemplatedParent}">
                <Binding.Converter>
                  <ve:DockTypeToThicknessConverter All="3" Inverted="True" />
                </Binding.Converter>
              </Binding>
            </Border.BorderThickness>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
