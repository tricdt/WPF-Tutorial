  <!-- TextSplitterThemeKey -->
  <Style x:Key="{dxbt:TextSplitterThemeKey ResourceKey=BarSubItem}" TargetType="{x:Type dxb:TextSplitterControl}">
    <Setter Property="FirstStringHorizontalAlignment" Value="Center" />
    <Setter Property="SecondStringHorizontalAlignment" Value="Center" />
    <Setter Property="FirstStringMargin" Value="0,-5,0,4" />
    <Setter Property="SecondStringMargin" Value="0,-6,0,-3" />
    <Setter Property="Margin" Value="0" />
    <Setter Property="NormalArrowTemplate">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type ContentControl}">
          <Path Data="F1 M 0,0L 6,0L 3,3L 0,0 Z " Fill="{TemplateBinding Foreground}" Height="3" HorizontalAlignment="Center" SnapsToDevicePixels="True" Stretch="Fill" VerticalAlignment="Center" Width="6" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="SelectedArrowTemplate">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type ContentControl}">
          <Path Data="F1 M 0,0L 6,0L 3,3L 0,0 Z " Fill="{TemplateBinding Foreground}" Height="3" HorizontalAlignment="Center" SnapsToDevicePixels="True" Stretch="Fill" VerticalAlignment="Center" Width="6" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="NormalTextStyle">
      <Setter.Value>
        <Style TargetType="{x:Type ContentControl}">
          <Setter Property="Foreground" Value="#FF444444" />
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="SelectedTextStyle">
      <Setter.Value>
        <Style TargetType="{x:Type ContentControl}">
          <Setter Property="Foreground" Value="#FF444444" />
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="BottomSideArrowContainerStyle">
      <Setter.Value>
        <Style TargetType="{x:Type ContentControl}">
          <Setter Property="Margin" Value="0,0,0,0" />
          <Setter Property="HorizontalAlignment" Value="Center" />
          <Setter Property="VerticalAlignment" Value="Center" />
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="RightSideArrowContainerStyle">
      <Setter.Value>
