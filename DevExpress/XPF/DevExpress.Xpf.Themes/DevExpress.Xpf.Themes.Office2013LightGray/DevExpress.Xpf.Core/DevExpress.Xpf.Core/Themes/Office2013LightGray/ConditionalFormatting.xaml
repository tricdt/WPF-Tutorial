  <Color x:Key="{dxdst:FormatThemeKey ResourceKey=DataBarPurpleColor}">#FFD6007B</Color>
  <LinearGradientBrush x:Key="{dxdst:FormatThemeKey ResourceKey=DataBarNegativeBrush_Gradient}" EndPoint="1,0" StartPoint="0,0">
    <GradientStop Color="#FFFFD8D8" Offset="0" />
    <GradientStop Color="{StaticResource {dxdst:FormatThemeKey ResourceKey=DataBarNegativeColor}}" Offset="1" />
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="{dxdst:FormatThemeKey ResourceKey=DataBarBlueBrush_Gradient}" EndPoint="1,0" StartPoint="0,0">
    <GradientStop Color="{StaticResource {dxdst:FormatThemeKey ResourceKey=DataBarBlueColor}}" Offset="0" />
    <GradientStop Color="#FFE6EDF5" Offset="1" />
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="{dxdst:FormatThemeKey ResourceKey=DataBarGreenBrush_Gradient}" EndPoint="1,0" StartPoint="0,0">
