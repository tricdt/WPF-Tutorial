    <GradientStop Color="#FFE6F5EB" Offset="1" />
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="{dxdst:FormatThemeKey ResourceKey=DataBarRedBrush_Gradient}" EndPoint="1,0" StartPoint="0,0">
