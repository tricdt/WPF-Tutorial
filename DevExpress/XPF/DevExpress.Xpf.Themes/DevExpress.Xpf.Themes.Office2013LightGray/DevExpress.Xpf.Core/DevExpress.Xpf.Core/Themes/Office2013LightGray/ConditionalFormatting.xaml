  <Color x:Key="{dxdst:FormatThemeKey ResourceKey=ColorScaleRed}">#FFF8696B</Color>
  <Color x:Key="{dxdst:FormatThemeKey ResourceKey=ColorScaleYellow}">#FFFFEB84</Color>
  <Color x:Key="{dxdst:FormatThemeKey ResourceKey=ColorScaleGreen}">#FF63BE7B</Color>
  <Color x:Key="{dxdst:FormatThemeKey ResourceKey=ColorScaleBlue}">#FF5A8AC6</Color>
