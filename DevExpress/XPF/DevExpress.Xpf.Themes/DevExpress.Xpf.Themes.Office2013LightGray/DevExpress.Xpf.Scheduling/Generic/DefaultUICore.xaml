<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:dxschc="http://schemas.devexpress.com/winfx/2008/xaml/scheduling" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <SolidColorBrush x:Key="BrushSet.StatusBusy" Color="#FF4A87E2" />
  <SolidColorBrush x:Key="BrushSet.StatusFree" Color="White" />
  <SolidColorBrush x:Key="BrushSet.StatusOutOfOffice" Color="#FFD95353" />
  <LinearGradientBrush x:Key="BrushSet.StatusTentative" MappingMode="Absolute" StartPoint="0,0" EndPoint="4,4" SpreadMethod="Repeat">
    <GradientStop Offset="0" Color="#FF4A87E2" />
    <GradientStop Offset="0.5" Color="#FF4A87E2" />
    <GradientStop Offset="0.5" Color="White" />
    <GradientStop Offset="1" Color="White" />
  </LinearGradientBrush>
  <DrawingBrush x:Key="BrushSet.StatusWorkingElsewhere" Viewport="0,0,4,4" ViewportUnits="Absolute" TileMode="Tile">
    <DrawingBrush.Drawing>
      <DrawingGroup>
        <GeometryDrawing Brush="White">
          <GeometryDrawing.Geometry>
            <GeometryGroup FillRule="Nonzero">
              <RectangleGeometry Rect="0,0,80,80" />
            </GeometryGroup>
          </GeometryDrawing.Geometry>
        </GeometryDrawing>
        <GeometryDrawing Brush="#FF937BD1">
          <GeometryDrawing.Geometry>
            <GeometryGroup FillRule="Nonzero">
              <RectangleGeometry Rect="0,0,20,20" />
              <RectangleGeometry Rect="40,40,20,20" />
            </GeometryGroup>
          </GeometryDrawing.Geometry>
        </GeometryDrawing>
      </DrawingGroup>
    </DrawingBrush.Drawing>
  </DrawingBrush>
  <dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color1.Transform.Light" Brightness="-0.4" OverrideA="#59" />
  <dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color2.Transform.Light" Brightness="0.6" OverrideA="#40" />
  <dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color1.Transform.Dark" Brightness="0.6" OverrideA="#59" />
  <dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color2.Transform.Dark" Brightness="-0.4" OverrideA="#40" />
  <!--ScheduleView-->
  <SolidColorBrush x:Key="ScheuleView.TimeScale.TickBrush" Color="DarkGray" />
  <SolidColorBrush x:Key="ScheuleView.TimeScale.BorderBrush" Color="Black" />
  <sys:Boolean x:Key="ViewSelectorShowBackground">False</sys:Boolean>
</ResourceDictionary>