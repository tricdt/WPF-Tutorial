<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxsps="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet" xmlns:themes="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet/themekeys" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <ControlTemplate TargetType="{x:Type Control}" x:Key="{themes:SpreadsheetThemeKey ResourceKey=EmptyFormulaBarTemplate}" />
  <ControlTemplate TargetType="{x:Type Control}" x:Key="{themes:SpreadsheetThemeKey ResourceKey=FormulaBarTemplate}">
    <dxsps:SpreadsheetFormulaBarControl SpreadsheetControl="{Binding RelativeSource={RelativeSource Self}, Path=(dxsps:SpreadsheetControl.Spreadsheet)}" IsExpanded="{Binding RelativeSource={RelativeSource Self}, Path=(dxsps:SpreadsheetControl.Spreadsheet).FormulaBarExpanded}" ShowNameBox="{Binding RelativeSource={RelativeSource Self}, Path=(dxsps:SpreadsheetControl.Spreadsheet).FormulaBarShowNameBox}" ShowFormulaBarButtons="{Binding RelativeSource={RelativeSource Self}, Path=(dxsps:SpreadsheetControl.Spreadsheet).FormulaBarShowButtons}" ShowFormulaBarEditor="{Binding RelativeSource={RelativeSource Self}, Path=(dxsps:SpreadsheetControl.Spreadsheet).FormulaBarShowEditor}" NameBoxItemDisplayMode="{Binding RelativeSource={RelativeSource Self}, Path=(dxsps:SpreadsheetControl.Spreadsheet).FormulaBarNameBoxItemDisplayMode}" />
  </ControlTemplate>
</ResourceDictionary>