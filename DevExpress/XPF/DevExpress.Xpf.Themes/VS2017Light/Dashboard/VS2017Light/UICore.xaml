<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxdash="http://schemas.devexpress.com/winfx/2008/xaml/dashboard" xmlns:dxdashi="http://schemas.devexpress.com/winfx/2008/xaml/dashboardi" xmlns:dxdasht="http://schemas.devexpress.com/winfx/2008/xaml/dashboard/themekeys" xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:wn="clr-namespace:System.Windows;assembly=PresentationCore" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <ResourceDictionary.MergedDictionaries>
    <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.VS2017Light.v24.1;component/Dashboard/VS2017Light/UICustom.xaml" />
    <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.VS2017Light.v24.1;component/Dashboard/VS2017Light/UIPreset.xaml" />
  </ResourceDictionary.MergedDictionaries>
  <!-- DashboardControl -->
  <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SelectedItemBackground}" Color="#FFC9DEF5" x:Uid="@Color=HoverBackground" />
  <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SelectedItemBorder}" Color="#FFCCCEDB" x:Uid="@Color=Border" />
  <sys:Boolean x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=IsDarkTheme}">False</sys:Boolean>
  <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=Background}" Color="#FFEEEEF2" x:Uid="@Color=Window.Background" />
  <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=TitleForeground}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SubTitleForeground}" Color="#FF1E1E1E" Opacity="0.62" x:Uid="@Color=Foreground" />
  <Color x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=MasterFilterIconColor}" x:Uid="@Content_Property=Foreground">#FF1E1E1E</Color>
  <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=FormatConditionBarDrawAxisBrush}" Color="#FF007ACC" x:Uid="@Color=Focused" />
  <!-- DashboardLayoutItem -->
  <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=Background}" Color="#FFFFFFFF" x:Uid="@Color=Editor.Background" />
  <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=BorderBrush}" Color="#FFCCCEDB" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=CaptionBackground}" Color="#FFF5F5F7" x:Uid="@Color=Control.Background" />
  <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=CaptionForeground}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=WaitIndicatorForeground}" Color="#FF007ACC" x:Uid="@Color=Focused" />
  <!-- CardItem -->
  <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=Background}" x:Uid="@Content_Property=Control.Background">#FFF5F5F7</SolidColorBrush>
  <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=MainTextForeground}" x:Uid="@Content_Property=Foreground">#FF1E1E1E</SolidColorBrush>
  <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=CardItemBackground}" x:Uid="@Content_Property=Control.Background">#FFF5F5F7</SolidColorBrush>
  <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=CardItemBorderBrush}" x:Uid="@Content_Property=Border">#FFCCCEDB</SolidColorBrush>
  <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=HighlightedBackground}" x:Uid="@Content_Property=Window.Background">#FFEEEEF2</SolidColorBrush>
  <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=HighlightedBorderBrush}" x:Uid="@Content_Property=Border">#FFCCCEDB</SolidColorBrush>
  <!-- GaugeItem -->
  <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeHighlightedBackgroundBrush}" Color="#FFC9DEF5" x:Uid="@Color=HoverBackground" />
  <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeHighlightedBorderBrush}" Color="#FFC9DEF5" x:Uid="@Color=HoverBorder" />
  <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeCaptionForeground}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeScaleLabelForeground}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <sys:Double x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeCaptionFontSize}">12</sys:Double>
  <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeSubCaptionForeground}" Color="#FF1E1E1E" Opacity="0.62" x:Uid="@Color=Foreground" />
  <sys:Double x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeSubCaptionFontSize}">12</sys:Double>
  <!-- MapItem -->
  <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointBackgroundBrush}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointHighLightedBackgroundBrush}" Color="#FF007ACC" x:Uid="@Color=Focused" />
  <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointBorderBrush}" Color="#FFFFFFFF" x:Uid="@Color=Backstage.Foreground" />
  <!-- PieItem -->
  <SolidColorBrush x:Key="{dxdasht:PieItemThemeKey ResourceKey=PieHighlightedBackgroundBrush}" Color="#FFC9DEF5" x:Uid="@Color=HoverBackground" />
  <SolidColorBrush x:Key="{dxdasht:PieItemThemeKey ResourceKey=PieHighlightedBorderBrush}" Color="#FFC9DEF5" x:Uid="@Color=HoverBorder" />
  <!-- ChartItem -->
  <SolidColorBrush x:Key="{dxdasht:ChartItemThemeKey ResourceKey=LegendBackgroundBrush}" Color="#FFFFFFFF" x:Uid="@Color=Editor.Background" />
  <SolidColorBrush x:Key="{dxdasht:ChartItemThemeKey ResourceKey=LegendForegroundBrush}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <!-- TreemapItem -->
  <SolidColorBrush x:Key="{dxdasht:TreemapItemThemeKey ResourceKey=ToolTipForeground}" Color="#FFFFFFFF" x:Uid="@Color=Backstage.Foreground" />
  <sys:Double x:Key="{dxdasht:TreemapItemThemeKey ResourceKey=ToolTipFontSize}">12</sys:Double>
  <FontWeight x:Key="{dxdasht:TreemapItemThemeKey ResourceKey=ToolTipFontWeight}">Bold</FontWeight>
  <!-- ListBox -->
  <SolidColorBrush x:Key="{dxdasht:ListBoxItemThemeKey ResourceKey=Foreground}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <!-- Other -->
  <Color x:Key="{dxdasht:PivotItemThemeKey ResourceKey=DefaultBackgroundColor}" x:Uid="@Content_Property=Editor.Background">#FFFFFFFF</Color>
  <Color x:Key="{dxdasht:GridItemThemeKey ResourceKey=DefaultBackgroundColor}" x:Uid="@Content_Property=Editor.Background">#FFFFFFFF</Color>
  <Color x:Key="{dxdasht:CardItemThemeKey ResourceKey=DefaultBackgroundColor}" x:Uid="@Content_Property=Editor.Background">#FFFFFFFF</Color>
  <Color x:Key="{dxdasht:ChartItemThemeKey ResourceKey=DefaultBackgroundColor}" x:Uid="@Content_Property=Editor.Background">#FFFFFFFF</Color>
</ResourceDictionary>