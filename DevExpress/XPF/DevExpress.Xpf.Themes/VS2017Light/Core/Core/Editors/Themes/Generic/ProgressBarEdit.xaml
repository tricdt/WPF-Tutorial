<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxtEx="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dxei="http://schemas.devexpress.com/winfx/2008/xaml/editors/internal" xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys" xmlns:dxeh="http://schemas.devexpress.com/winfx/2008/xaml/editors/internal" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:native="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:interactivity="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <!--BEGIN WPF-->
  <dxe:RectConverter x:Key="RectConverter" />
  <!--END WPF-->
  <Thickness x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelNoContentHorizontalMargin}">0</Thickness>
  <Thickness x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=PanelNoContentVerticalMargin}">0</Thickness>
  <sys:Boolean x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=ShowBorderDefault}">True</sys:Boolean>
  <SolidColorBrush x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=AdditionalForeground}">White</SolidColorBrush>
  <SolidColorBrush x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=Foreground}">Black</SolidColorBrush>
  <DataTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=ContentTemplate_Content}">
    <Grid>
      <ContentPresenter Content="{Binding}" HorizontalAlignment="{Binding Path=HorizontalContentAlignment, RelativeSource={RelativeSource FindAncestor, AncestorType=dxe:ProgressBarEdit}, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="{Binding Path=VerticalContentAlignment, RelativeSource={RelativeSource FindAncestor, AncestorType=dxe:ProgressBarEdit}, UpdateSourceTrigger=PropertyChanged}" />
    </Grid>
  </DataTemplate>
  <DataTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=ContentTemplate_None}" />
  <DataTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=ContentTemplate}">
    <Grid>
      <Grid x:Name="root">
        <Grid.Resources>
          <dxe:GridLengthConverter x:Key="GridLengthConverter" />
        </Grid.Resources>
        <Grid.ColumnDefinitions>
          <ColumnDefinition x:Name="leftColumn" Width="{Binding Path=(dxe:RangeEditBaseInfo.LayoutInfo).LeftSidePosition, Converter={StaticResource GridLengthConverter}, RelativeSource={RelativeSource TemplatedParent}}" />
          <ColumnDefinition x:Name="rightColumn" Width="{Binding Path=(dxe:RangeEditBaseInfo.LayoutInfo).RightSidePosition, Converter={StaticResource GridLengthConverter}, RelativeSource={RelativeSource TemplatedParent}}" />
        </Grid.ColumnDefinitions>
        <dxeh:EmptySizePanel Grid.ColumnSpan="2" HorizontalAlignment="Stretch">
          <TextBlock Text="{Binding}" HorizontalAlignment="Stretch" VerticalAlignment="Center" TextAlignment="Center" />
        </dxeh:EmptySizePanel>
        <dxeh:EmptySizePanel Grid.ColumnSpan="2" HorizontalAlignment="Stretch">
          <TextBlock Text="{Binding}" Foreground="{Binding Path=(dxe:BaseEdit.OwnerEdit).AdditionalForeground, RelativeSource={RelativeSource TemplatedParent}}" HorizontalAlignment="Stretch" VerticalAlignment="Center" TextAlignment="Center" x:Name="editor">
            <interactivity:Interaction.Behaviors>
              <dxe:ProgressBarAdditionalForegroundBehavior>
                <dxe:ProgressBarAdditionalForegroundBehavior.ClipRect>
                  <MultiBinding Converter="{StaticResource RectConverter}">
                    <Binding ElementName="Indicator" Path="ActualWidth" UpdateSourceTrigger="PropertyChanged" />
                    <Binding ElementName="editor" Path="ActualHeight" UpdateSourceTrigger="PropertyChanged" />
                  </MultiBinding>
                </dxe:ProgressBarAdditionalForegroundBehavior.ClipRect>
              </dxe:ProgressBarAdditionalForegroundBehavior>
            </interactivity:Interaction.Behaviors>
          </TextBlock>
        </dxeh:EmptySizePanel>
        <Rectangle Grid.Column="0" x:Name="Indicator" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0" Opacity="0" />
      </Grid>
    </Grid>
  </DataTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=Template}" TargetType="{x:Type dxe:ProgressBarEdit}">
    <ContentControl Template="{TemplateBinding ActualBorderTemplate}" x:Name="Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" native:AppearanceOptions.CornerRadius="{Binding Path=(dxn:AppearanceOptions.CornerRadius), RelativeSource={RelativeSource TemplatedParent}}" native:AppearanceOptions.HasCornerRadius="{Binding Path=(dxn:AppearanceOptions.HasCornerRadius), RelativeSource={RelativeSource TemplatedParent}}" native:AppearanceOptions.BorderThickness="{Binding Path=(dxn:AppearanceOptions.BorderThickness), RelativeSource={RelativeSource TemplatedParent}}" native:AppearanceOptions.HasBorderThickness="{Binding Path=(dxn:AppearanceOptions.HasBorderThickness), RelativeSource={RelativeSource TemplatedParent}}" dxc:FocusHelper2.Focusable="False" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
      <dxe:EditorControl x:Name="PART_Content" native:AppearanceOptions.Padding="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxn:AppearanceOptions.Padding), RelativeSource={RelativeSource Self}}" native:AppearanceOptions.HasPadding="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxn:AppearanceOptions.HasPadding), RelativeSource={RelativeSource Self}}" Template="{Binding Path=(dxe:BaseEdit.OwnerEdit).ActualEditorControlTemplate, RelativeSource={RelativeSource Self}}" />
    </ContentControl>
  </ControlTemplate>
  <BooleanToVisibilityConverter x:Key="boolToVisibilityConverter" />
  <dxc:BoolToVisibilityViaOpacityConverter x:Key="boolToHiddenConverter" />
  <ControlTemplate x:Key="{dxet:ProgressBarEditStyleThemeKey ResourceKey=PanelTemplateHorizontal}" TargetType="{x:Type dxe:RangeEditBasePanel}">
    <Grid>
      <Grid x:Name="root">
        <Grid.Resources>
          <dxe:GridLengthConverter x:Key="GridLengthConverter" />
        </Grid.Resources>
        <Grid.ColumnDefinitions>
          <ColumnDefinition x:Name="leftColumn" Width="{Binding Path=(dxe:RangeEditBaseInfo.LayoutInfo).LeftSidePosition, Converter={StaticResource GridLengthConverter}}" />
          <ColumnDefinition x:Name="rightColumn" Width="{Binding Path=(dxe:RangeEditBaseInfo.LayoutInfo).RightSidePosition, Converter={StaticResource GridLengthConverter}}" />
        </Grid.ColumnDefinitions>
        <ContentControl x:Name="back" native:AppearanceOptions.Background="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxn:AppearanceOptions.Background), RelativeSource={RelativeSource Self}}" native:AppearanceOptions.HasBackground="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxn:AppearanceOptions.HasBackground), RelativeSource={RelativeSource Self}}" Grid.ColumnSpan="2" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelBackgroundHorizontalStyle}}" />
        <ContentControl x:Name="left" Grid.Column="0" dxeh:BaseEditAppearanceOptions.ProgressBrush="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxei:BaseEditAppearanceOptions.ProgressBrush), RelativeSource={RelativeSource Self}}" dxeh:BaseEditAppearanceOptions.HasProgressBrush="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxei:BaseEditAppearanceOptions.HasProgressBrush), RelativeSource={RelativeSource                                  Self}}" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideHorizontalStyle}}" />
        <ContentControl x:Name="right" Grid.Column="1" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideHorizontalStyle}}" />
        <ContentControl x:Name="foreground" Grid.ColumnSpan="2" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelForegroundStyle}}" />
      </Grid>
      <ContentControl x:Name="PART_InnerContent" Grid.ColumnSpan="2" HorizontalAlignment="Stretch" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=ContentStyle}}" HorizontalContentAlignment="{Binding Path=(dxe:BaseEdit.OwnerEdit).HorizontalContentAlignment}" Opacity="{Binding Path=(dxe:BaseEdit.OwnerEdit).ContentDisplayMode, Converter={StaticResource boolToHiddenConverter}}" Content="{Binding Path=(dxe:BaseEdit.OwnerEdit).DisplayText}" ContentTemplate="{Binding Path=(dxe:BaseEdit.OwnerEdit).ContentTemplate}" />
    </Grid>
    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).ContentDisplayMode, RelativeSource={RelativeSource TemplatedParent}}" Value="Content">
        <Setter Property="Content" Value="{Binding Path=(dxe:BaseEdit.OwnerEdit).Content}" TargetName="PART_InnerContent" />
      </DataTrigger>
      <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).ContentDisplayMode, RelativeSource={RelativeSource TemplatedParent}}" Value="None">
        <Setter Property="Margin" TargetName="root" Value="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelNoContentHorizontalMargin}}" />
      </DataTrigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditStyleThemeKey ResourceKey=PanelTemplateVertical}" TargetType="{x:Type dxe:RangeEditBasePanel}">
    <Grid>
      <Grid x:Name="root">
        <Grid.Resources>
          <dxe:GridLengthConverter x:Key="GridLengthConverter" />
        </Grid.Resources>
        <Grid.RowDefinitions>
          <RowDefinition x:Name="rightColumn" Height="{Binding Path=(dxe:RangeEditBaseInfo.LayoutInfo).RightSidePosition, Converter={StaticResource GridLengthConverter}}" />
          <RowDefinition x:Name="leftColumn" Height="{Binding Path=(dxe:RangeEditBaseInfo.LayoutInfo).LeftSidePosition, Converter={StaticResource GridLengthConverter}}" />
        </Grid.RowDefinitions>
        <ContentControl x:Name="back" Grid.RowSpan="2" native:AppearanceOptions.Background="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxn:AppearanceOptions.Background), RelativeSource={RelativeSource Self}}" native:AppearanceOptions.HasBackground="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxn:AppearanceOptions.HasBackground), RelativeSource={RelativeSource Self}}" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelBackgroundVerticalStyle}}" />
        <ContentControl x:Name="right" Grid.Row="0" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelRightSideVerticalStyle}}" />
        <ContentControl x:Name="left" Grid.Row="1" dxeh:BaseEditAppearanceOptions.ProgressBrush="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxei:BaseEditAppearanceOptions.ProgressBrush), RelativeSource={RelativeSource Self}}" dxeh:BaseEditAppearanceOptions.HasProgressBrush="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxei:BaseEditAppearanceOptions.HasProgressBrush),                                  RelativeSource={RelativeSource Self}}" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelLeftSideVerticalStyle}}" />
        <ContentControl x:Name="foreground" Grid.RowSpan="2" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelForegroundStyle}}" />
      </Grid>
      <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Grid.RowSpan="2">
        <ContentControl x:Name="PART_InnerContent" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=ContentStyle}}" Opacity="{Binding Path=(dxe:BaseEdit.OwnerEdit).ContentDisplayMode, Converter={StaticResource boolToHiddenConverter}}" Content="{Binding Path=(dxe:BaseEdit.OwnerEdit).DisplayText}" ContentTemplate="{Binding Path=(dxe:BaseEdit.OwnerEdit).ContentTemplate}" VerticalAlignment="Stretch">
          <ContentControl.RenderTransformOrigin>0.5,0.5</ContentControl.RenderTransformOrigin>
          <ContentControl.LayoutTransform>
            <RotateTransform Angle="-90" />
          </ContentControl.LayoutTransform>
        </ContentControl>
      </StackPanel>
    </Grid>
    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).ContentDisplayMode, RelativeSource={RelativeSource TemplatedParent}}" Value="Content">
        <Setter Property="Content" Value="{Binding Path=(dxe:BaseEdit.OwnerEdit).Content}" TargetName="PART_InnerContent" />
      </DataTrigger>
      <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).ContentDisplayMode, RelativeSource={RelativeSource TemplatedParent}}" Value="None">
        <Setter Property="Margin" TargetName="root" Value="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelNoContentVerticalMargin}}" />
      </DataTrigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditStyleThemeKey ResourceKey=ProgressBarMarqueeTemplateHorizontal}" TargetType="{x:Type dxe:RangeEditBasePanel}">
    <Grid>
      <ContentControl x:Name="back" Grid.ColumnSpan="2" native:AppearanceOptions.Background="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxn:AppearanceOptions.Background), RelativeSource={RelativeSource Self}}" native:AppearanceOptions.HasBackground="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxn:AppearanceOptions.HasBackground), RelativeSource={RelativeSource Self}}" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelBackgroundHorizontalStyle}}" />
      <dxe:ProgressBarMarqueeHorizontalAnimationControl dxc:FocusHelper2.Focusable="False" />
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditStyleThemeKey ResourceKey=ProgressBarMarqueeTemplateVertical}" TargetType="{x:Type dxe:RangeEditBasePanel}">
    <Grid>
      <ContentControl x:Name="back" Grid.RowSpan="2" native:AppearanceOptions.Background="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxn:AppearanceOptions.Background), RelativeSource={RelativeSource Self}}" native:AppearanceOptions.HasBackground="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxn:AppearanceOptions.HasBackground), RelativeSource={RelativeSource Self}}" Style="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=PanelBackgroundVerticalStyle}}" />
      <dxe:ProgressBarMarqueeVerticalAnimationControl dxc:FocusHelper2.Focusable="False" />
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxet:ProgressBarEditThemeKey ResourceKey=DisplayTemplate}">
    <Border x:Name="Root" UseLayoutRounding="True" Margin="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=DisplayInplaceTemplateMargin}}">
      <dxe:RangeEditBasePanel x:Name="PART_Editor" />
    </Border>
    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).EditMode, RelativeSource={RelativeSource Self}}" Value="Standalone">
        <Setter TargetName="Root" Property="Margin" Value="{DynamicResource {dxet:ProgressBarEditThemeKey ResourceKey=DisplayTemplateMargin}}" />
      </DataTrigger>
      <Trigger Property="dxn:AppearanceOptions.HasPadding" Value="True">
        <Setter Property="Margin" TargetName="Root" Value="{Binding Path=(dxn:AppearanceOptions.Padding), RelativeSource={RelativeSource TemplatedParent}}" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
</ResourceDictionary>