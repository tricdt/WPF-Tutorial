  <ControlTemplate x:Key="{dxet:TrackBarEditThumbThemeKey ResourceKey=LeftHorizontalThumbTickPlacementTopLeftTemplate}" TargetType="{x:Type Thumb}">
    <Grid x:Name="grid" Height="10" Width="5">
      <Grid x:Name="grid1">
        <Border Background="#FFF0F0F0" />
        <Grid Margin="1">
          <Border x:Name="DefaultState" Background="{StaticResource TrackBarEditThumb.DefaultState}" />
          <Border x:Name="HoverState" Background="{StaticResource TrackBarEditThumb.HoverState}" Opacity="0" />
          <Border x:Name="PressedState" Background="{StaticResource TrackBarEditThumb.PressedState}" Opacity="0" />
          <Border x:Name="DisabledState" Background="{StaticResource TrackBarEditThumb.DisabledState}" Opacity="0" />
        </Grid>
      </Grid>
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="IsMouseOver" Value="True">
        <Setter Property="Opacity" TargetName="DefaultState" Value="0" />
        <Setter Property="Opacity" TargetName="HoverState" Value="1" />
        <Setter Property="Opacity" TargetName="PressedState" Value="0" />
      </Trigger>
      <Trigger Property="IsFocused" Value="True">
        <Setter Property="Opacity" TargetName="DefaultState" Value="0" />
        <Setter Property="Opacity" TargetName="HoverState" Value="1" />
        <Setter Property="Opacity" TargetName="PressedState" Value="0" />
      </Trigger>
      <Trigger Property="IsDragging" Value="True">
        <Setter Property="Opacity" TargetName="DefaultState" Value="0" />
        <Setter Property="Opacity" TargetName="HoverState" Value="0" />
        <Setter Property="Opacity" TargetName="PressedState" Value="1" />
      </Trigger>
      <Trigger Property="IsEnabled" Value="False">
        <Setter Property="Opacity" TargetName="DefaultState" Value="0" />
        <Setter Property="Opacity" TargetName="DisabledState" Value="1" />
      </Trigger>
      <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
        <Setter Property="Width" TargetName="grid" Value="43" />
        <Setter Property="Height" TargetName="grid" Value="44" />
        <Setter Property="Margin" TargetName="grid1" Value="38,14,0,14" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
