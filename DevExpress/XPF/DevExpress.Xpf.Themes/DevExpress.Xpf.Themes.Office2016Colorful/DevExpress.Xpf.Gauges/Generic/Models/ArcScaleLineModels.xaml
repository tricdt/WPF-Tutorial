  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.DefaultArcScaleLineControl}" TargetType="local:DefaultArcScaleLineControl">
    <Setter Property="Template">
      <Setter.Value>
