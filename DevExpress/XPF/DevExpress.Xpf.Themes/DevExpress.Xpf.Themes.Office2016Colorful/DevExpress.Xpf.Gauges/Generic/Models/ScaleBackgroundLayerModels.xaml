              </Ellipse.Fill>
              <Ellipse.OpacityMask>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                  <GradientStop Color="Black" Offset="0" />
                  <GradientStop Color="White" Offset="1" />
                </LinearGradientBrush>
              </Ellipse.OpacityMask>
            </Ellipse>
            <Grid Margin="3">
              <Grid>
                <Ellipse Margin="0">
                  <Ellipse.Fill>
                    <RadialGradientBrush Center="0.5,1" GradientOrigin="0.5,1" RadiusY="1" RadiusX="1">
                      <RadialGradientBrush.RelativeTransform>
                        <RotateTransform CenterY="1" CenterX="0.5" Angle="-45" />
                      </RadialGradientBrush.RelativeTransform>
                      <GradientStop Color="#FFCCCED5" Offset="1" />
                      <GradientStop Color="#FFA1A9C3" />
                    </RadialGradientBrush>
