  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.CleanWhiteArcScaleBackgroundLayerControl}" TargetType="local:CleanWhiteArcScaleBackgroundLayerControl">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="local:CleanWhiteArcScaleBackgroundLayerControl">
          <Grid RenderTransformOrigin="0.5, 0.5">
            <Ellipse Margin="2,4,2,0" Opacity="0.15">
