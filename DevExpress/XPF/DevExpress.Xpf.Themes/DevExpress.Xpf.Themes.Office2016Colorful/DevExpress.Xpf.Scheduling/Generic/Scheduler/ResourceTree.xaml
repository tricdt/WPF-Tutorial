<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduling" xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduling/themekeys" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:dxschc="http://schemas.devexpress.com/winfx/2008/xaml/scheduling" xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <ResourceDictionary.MergedDictionaries>
    <dxdst:ResourceDictionaryEx Source="{dx:SchedulingResource UICore.xaml}" />
    <dxdst:ResourceDictionaryEx Source="{dx:SchedulingResource UIPreset.xaml}" />
  </ResourceDictionary.MergedDictionaries>
  <DataTemplate x:Key="ContextMenu">
    <dxb:PopupMenu>
      <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxsch:DefaultBarItemNames.ResourceTree_ContextMenu_Items_Actions_NewResource}" Style="{DynamicResource {dxscht:ResourceTreeCommandBarThemeKey ResourceKey=ContextMenu_NewResource}}" />
      <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxsch:DefaultBarItemNames.ResourceTree_ContextMenu_Items_Actions_NewGroup}" Style="{DynamicResource {dxscht:ResourceTreeCommandBarThemeKey ResourceKey=ContextMenu_NewGroup}}" />
      <dxb:BarItemSeparator />
      <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxsch:DefaultBarItemNames.ResourceTree_ContextMenu_Items_Actions_Rename}" Style="{DynamicResource {dxscht:ResourceTreeCommandBarThemeKey ResourceKey=ContextMenu_Rename}}" />
      <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxsch:DefaultBarItemNames.ResourceTree_ContextMenu_Items_Actions_Delete}" Style="{DynamicResource {dxscht:ResourceTreeCommandBarThemeKey ResourceKey=ContextMenu_Delete}}" />
      <dxb:BarItemSeparator />
      <dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxsch:DefaultBarItemNames.ResourceTree_ContextMenu_Items_Actions_Find}" Style="{DynamicResource {dxscht:ResourceTreeCommandBarThemeKey ResourceKey=ContextMenu_Find}}" />
    </dxb:PopupMenu>
  </DataTemplate>
  <Style x:Key="ResourceTreeControl.DefaultStyle" TargetType="dxsch:ResourceTreeControl">
    <Setter Property="Template" Value="{DynamicResource {dxscht:ResourceTreeThemeKey ResourceKey=Template}}" />
    <Setter Property="NewResourceText" Value="{dxschc:SchedulerLocalizer StringId=ResourceTree_NewResourceText}" />
    <Setter Property="NewGroupText" Value="{dxschc:SchedulerLocalizer StringId=ResourceTree_NewGroupText}" />
    <Setter Property="RowContextMenu" Value="{dxmvvm:ValueFactory ValueTemplate={StaticResource ContextMenu}}" />
  </Style>
</ResourceDictionary>