<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduling" xmlns:dxschv="http://schemas.devexpress.com/winfx/2008/xaml/scheduling/visual" xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduling/themekeys" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <ResourceDictionary.MergedDictionaries>
    <dxdst:ResourceDictionaryEx Source="{dx:SchedulingResource UICore.xaml}" />
    <dxdst:ResourceDictionaryEx Source="{dx:SchedulingResource UIPreset.xaml}" />
  </ResourceDictionary.MergedDictionaries>
  <ControlTemplate x:Key="NavigationControl.DefaultTemplate" TargetType="dxschv:NavigationControl">
    <DockPanel LastChildFill="False">
      <dxschv:SchedulerButton DockPanel.Dock="Left" Kind="NavigationButtonBackward" Visibility="{TemplateBinding BackButtonVisibility}" Command="{TemplateBinding GoBackCommand}" />
      <dxschv:SchedulerButton DockPanel.Dock="Right" Kind="NavigationButtonForward" Visibility="{TemplateBinding NextButtonVisibility}" Command="{TemplateBinding GoNextCommand}" />
    </DockPanel>
  </ControlTemplate>
  <Style x:Key="NavigationControl.DefaultStyle" TargetType="dxschv:NavigationControl">
    <Setter Property="Template" Value="{StaticResource NavigationControl.DefaultTemplate}" />
  </Style>
</ResourceDictionary>