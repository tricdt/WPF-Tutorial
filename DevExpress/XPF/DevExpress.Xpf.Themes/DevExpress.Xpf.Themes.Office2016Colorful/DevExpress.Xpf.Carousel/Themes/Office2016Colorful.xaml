        <RepeatButton x:Name="FirstItem" Command="dxc:CarouselPanel.FirstItem" CommandTarget="{TemplateBinding Carousel}" Grid.Column="0" Interval="{Binding Carousel.AnimationTime, RelativeSource={RelativeSource TemplatedParent}}" Template="{DynamicResource {dxt:CarouselNavigatorThemeKey ResourceKey=FirstButtonTemplate}}">
          <Grid x:Name="Grid1" Height="10" HorizontalAlignment="Center" VerticalAlignment="Center" Width="10">
