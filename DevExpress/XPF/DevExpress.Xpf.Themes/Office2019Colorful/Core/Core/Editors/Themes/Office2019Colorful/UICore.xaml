  <SolidColorBrush x:Key="{dxet:FlyoutControlThemeKey ResourceKey=Foreground}" Color="#FF444444" x:Uid="@Color=Foreground" />
  <!-- Color Edit -->
  <SolidColorBrush x:Key="{dxet:ColorEditThemeKey ResourceKey=Background}" Color="#FFF0F0F0" x:Uid="@Color=Control.Background" />
