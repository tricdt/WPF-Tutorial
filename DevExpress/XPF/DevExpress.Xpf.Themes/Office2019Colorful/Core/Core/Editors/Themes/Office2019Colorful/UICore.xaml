  <!-- ImageEdit -->
  <SolidColorBrush x:Key="{dxet:ImageEditThemeKey ResourceKey=ImageBackgroundBrush}" Color="#FFFFFFFF" x:Uid="@Color=Editor.Background" />
  <!-- BrushEdit -->
  <SolidColorBrush x:Key="{dxet:PopupBrushEditThemeKey ResourceKey=ChipBorderBrush}" Color="#FFC12600" x:Uid="@Color=Custom.Red" />
