  <SolidColorBrush x:Key="{dxet:TextEditThemeKey ResourceKey=NullTextForeground}" Color="#FF444444" Opacity="0.62" x:Uid="@Color=Foreground" />
  <!-- Range -->
  <SolidColorBrush x:Key="{dxet:CalendarClientThemeKey ResourceKey=CalendarClientForegroundBrush}" Color="#FFFFFFFF" x:Uid="@Color=Editor.Background" />
