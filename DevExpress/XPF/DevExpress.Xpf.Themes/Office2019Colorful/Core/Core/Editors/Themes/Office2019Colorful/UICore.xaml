  <Color x:Key="{dxet:SpinEditThemeKey ResourceKey=ArrowBrushColor}" x:Uid="@Content_Property=Foreground">#FF444444</Color>
  <SolidColorBrush x:Key="{dxet:ButtonsThemeKey ResourceKey=Foreground}" Color="#FF444444" x:Uid="@Color=Foreground" />
  <!-- TrackBarEdit -->
  <SolidColorBrush x:Key="{dxet:TrackBarEditThemeKey ResourceKey=Background}" x:Uid="@Content_Property=Window.Background">#FFF8F8F8</SolidColorBrush>
  <SolidColorBrush x:Key="{dxet:TrackBarEditThemeKey ResourceKey=Foreground}" x:Uid="@Content_Property=Border">#FFABABAB</SolidColorBrush>
  <!-- DateEdit -->
  <SolidColorBrush x:Key="{dxet:DateEditThemeKey ResourceKey=PopupContentBackground}" Color="Transparent" />
  <!-- DateNavigator -->
  <SolidColorBrush x:Key="{dxet:DateNavigatorThemeKey ResourceKey=BackgroundBrush}" Color="Transparent" />
