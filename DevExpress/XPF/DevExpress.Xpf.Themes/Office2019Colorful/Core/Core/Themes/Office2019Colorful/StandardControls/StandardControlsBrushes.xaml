<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- MENU BRUSHES -->
  <SolidColorBrush x:Key="Menu_Background" Color="#FFF8F8F8" x:Uid="@Color=Window.Background" />
  <SolidColorBrush x:Key="Menu_Border" Color="#FFABABAB" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="Menu_Foreground" Color="#FF444444" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="MenuItem_Foreground" Color="#FF444444" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="MenuItem_HoverForeground" Color="#FF444444" x:Uid="@Color=HoverForeground" />
  <SolidColorBrush x:Key="MenuItem_PressedForeground" Color="#FF444444" x:Uid="@Color=SelectionForeground" />
  <SolidColorBrush x:Key="MenuItem_HoverBackground" Color="#FFCBCACA" x:Uid="@Color=HoverAltBackground" />
  <SolidColorBrush x:Key="MenuItem_HoverBorder" Color="#FFCBCACA" x:Uid="@Color=HoverAltBorder" />
  <SolidColorBrush x:Key="MenuItem_SelectionBackground" Color="#FFBABAB9" x:Uid="@Color=SelectionAltBackground" />
  <SolidColorBrush x:Key="MenuItem_SelectionBorder" Color="#FFBABAB9" x:Uid="@Color=SelectionAltBorder" />
  <!-- CONTEXT MENU BRUSHES -->
  <SolidColorBrush x:Key="ContextMenu_Background" Color="#FFFFFFFF" x:Uid="@Color=Editor.Background" />
  <SolidColorBrush x:Key="ContextMenu_Border" Color="#FFABABAB" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="ContextMenuItem_Foreground" Color="#FF444444" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverForeground" Color="#FF444444" x:Uid="@Color=HoverForeground" />
  <SolidColorBrush x:Key="ContextMenuItem_PressedForeground" Color="#FF444444" x:Uid="@Color=SelectionForeground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverBackground" Color="#FFCBCACA" x:Uid="@Color=HoverAltBackground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverBorder" Color="#FFCBCACA" x:Uid="@Color=HoverAltBorder" />
  <SolidColorBrush x:Key="ContextMenuItem_SelectionBackground" Color="#FFBABAB9" x:Uid="@Color=SelectionAltBackground" />
  <SolidColorBrush x:Key="ContextMenuItem_SelectionBorder" Color="#FFBABAB9" x:Uid="@Color=SelectionAltBorder" />
</ResourceDictionary>