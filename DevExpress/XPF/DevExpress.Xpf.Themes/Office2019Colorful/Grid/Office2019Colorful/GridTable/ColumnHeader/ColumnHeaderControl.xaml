      </Grid>
    </Grid>
    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding IsMouseOver, RelativeSource={RelativeSource AncestorType={x:Type dxg:GridColumnHeader}}}" Value="True">
        <Setter Property="Fill" TargetName="Glyph" Value="#FF444444" cs:id="Setter_0001" x:Uid="@Value=Foreground" />
      </DataTrigger>
      <DataTrigger Binding="{Binding IsPressed, RelativeSource={RelativeSource AncestorType={x:Type dxg:GridColumnHeader}}}" Value="True">
        <Setter Property="Fill" TargetName="Glyph" Value="#FF444444" cs:id="Setter_0002" x:Uid="@Value=Foreground" />
      </DataTrigger>
      <Trigger Property="IsMouseOver" Value="True">
        <Setter Property="Opacity" TargetName="Glyph" Value="1" />
      </Trigger>
      <Trigger Property="IsPressed" Value="True">
        <Setter Property="Opacity" TargetName="Glyph" Value="1" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=HeaderFilterEditClearButtonTemplate}" TargetType="{x:Type Button}">
    <Grid Background="Transparent" cs:id="Grid_0000">
      <Grid Height="16" HorizontalAlignment="Center" VerticalAlignment="Center" Width="16">
        <Path x:Name="Glyph" Data="F1 M 6.5,0.00012207C 10.0899,0.00012207 13,2.91028 13,6.50012C 13,10.09 10.0898,13.0001 6.5,13.0001C 2.91014,13.0001 2.58287e-006,10.09 2.58287e-006,6.50012C 2.58287e-006,2.91028 2.91004,0.00012207 6.5,0.00012207 Z M 3.00002,3.99988L 5.50002,6.49988L 3.00003,8.99988L 3.00003,9.99988L 4.00003,9.99988L 6.49998,7.49988L 9.00001,9.99988L 10,9.99988L 10,8.99988L 7.49994,6.49988L 10,3.99976L 10,2.99988L 9.00003,2.99988L 6.49998,5.49988L 4.00001,2.99988L 3.00001,2.99988L 3.00002,3.99988 Z " Fill="#FF444444" Height="12" HorizontalAlignment="Center" Opacity="0.62" Stretch="Fill" VerticalAlignment="Center" Width="12" cs:id="Path_0001" x:Uid="@Fill=Foreground" />
      </Grid>
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="IsMouseOver" Value="True">
        <Setter Property="Opacity" TargetName="Glyph" Value="1" />
      </Trigger>
      <Trigger Property="IsPressed" Value="True">
        <Setter Property="Opacity" TargetName="Glyph" Value="1" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
</ResourceDictionary>