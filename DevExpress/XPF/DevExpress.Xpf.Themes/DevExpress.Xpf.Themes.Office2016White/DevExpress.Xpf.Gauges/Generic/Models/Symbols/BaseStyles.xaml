<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="http://schemas.devexpress.com/winfx/2008/xaml/gauges" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
  <Style x:Key="SymbolStyleBase" TargetType="local:SymbolPresentationControl">
    <Setter Property="Margin" Value="{Binding Margin}" />
    <Setter Property="RenderTransform" Value="{Binding RenderTransform}" />
    <Setter Property="RenderTransformOrigin" Value="0.5, 0.5" />
  </Style>
</ResourceDictionary>