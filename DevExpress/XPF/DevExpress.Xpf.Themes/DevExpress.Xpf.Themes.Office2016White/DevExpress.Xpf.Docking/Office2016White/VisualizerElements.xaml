  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Docking.VisualElements.DocumentSelectorContainer}" TargetType="{x:Type ve:DocumentSelectorContainer}">
    <Style.Setters>
      <Setter Property="Foreground" Value="#FF3C3C3C" />
      <Setter Property="Template">
        <Setter.Value>
          <ControlTemplate TargetType="{x:Type ve:DocumentSelectorContainer}">
            <Grid>
              <Border x:Name="Window" Background="#FFD6D6D6">
                <Border Background="#FFFFFFFF" Margin="1">
                  <Grid>
                    <Grid.RowDefinitions>
                      <RowDefinition Height="Auto" />
                      <RowDefinition />
                      <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Border x:Name="Header" Background="#FFF0F0F0" Margin="1,1,1,0">
                      <StackPanel HorizontalAlignment="Left" Margin="12,8" Orientation="Horizontal" VerticalAlignment="Center">
                        <Image HorizontalAlignment="Left" Margin="0,0,6,0" Source="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionImage}" Stretch="None" VerticalAlignment="Top" />
                        <StackPanel>
                          <ContentControl Content="{Binding Path=(local:DockLayoutManager.LayoutItem).Caption}" ContentTemplate="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionTemplate}" ContentTemplateSelector="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionTemplateSelector}" Focusable="False" FontSize="18" FontWeight="Light" Foreground="#FF0072C6" HorizontalAlignment="Left" VerticalAlignment="Bottom" />
                          <TextBlock HorizontalAlignment="Left" Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Description}" TextWrapping="Wrap" VerticalAlignment="Top" />
                        </StackPanel>
                      </StackPanel>
                    </Border>
                    <Border x:Name="Content" Background="#FFFFFFFF" Grid.Row="1">
                      <ContentPresenter x:Name="PART_ContainerContent" Content="{Binding}" Margin="12" />
                    </Border>
                    <Border x:Name="Footer" Background="#FFF0F0F0" Grid.Row="2" Margin="1,0,1,1" MinHeight="24">
                      <ContentPresenter Content="{Binding Path=(local:DockLayoutManager.LayoutItem).FooterDescription}" HorizontalAlignment="Left" Margin="12,0,0,0" VerticalAlignment="Center" />
                    </Border>
                  </Grid>
                </Border>
              </Border>
            </Grid>
          </ControlTemplate>
        </Setter.Value>
      </Setter>
    </Style.Setters>
  </Style>
  <ControlTemplate x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=ListBoxItem}" TargetType="{x:Type ListBoxItem}">
    <Grid>
      <Grid x:Name="HoverState" Opacity="0">
        <Border Background="#FFCDE6F7" Margin="0,0,0,1" />
      </Grid>
      <Grid x:Name="SelectedState" Opacity="0">
        <Border Background="#FF92C0E0" Margin="0,0,0,1" />
      </Grid>
      <Border Padding="{TemplateBinding Padding}">
        <ve:TemplatedCaptionControl x:Name="caption" LayoutItem="{Binding}" Margin="3,2" />
      </Border>
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="IsSelected" Value="True">
        <Setter Property="Opacity" TargetName="SelectedState" Value="1" />
      </Trigger>
      <Trigger Property="IsMouseOver" Value="True">
        <Setter Property="Opacity" TargetName="HoverState" Value="1" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <DataTemplate x:Key="{dxt:DockPaneElementsThemeKey ResourceKey=ContentPreview}">
    <Border Background="#FFFFFFFF" BorderBrush="#FF3C3C3C" BorderThickness="1" Padding="12">
      <ContentPresenter Content="{TemplateBinding Content}" />
    </Border>
  </DataTemplate>
  <DataTemplate x:Key="{dxt:DocumentElementsThemeKey ResourceKey=ContentPreview}">
    <Border Background="#FFFFFFFF" BorderBrush="#FF3C3C3C" BorderThickness="1" Padding="1">
