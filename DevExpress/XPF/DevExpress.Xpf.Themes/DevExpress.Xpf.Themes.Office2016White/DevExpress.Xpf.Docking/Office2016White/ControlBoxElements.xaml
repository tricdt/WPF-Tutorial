  <Style x:Key="DockPane_PinButton" TargetType="{x:Type ve:ControlBoxGlyphPresenter}">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type ve:ControlBoxGlyphPresenter}">
          <Border x:Name="Border1">
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="CommonStates">
                <VisualState x:Name="Normal" />
                <VisualState x:Name="MouseOver">
                  <Storyboard>
                    <ColorAnimation Duration="0" Storyboard.TargetName="PinnedGlyph" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF0072C6" />
                    <ColorAnimation Duration="0" Storyboard.TargetName="UnpinnedGlyph" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF0072C6" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Pressed">
                  <Storyboard>
                    <ColorAnimation Duration="0" Storyboard.TargetName="PinnedGlyph" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF0072C6" />
                    <ColorAnimation Duration="0" Storyboard.TargetName="UnpinnedGlyph" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF0072C6" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Disabled" />
              </VisualStateGroup>
              <VisualStateGroup x:Name="PinnedStates">
                <VisualState x:Name="Unpinned" />
                <VisualState x:Name="Pinned">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PinnedGlyph" Storyboard.TargetProperty="(UIElement.Visibility)">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Visible</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="UnpinnedGlyph" Storyboard.TargetProperty="(UIElement.Visibility)">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Collapsed</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
              <VisualStateGroup x:Name="ActiveStates">
                <VisualState x:Name="EmptyActiveState" />
                <VisualState x:Name="Inactive">
                  <Storyboard>
                    <ColorAnimation Duration="0" Storyboard.TargetName="PinnedGlyph" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF666666" />
                    <ColorAnimation Duration="0" Storyboard.TargetName="UnpinnedGlyph" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF666666" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Active">
                  <Storyboard>
                    <ColorAnimation Duration="0" Storyboard.TargetName="PinnedGlyph" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF3C3C3C" />
                    <ColorAnimation Duration="0" Storyboard.TargetName="UnpinnedGlyph" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF3C3C3C" />
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
              <Path x:Name="UnpinnedGlyph" Data="F1 M 0,3L 3,3L 3,0L 4,0L 4,1L 9,1L 9,6L 4,6L 4,7L 3,7L 3,4L 0,4L 0,3 Z M 4,2L 4,3L 8,3L 8,2L 4,2 Z " Fill="#FF666666" Height="7" HorizontalAlignment="Center" Stretch="Fill" VerticalAlignment="Center" Width="9" />
              <Path x:Name="PinnedGlyph" Data="F1 M 3,9L 3,6L 0,6L 0,5L 1,5L 1,0L 6,0L 6,5L 7,5L 7,6L 4,6L 4,9L 3,9 Z M 2,5L 3,5L 3,1L 2,1L 2,5 Z " Fill="#FF666666" Height="9" HorizontalAlignment="Center" Margin="0,1,0,0" Stretch="Fill" VerticalAlignment="Center" Visibility="Collapsed" Width="7" />
            </Grid>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="ve:DockPane.HitTestType" Value="PinButton" />
  </Style>
