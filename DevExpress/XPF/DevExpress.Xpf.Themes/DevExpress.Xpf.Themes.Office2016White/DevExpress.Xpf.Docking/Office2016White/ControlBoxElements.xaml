                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
              <VisualStateGroup x:Name="DockStates">
                <VisualState x:Name="Left" />
                <VisualState x:Name="Right">
                  <Storyboard>
                    <DoubleAnimation Duration="0" Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(UIElement.RenderTransform).Children[1].ScaleX" To="-1" />
