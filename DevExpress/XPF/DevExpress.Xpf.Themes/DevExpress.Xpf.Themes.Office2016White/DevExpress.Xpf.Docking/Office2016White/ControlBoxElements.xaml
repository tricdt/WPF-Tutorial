          <Grid x:Name="Button" Background="#00000000" ve:DockPane.HitTestType="ExpandButton">
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="CommonStates">
                <VisualState x:Name="Normal" />
                <VisualState x:Name="MouseOver">
                  <Storyboard>
                    <ColorAnimation Duration="0" Storyboard.TargetName="GlyphExpanded" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF2A8DD4" />
                    <ColorAnimation Duration="0" Storyboard.TargetName="GlyphCollapsed" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF2A8DD4" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Pressed">
                  <Storyboard>
                    <ColorAnimation Duration="0" Storyboard.TargetName="GlyphExpanded" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF0072C6" />
                    <ColorAnimation Duration="0" Storyboard.TargetName="GlyphCollapsed" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" To="#FF0072C6" />
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
              <VisualStateGroup x:Name="ExpandedStates">
                <VisualState x:Name="Expanded" />
                <VisualState x:Name="Collapsed">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="GlyphExpanded" Storyboard.TargetProperty="(UIElement.Visibility)" />
                    <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible" Storyboard.TargetName="GlyphCollapsed" Storyboard.TargetProperty="(UIElement.Visibility)" />
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Path x:Name="GlyphCollapsed" Data="F1 M 0,1.52588e-005L 1.22465e-016,2.99998L 4,7L 8,3L 8,1.52588e-005L 4,4.00002L 0,1.52588e-005 Z " Fill="#FF666666" Height="6" Stretch="Fill" Visibility="Collapsed" Width="7" />
