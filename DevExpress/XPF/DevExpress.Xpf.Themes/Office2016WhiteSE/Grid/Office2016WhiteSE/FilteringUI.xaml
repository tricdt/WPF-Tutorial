  <Thickness x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=FilterValueContainerTopAreaMarginTouch}">11,10</Thickness>
  <SolidColorBrush x:Key="{dxfuit:ExcelStyleFilterElementThemeKey ResourceKey=HeaderPanelBackground}" Color="#FFFFFFFF" x:Uid="@Color=Control.Background" />
  <!--#endregion-->
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlBackground}" Color="#FFFFFFFF" x:Uid="@Color=Control.Background" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlButtonsBackgroundPressed}" Color="#FF92C0E0" x:Uid="@Color=SelectionBackground" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlButtonsBorderBrushPressed}" Color="#FF92C0E0" x:Uid="@Color=SelectionBorder" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlButtonsBackgroundHover}" Color="#FFCDE6F7" x:Uid="@Color=HoverBackground" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlButtonsBorderBrushHover}" Color="#FFCDE6F7" x:Uid="@Color=HoverBorder" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ControlBorderBrush}" Color="#FFE1E1E1" x:Uid="@Color=Delimiter" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=PropertyBackground}" Color="#FFF9D3B0" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ValueBackground}" Color="#FFBBE2C5" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=HighlightedBackground}" Color="#FFBCD7E7" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ElementForeground}" Color="#FF000000" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=Foreground}" Color="#FF444444" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=BracketBorderBrush}" Color="#FFE1E1E1" x:Uid="@Color=Delimiter" />
  <dx:WpfSvgPalette x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=SvgPalette}">
    <SolidColorBrush x:Key="BlackColor" Color="#FF444444" cs:id="SolidColorBrush_0001" x:Uid="@Color=Foreground" />
  </dx:WpfSvgPalette>
  <dx:WpfSvgPalette x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=ButtonSvgPalette}">
    <SolidColorBrush x:Key="BlackColor" Color="#FF444444" cs:id="SolidColorBrush_0001" x:Uid="@Color=Foreground" />
    <dx:WpfSvgPalette.States>
      <dx:WpfSvgPalette x:Key="Hover">
        <SolidColorBrush x:Key="BlackColor" Color="#FF444444" cs:id="SolidColorBrush_0002" x:Uid="@Color=Foreground" />
      </dx:WpfSvgPalette>
      <dx:WpfSvgPalette x:Key="Pressed">
        <SolidColorBrush x:Key="BlackColor" Color="#FF444444" cs:id="SolidColorBrush_0003" x:Uid="@Color=Foreground" />
      </dx:WpfSvgPalette>
    </dx:WpfSvgPalette.States>
  </dx:WpfSvgPalette>
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=PopupElementHoverBackground}" Color="#FFD4D4D4" Opacity="0.13" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="{dxfuit:FilteringPanelThemeKey ResourceKey=PopupElementPressedBackground}" Color="#FFD4D4D4" Opacity="0.22" x:Uid="@Color=Border" />
</ResourceDictionary>