<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey ErrorForeground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey ErrorBackground}" Color="#FFE94849"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey ErrorBorderBrush}" Color="#FFE94849"/>

    <Thickness x:Key="{dxt:BadgesThemeKey ErrorBorderThickness}">0</Thickness>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey WarningForeground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey WarningBackground}" Color="#FFEEBF4A"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey WarningBorderBrush}" Color="#FFEEBF4A"/>

    <Thickness x:Key="{dxt:BadgesThemeKey WarningBorderThickness}">0</Thickness>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey SuccessForeground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey SuccessBackground}" Color="#FF4AB66B"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey SuccessBorderBrush}" Color="#FF4AB66B"/>

    <Thickness x:Key="{dxt:BadgesThemeKey SuccessBorderThickness}">0</Thickness>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey InformationForeground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey InformationBackground}" Color="#FF4F78C3"/>

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey InformationBorderBrush}" Color="#FF4F78C3"/>

    <Thickness x:Key="{dxt:BadgesThemeKey InformationBorderThickness}">0</Thickness>

    <Thickness x:Key="{dxt:BadgesThemeKey Padding}">3,3,3,4</Thickness>

    <Thickness x:Key="{dxt:BadgesThemeKey TouchPadding}">5,5,5,6</Thickness>

</ResourceDictionary>