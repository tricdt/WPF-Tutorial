    <SolidColorBrush x:Key="{dxet:DateNavigatorThemeKey ResourceKey=BackgroundBrush}" Color="#FFFFFFFF"/>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=ClearButtonTemplate}" TargetType="{x:Type Button}">
        <Grid x:Name="Root" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
            <Grid x:Name="DefaultState">
                <Border Background="{StaticResource ButtonTemplate.DefaultState.Border2}"
                        BorderBrush="{StaticResource ButtonTemplate.DefaultState.Border1}"
                        BorderThickness="1"
                        CornerRadius="2">
                    <Border Background="{StaticResource ButtonTemplate.DefaultState.Border3}"
                            CornerRadius="1"
                            Margin="1"/>
                </Border>
            </Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Border Background="{StaticResource ButtonTemplate.HoverState.Border2}"
                        BorderBrush="{StaticResource ButtonTemplate.HoverState.Border1}"
                        BorderThickness="1"
                        CornerRadius="2">
                    <Border Background="{StaticResource ButtonTemplate.HoverState.Border3}"
                            CornerRadius="1"
                            Margin="1"/>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border Background="{StaticResource ButtonTemplate.PressedState.Border2}"
                        BorderBrush="{StaticResource ButtonTemplate.PressedState.Border1}"
                        BorderThickness="1"
                        CornerRadius="2">
                    <Border Background="{StaticResource ButtonTemplate.PressedState.Border3}"
                            CornerRadius="1"
                            Margin="1"/>
                </Border>
            </Grid>
            <Grid x:Name="FocusState" Opacity="0">
                <Rectangle Style="{StaticResource ButtonTemplateFocusTemplate}"/>
            </Grid>
            <Grid Margin="2">
                <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                  Margin="{TemplateBinding Padding}"
                                  RecognizesAccessKey="True"
                                  SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                  VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity"
                        TargetName="DefaultState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="0"/>
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="Opacity"
                        TargetName="FocusState"
                        Value="1"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Opacity"
                        TargetName="DefaultState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="FocusState"
                        Value="0"/>
            </Trigger>
            <Trigger Property="Button.IsDefaulted" Value="True">
                <Setter Property="Opacity"
                        TargetName="FocusState"
                        Value="1"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity"
                        TargetName="Root"
                        Value="0.5"/>
            </Trigger>
            <Trigger Property="ToggleButton.IsChecked" Value="True">
                <Setter Property="Opacity"
                        TargetName="DefaultState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="FocusState"
                        Value="0"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

