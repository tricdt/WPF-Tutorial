<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduler"
                    xmlns:dxschd="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/internal"
                    xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/themekeys"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <sys:Int32 x:Key="{dxscht:SchedulerControlThemeKey ResourceKey=DefaultAppointmentHeight}">22</sys:Int32>

    <SolidColorBrush x:Key="DefaultBorderBrush" Color="#FF9DA0AA"/>

    <SolidColorBrush x:Key="DefaultBackgroundBrush" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderForegroundColor}" Color="#FF201F35"/>

    <SolidColorBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=AppointmentForegroundColor}" Color="#FF201F35"/>

    <SolidColorBrush x:Key="{dxscht:DayViewThemeKey ResourceKey=AllDayAreaBottomBorderBackground}" Color="#FFFFFFFF"/>

    <dxsch:SchedulerColorSchemaCollection x:Key="{dxscht:SchedulerControlThemeKey ResourceKey=ResourceColorSchemaTemplate}">
        <dxsch:SchedulerColorSchema Cell="#FFFAFAFA"
                                    CellBorder="#FFD0D0D0"
                                    CellBorderDark="#FFDADADA"
                                    CellLight="#FFFFFFFF"
                                    CellLightBorder="#FFDADADA"
                                    CellLightBorderDark="#FFD0D0D0"/>
        <dxsch:SchedulerColorSchema Cell="#FFE2EED4"
                                    CellBorder="#FFC9D5BB"
                                    CellBorderDark="#FFD2DEC4"
                                    CellLight="#FFF3FFE5"
                                    CellLightBorder="#FFD2DEC4"
                                    CellLightBorderDark="#FFC9D5BB"/>
        <dxsch:SchedulerColorSchema Cell="#FFD4EAEE"
                                    CellBorder="#FFBBD1D5"
                                    CellBorderDark="#FFC4D9DE"
                                    CellLight="#FFE5FBFF"
                                    CellLightBorder="#FFC4D9DE"
                                    CellLightBorderDark="#FFBBD1D5"/>
        <dxsch:SchedulerColorSchema Cell="#FFEEE0D4"
                                    CellBorder="#FFD5C7BB"
                                    CellBorderDark="#FFDED0C4"
                                    CellLight="#FFFFF1E5"
                                    CellLightBorder="#FFDED0C4"
                                    CellLightBorderDark="#FFD5C7BB"/>
        <dxsch:SchedulerColorSchema Cell="#FFDFD4EE"
                                    CellBorder="#FFC6BBD5"
                                    CellBorderDark="#FFCFC4DE"
                                    CellLight="#FFF0E5FF"
                                    CellLightBorder="#FFCFC4DE"
                                    CellLightBorderDark="#FFC6BBD5"/>
        <dxsch:SchedulerColorSchema Cell="#FFD4EEDA"
                                    CellBorder="#FFBBD5C1"
                                    CellBorderDark="#FFC4DECA"
                                    CellLight="#FFE5FFEB"
                                    CellLightBorder="#FFC4DECA"
                                    CellLightBorderDark="#FFBBD5C1"/>
        <dxsch:SchedulerColorSchema Cell="#FFEED4E1"
                                    CellBorder="#FFD5BBC8"
                                    CellBorderDark="#FFDEC4D1"
                                    CellLight="#FFFFE5F2"
                                    CellLightBorder="#FFDEC4D1"
                                    CellLightBorderDark="#FFD5BBC8"/>
        <dxsch:SchedulerColorSchema Cell="#FFEEE7D4"
                                    CellBorder="#FFD5CEBB"
                                    CellBorderDark="#FFDED7C4"
                                    CellLight="#FFFFF8E5"
                                    CellLightBorder="#FFDED7C4"
                                    CellLightBorderDark="#FFD5CEBB"/>
        <dxsch:SchedulerColorSchema Cell="#FFEED7D4"
                                    CellBorder="#FFD5BEBB"
                                    CellBorderDark="#FFDEC7C4"
                                    CellLight="#FFFFE8E5"
                                    CellLightBorder="#FFDEC7C4"
                                    CellLightBorderDark="#FFD5BEBB"/>
        <dxsch:SchedulerColorSchema Cell="#FFD4ECEE"
                                    CellBorder="#FFBBD2D5"
                                    CellBorderDark="#FFC4DBDE"
                                    CellLight="#FFE5FDFF"
                                    CellLightBorder="#FFC4DBDE"
                                    CellLightBorderDark="#FFBBD2D5"/>
        <dxsch:SchedulerColorSchema Cell="#FFDEEED4"
                                    CellBorder="#FFC5D5BB"
                                    CellBorderDark="#FFCEDEC4"
                                    CellLight="#FFEFFFE5"
                                    CellLightBorder="#FFCEDEC4"
                                    CellLightBorderDark="#FFC5D5BB"/>
        <dxsch:SchedulerColorSchema Cell="#FFECEED4"
                                    CellBorder="#FFD0D0D0"
                                    CellBorderDark="#FFDADADA"
                                    CellLight="#FFFDFFE5"
                                    CellLightBorder="#FFDADADA"
                                    CellLightBorderDark="#FFD0D0D0"/>
    </dxsch:SchedulerColorSchemaCollection>

    <SolidColorBrush x:Key="ResourceNavigatorGlyphBrush" Color="#FF201F35"/>

    <dxschd:ColorCollection x:Key="HeaderBaseColors">
        <dxschd:ColorDefinition Name="ColorDefinition_HeaderBaseBorderColor" Value="#FFCACBD3"/>
        <dxschd:ColorDefinition Name="ColorDefinition_NormalGradientStop1" Value="#FFF2F3F4"/>
        <dxschd:ColorDefinition Name="ColorDefinition_NormalGradientStop2" Value="#FFEDEEEF"/>
        <dxschd:ColorDefinition Name="ColorDefinition_NormalGradientStop3" Value="#FFEFF0F1"/>
        <dxschd:ColorDefinition Name="ColorDefinition_AltNormalGradientStop1" Value="#FFEFE8F9"/>
        <dxschd:ColorDefinition Name="ColorDefinition_AltNormalGradientStop2" Value="#FFECE3F8"/>
        <dxschd:ColorDefinition Name="ColorDefinition_AltNormalGradientStop3" Value="#FFEDE5F9"/>
    </dxschd:ColorCollection>

    <SolidColorBrush x:Key="TimeRulerTextForeground" Color="#FF80838F"/>

    <!-- Resource Navigator -->

    <SolidColorBrush x:Key="HorizontalResourceNavigatorBackground" Color="#FFF5F5F5"/>

    <SolidColorBrush x:Key="VerticalResourceNavigatorBackground" Color="#FFF5F5F5"/>

    <LinearGradientBrush x:Key="HorizontalButtonStyle.HoverState.Border1"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#3F888C99" Offset="0"/>
        <GradientStop Color="#3F888C99" Offset="1"/>
        <GradientStop Color="#FF888C99" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="HorizontalButtonStyle.HoverState.Border2"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#3FCBD6F3" Offset="0"/>
        <GradientStop Color="#3FCBD6F3" Offset="1"/>
        <GradientStop Color="#FFCBD6F3" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="HorizontalButtonStyle.PressedState.Border1"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#3F888C99" Offset="0"/>
        <GradientStop Color="#3F888C99" Offset="1"/>
        <GradientStop Color="#FF888C99" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="HorizontalButtonStyle.PressedState.Border2"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#3FCED2D9" Offset="0"/>
        <GradientStop Color="#3FCED2D9" Offset="1"/>
        <GradientStop Color="#FFCED2D9" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VerticalButtonStyle.HoverState.Border1"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#3F888C99" Offset="0"/>
        <GradientStop Color="#3F888C99" Offset="1"/>
        <GradientStop Color="#FF888C99" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VerticalButtonStyle.HoverState.Border2"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#3FCBD6F3" Offset="0"/>
        <GradientStop Color="#3FCBD6F3" Offset="1"/>
        <GradientStop Color="#FFCBD6F3" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VerticalButtonStyle.PressedState.Border1"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#3F888C99" Offset="0"/>
        <GradientStop Color="#3F888C99" Offset="1"/>
        <GradientStop Color="#FF888C99" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VerticalButtonStyle.PressedState.Border2"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#3FCED2D9" Offset="0"/>
        <GradientStop Color="#3FCED2D9" Offset="1"/>
        <GradientStop Color="#FFCED2D9" Offset="0.5"/>
    </LinearGradientBrush>

</ResourceDictionary>