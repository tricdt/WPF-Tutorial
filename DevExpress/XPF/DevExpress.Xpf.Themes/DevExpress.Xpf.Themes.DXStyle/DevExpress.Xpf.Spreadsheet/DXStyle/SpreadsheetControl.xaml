                                   Stretch="None"
                                   VerticalAlignment="Center"
                                   Visibility="{TemplateBinding IsEnabled, Converter={StaticResource BoolToVisibilityInverseConverter}}"/>
                            <Image dxb:ImageColorizer.Color="#FF201F35"
