<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduling"
                    xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduling/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="{dx:SchedulingResource Generic/DefaultUICore.xaml}"/>
    </ResourceDictionary.MergedDictionaries>

    <Color x:Key="BrushSet.Resource1.Color">#FFF3FFE5</Color>

    <Color x:Key="BrushSet.Resource2.Color">#FFE5FBFF</Color>

    <Color x:Key="BrushSet.Resource3.Color">#FFFFF1E5</Color>

    <Color x:Key="BrushSet.Resource4.Color">#FFF0E5FF</Color>

    <Color x:Key="BrushSet.Resource5.Color">#FFE5FFEB</Color>

    <Color x:Key="BrushSet.Resource6.Color">#FFFFE5F2</Color>

    <Color x:Key="BrushSet.Resource7.Color">#FFFFF8E5</Color>

    <Color x:Key="BrushSet.Resource8.Color">#FFFFE8E5</Color>

    <Color x:Key="BrushSet.Resource9.Color">#FFE5FDFF</Color>

    <SolidColorBrush x:Key="BrushSet.LabelNone" Color="Transparent"/>

    <SolidColorBrush x:Key="BrushSet.LabelImportant" Color="#FFFFAFB3"/>

    <SolidColorBrush x:Key="BrushSet.LabelBusiness" Color="#FFAFCAFF"/>

    <SolidColorBrush x:Key="BrushSet.LabelPersonal" Color="#FFC4F7C1"/>

    <SolidColorBrush x:Key="BrushSet.LabelVacation" Color="#FFFFD5AF"/>

    <SolidColorBrush x:Key="BrushSet.LabelMustAttend" Color="#FFFFC5AF"/>

    <SolidColorBrush x:Key="BrushSet.LabelTravelRequired" Color="#FF8FE2FA"/>

    <SolidColorBrush x:Key="BrushSet.LabelNeedsPreparation" Color="#FFD2E186"/>

    <SolidColorBrush x:Key="BrushSet.LabelBirthday" Color="#FFEBB5D9"/>

    <SolidColorBrush x:Key="BrushSet.LabelAnniversary" Color="#FF8CE4C6"/>

    <SolidColorBrush x:Key="BrushSet.LabelPhoneCall" Color="#FFF7DD93"/>

    <SolidColorBrush x:Key="CellInplaceEditorControl.Background" Color="#FF6589BD"/>

    <SolidColorBrush x:Key="ResourceForeground" Color="Black"/>

    <SolidColorBrush x:Key="DefaultHeaderForeground" Color="Black"/>

    <SolidColorBrush x:Key="DefaultLabelColor" Color="White"/>

    <SolidColorBrush x:Key="LightCellBackground" Color="#ffffff"/>

    <SolidColorBrush x:Key="LightCellLightBorder" Color="#ececec"/>

    <SolidColorBrush x:Key="LightCellDarkBorder" Color="#cbcbcb"/>

    <SolidColorBrush x:Key="DarkCellBackground" Color="#f1f1f1"/>

    <SolidColorBrush x:Key="DarkCellLightBorder" Color="#e1e1e1"/>

    <SolidColorBrush x:Key="DarkCellDarkBorder" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="Transparent" Color="Transparent"/>

    <SolidColorBrush x:Key="AppointmentControl.ResizeGlyph.Background" Color="#ffffff"/>

    <SolidColorBrush x:Key="AppointmentControl.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="HeaderControl.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="HeaderControl.TodayForeground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="CellControl.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="TimeRulerCellControl.Foreground" Color="#80838f"/>

    <SolidColorBrush x:Key="TimeRulerHeaderControl.Foreground" Color="#80838f"/>

    <SolidColorBrush x:Key="MoreButton.Normal.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="MoreButton.Hover.Foreground" Color="#AF201F35"/>

    <SolidColorBrush x:Key="MoreButton.Pressed.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="NavigationButton.Normal.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="NavigationButton.Hover.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="NavigationButton.Pressed.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.Normal.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.Hover.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.Pressed.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.Normal.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.Hover.Foreground" Color="#AF201F35"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.Pressed.Foreground" Color="#FF201F35"/>

    <SolidColorBrush x:Key="AllDayAreaCorner.Background" Color="#edeeef"/>

    <SolidColorBrush x:Key="AllDayAreaCorner.BorderBrush" Color="#d3d3d3"/>

    <SolidColorBrush x:Key="CellControl.SelectionBackground"
                     Color="#FFBAD8FC"
                     Opacity="0.5"/>

    <LinearGradientBrush x:Key="MonthCellControl.TodayBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFEFE8F9" Offset="0"/>
        <GradientStop Color="#FFECE3F8" Offset="0.5"/>
        <GradientStop Color="#FFEDE5F9" Offset="1"/>
    </LinearGradientBrush>

    <!-- AppointmentControl -->

    <SolidColorBrush x:Key="AppointmentControl.StatusContainer.Background" Color="#FFFFFF"/>

    <SolidColorBrush x:Key="AppointmentControl.StatusContainer.BorderBrush" Color="#acafb5"/>

    <SolidColorBrush x:Key="AppointmentControl.BorderBrush" Color="#acafb5"/>

    <SolidColorBrush x:Key="AppointmentControl.SelectedBorderBrush" Color="Black"/>

    <!-- Separators -->

    <SolidColorBrush x:Key="GroupSeparator.BorderBrush" Color="#d0d0d0"/>

    <SolidColorBrush x:Key="GroupSeparator.Background" Color="#FFFFFF"/>

    <SolidColorBrush x:Key="AllDaySeparator.BorderBrush" Color="#d0d0d0"/>

    <SolidColorBrush x:Key="AllDaySeparator.Background" Color="#FFFFFF"/>

    <!-- HeaderControl -->

    <SolidColorBrush x:Key="HeaderControl.BorderBrush" Color="#d3d3d3"/>

    <LinearGradientBrush x:Key="HeaderControl.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFF2F3F4" Offset="0"/>
        <GradientStop Color="#FFEDEEEF" Offset="0.5"/>
        <GradientStop Color="#FFEFF0F1" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="HeaderControl.TodayBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFEFE8F9" Offset="0"/>
        <GradientStop Color="#FFECE3F8" Offset="0.5"/>
        <GradientStop Color="#FFEDE5F9" Offset="1"/>
    </LinearGradientBrush>

    <!-- Buttons & ResourceNavigator -->

    <SolidColorBrush x:Key="MoreButton.NormalBorderBrush" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="MoreButton.NormalBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="MoreButton.HoverBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="MoreButton.HoverBorderBrush" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="MoreButton.PressedBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="MoreButton.PressedBorderBrush" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="MoreButton.DisabledBorderBrush"
                     Color="#FFA9ACB5"
                     Opacity="0.35"/>

    <SolidColorBrush x:Key="MoreButton.DisabledBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="NavigationButton.NormalBorderBrush" Color="#FFA9ACB5"/>

    <LinearGradientBrush x:Key="NavigationButton.NormalBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFF2F3F5" Offset="0"/>
        <GradientStop Color="#FFECEEF2" Offset="1"/>
        <GradientStop Color="#FFDEE0E6" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="NavigationButton.HoverBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE3EBFE" Offset="0"/>
        <GradientStop Color="#FFDEE7FE" Offset="1"/>
        <GradientStop Color="#FFD3DEFC" Offset="0.5"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="NavigationButton.HoverBorderBrush" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="NavigationButton.PressedBackground" Color="#FFDCDDE1"/>

    <SolidColorBrush x:Key="NavigationButton.PressedBorderBrush" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="NavigationButton.DisabledBorderBrush"
                     Color="#FFA9ACB5"
                     Opacity="0.35"/>

    <LinearGradientBrush x:Key="NavigationButton.DisabledBackground"
                         EndPoint="0.5,1"
                         Opacity="0.35"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFF2F3F5" Offset="0"/>
        <GradientStop Color="#FFECEEF2" Offset="1"/>
        <GradientStop Color="#FFDEE0E6" Offset="0.5"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ResourceNavigatorButton.NormalBorderBrush" Color="#f5f5f5"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.NormalBackground" Color="#f5f5f5"/>

    <LinearGradientBrush x:Key="ResourceNavigatorButton.HoverBorderBrush"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#3F888C99" Offset="0"/>
        <GradientStop Color="#3F888C99" Offset="1"/>
        <GradientStop Color="#FF888C99" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ResourceNavigatorButton.HoverBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#3FCBD6F3" Offset="0"/>
        <GradientStop Color="#3FCBD6F3" Offset="1"/>
        <GradientStop Color="#FFCBD6F3" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ResourceNavigatorButton.PressedBorderBrush"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#3F888C99" Offset="0"/>
        <GradientStop Color="#3F888C99" Offset="1"/>
        <GradientStop Color="#FF888C99" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ResourceNavigatorButton.PressedBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#3FCED2D9" Offset="0"/>
        <GradientStop Color="#3FCED2D9" Offset="1"/>
        <GradientStop Color="#FFCED2D9" Offset="0.5"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ResourceNavigatorButton.DisabledBorderBrush" Color="#f5f5f5"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.DisabledBackground" Color="#f5f5f5"/>

    <SolidColorBrush x:Key="ResourceNavigator.BorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="ResourceNavigator.Background" Color="Transparent"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.NormalBorderBrush" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.NormalBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.HoverBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.HoverBorderBrush" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.PressedBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.PressedBorderBrush" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.DisabledBorderBrush"
                     Color="#FFA9ACB5"
                     Opacity="0.35"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.DisabledBackground" Color="Transparent"/>

    <!-- TimeRuler -->

    <SolidColorBrush x:Key="TimeIndicatorControl.BorderBrush" Color="#FFE1BDF4"/>

    <SolidColorBrush x:Key="TimeIndicatorControl.Background" Color="#FFE1BDF4"/>

    <SolidColorBrush x:Key="TimeMarkerControl.BorderBrush" Color="#FFE1BDF4"/>

    <SolidColorBrush x:Key="TimeMarkerControl.Background" Color="#FFE1BDF4"/>

    <LinearGradientBrush x:Key="TimeRulerCellControl.Delimiter.Long.Background"
                         EndPoint="1,0.5"
                         StartPoint="0,0.5">
        <GradientStop Color="#3FCACBD3" Offset="0"/>
        <GradientStop Color="#FFCACBD3" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="TimeRulerCellControl.Delimiter.Short.Background" Color="Transparent"/>

    <SolidColorBrush x:Key="TimeRulerHeaderSeparator.BorderBrush" Color="#cacbd3"/>

    <!-- Scheduler -->

    <SolidColorBrush x:Key="Scheduler.Background" Color="#ffffff"/>

    <SolidColorBrush x:Key="Scheduler.Foreground" Color="#000000"/>

    <SolidColorBrush x:Key="Scheduler.Background.Legacy" Color="#ffffff"/>

    <SolidColorBrush x:Key="Scheduler.BorderBrush" Color="#cacbd3"/>

    <SolidColorBrush x:Key="Scheduler.Corner.Background" Color="#f5f5f5"/>

</ResourceDictionary>


