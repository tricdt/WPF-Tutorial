<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxcot="http://schemas.devexpress.com/winfx/2008/xaml/controls/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Duration x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressAnimationDuration}">0:0:0.5</Duration>

    <sys:Boolean x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ShowOuterBorder}">False</sys:Boolean>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderBrush}" Color="Transparent"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterIndentBrush}" Color="Transparent"/>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderIndent}">0</sys:Double>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderSize}">0</sys:Double>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBarThickness}">6</sys:Double>

    <Thickness x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBorderThickness}">1</Thickness>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBackground}" Color="White"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressForeground}"
                         EndPoint="0,1"
                         StartPoint="0,0">
        <GradientStop Color="#FFEACAF9"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFE7CDF7"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FFE1BDF4"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
        <GradientStop Color="#FFDCAFF0"
                      Offset="0.5"
                      cs:id="GradientStop_0003"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressAccentBorderBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#7F626777"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#7FD8D9DC"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressNeutralBorderBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#7F626777"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#7FD8D9DC"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorBorderSize}">1</sys:Double>

    <Thickness x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorPanelMargin}">3</Thickness>

    <EllipseGeometry x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorGeometry}"
                     Center="20,20"
                     RadiusX="20"
                     RadiusY="20"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedItemForeground}" Color="#363636"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedIndicatorFill}" Color="White"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedIndicatorStroke}" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedMouseOverFill}" Color="#EFF1F4"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedMouseOverStroke}" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateItemForeground}" Color="#363636"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedIndicatorFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFF2F3F5"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFECEEF2"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FFDEE0E6"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateIndicatorStroke}" Color="#FFA9ACB5"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateMouseOverFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE3EBFE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFDEE7FE"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FFD3DEFC"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateMouseOverStroke}" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedItemForeground}" Color="#FF000000"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateIndicatorFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE3EBFE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFDEE7FE"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FFD3DEFC"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedIndicatorStroke}" Color="#FFA9ACB5"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedMouseOverFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE3EBFE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFDEE7FE"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FFD3DEFC"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedMouseOverStroke}" Color="#FFA9ACB5"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledNotStartedIndicatorFill}" Color="#F3F4F5"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledNotStartedIndicatorStroke}" Color="#D1D2D8"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledIndeterminateIndicatorFill}" Color="#E4E9F5"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledIndeterminateIndicatorStroke}" Color="#D1D2D8"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledCompletedIndicatorFill}" Color="#EAEBEE"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledCompletedIndicatorStroke}" Color="#C8CAD0"/>

</ResourceDictionary>