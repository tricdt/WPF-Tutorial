<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxgnt="http://schemas.devexpress.com/winfx/2008/xaml/gantt/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.DXStyle.v24.1;component/DevExpress.Xpf.Gantt/Generic/Resources.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!-- RowSelection -->

    <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=RowSelectedBackground}" Color="#FFD9E5FF"/>

    <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=RowFadeSelectionBackground}"
                     Color="#FFD9E5FF"
                     Opacity="0.5"/>

    <!-- Ruler -->

    <SolidColorBrush x:Key="{dxgnt:TimescaleThemeKeys ResourceKey=TickBrush}" Color="#FFCACBD3"/>

    <SolidColorBrush x:Key="{dxgnt:TimescaleThemeKeys ResourceKey=BorderBrush}" Color="#FFCACBD3"/>

    <!-- Task -->

    <LinearGradientBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskBrush}"
                         EndPoint="0.5,1"
                         Opacity="0.62"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFD6B5EE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFAE78DD"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskProgressBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFD6B5EE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFAE78DD"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskCriticalPathBrush}"
                     Color="#FFCC0000"
                     Opacity="0.62"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskCriticalPathProgressBrush}" Color="#FFCC0000"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskForeground}" Color="#FF201F35"/>

    <!-- SummaryTask -->

    <LinearGradientBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskBrush}"
                         EndPoint="0.5,1"
                         Opacity="0.62"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFD6B5EE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFAE78DD"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskProgressBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFD6B5EE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFAE78DD"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskForeground}" Color="#FF201F35"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskBracketBrush}" Color="#FF201F35"/>

    <!-- Milestone -->

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=MilestoneBrush}" Color="#FFAE78DD"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=MilestoneCriticalPathBrush}" Color="#FFCC0000"/>

    <!-- Baseline -->

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskBaselineBrush}" Color="#7F201F35"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskBaselineBrush}" Color="#7F201F35"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=MilestoneBaselineBrush}" Color="#7F201F35"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=MilestoneBaselineBorderBrush}" Color="#7F201F35"/>

    <!-- Splitter -->

    <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=SplitterBorderBrush}" Color="#FFCACBD3"/>

    <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=SplitterBackground}" Color="#FFF8F8F9"/>

    <!-- Other -->

    <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=RowBackground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=ConnectorStroke}" Color="#FFAE78DD"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=ConnectorHighlightedShadowBrush}" Color="#40AE78DD"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=InvalidConnectorStroke}" Color="#FFFF0000"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=InvalidConnectorHighlightedShadowBrush}" Color="#30FF0000"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=ConnectorCriticalPathStroke}" Color="#FFCC0000"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=CriticalPathConnectorHighlightedShadowBrush}" Color="#40CC0000"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=NonWorkingTimeBrush}"
                     Color="#FFCACBD3"
                     Opacity="0.25"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=GroupHighlightBrush}"
                     Color="#FFAE78DD"
                     Opacity="0.16"/>

    <sys:Double x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=RowIndent}">12</sys:Double>

    <LinearGradientBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskPreviewBrush}"
                         EndPoint="0.5,1"
                         Opacity="0.4"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFD6B5EE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFAE78DD"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskPreviewBorderBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFD6B5EE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFAE78DD"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskPreviewBrush}"
                         EndPoint="0.5,1"
                         Opacity="0.4"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFD6B5EE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFAE78DD"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskPreviewBorderBrush}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFD6B5EE"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFAE78DD"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=MilestonePreviewBrush}" Color="#80AE78DD"/>

    <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=MilestonePreviewBorderBrush}" Color="#FFAE78DD"/>

    <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=StripLineBackground}"
                     Color="#FF4AB66B"
                     Opacity="0.2"/>

    <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=CurrentDateTimeStripLineBackground}" Color="#FFE94849"/>

</ResourceDictionary>