    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=RightRepeatButtonTemplate}" TargetType="{x:Type RepeatButton}">
        <Grid Height="16"
              Margin="2"
              SnapsToDevicePixels="True"
              Width="16">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="HoverState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF201F35"
                                            cs:id="ColorAnimation_0000"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="Shadow"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="PressedState"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"/>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF201F35"
                                            cs:id="ColorAnimation_0001"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="States"
                                             Storyboard.TargetProperty="(Grid.Effect).(DropShadowEffect.Opacity)"
                                             To="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="States"
                  Background="#FFFFFFFF"
                  cs:id="Grid_0000">
                <Grid.OpacityMask>
                    <LinearGradientBrush EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                         cs:id="LinearGradientBrush_0000">
                        <GradientStop Color="#00FFFFFF"
                                      Offset="0"
                                      cs:id="GradientStop_0000"/>
                        <GradientStop Color="#00FFFFFF"
                                      Offset="1"
                                      cs:id="GradientStop_0001"/>
                        <GradientStop Color="#FFFFFFFF"
                                      Offset="0.5"
                                      cs:id="GradientStop_0002"/>
                    </LinearGradientBrush>
                </Grid.OpacityMask>
                <Grid.Effect>
                    <DropShadowEffect x:Name="Shadow"
                                      BlurRadius="5"
                                      Direction="270"
                                      Opacity="0.12"
                                      ShadowDepth="3"/>
                </Grid.Effect>
                <Border x:Name="HoverState"
                        Background="#FFCBD6F3"
                        Opacity="0"
                        cs:id="Border_0000"/>
                <Border x:Name="PressedState"
                        Background="#FFCED2D9"
                        Opacity="0"
                        cs:id="Border_0001"/>
                <Border x:Name="Separator"
                        Background="#FF9DA0AA"
                        HorizontalAlignment="Left"
                        Width="1"
                        cs:id="Border_0002"/>
                <Path x:Name="Glyph"
                      Data="F1 M 206,7.00006L 206,15.0001L 210,11.0001L 206,7.00006 Z"
                      Fill="#FF80838F"
                      Height="8"
                      HorizontalAlignment="Center"
                      Margin="2,0,0,0"
                      Stretch="Fill"
                      VerticalAlignment="Center"
                      Width="4"
                      cs:id="Path_0000"/>
            </Grid>
        </Grid>
    </ControlTemplate>

