    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=HeaderBorderTemplateInRibbonWindow}" TargetType="{x:Type ContentControl}">
        <Grid Background="#FFEBECEF"
              SnapsToDevicePixels="True"
              cs:id="Grid_0000">
            <Border x:Name="PART_Header"
                    BorderBrush="#FFEDEDF0"
                    BorderThickness="0,0,0,0"
                    Margin="-2,0,-2,0"
                    cs:id="Border_0000">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                         cs:id="LinearGradientBrush_0000">
                        <GradientStop Color="#FFD9DAE0"
                                      Offset="0"
                                      cs:id="GradientStop_0000"/>
                        <GradientStop Color="#FFC8C9D0"
                                      Offset="1"
                                      cs:id="GradientStop_0001"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(dx:FloatingContainer.IsActive), RelativeSource={RelativeSource Mode=TemplatedParent}}" Value="False">
                <Setter Property="Opacity"
                        TargetName="PART_Header"
                        Value="0.6"/>
