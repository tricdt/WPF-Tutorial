  <SolidColorBrush x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=SpreadsheetBorderBrush}" Color="#FF686868" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=SpreadsheetForegroundBrush}" Color="#FFDADADA" x:Uid="@Color=ControlLight.Foreground" />
  <SolidColorBrush x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=SpreadsheetBackgroundBrush}" Color="#FF444444" x:Uid="@Color=Editor.Background" />
