  <Color x:Key="UIColor.Spreadsheet.FormulaBar.Foreground.Ok" x:Uid="@Content_Property=Custom.Green">#FF99FF99</Color>
  <Color x:Key="UIColor.Spreadsheet.FormulaBar.Foreground.Function" x:Uid="@Content_Property=Custom.Blue">#FF99CCFF</Color>
