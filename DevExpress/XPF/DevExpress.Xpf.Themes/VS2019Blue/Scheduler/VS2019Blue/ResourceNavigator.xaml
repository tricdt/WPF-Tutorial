  <!-- General -->
  <ControlTemplate x:Key="ButtonTemplate" TargetType="{x:Type Button}">
    <Grid x:Name="PART_Root">
      <Border x:Name="IsDefault" Background="#FFF2F5FC" cs:id="Border_0000" x:Uid="@Background=Button.Background">
        <ContentPresenter x:Name="Content" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
      </Border>
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="IsMouseOver" Value="True">
        <Setter Property="Background" TargetName="IsDefault" Value="#FFFBE0B3" cs:id="Setter_0000" x:Uid="@Value=HoverBackground" />
      </Trigger>
      <Trigger Property="IsPressed" Value="True">
        <Setter Property="Background" TargetName="IsDefault" Value="#FFFCD89F" cs:id="Setter_0001" x:Uid="@Value=SelectionBackground" />
      </Trigger>
      <Trigger Property="IsEnabled" Value="False">
        <Setter Property="Opacity" TargetName="Content" Value="0.38" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <Style x:Key="{dxscht:ResourceNavigatorThemeKey ResourceKey=HorizontalButtonStyle}" TargetType="Button">
    <Setter Property="Template" Value="{StaticResource ButtonTemplate}" />
  </Style>
  <Style x:Key="{dxscht:ResourceNavigatorThemeKey ResourceKey=VerticalButtonStyle}" TargetType="Button">
    <Setter Property="Template" Value="{StaticResource ButtonTemplate}" />
  </Style>
