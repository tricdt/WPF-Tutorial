  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskForeground}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskBracketBrush}" Color="#FF1E1E1E" x:Uid="@Color=Foreground" />
  <!-- Milestone -->
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=MilestoneBrush}" Color="#FFF5CC84" x:Uid="@Color=Focused" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=MilestoneCriticalPathBrush}" Color="#FFC12600" x:Uid="@Color=Custom.Red" />
  <!-- Baseline -->
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskBaselineBrush}" Color="#FFBFC3D9" Opacity="0.62" x:Uid="@Color=Delimiter" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskBaselineBrush}" Color="#FFBFC3D9" Opacity="0.62" x:Uid="@Color=Delimiter" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=MilestoneBaselineBrush}" Color="#FFBFC3D9" Opacity="0.62" x:Uid="@Color=Delimiter" />
