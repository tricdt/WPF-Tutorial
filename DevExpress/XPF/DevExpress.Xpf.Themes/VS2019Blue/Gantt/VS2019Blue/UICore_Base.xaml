  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=CriticalPathConnectorHighlightedShadowBrush}" Color="#FFC12600" Opacity="0.32" x:Uid="@Color=Custom.Red" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=NonworkingTimeBrush}" Color="#FFBFC3D9" Opacity="0.25" x:Uid="@Color=Delimiter" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=GroupHighlightBrush}" Color="#FFC4D5FF" Opacity="0.16" x:Uid="@Color=ControlLight.SelectionBackground" />
  <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=StripLineBackground}" Color="#FF76A797" Opacity="0.25" />
  <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=CurrentDateTimeStripLineBackground}" Color="#FFD86344" />
</ResourceDictionary>