              <dxb:RenderArrowControl RenderTemplateSelector="{StaticResource {dxbt:BarItemRenderThemeKey ResourceKey=RenderArrowControlTemplateSelector}}" />
            </dxcn:RenderPanel>
          </dxcn:RenderPanel>
        </dxcn:RenderPanel>
      </dxcn:RenderPanel>
    </dxcn:RenderBorder>
