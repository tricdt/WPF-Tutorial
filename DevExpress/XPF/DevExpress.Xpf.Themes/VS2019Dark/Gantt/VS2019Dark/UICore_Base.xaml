  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=SummaryTaskBracketBrush}" Color="#FFF1F1F1" x:Uid="@Color=Foreground" />
  <!-- Milestone -->
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=MilestoneBrush}" Color="#FF007ACC" x:Uid="@Color=Focused" />
