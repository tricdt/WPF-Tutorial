    <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.VS2019Dark.v24.1;component/Gantt/VS2019Dark/UIPreset.xaml" />
  </ResourceDictionary.MergedDictionaries>
  <!-- RowSelection -->
  <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=RowSelectedBackground}" Color="#FF007ACC" x:Uid="@Color=SelectionBackground" />
  <SolidColorBrush x:Key="{dxgnt:GanttControlThemeKeys ResourceKey=RowFadeSelectionBackground}" Color="#FF3E3E40" x:Uid="@Color=HoverBackground" />
  <!-- Ruler -->
  <SolidColorBrush x:Key="{dxgnt:TimescaleThemeKeys ResourceKey=TickBrush}" Color="#FF3F3F46" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="{dxgnt:TimescaleThemeKeys ResourceKey=BorderBrush}" Color="#FF3F3F46" x:Uid="@Color=Border" />
  <!-- Task -->
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskBrush}" Color="#FF007ACC" Opacity="0.62" x:Uid="@Color=Focused" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskProgressBrush}" Color="#FF007ACC" x:Uid="@Color=Focused" />
  <SolidColorBrush x:Key="{dxgnt:GanttItemsThemeKeys ResourceKey=TaskCriticalPathBrush}" Color="#FFFFB6C1" Opacity="0.62" x:Uid="@Color=Custom.Red" />
