  <!-- General -->
  <dxi:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=RowCommandButton}">
    <dxi:RenderPanel Name="Root" Height="8" Margin="6,0" Width="8">
      <dxi:RenderBorder Name="Content" Background="#FF303030" BorderBrush="#FF7D7D7D" BorderThickness="1" cs:id="RenderBorder_0001" x:Uid="@Background=Button.SimpleButtonBackground;BorderBrush=Border" />
    </dxi:RenderPanel>
    <dxi:RenderTemplate.Triggers>
      <dxi:RenderStateGroup Name="CommonStates">
        <dxi:RenderStateTrigger Name="Normal" />
        <dxi:RenderStateTrigger Name="MouseOver">
          <dxi:RenderSetter Property="Background" TargetName="Content" Value="#0FFFFFFF" cs:id="RenderSetter_0000" x:Uid="@Value=ListItem.HoverBackground" />
          <dxi:RenderSetter Property="BorderBrush" TargetName="Content" Value="#FF60CDFF" cs:id="RenderSetter_0001" x:Uid="@Value=Accent" />
        </dxi:RenderStateTrigger>
      </dxi:RenderStateGroup>
      <dxi:RenderTrigger DependencyProperty="{x:Static dxpg:RowControl.ShouldHighlightValueProperty}" Value="True">
        <dxi:RenderSetter Property="Background" TargetName="Content" Value="#FF7D7D7D" cs:id="RenderSetter_0002" x:Uid="@Value=Border" />
        <dxi:RenderSetter Property="BorderBrush" TargetName="Content" Value="#FFFFFFFF" cs:id="RenderSetter_0003" x:Uid="@Value=Foreground.Primary" />
      </dxi:RenderTrigger>
      <dxi:RenderTrigger DependencyProperty="{x:Static dx:ThemeManager.IsTouchEnabledProperty}" Value="True">
        <dxi:RenderSetter Property="Width" TargetName="Root" Value="28" />
        <dxi:RenderSetter Property="Height" TargetName="Root" Value="28" />
        <dxi:RenderSetter Property="Margin" TargetName="Root" Value="8" />
      </dxi:RenderTrigger>
    </dxi:RenderTemplate.Triggers>
  </dxi:RenderTemplate>
