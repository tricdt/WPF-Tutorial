  </ResourceDictionary.MergedDictionaries>
  <!-- General -->
  <ControlTemplate x:Key="{dxet:TrackBarEditThumbThemeKey ResourceKey=VerticalThumbTemplate}" TargetType="{x:Type Thumb}">
    <Canvas x:Name="canvas" ClipToBounds="False" Height="12" Width="16">
      <Border x:Name="Outer" Background="#FF292929" BorderThickness="1" Canvas.Left="-3" Canvas.Top="-5" CornerRadius="99" Height="22" Width="22" cs:id="Border_0001" x:Uid="@Background=Editor.Background">
        <Border.BorderBrush>
          <LinearGradientBrush EndPoint="0,1" Opacity="0.85" StartPoint="0,0" cs:id="LinearGradientBrush_0000">
            <LinearGradientBrush.GradientStops>
              <GradientStop Color="#FF7D7D7D" Offset="0" cs:id="GradientStop_0000" x:Uid="@Color=Border" />
              <GradientStop Color="#FF7D7D7D" Offset="0.6" cs:id="GradientStop_0001" x:Uid="@Color=Border" />
              <GradientStop Color="#88777777" Offset="1" cs:id="GradientStop_0003" />
            </LinearGradientBrush.GradientStops>
          </LinearGradientBrush>
        </Border.BorderBrush>
        <Border x:Name="Inner" CornerRadius="99" Height="14" HorizontalAlignment="Center" VerticalAlignment="Center" Width="14" cs:id="Border_0000">
          <Border.Background>
            <RadialGradientBrush cs:id="RadialGradientBrush_0000">
              <GradientStop x:Name="GradientStop1" Color="#FF60CDFF" Offset="0.8" cs:id="GradientStop_0004" x:Uid="@Color=Accent" />
              <GradientStop x:Name="GradientStop2" Color="Transparent" Offset="0.8" cs:id="GradientStop_0005" />
              <GradientStop x:Name="GradientStop3" Color="Transparent" Offset="1" cs:id="GradientStop_0002" />
            </RadialGradientBrush>
          </Border.Background>
        </Border>
      </Border>
    </Canvas>
    <ControlTemplate.Triggers>
      <Trigger Property="IsMouseOver" Value="True">
        <Trigger.EnterActions>
          <BeginStoryboard>
            <Storyboard>
              <DoubleAnimation Duration="0:0:0.09" From="0.8" Storyboard.TargetName="GradientStop1" Storyboard.TargetProperty="Offset" To="1" />
              <DoubleAnimation Duration="0:0:0.09" From="0.8" Storyboard.TargetName="GradientStop2" Storyboard.TargetProperty="Offset" To="1" />
            </Storyboard>
          </BeginStoryboard>
        </Trigger.EnterActions>
        <Trigger.ExitActions>
          <BeginStoryboard>
            <Storyboard>
              <DoubleAnimation Duration="0:0:0.09" From="1" Storyboard.TargetName="GradientStop1" Storyboard.TargetProperty="Offset" To="0.8" />
              <DoubleAnimation Duration="0:0:0.09" From="1" Storyboard.TargetName="GradientStop2" Storyboard.TargetProperty="Offset" To="0.8" />
            </Storyboard>
          </BeginStoryboard>
        </Trigger.ExitActions>
      </Trigger>
      <Trigger Property="IsFocused" Value="True">
        <Trigger.EnterActions>
          <BeginStoryboard>
            <Storyboard>
              <DoubleAnimation Duration="0:0:0.09" From="0.8" Storyboard.TargetName="GradientStop1" Storyboard.TargetProperty="Offset" To="1" />
              <DoubleAnimation Duration="0:0:0.09" From="0.8" Storyboard.TargetName="GradientStop2" Storyboard.TargetProperty="Offset" To="1" />
            </Storyboard>
          </BeginStoryboard>
        </Trigger.EnterActions>
        <Trigger.ExitActions>
          <BeginStoryboard>
            <Storyboard>
              <DoubleAnimation Duration="0:0:0.09" From="1" Storyboard.TargetName="GradientStop1" Storyboard.TargetProperty="Offset" To="0.8" />
              <DoubleAnimation Duration="0:0:0.09" From="1" Storyboard.TargetName="GradientStop2" Storyboard.TargetProperty="Offset" To="0.8" />
            </Storyboard>
          </BeginStoryboard>
        </Trigger.ExitActions>
      </Trigger>
      <Trigger Property="IsDragging" Value="True">
        <Trigger.EnterActions>
          <BeginStoryboard>
            <Storyboard>
              <DoubleAnimation Duration="0:0:0.19" From="1" Storyboard.TargetName="GradientStop1" Storyboard.TargetProperty="Offset" To="0.7" />
              <DoubleAnimation Duration="0:0:0.19" From="1" Storyboard.TargetName="GradientStop2" Storyboard.TargetProperty="Offset" To="0.7" />
            </Storyboard>
          </BeginStoryboard>
        </Trigger.EnterActions>
        <Trigger.ExitActions>
          <BeginStoryboard>
            <Storyboard>
              <DoubleAnimation Duration="0:0:0.19" From="0.7" Storyboard.TargetName="GradientStop1" Storyboard.TargetProperty="Offset" To="0.8" />
              <DoubleAnimation Duration="0:0:0.19" From="0.7" Storyboard.TargetName="GradientStop2" Storyboard.TargetProperty="Offset" To="0.8" />
            </Storyboard>
          </BeginStoryboard>
        </Trigger.ExitActions>
      </Trigger>
      <Trigger Property="IsEnabled" Value="False">
        <Setter Property="Border.Background" TargetName="Inner" Value="#FFA0A0A0" cs:id="Setter_0000" x:Uid="@Value=Control.NeutralBackground" />
        <Setter Property="Border.BorderBrush" TargetName="Outer" Value="#FF7D7D7D" cs:id="Setter_0001" x:Uid="@Value=Border" />
        <Setter Property="Opacity" TargetName="Inner" Value="0.38" />
      </Trigger>
      <!--<Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight"
                        TargetName="IsDefault"
                        Value="16"/>
                <Setter Property="MinWidth"
                        TargetName="IsDefault"
                        Value="26"/>
                <Setter Property="MinHeight"
                        TargetName="Back"
                        Value="16"/>
                <Setter Property="MinWidth"
                        TargetName="Back"
                        Value="26"/>
            </Trigger>-->
    </ControlTemplate.Triggers>
  </ControlTemplate>
