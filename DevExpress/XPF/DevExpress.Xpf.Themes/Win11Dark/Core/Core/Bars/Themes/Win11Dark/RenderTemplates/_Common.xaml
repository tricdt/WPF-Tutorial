  <dxcn:RenderTemplate x:Key="{dxbt:BarItemRenderThemeKey ResourceKey=GlyphBackground}">
    <dxcn:RenderPanel Name="Root">
      <dxcn:RenderBorder Name="PART_Border" Background="#0AFFFFFF" BorderBrush="#0AFFFFFF" BorderThickness="1" Margin="-2" cs:id="RenderBorder_0000" x:Uid="@Background=ListItem.SelectionBackground;BorderBrush=ListItem.SelectionBorder" />
    </dxcn:RenderPanel>
  </dxcn:RenderTemplate>
  <dxcn:RenderTemplate x:Key="{dxbt:BarItemRenderThemeKey ResourceKey=GlyphTemplate}">
    <dxcn:RenderImage Name="Image" HorizontalAlignment="Center" Stretch="Uniform" VerticalAlignment="Center" />
    <dxcn:RenderTemplate.Triggers>
      <dxcn:RenderBinding TargetName="Image" TargetProperty="Source" ValueSource="DataContext" />
      <dxcn:ConditionalRenderBinding DependencyProperty="{x:Static dxcn:AppearanceOptions.SvgPaletteProperty}" TargetName="Image" TargetProperty="SvgPalette" ValueSource="Chrome">
        <dxcn:RenderCondition DependencyProperty="{x:Static dxcn:AppearanceOptions.HasSvgPaletteProperty}" Value="True" ValueSource="Chrome" />
      </dxcn:ConditionalRenderBinding>
    </dxcn:RenderTemplate.Triggers>
  </dxcn:RenderTemplate>
  <!-- HideBorder -->
  <dxcn:RenderStylesCollection x:Key="{dxbt:BarItemRenderThemeKey ResourceKey=HideBorderSideStyles}">
    <dxcn:RenderStylesCollection.Triggers>
      <dxcn:RenderValueSetter Property="BorderThickness" TargetName="Border" Value="1" />
      <dxcn:RenderStateGroup Name="HideBorderSide">
        <dxcn:RenderStateTrigger Name="None" />
        <dxcn:RenderStateTrigger Name="Left">
          <dxcn:RenderSetter Property="BorderThickness" TargetName="Border" Value="0,1,1,1" />
        </dxcn:RenderStateTrigger>
        <dxcn:RenderStateTrigger Name="Top">
          <dxcn:RenderSetter Property="BorderThickness" TargetName="Border" Value="1,0,1,1" />
        </dxcn:RenderStateTrigger>
        <dxcn:RenderStateTrigger Name="Right">
          <dxcn:RenderSetter Property="BorderThickness" TargetName="Border" Value="1,1,0,1" />
        </dxcn:RenderStateTrigger>
        <dxcn:RenderStateTrigger Name="Bottom">
          <dxcn:RenderSetter Property="BorderThickness" TargetName="Border" Value="1,1,1,0" />
        </dxcn:RenderStateTrigger>
      </dxcn:RenderStateGroup>
    </dxcn:RenderStylesCollection.Triggers>
  </dxcn:RenderStylesCollection>
