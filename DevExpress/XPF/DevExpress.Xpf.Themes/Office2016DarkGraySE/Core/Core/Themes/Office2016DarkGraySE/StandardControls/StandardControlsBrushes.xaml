<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- MENU BRUSHES -->
  <SolidColorBrush x:Key="Menu_Background" Color="#FF666666" x:Uid="@Color=Window.Background" />
  <SolidColorBrush x:Key="Menu_Border" Color="#FF444444" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="Menu_Foreground" Color="#FFEFEFEF" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="MenuItem_Foreground" Color="#FFEFEFEF" x:Uid="@Color=Foreground" />
  <SolidColorBrush x:Key="MenuItem_HoverForeground" Color="#FFEFEFEF" x:Uid="@Color=HoverForeground" />
  <SolidColorBrush x:Key="MenuItem_PressedForeground" Color="#FFEFEFEF" x:Uid="@Color=SelectionForeground" />
  <SolidColorBrush x:Key="MenuItem_HoverBackground" Color="#FF575757" x:Uid="@Color=HoverBackground" />
  <SolidColorBrush x:Key="MenuItem_HoverBorder" Color="#FF575757" x:Uid="@Color=HoverBorder" />
  <SolidColorBrush x:Key="MenuItem_SelectionBackground" Color="#FF444444" x:Uid="@Color=SelectionBackground" />
  <SolidColorBrush x:Key="MenuItem_SelectionBorder" Color="#FF444444" x:Uid="@Color=SelectionBorder" />
  <!-- CONTEXT MENU BRUSHES -->
  <SolidColorBrush x:Key="ContextMenu_Background" Color="#FFD4D4D4" x:Uid="@Color=Editor.Background" />
  <SolidColorBrush x:Key="ContextMenu_Border" Color="#FF444444" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="ContextMenuItem_Foreground" Color="#FF262626" x:Uid="@Color=ControlLight.Foreground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverForeground" Color="#FF000000" x:Uid="@Color=ControlLight.HoverForeground" />
  <SolidColorBrush x:Key="ContextMenuItem_PressedForeground" Color="#FF000000" x:Uid="@Color=ControlLight.SelectionForeground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverBackground" Color="#FF969696" x:Uid="@Color=ControlLight.HoverBackground" />
  <SolidColorBrush x:Key="ContextMenuItem_HoverBorder" Color="#FF969696" x:Uid="@Color=ControlLight.HoverBackground" />
  <SolidColorBrush x:Key="ContextMenuItem_SelectionBackground" Color="#FF868686" x:Uid="@Color=ControlLight.SelectionBackground" />
  <SolidColorBrush x:Key="ContextMenuItem_SelectionBorder" Color="#FF868686" x:Uid="@Color=ControlLight.SelectionBackground" />
</ResourceDictionary>