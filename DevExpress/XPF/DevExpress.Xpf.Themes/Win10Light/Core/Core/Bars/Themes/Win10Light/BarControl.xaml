              <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" To="#FFCCE8FF" cs:id="ColorAnimation_0008" x:Uid="@To=SelectionBackground" />
