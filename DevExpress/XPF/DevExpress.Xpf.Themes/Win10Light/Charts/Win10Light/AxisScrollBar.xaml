  <!-- Thumb -->
  <ControlTemplate x:Key="VerticalThumbTemplate" TargetType="dxc:AxisScrollBarThumb">
    <Grid x:Name="Root">
      <Border x:Name="IsDefault" Background="#FFCFCFCF" BorderBrush="#FFCFCFCF" BorderThickness="1" Margin="1" cs:id="Border_0001" x:Uid="@Background=Border;BorderBrush=Border" />
      <Border x:Name="OpacityMask" Background="#FF000000" BorderBrush="#FF000000" BorderThickness="1" Margin="1" Opacity="0.2" Visibility="Hidden" cs:id="Border_0002" />
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="CommonStates">
          <VisualState x:Name="Normal" />
          <VisualState x:Name="MouseOver">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)" To="#FFCCCCCC" cs:id="ColorAnimation_0000" x:Uid="@To=AltHover" />
              <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" To="#FFCCCCCC" cs:id="ColorAnimation_0001" x:Uid="@To=AltHover" />
