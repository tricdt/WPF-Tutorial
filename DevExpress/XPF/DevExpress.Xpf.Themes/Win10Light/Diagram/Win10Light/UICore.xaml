  <SolidColorBrush x:Key="{dxdiagt:RulerThemeKey ResourceKey=HorizontalBackground}" Color="#FFFFFFFF" x:Uid="@Color=Window.Background" />
  <SolidColorBrush x:Key="{dxdiagt:RulerThemeKey ResourceKey=VerticalBackground}" Color="#FFFFFFFF" x:Uid="@Color=Window.Background" />
  <SolidColorBrush x:Key="{dxdiagt:RulerThemeKey ResourceKey=BorderBrush}" Color="#FFCFCFCF" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="{dxdiagt:RulerThemeKey ResourceKey=TickBrush}" Color="#FFCFCFCF" x:Uid="@Color=Border" />
