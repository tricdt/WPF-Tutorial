<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdash="http://schemas.devexpress.com/winfx/2008/xaml/dashboard"
                    xmlns:dxdashi="http://schemas.devexpress.com/winfx/2008/xaml/dashboardi"
                    xmlns:dxdasht="http://schemas.devexpress.com/winfx/2008/xaml/dashboard/themekeys"
                    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:wn="clr-namespace:System.Windows;assembly=PresentationCore"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.TouchlineDark.v24.1;component/DevExpress.Xpf.Dashboard/TouchlineDark/UICustom.xaml"/>
        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.TouchlineDark.v24.1;component/DevExpress.Xpf.Dashboard/TouchlineDark/UIPreset.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!-- DashboardControl -->

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SelectedItemBackground}"
                     Color="#FFF78A09"
                     Opacity="0.5"/>

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SelectedItemBorder}"
                     Color="#FFF78A09"
                     Opacity="0.5"/>

    <sys:Boolean x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=IsDarkTheme}">True</sys:Boolean>

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=Background}" Color="#FF333333"/>

    <Thickness x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=BorderThickness}">1</Thickness>

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=TitleForeground}" Color="#FFEFEFEF"/>

    <sys:Double x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=TitleFontSize}">18</sys:Double>

    <FontWeight x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=TitleFontWeight}">Normal</FontWeight>

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SubTitleForeground}"
                     Color="#FFEFEFEF"
                     Opacity="0.62"/>

    <sys:Double x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SubTitleFontSize}">14</sys:Double>

    <FontWeight x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=SubTitleFontWeight}">Normal</FontWeight>

    <Color x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=MasterFilterIconColor}">#FF1E3662</Color>

    <SolidColorBrush x:Key="{dxdasht:DashboardControlThemeKey ResourceKey=FormatConditionBarDrawAxisBrush}" Color="#FFF78A09"/>

    <!-- DashboardLayoutItem -->

    <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=Background}" Color="#FF333333"/>

    <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=BorderBrush}" Color="#FF515151"/>

    <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=CaptionBackground}" Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=CaptionForeground}" Color="#FFEFEFEF"/>

    <sys:Double x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=CaptionFontSize}">18</sys:Double>

    <FontWeight x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=CaptionFontWeight}">Normal</FontWeight>

    <SolidColorBrush x:Key="{dxdasht:DashboardLayoutItemThemeKey ResourceKey=WaitIndicatorForeground}" Color="#FFF78A09"/>

    <!-- CardItem -->

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=Background}">Transparent</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=MainTextForeground}">#FFEFEFEF</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=CardItemBackground}">#FF3C3C3C</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=CardItemBorderBrush}">#FF515151</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=HighlightedBackground}"
                     Color="#FFF78A09"
                     Opacity="0.3"/>

    <SolidColorBrush x:Key="{dxdasht:CardItemThemeKey ResourceKey=HighlightedBorderBrush}"
                     Color="#FFF78A09"
                     Opacity="0.3"/>

    <CornerRadius x:Key="{dxdasht:CardItemThemeKey ResourceKey=CardBackgroundCornerRadius}">0</CornerRadius>

    <!-- DeltaValue -->

    <SolidColorBrush x:Key="{dxdasht:DeltaValuePresenterThemeKey ResourceKey=GoodForeground}">#FF5F957D</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:DeltaValuePresenterThemeKey ResourceKey=BadForeground}">#FFBA4D51</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:DeltaValuePresenterThemeKey ResourceKey=WarningForeground}">#FFAF8A53</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:DeltaValuePresenterThemeKey ResourceKey=NeutralForeground}">#FF666666</SolidColorBrush>

    <!-- GaugeItem -->

    <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeHighlightedBackgroundBrush}"
                     Color="#FFF78A09"
                     Opacity="0.3"/>

    <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeHighlightedBorderBrush}"
                     Color="#FFF78A09"
                     Opacity="0.3"/>

    <CornerRadius x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeBackgroundCornerRadius}">0</CornerRadius>

    <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeCaptionForeground}" Color="#FFEFEFEF"/>

    <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeScaleLabelForeground}" Color="#FFEFEFEF"/>

    <sys:Double x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeCaptionFontSize}">18</sys:Double>

    <SolidColorBrush x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeSubCaptionForeground}"
                     Color="#FFEFEFEF"
                     Opacity="0.62"/>

    <sys:Double x:Key="{dxdasht:GaugeItemThemeKey ResourceKey=GaugeSubCaptionFontSize}">18</sys:Double>

    <!-- MapItem -->

    <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointBackgroundBrush}" Color="#FF515151"/>

    <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointHighLightedBackgroundBrush}" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointBorderBrush}" Color="#FFFFFFFF"/>

    <sys:Double x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointDisplayTextFontSize}">18</sys:Double>

    <wn:FontWeight x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointDisplayTextFontWeight}">Normal</wn:FontWeight>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=GradientColorStart}">#FF7482AA</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=GradientColorEnd}">#FFAA7474</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=DeltaColorGood}">#FF5F957D</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=DeltaColorBad}">#FFBA4D51</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=DeltaColorWarning}">#FFAF8A53</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorTens}">#FF5F6595</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorHundreds}">#FF00CC00</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorThousands}">#FFAF8A53</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorMillions}">#FFBA4D51</Color>

    <!-- PieItem -->

    <SolidColorBrush x:Key="{dxdasht:PieItemThemeKey ResourceKey=PieHighlightedBackgroundBrush}"
                     Color="#FFF78A09"
                     Opacity="0.3"/>

    <SolidColorBrush x:Key="{dxdasht:PieItemThemeKey ResourceKey=PieHighlightedBorderBrush}"
                     Color="#FFF78A09"
                     Opacity="0.3"/>

    <CornerRadius x:Key="{dxdasht:PieItemThemeKey ResourceKey=PieBackgroundCornerRadius}">0</CornerRadius>

    <!-- ChartItem -->

    <SolidColorBrush x:Key="{dxdasht:ChartItemThemeKey ResourceKey=LegendBackgroundBrush}">#FF333333</SolidColorBrush>

    <SolidColorBrush x:Key="{dxdasht:ChartItemThemeKey ResourceKey=LegendForegroundBrush}">#bfffffff</SolidColorBrush>

    <!-- TreemapItem -->

    <SolidColorBrush x:Key="{dxdasht:TreemapItemThemeKey ResourceKey=ToolTipForeground}">#FFF0F0F0</SolidColorBrush>

    <sys:Double x:Key="{dxdasht:TreemapItemThemeKey ResourceKey=ToolTipFontSize}">18</sys:Double>

    <FontWeight x:Key="{dxdasht:TreemapItemThemeKey ResourceKey=ToolTipFontWeight}">Light</FontWeight>

    <!-- ListBox -->

    <SolidColorBrush x:Key="{dxdasht:ListBoxItemThemeKey ResourceKey=Foreground}" Color="#bfffffff"/>

    <!-- Other -->

    <Color x:Key="{dxdasht:PivotItemThemeKey ResourceKey=DefaultBackgroundColor}">#FF333333</Color>

    <Color x:Key="{dxdasht:GridItemThemeKey ResourceKey=DefaultBackgroundColor}">#FF333333</Color>

    <Color x:Key="{dxdasht:CardItemThemeKey ResourceKey=DefaultBackgroundColor}">#FF333333</Color>

    <Color x:Key="{dxdasht:ChartItemThemeKey ResourceKey=DefaultBackgroundColor}">#FF333333</Color>

</ResourceDictionary>