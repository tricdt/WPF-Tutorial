<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxpt="http://schemas.devexpress.com/winfx/2008/xaml/printing/themekeys"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="{dxpt:PrintingThemeKey ResourceKey=ComboBoxEditPDFDialog}" TargetType="dxe:ComboBoxEdit">
        <Setter Property="Width" Value="300"></Setter>
    </Style>

    <Style x:Key="{dxpt:PrintingThemeKey ResourceKey=PasswordBoxEditPDFDialog}" TargetType="dxe:PasswordBoxEdit">
        <Setter Property="Width" Value="300"></Setter>
    </Style>

    <Thickness x:Key="{dxpt:PrintingThemeKey ResourceKey=WatermarkCheckEditMargin}">18,0,0,12</Thickness>

    <Thickness x:Key="{dxpt:PrintingThemeKey ResourceKey=WatermarkTextBlockMargin}">0,0,18,12</Thickness>

    <Thickness x:Key="{dxpt:PrintingThemeKey ResourceKey=WatermarkEditorMargin}">0,0,0,12</Thickness>

    <sys:Double x:Key="{dxpt:PrintingThemeKey ResourceKey=WatermarkPreviewWidth}">385</sys:Double>

    <Thickness x:Key="{dxpt:PrintingThemeKey ResourceKey=clientAreaMargin}">12</Thickness>

    <Thickness x:Key="{dxpt:PrintingThemeKey ResourceKey=controlVerticalSpacing}">0,0,0,4</Thickness>

    <Thickness x:Key="{dxpt:PrintingThemeKey ResourceKey=controlHorizontalSpacing}">0,0,4,0</Thickness>

    <Thickness x:Key="{dxpt:PrintingThemeKey ResourceKey=groupVerticalSpacing}">0,0,0,12</Thickness>

</ResourceDictionary>