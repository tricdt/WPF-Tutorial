<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Text.Foreground" Color="#bfffffff"/>

    <Color x:Key="LayerSeparator.NormalColor" cs:Name="Color_0001">#FF515151</Color>

    <Color x:Key="LayerSeparator.MouseOverColor" cs:Name="Color_0002">#FF666666</Color>

    <Color x:Key="ElementSizer.GlyphNormalColor" cs:Name="Color_0003">#FF515151</Color>

    <Color x:Key="ElementSizer.GlyphMouseOverColor" cs:Name="Color_0004">#FF666666</Color>

    <Color x:Key="ElementSizer.GlyphBackgroundColor" cs:Name="Color_0005">#FF515151</Color>

    <Color x:Key="GroupBoxButton.NormalColor" cs:Name="Color_0006">#FFFFFFFF</Color>

    <Color x:Key="GroupBoxButton.MouseOverColor" cs:Name="Color_0007">#FFF78A09</Color>

    <Color x:Key="GroupBoxButton.PressedColor" cs:Name="Color_0008">#FFF78A09</Color>

    <SolidColorBrush x:Key="GroupBox.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="GroupBox.BorderBrush" Color="#FF515151"/>

    <SolidColorBrush x:Key="GroupBox.SeparatorBrush" Color="#FF515151"/>

    <SolidColorBrush x:Key="GroupBox.TitleBackground" Color="#FF515151"/>

    <SolidColorBrush x:Key="GroupSeparator.Brush" Color="#FF515151"/>

</ResourceDictionary>