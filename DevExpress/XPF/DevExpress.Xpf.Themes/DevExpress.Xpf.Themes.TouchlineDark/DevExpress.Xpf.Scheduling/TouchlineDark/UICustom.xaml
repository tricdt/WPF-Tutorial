<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduling"
                    xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduling/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Shared: MetropolisDark, TouchlineDark-->

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="{dx:SchedulingResource Generic/DefaultUICustom.xaml}"/>
    </ResourceDictionary.MergedDictionaries>

    <dxsch:BrushProvider x:Shared="False"
                         x:Key="{dxscht:SchedulerThemeKey ResourceKey=BrushProvider}"
                         BasedOn="{StaticResource BrushProvider}"
                         ResourceNavigationButtonDisabledBackground="{dxsch:OverrideBrushTransform OverrideBrush=Transparent}"
                         ResourceNavigationButtonDisabledBorderBrush="{dxsch:OverrideBrushTransform OverrideBrush=Transparent}"
                         ResourceNavigationButtonHoverBackground="{dxsch:OverrideBrushTransform OverrideBrush=Transparent}"
                         ResourceNavigationButtonHoverBorderBrush="{dxsch:OverrideBrushTransform OverrideBrush=Transparent}"
                         ResourceNavigationButtonHoverForeground="{dxsch:BrightnessBrushTransform Brightness=0.5}"
                         ResourceNavigationButtonNormalBackground="{dxsch:OverrideBrushTransform OverrideBrush=Transparent}"
                         ResourceNavigationButtonNormalBorderBrush="{dxsch:OverrideBrushTransform OverrideBrush=Transparent}"
                         ResourceNavigationButtonNormalForeground="{dxsch:BrightnessBrushTransform Brightness=-0.1}"
                         ResourceNavigationButtonPressedBackground="{dxsch:OverrideBrushTransform OverrideBrush=Transparent}"
                         ResourceNavigationButtonPressedBorderBrush="{dxsch:OverrideBrushTransform OverrideBrush=Transparent}"
                         ResourceNavigationButtonPressedForeground="{dxsch:BrightnessBrushTransform Brightness=0.5}"/>

    <dxsch:BrushSet x:Key="{dxscht:SchedulerThemeKey ResourceKey=BrushSet}" BasedOn="{StaticResource DefaultBrushSet.Dark}"/>

    <!--#region Legacy-->

    <dxsch:BrushProvider x:Shared="False"
                         x:Key="{dxscht:SchedulerThemeKey ResourceKey=BrushProvider_Classic}"
                         BasedOn="{StaticResource BrushProvider_Classic}"/>

