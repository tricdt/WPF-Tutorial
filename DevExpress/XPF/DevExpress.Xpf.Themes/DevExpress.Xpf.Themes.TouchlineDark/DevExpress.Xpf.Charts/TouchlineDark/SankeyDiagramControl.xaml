<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxsat="http://schemas.devexpress.com/winfx/2008/xaml/sankey/themekeys"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=PaddingValue}">32</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=BorderThickness}">1</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=ToolTipContentMargin}">7</Thickness>

    <Thickness x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TitleMargin}">0,0,0,10</Thickness>

    <sys:Double x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TitleFontSize}">21</sys:Double>

    <sys:Double x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=TextElementFontSize}">16</sys:Double>

    <FontWeight x:Key="{dxsat:SankeyDiagramThemeKey ResourceKey=NodeLabelFontWeight}">Bold</FontWeight>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=BackgroundBrush}" Color="#FF333333"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=BorderBrush}" Color="#FF515151"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipForeground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipBackground}" Color="#FF333333"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=ToolTipBorder}" Color="#5AFFFFFF"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=TitleForeground}" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=TextElementForeground}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxsat:SankeyBrushesThemeKey ResourceKey=NodeLabelForeground}" Color="#bfffffff"/>

</ResourceDictionary>