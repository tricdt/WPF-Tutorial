<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxprg="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid"
                    xmlns:dxprgt="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid/themekeys"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <dxn:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=RowCommandButton}">
        <dxn:RenderPanel Margin="10">
            <dxn:RenderBorder Height="18" Width="18"/>
            <dxn:RenderBorder Name="Root" Background="#FF333333">
                <dxn:RenderPanel>
                    <dxn:RenderPath Name="Path"
                                    Data="F1 M 10,10L 0,10L 0,0L 10,0L 10,10 Z M 1,1L 1,9L 5,9L 9,5L 9,1L 1,1 Z "
                                    Fill="#FF515151"
                                    Height="18"
                                    Stretch="Fill"
                                    Width="18"/>
                    <dxn:RenderBorder Name="ShouldHightlightValue"
                                      Background="#FF515151"
                                      Height="18"
                                      Opacity="0"
                                      Width="18"/>
                </dxn:RenderPanel>
            </dxn:RenderBorder>
            <dxn:RenderContentPresenter Name="PART_ContentPresenter" Visibility="Collapsed"/>
        </dxn:RenderPanel>
        <dxn:RenderTemplate.Triggers>
            <dxn:RenderStateGroup Name="CommonStates">
                <dxn:RenderStateTrigger Name="Normal"/>
                <dxn:RenderStateTrigger Name="MouseOver">
                    <dxn:RenderSetter Property="Background"
                                      TargetName="ShouldHightlightValue"
                                      Value="#FF666666"/>
                    <dxn:RenderSetter Property="Fill"
                                      TargetName="Path"
                                      Value="#FF666666"/>
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
            <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.ShouldHighlightValueProperty}" Value="True">
                <dxn:RenderSetter Property="Opacity"
                                  TargetName="ShouldHightlightValue"
                                  Value="1"/>
            </dxn:RenderTrigger>
            <dxn:MultiRenderTrigger>
                <dxn:MultiRenderTrigger.Conditions>
                    <dxn:RenderCondition DependencyProperty="{x:Static dxprg:RowControl.MenuButtonTemplateProperty}"
                                         Operator="Not"
                                         Value="{x:Null}"/>
                </dxn:MultiRenderTrigger.Conditions>
                <dxn:RenderSetter Property="Visibility"
                                  TargetName="PART_ContentPresenter"
                                  Value="Visible"/>
                <dxn:RenderSetter Property="Visibility"
                                  TargetName="Root"
                                  Value="Collapsed"/>
            </dxn:MultiRenderTrigger>
        </dxn:RenderTemplate.Triggers>
    </dxn:RenderTemplate>

    <dxn:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=RowCollectionButton}">
        <dxn:RenderPanel>
            <dxn:RenderPanel Name="PART_Add" Opacity="0.75">
                <dxn:RenderPath Data="F1 M 6,0C 9.31371,0 12,2.68629 12,6C 12,9.31371 9.31371,12 6,12C 2.68629,12 -6.35783e-007,9.31371 -6.35783e-007,6C -6.35783e-007,2.68629 2.68629,0 6,0 Z M 2,5L 2,7L 5,7L 5,10L 7,10L 7,7L 10,7L 10,5L 7,5L 7,2L 5,2L 5,5L 2,5 Z "
                                Fill="#FF666666"
                                Height="18"
                                Stretch="Fill"
                                Width="18"/>
            </dxn:RenderPanel>
            <dxn:RenderPanel Name="PART_Remove"
                             Opacity="0.5"
                             Visibility="Collapsed">
                <dxn:RenderPath Name="Path"
                                Data="F1 M 1.75736,1.75736C 4.1005,-0.585785 7.89949,-0.585785 10.2426,1.75736C 12.5858,4.10051 12.5858,7.8995 10.2426,10.2426C 7.89949,12.5858 4.1005,12.5858 1.75736,10.2426C -0.585787,7.8995 -0.585787,4.10051 1.75736,1.75736 Z M 2.46447,8.12132L 3.87868,9.53553L 6,7.41421L 8.12132,9.53553L 9.53553,8.12132L 7.41421,6L 9.53553,3.87868L 8.12132,2.46447L 6,4.58579L 3.87868,2.46447L 2.46447,3.87868L 4.58579,6L 2.46447,8.12132 Z "
                                Fill="#FFCC6666"
                                Height="18"
                                Stretch="Fill"
                                Width="18"/>
            </dxn:RenderPanel>
        </dxn:RenderPanel>
        <dxn:RenderTemplate.Triggers>
            <dxn:RenderStateGroup Name="CommonStates">
                <dxn:RenderStateTrigger Name="Normal"/>
                <dxn:RenderStateTrigger Name="MouseOver">
                    <dxn:RenderSetter Property="Opacity"
                                      TargetName="PART_Add"
                                      Value="1"/>
                    <dxn:RenderSetter Property="Opacity"
                                      TargetName="PART_Remove"
                                      Value="1"/>
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
            <dxn:RenderStateGroup Name="ButtonKind">
                <dxn:RenderStateTrigger Name="Add"/>
                <dxn:RenderStateTrigger Name="Remove">
                    <dxn:RenderSetter Property="Visibility"
                                      TargetName="PART_Remove"
                                      Value="Visible"/>
                    <dxn:RenderSetter Property="Visibility"
                                      TargetName="PART_Add"
                                      Value="Collapsed"/>
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
        </dxn:RenderTemplate.Triggers>
    </dxn:RenderTemplate>

    <dxn:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=Expander}">
        <dxn:RenderPanel>
            <dxn:RenderPath Name="GlyphCollapsed"
                            Data="F1 M 349,770C 341.82,770 336,775.82 336,783C 336,790.18 341.82,796 349,796C 356.179,796 362,790.18 362,783C 362,775.82 356.179,770 349,770 Z M 349,772C 355.075,772 360,776.925 360,783C 360,789.075 355.075,794 349,794C 342.925,794 338,789.075 338,783C 338,776.925 342.925,772 349,772 Z M 358,783L 350,775L 347,775L 354,782L 341,782L 341,784L 354,784L 347,791L 350,791L 358,783 Z "
                            Fill="#DBFFFFFF"
                            Height="26"
                            Stretch="Fill"
                            Width="26"/>
            <dxn:RenderPath Name="GlyphExpanded"
                            Data="F1 M 362,795C 369.18,795 375,800.82 375,808C 375,815.18 369.18,821 362,821C 354.82,821 349,815.18 349,808C 349,800.82 354.82,795 362,795 Z M 362,797C 355.925,797 351,801.925 351,808C 351,814.075 355.925,819 362,819C 368.075,819 373,814.075 373,808C 373,801.925 368.075,797 362,797 Z M 366,802L 366,801L 368,803L 368,812.076L 368.003,812.079L 368,812.079L 368,814L 357,814L 355,812L 364,812L 355,803L 357,801L 366,810.062L 366,802 Z "
                            Fill="#DBFFFFFF"
                            Height="26"
                            Opacity="0"
                            Stretch="Fill"
                            Width="26"/>
        </dxn:RenderPanel>
        <dxn:RenderTemplate.Triggers>
            <dxn:RenderStateGroup Name="CommonStates">
                <dxn:RenderStateTrigger Name="Normal"/>
                <dxn:RenderStateTrigger Name="MouseOver">
                    <dxn:RenderSetter Property="Fill"
                                      TargetName="GlyphExpanded"
                                      Value="#FFFFFFFF"/>
                    <dxn:RenderSetter Property="Fill"
                                      TargetName="GlyphCollapsed"
                                      Value="#FFFFFFFF"/>
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
            <dxn:RenderStateGroup Name="CheckedStates">
                <dxn:RenderStateTrigger Name="Checked">
                    <dxn:RenderSetter Property="Opacity"
                                      TargetName="GlyphExpanded"
                                      Value="1"/>
                    <dxn:RenderSetter Property="Opacity"
                                      TargetName="GlyphCollapsed"
                                      Value="0"/>
                </dxn:RenderStateTrigger>
                <dxn:RenderStateTrigger Name="Unchecked"/>
            </dxn:RenderStateGroup>
        </dxn:RenderTemplate.Triggers>
    </dxn:RenderTemplate>

</ResourceDictionary>