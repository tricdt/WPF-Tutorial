<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Fonts -->

    <system:Double x:Key="{dxt:FloatingContainerThemeKey ResourceKey=GlobalFontSize}">18</system:Double>

    <FontFamily x:Key="{dxt:FloatingContainerThemeKey ResourceKey=GlobalFontFamily}">Segoe UI</FontFamily>

    <!-- General -->

    <Color x:Key="DefaultBackgroundColor" cs:Name="Color_0001">#FF333333</Color>

    <Color x:Key="DefaultForegroundColor" cs:Name="Color_0002">#bfffffff</Color>

    <SolidColorBrush x:Key="{dxt:FloatingContainerThemeKey ResourceKey=FloatingContainerBackground}" Color="{StaticResource DefaultBackgroundColor}"/>

    <SolidColorBrush x:Key="{dxt:ThemeColorsThemeKey ResourceKey=WhiteForegroundColor}" Color="#bfffffff"/>

    <SolidColorBrush x:Key="{dxt:ThemeColorsThemeKey ResourceKey=BlackForegroundColor}" Color="#bfffffff"/>

    <SolidColorBrush x:Key="{dxt:ThemeColorsThemeKey ResourceKey=DXMessageBoxForegroundColor}" Color="#bfffffff"/>

    <SolidColorBrush x:Key="Page.Background" Color="{StaticResource DefaultBackgroundColor}"/>

    <SolidColorBrush x:Key="Text.Foreground" Color="{StaticResource DefaultForegroundColor}"/>

    <SolidColorBrush x:Key="Text.DisabledState.Foreground" Color="#3FE3E3E3"/>

    <SolidColorBrush x:Key="Shadow.Background" Color="Transparent"/>

    <SolidColorBrush x:Key="Text.Selection.Background" Color="#FFFF8800"/>

    <SolidColorBrush x:Key="Text.CaretBrush" Color="#FFFFFFFF"/>

    <!-- Button -->

    <SolidColorBrush x:Key="Button.DefaultState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="Button.DefaultState.Border2" Color="#FF333333"/>

    <SolidColorBrush x:Key="Button.HoverState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="Button.HoverState.Border2" Color="#FF515151"/>

    <SolidColorBrush x:Key="Button.PressedState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="Button.PressedState.Border2" Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="Button.CheckedState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="Button.CheckedState.Border2" Color="#FF515151"/>

    <!-- CheckBox -->

    <SolidColorBrush x:Key="CheckBox.DefaultState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="CheckBox.DefaultState.Border2" Color="#FF333333"/>

    <SolidColorBrush x:Key="CheckBox.HoverState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="CheckBox.HoverState.Border2" Color="#FF515151"/>

    <SolidColorBrush x:Key="CheckBox.PressedState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="CheckBox.PressedState.Border2" Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="CheckBox.IndeterminateState.Border" Color="#bfffffff"/>

    <!-- TextBox -->

    <SolidColorBrush x:Key="TextBox.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="TextBox.BorderBrush" Color="#FF515151"/>

    <SolidColorBrush x:Key="TextBox.ReadOnly.Background" Color="#FF3C3C3C"/>

    <!-- ListBox -->

    <SolidColorBrush x:Key="ListBox.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="ListBox.BorderBrush" Color="#FF515151"/>

    <!-- ListBoxItem -->

    <SolidColorBrush x:Key="ListBoxItem.Background" Color="Transparent"/>

    <SolidColorBrush x:Key="ListBoxItem.BorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="ListBoxItem.Foreground" Color="#bfffffff"/>

    <SolidColorBrush x:Key="ListBoxItem.DisabledState.Foreground" Color="#FF6D6D6D"/>

    <SolidColorBrush x:Key="ListBoxItem.Selection.Border" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="ListBoxItem.HoverSelection.Border" Color="#FF666666"/>

    <SolidColorBrush x:Key="ListBoxItem.InactiveSelection.Border" Color="#FF666666"/>

    <!-- RadioButton -->

    <SolidColorBrush x:Key="RadioButton.DefaultState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="RadioButton.DefaultState.Border2" Color="#FF333333"/>

    <SolidColorBrush x:Key="RadioButton.HoverState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="RadioButton.HoverState.Border2" Color="#FF515151"/>

    <SolidColorBrush x:Key="RadioButton.PressedState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="RadioButton.PressedState.Border2" Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="RadioButton.DisabledState.Border1" Color="#FF515151"/>

    <SolidColorBrush x:Key="RadioButton.DisabledState.Border2" Color="#FF333333"/>

    <SolidColorBrush x:Key="RadioButton.Icon.Background" Color="#bfffffff"/>

    <!-- ComboBox -->

    <SolidColorBrush x:Key="ComboBox.Path.DefaultState" Color="#bfffffff"/>

    <SolidColorBrush x:Key="ComboBox.Path.DisabledState" Color="#7FE3E3E3"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.DefaultState.Border" Color="#FF515151"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.DefaultState.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.HoverState.Border" Color="#FF515151"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.HoverState.Background" Color="#FF515151"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.PressedState.Border" Color="#FF515151"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.PressedState.Background" Color="#FF3C3C3C"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.DisabledState.Border" Color="Transparent"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.DisabledState.Background" Color="Transparent "/>

    <SolidColorBrush x:Key="ComboBox.DropDownBorder.BorderBrush" Color="#FF515151"/>

    <SolidColorBrush x:Key="ComboBox.DropDownBorder.Background" Color="#FF333333"/>

    <!-- ComboBoxItem -->

    <!-- Label -->

    <SolidColorBrush x:Key="Label.Background" Color="Transparent"/>

    <SolidColorBrush x:Key="Label.Foreground" Color="#bfffffff"/>

    <!-- ProgressBar -->

    <SolidColorBrush x:Key="ProgressBar.BorderBrush" Color="#FF515151"/>

    <SolidColorBrush x:Key="ProgressBar.Background" Color="#FF515151"/>

    <SolidColorBrush x:Key="ProgressBar.IndicatorBackground" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="ProgressBar.IndicatorAnimatedFill" Color="#FFF78A09"/>

    <!-- ScrollViewer -->

    <SolidColorBrush x:Key="ScrollViewer.BorderBrush" Color="#FF515151"/>

    <SolidColorBrush x:Key="ScrollViewer.Corner.Background" Color="#FF333333"/>

    <!-- ScrollBar -->

    <SolidColorBrush x:Key="ScrollBar.Vertical.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="ScrollBar.Vertical.BorderBrush" Color="#FF515151"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.BorderBrush" Color="#FF515151"/>

    <SolidColorBrush x:Key="ScrollBar.Vertical.PageButton.Background" Color="Transparent"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.PageButton.Background" Color="Transparent"/>

    <SolidColorBrush x:Key="ScrollBar.Thumb.Path.Background" Color="#FF666666"/>

    <SolidColorBrush x:Key="ScrollBar.Path.Background" Color="#FF666666"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.DefaultState.Border" Color="#FF666666"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.HoverState.Border" Color="#bfffffff"/>

    <SolidColorBrush x:Key="ScrollBar.Horizontal.DraggingState.Border" Color="#FF666666"/>

    <SolidColorBrush x:Key="ScrollBar.Vertical.DefaultState.Border" Color="#FF666666"/>

    <SolidColorBrush x:Key="ScrollBar.Vertical.HoverState.Border" Color="#bfffffff"/>

    <SolidColorBrush x:Key="ScrollBar.Vertical.DraggingState.Border" Color="#FF666666"/>

    <!-- Separator -->

    <SolidColorBrush x:Key="Separator.BorderBrush" Color="#FF515151"/>

    <!-- TabControl -->

    <SolidColorBrush x:Key="TabControl.BorderBrush" Color="#FF515151"/>

    <SolidColorBrush x:Key="TabControl.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="TabControl.Foreground" Color="#bfffffff"/>

    <!-- TabItem -->

    <SolidColorBrush x:Key="TabItem.Foreground" Color="#FFB4B4B4"/>

    <SolidColorBrush x:Key="TabItem.HoverState.Foreground" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="TabItem.SelectedState.Foreground" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="TabItem.BorderBrush" Color="#FF515151"/>

    <SolidColorBrush x:Key="TabItem.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="TabItem.HoverState.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="TabItem.SelectedState.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Top" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Bottom" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Left" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Right" Color="#FFF78A09"/>

    <!-- Slider -->

    <SolidColorBrush x:Key="Slider.Track.Background" Color="#FF515151"/>

    <SolidColorBrush x:Key="Slider.Tick.Background" Color="#FF515151"/>

    <SolidColorBrush x:Key="Slider.SelectionRange.Background" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="SliderThumb.DefaultState" Color="#bfffffff"/>

    <SolidColorBrush x:Key="SliderThumb.HoverState" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="SliderThumb.PressedState" Color="#FFF78A09"/>

    <SolidColorBrush x:Key="SliderThumb.DisabledState" Color="#FF515151"/>

    <!-- GridSplitter -->

    <SolidColorBrush x:Key="GridSplitter.Background" Color="#FF515151"/>

    <SolidColorBrush x:Key="GridSplitter.PreviewBrush" Color="#FFF78A09"/>

    <!-- ToolTip -->

    <SolidColorBrush x:Key="ToolTip.BorderBrush" Color="#FF666666"/>

    <SolidColorBrush x:Key="ToolTip.Background" Color="#FF333333"/>

    <SolidColorBrush x:Key="ToolTip.Foreground" Color="{StaticResource DefaultForegroundColor}"/>

    <!-- Expander -->

    <SolidColorBrush x:Key="Expander.Background" Color="Transparent"/>

    <SolidColorBrush x:Key="Expander.BorderBrush" Color="Transparent"/>

</ResourceDictionary>