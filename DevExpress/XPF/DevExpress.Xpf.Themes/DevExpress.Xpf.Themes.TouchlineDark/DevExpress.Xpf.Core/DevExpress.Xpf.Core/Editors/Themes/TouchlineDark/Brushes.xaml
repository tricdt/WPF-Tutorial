<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=TextEditPadding}">12,13,12,13</Thickness>

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=ValidationErrorPadding}">12,0,0,0</Thickness>

    <Thickness x:Key="leftButtonsMargin">0</Thickness>

    <Thickness x:Key="rightButtonsMargin">0</Thickness>

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=CheckEditPadding}">0,0,0,0</Thickness>

    <Thickness x:Key="{dxet:ComboBoxEditThemeKey ResourceKey=SelectedItemImagePadding}">8,0,0,0</Thickness>

    <Thickness x:Key="{dxet:ComboBoxEditThemeKey ResourceKey=SelectedItemContentPadding}">8,10,8,10</Thickness>

    <Color x:Key="{dxet:SpinEditThemeKey ResourceKey=ArrowBrushColor}">#bfffffff</Color>

    <SolidColorBrush x:Key="{dxet:ButtonsThemeKey ResourceKey=Foreground}" Color="#bfffffff"/>

</ResourceDictionary>   