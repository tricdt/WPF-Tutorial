    <ControlTemplate x:Key="{dxet:TrackBarEditThumbThemeKey ResourceKey=HorizontalThumbTickPlacementTopLeftTemplate}" TargetType="{x:Type Thumb}">
        <Grid Height="32" Width="15">
            <Border Background="#FF333333"/>
            <Grid Margin="1">
                <Border x:Name="DefaultState" Background="{StaticResource TrackBarEditThumb.DefaultState}"/>
                <Border x:Name="HoverState"
                        Background="{StaticResource TrackBarEditThumb.HoverState}"
                        Opacity="0"/>
                <Border x:Name="PressedState"
                        Background="{StaticResource TrackBarEditThumb.PressedState}"
                        Opacity="0"/>
                <Border x:Name="DisabledState"
                        Background="{StaticResource TrackBarEditThumb.DisabledState}"
                        Opacity="0"/>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity"
                        TargetName="DefaultState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="0"/>
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="Opacity"
                        TargetName="DefaultState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="0"/>
            </Trigger>
            <Trigger Property="IsDragging" Value="True">
                <Setter Property="Opacity"
                        TargetName="DefaultState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="PressedState"
                        Value="1"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity"
                        TargetName="DefaultState"
                        Value="0"/>
                <Setter Property="Opacity"
                        TargetName="DisabledState"
                        Value="1"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

