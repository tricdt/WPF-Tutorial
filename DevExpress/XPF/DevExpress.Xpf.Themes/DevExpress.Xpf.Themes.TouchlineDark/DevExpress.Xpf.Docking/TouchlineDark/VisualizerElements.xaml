                                                Background="#FF515151"
                                                cs:Name="Border_0035">
                                            <StackPanel HorizontalAlignment="Left"
                                                        Margin="0,16,24,16"
                                                        Orientation="Horizontal"
                                                        VerticalAlignment="Center">
                                                <Image HorizontalAlignment="Left"
                                                       Margin="0,0,24,0"
                                                       Source="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionImage}"
                                                       Stretch="None"
                                                       VerticalAlignment="Top"/>
                                                <StackPanel>
                                                    <ContentControl Content="{Binding Path=(local:DockLayoutManager.LayoutItem).Caption}"
                                                                    ContentTemplate="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionTemplate}"
                                                                    ContentTemplateSelector="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionTemplateSelector}"
                                                                    Focusable="False"
                                                                    Foreground="#FFF78A09"
                                                                    HorizontalAlignment="Left"
                                                                    VerticalAlignment="Bottom"/>
                                                    <TextBlock HorizontalAlignment="Left"
                                                               Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Description}"
                                                               TextWrapping="Wrap"
                                                               VerticalAlignment="Top"/>
                                                </StackPanel>
                                            </StackPanel>
                                        </Border>
                                        <Border x:Name="Content"
                                                Background="#FF333333"
                                                cs:Name="Border_0036"
                                                Grid.Row="1">
                                            <ContentPresenter x:Name="PART_ContainerContent"
                                                              Content="{Binding}"
                                                              Margin="24"/>
                                        </Border>
                                        <Border x:Name="Footer"
                                                Background="#FF515151"
                                                cs:Name="Border_0037"
                                                Grid.Row="2"
                                                MinHeight="48">
                                            <ContentPresenter Content="{Binding Path=(local:DockLayoutManager.LayoutItem).FooterDescription}"
                                                              HorizontalAlignment="Left"
                                                              Margin="24,0,0,0"
                                                              VerticalAlignment="Center"/>
                                        </Border>
                                    </Grid>
                                </Border>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <ControlTemplate x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=ListBoxItem}" TargetType="{x:Type ListBoxItem}">
        <Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Border Background="#FF666666" Margin="0,0,0,1"/>
            </Grid>
            <Grid x:Name="SelectedState" Opacity="0">
                <Border Background="#FFF78A09" Margin="0,0,0,1"/>
            </Grid>
            <Border Padding="{TemplateBinding Padding}">
                <ve:TemplatedCaptionControl x:Name="caption"
                                            LayoutItem="{Binding}"
                                            Margin="0,0,0,1"/>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Opacity"
                        TargetName="SelectedState"
                        Value="1"/>
                <Setter Property="Foreground"
                        TargetName="caption"
                        Value="#FF000000"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity"
                        TargetName="HoverState"
                        Value="1"/>
                <Setter Property="Foreground"
                        TargetName="caption"
                        Value="#FF000000"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <DataTemplate x:Key="{dxt:DockPaneElementsThemeKey ResourceKey=ContentPreview}">
        <Border Background="#FF333333"
                BorderBrush="#FFF78A09"
                BorderThickness="1"
                cs:Name="Border_0041"
                Padding="12">
            <ContentPresenter Content="{TemplateBinding Content}"/>
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="{dxt:DocumentElementsThemeKey ResourceKey=ContentPreview}">
        <Border Background="#FF333333"
                BorderBrush="#FFF78A09"
                BorderThickness="1"
                cs:Name="Border_0043"
                Padding="1">
            <ContentPresenter Content="{TemplateBinding Content}"/>
        </Border>
    </DataTemplate>

    <!--END WPF-->

</ResourceDictionary>