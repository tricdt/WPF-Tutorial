    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=VerticalHeaderNormalTemplate}" TargetType="{x:Type dxschi:VerticalHeaderControl}">
