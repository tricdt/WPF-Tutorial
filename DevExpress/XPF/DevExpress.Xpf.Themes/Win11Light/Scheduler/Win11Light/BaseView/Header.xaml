  <ControlTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=VerticalWeekCellAlternateTemplate}" TargetType="{x:Type dxschi:VisualDateCell}">
    <Grid x:Name="Root" Background="Transparent" cs:id="Grid_0000">
