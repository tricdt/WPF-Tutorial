  <!-- General -->
  <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderNormalTemplate}" TargetType="{x:Type dxschi:HeaderControl}">
    <Border>
      <Border.Resources>
        <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=TemplateCore}" TargetType="{x:Type ContentControl}">
          <dxschi:HeaderBorder Background="#FFFFFFFF" BorderBrush="#FFD0D0D0" DefaultBorderThickness="1" ElementPosition="{Binding Path=(dxschi:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" cs:id="HeaderBorder_0001" x:Uid="@Background=Button.SimpleButtonBackground;BorderBrush=Border">
