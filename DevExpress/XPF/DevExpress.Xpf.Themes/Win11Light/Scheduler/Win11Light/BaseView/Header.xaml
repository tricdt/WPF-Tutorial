      <dxschi:MultiColorBorder Background="{TemplateBinding Background}" DefaultBorderThickness="1,0,1,1" ElementPosition="{Binding Path=(dxschi:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" Grid.Row="0" Grid.RowSpan="2" InnerBorderBrush="{TemplateBinding BorderBrush}" OuterBorderBrush="#FFD0D0D0" cs:id="MultiColorBorder_0000" x:Uid="@OuterBorderBrush=Border" />
