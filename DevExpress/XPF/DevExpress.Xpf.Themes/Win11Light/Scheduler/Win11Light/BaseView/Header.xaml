          </dxschi:HeaderBorder>
        </ControlTemplate>
      </Border.Resources>
      <ContentControl Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" dxschi:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschi:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" Template="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=TemplateCore}}" />
