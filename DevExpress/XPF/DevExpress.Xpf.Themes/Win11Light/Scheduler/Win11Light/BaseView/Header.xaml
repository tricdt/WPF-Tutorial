      <dxschi:CellContent x:Name="PART_CONTENT" Background="Transparent" dxsch:SchedulerControl.HitTestType="Cell" ElementPosition="{Binding Path=(dxschi:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" Grid.Row="1" IsHitTestVisible="True" cs:id="CellContent_0000" />
