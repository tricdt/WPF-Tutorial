<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxdiag="http://schemas.devexpress.com/winfx/2008/xaml/diagram" xmlns:dxdiagn="http://schemas.devexpress.com/winfx/2008/xaml/diagram/internal" xmlns:dxdiagt="http://schemas.devexpress.com/winfx/2008/xaml/diagram/themekeys" xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <ResourceDictionary.MergedDictionaries>
    <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Win11Light.v24.1;component/Diagram/Win11Light/UICore.xaml" />
  </ResourceDictionary.MergedDictionaries>
  <!-- General -->
  <dxmvvm:ObjectToObjectConverter x:Key="IdToMenuPointerVisibilityConverter" DefaultTarget="Collapsed">
    <dxmvvm:MapItem Source="MoreShapes" Target="Visible" />
  </dxmvvm:ObjectToObjectConverter>
  <dxdiagn:ShapeToolTipConverter x:Key="ToolTipConverter" />
  <ControlTemplate x:Key="{dxdiagt:DiagramControlThemeKeys ResourceKey=PageBackgroundTemplate}" TargetType="ContentControl">
    <Border Background="{TemplateBinding Background}" BorderBrush="#FFD0D0D0" BorderThickness="{TemplateBinding BorderThickness}" cs:id="Border_0000" x:Uid="@BorderBrush=Border">
      <ContentPresenter ClipToBounds="True" />
    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxdiagt:DiagramControlThemeKeys ResourceKey=BackgroundTemplate}" TargetType="ContentControl">
    <Border Background="{TemplateBinding Background}">
      <ContentPresenter />
    </Border>
  </ControlTemplate>
  <DataTemplate x:Key="{dxdiagt:DiagramToolboxControlThemeKey ResourceKey=StencilsSelectorItemTemplate}">
    <Grid>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>
      <TextBlock x:Name="stencilName" Foreground="{DynamicResource {dxdiagt:DiagramToolboxControlThemeKey ResourceKey=StencilsSelectorForeground}}" Margin="0,1,3,1" Text="{Binding Name}" Visibility="{Binding IsCompact, Converter={dxmvvm:BooleanToVisibilityConverter Inverse=True}, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramToolboxControl}}}" />
      <Image Grid.ColumnSpan="2" Height="16" HorizontalAlignment="Center" Margin="-2,1,0,1" Source="{dxdiag:DiagramSvgImage Size='16,16', Path=Images/Svg/CompactView.svg}" Visibility="{Binding IsCompact, Converter={dxmvvm:BooleanToVisibilityConverter}, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramToolboxControl}}}" Width="16" />
      <Path Data="F1 M5,10z M0,0z M0.4375,9.6875C0.335938,9.6875 0.248047,9.65039 0.173828,9.57617 0.0996097,9.50195 0.0625004,9.41406 0.0625004,9.3125 0.0625004,9.21094 0.0996097,9.12305 0.173828,9.04883L4.0293,5.1875 0.173828,1.32617C0.0996094,1.25195 0.0625,1.16406 0.0625,1.0625 0.0625,0.960938 0.0996094,0.873047 0.173828,0.798828 0.248047,0.724609 0.335937,0.6875 0.4375,0.6875 0.539062,0.6875 0.626953,0.724609 0.701172,0.798828L4.82617,4.92383C4.90039,4.99805 4.9375,5.08594 4.9375,5.1875 4.9375,5.28906 4.90039,5.37695 4.82617,5.45117L0.701172,9.57617C0.626953,9.65039,0.539063,9.6875,0.4375,9.6875z" Fill="{Binding ElementName=stencilName, Path=Foreground}" Grid.Column="1" HorizontalAlignment="Right" Margin="0,0,6,0" VerticalAlignment="Center" Visibility="{Binding Id, Converter={StaticResource IdToMenuPointerVisibilityConverter}}" />
    </Grid>
  </DataTemplate>
  <Style x:Key="{dxdiagt:DiagramToolboxControlThemeKey ResourceKey=StencilsSelectorItemStyle}" TargetType="ListBoxItem">
    <Setter Property="FocusVisualStyle" Value="{x:Null}" />
    <Setter Property="Margin" Value="0,0,1,1" />
  </Style>
  <Style x:Key="{dxdiagt:DiagramToolboxControlThemeKey ResourceKey=ShapesSelectorItemStyle}" TargetType="ListBoxItem">
    <Setter Property="FocusVisualStyle" Value="{x:Null}" />
    <Setter Property="Margin" Value="0,0,1,1" />
    <Setter Property="Padding" Value="0" />
    <Setter Property="Tag" Value="{Binding RelativeSource={RelativeSource AncestorType={x:Type dxdiag:DiagramToolboxControl}}}" />
    <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
      <Setter.Value>
        <DataTemplate>
          <ItemsControl>
            <dxdiag:DiagramToolBehavior Diagram="{Binding AssociatedObject.Tag.DiagramControl, RelativeSource={RelativeSource Self}}" DiagramTool="{Binding}" />
          </ItemsControl>
        </DataTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="ToolTip">
      <Setter.Value>
        <MultiBinding Converter="{StaticResource ToolTipConverter}">
          <Binding Path="ToolName" />
          <Binding Path="Tag.IsCompact" RelativeSource="{RelativeSource Self}" />
          <Binding Path="Tag.ShapePreviewMode" RelativeSource="{RelativeSource Self}" />
        </MultiBinding>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>