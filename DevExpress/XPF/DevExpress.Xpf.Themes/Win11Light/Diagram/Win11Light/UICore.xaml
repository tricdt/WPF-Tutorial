<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dxdiagt="http://schemas.devexpress.com/winfx/2008/xaml/diagram/themekeys" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib">
  <ResourceDictionary.MergedDictionaries>
    <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Win11Light.v24.1;component/UICore/UIData.xaml" />
  </ResourceDictionary.MergedDictionaries>
  <!-- Toolbox -->
  <SolidColorBrush x:Key="{dxdiagt:DiagramToolboxControlThemeKey ResourceKey=Background}" Color="#FFF9F9F9" x:Uid="@Color=WindowBackground" />
  <!-- Ruler -->
  <SolidColorBrush x:Key="{dxdiagt:RulerThemeKey ResourceKey=Shadow}" Color="#FF1A1A1A" x:Uid="@Color=Foreground.Primary" />
  <SolidColorBrush x:Key="{dxdiagt:RulerThemeKey ResourceKey=Foreground}" Color="#FF1A1A1A" x:Uid="@Color=Foreground.Primary" />
  <SolidColorBrush x:Key="{dxdiagt:RulerThemeKey ResourceKey=HorizontalBackground}" Color="#FFF9F9F9" x:Uid="@Color=WindowBackground" />
  <SolidColorBrush x:Key="{dxdiagt:RulerThemeKey ResourceKey=VerticalBackground}" Color="#FFF9F9F9" x:Uid="@Color=WindowBackground" />
  <SolidColorBrush x:Key="{dxdiagt:RulerThemeKey ResourceKey=BorderBrush}" Color="#FFD0D0D0" x:Uid="@Color=Border" />
  <SolidColorBrush x:Key="{dxdiagt:RulerThemeKey ResourceKey=TickBrush}" Color="#FFD0D0D0" x:Uid="@Color=Border" />
  <!-- Control -->
  <SolidColorBrush x:Key="{dxdiagt:ControlsThemeKeys ResourceKey=SelectionPreviewBorderBrush}" Color="#0A000000" x:Uid="@Color=ListItem.SelectionBackground" />
  <SolidColorBrush x:Key="{dxdiagt:ControlsThemeKeys ResourceKey=SelectionPreviewBackground}" Color="#0A000000" Opacity="0.38" x:Uid="@Color=ListItem.SelectionBackground" />
  <SolidColorBrush x:Key="{dxdiagt:ControlsThemeKeys ResourceKey=GridLineBrush}" Color="#FFD0D0D0" Opacity="0.38" x:Uid="@Color=Delimiter" />
  <SolidColorBrush x:Key="{dxdiagt:ControlsThemeKeys ResourceKey=DragPreviewBrush}" Color="#0A000000" x:Uid="@Color=ListItem.SelectionBackground" />
  <SolidColorBrush x:Key="{dxdiagt:DiagramControlThemeKeys ResourceKey=DiagramControlBackground}" Color="#FFF9F9F9" x:Uid="@Color=WindowBackground" />
  <SolidColorBrush x:Key="{dxdiagt:DiagramToolboxControlThemeKey ResourceKey=ToolsAreaBackground}" Color="#FFFFFFFF" x:Uid="@Color=Editor.Background" />
  <SolidColorBrush x:Key="{dxdiagt:DiagramToolboxControlThemeKey ResourceKey=StencilsSelectorForeground}" Color="#FF1A1A1A" x:Uid="@Color=Foreground.Primary" />
  <SolidColorBrush x:Key="{dxdiagt:DiagramPanAndZoomControlThemeKeys ResourceKey=ClientFrameBorderBrush}" Color="#FF4D6AB8" />
  <SolidColorBrush x:Key="{dxdiagt:DiagramPanAndZoomControlThemeKeys ResourceKey=PreviewFrameBorderBrush}" Color="#FFA0A0A0" />
  <SolidColorBrush x:Key="{dxdiagt:DiagramPanAndZoomControlThemeKeys ResourceKey=PresenterBackground}" Color="Transparent" />
  <SolidColorBrush x:Key="{dxdiagt:DiagramPanAndZoomControlThemeKeys ResourceKey=ZoomTrackBarBackground}" Color="Transparent" />
  <sys:Double x:Key="{dxdiagt:RulerThemeKey ResourceKey=Size}">21</sys:Double>
  <sys:Double x:Key="{dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FontFamilyEditorWidth}">100</sys:Double>
  <sys:Double x:Key="{dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FontSizeEditorWidth}">70</sys:Double>
  <Thickness x:Key="{dxdiagt:ControlsThemeKeys ResourceKey=PageBorderThickness}">1</Thickness>
  <Thickness x:Key="{dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=DockLayoutManagerBorderThickness}">0</Thickness>
  <Thickness x:Key="{dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=DiagramControlMargin}">0</Thickness>
</ResourceDictionary>