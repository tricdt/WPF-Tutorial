  <!-- General -->
  <ControlTemplate x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=CategoryHeaderNormalTemplate}">
    <Border Background="Transparent" CornerRadius="2" Height="3" Margin="3,0,3,0" VerticalAlignment="Bottom" cs:id="Border_0002" />
  </ControlTemplate>
  <ControlTemplate x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=CategoryHeaderMouseOverTemplate}">
    <Border x:Name="HoverState" Background="#FFE3E3E3" CornerRadius="2" Height="3" Margin="3,0,3,0" VerticalAlignment="Bottom" cs:id="Border_0002" x:Uid="@Background=Button.SimpleButtonBorder" />
  </ControlTemplate>
  <ControlTemplate x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=CategoryHeaderSelectedTemplate}">
    <Border x:Name="PressedState" Background="#FF005FBA" CornerRadius="2" Height="3" Margin="3,0,3,0" VerticalAlignment="Bottom" cs:id="Border_0002" x:Uid="@Background=Accent" />
  </ControlTemplate>
</ResourceDictionary>