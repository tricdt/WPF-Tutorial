  <!-- Thumb -->
  <ControlTemplate x:Key="VerticalThumbTemplate" TargetType="dxc:AxisScrollBarThumb">
    <Grid x:Name="Root">
      <Grid.RowDefinitions>
        <RowDefinition x:Name="Row1" Height="4" />
        <RowDefinition x:Name="Row2" Height="*" />
        <RowDefinition x:Name="Row3" Height="4" />
      </Grid.RowDefinitions>
      <Rectangle x:Name="CornerSide1" Fill="#FF626262" Grid.Row="0" Height="4" Margin="0,-1,0,0" RadiusX="3" RadiusY="3" Width="{TemplateBinding Width}" cs:id="Rectangle_0000" x:Uid="@Fill=Foreground.Muted" />
      <Rectangle x:Name="IsDefault" Fill="#FF626262" Grid.Row="1" Height="{TemplateBinding Height}" Margin="0,-3,0,-3" MinHeight="{TemplateBinding MinHeight}" MinWidth="{TemplateBinding MinWidth}" Width="{TemplateBinding Width}" cs:id="Rectangle_0002" x:Uid="@Fill=Foreground.Muted" />
      <Rectangle x:Name="CornerSide2" Fill="#FF626262" Grid.Row="2" Height="4" Margin="0,0,0,-2" RadiusX="3" RadiusY="3" Width="{TemplateBinding Width}" cs:id="Rectangle_0001" x:Uid="@Fill=Foreground.Muted" />
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="CommonStates">
          <VisualState x:Name="Normal" />
          <VisualState x:Name="MouseOver">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide1" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF646464" cs:id="ColorAnimation_0000" x:Uid="@To=Foreground.Secondary" />
              <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF646464" cs:id="ColorAnimation_0002" x:Uid="@To=Foreground.Secondary" />
              <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide2" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF646464" cs:id="ColorAnimation_0004" x:Uid="@To=Foreground.Secondary" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Pressed">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide1" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF626262" cs:id="ColorAnimation_0001" x:Uid="@To=Foreground.Muted" />
              <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF626262" cs:id="ColorAnimation_0003" x:Uid="@To=Foreground.Muted" />
              <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide2" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF626262" cs:id="ColorAnimation_0005" x:Uid="@To=Foreground.Muted" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Disabled">
            <Storyboard>
              <DoubleAnimation Duration="0" Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="0.38" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="HorizontalThumbTemplate" TargetType="dxc:AxisScrollBarThumb">
    <Grid x:Name="Root">
      <Grid.ColumnDefinitions>
        <ColumnDefinition x:Name="Column1" Width="4" />
        <ColumnDefinition x:Name="Column2" Width="*" />
        <ColumnDefinition x:Name="Column3" Width="4" />
      </Grid.ColumnDefinitions>
      <Rectangle x:Name="CornerSide1" Fill="#FF626262" Grid.Column="0" Height="{TemplateBinding Height}" Margin="-1,0,0,0" RadiusX="3" RadiusY="3" Width="4" cs:id="Rectangle_0000" x:Uid="@Fill=Foreground.Muted" />
      <Rectangle x:Name="IsDefault" Fill="#FF626262" Grid.Column="1" Height="{TemplateBinding Height}" Margin="-3,0,-3,0" MinHeight="{TemplateBinding MinHeight}" MinWidth="{TemplateBinding MinWidth}" cs:id="Rectangle_0002" x:Uid="@Fill=Foreground.Muted" />
      <Rectangle x:Name="CornerSide2" Fill="#FF626262" Grid.Column="2" Height="{TemplateBinding Height}" Margin="0,0,-2,0" RadiusX="3" RadiusY="3 " Width="4" cs:id="Rectangle_0001" x:Uid="@Fill=Foreground.Muted" />
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="CommonStates">
          <VisualState x:Name="Normal" />
          <VisualState x:Name="MouseOver">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide1" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF646464" cs:id="ColorAnimation_0000" x:Uid="@To=Foreground.Secondary" />
              <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF646464" cs:id="ColorAnimation_0002" x:Uid="@To=Foreground.Secondary" />
              <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide2" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF646464" cs:id="ColorAnimation_0004" x:Uid="@To=Foreground.Secondary" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Pressed">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide1" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF626262" cs:id="ColorAnimation_0001" x:Uid="@To=Foreground.Muted" />
              <ColorAnimation Duration="0" Storyboard.TargetName="IsDefault" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF626262" cs:id="ColorAnimation_0003" x:Uid="@To=Foreground.Muted" />
              <ColorAnimation Duration="0" Storyboard.TargetName="CornerSide2" Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FF626262" cs:id="ColorAnimation_0005" x:Uid="@To=Foreground.Muted" />
