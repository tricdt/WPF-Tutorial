      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="HorizontalThumbResizerTemplate" TargetType="dxc:AxisScrollBarThumbResizer">
    <Grid Background="Transparent" cs:id="Grid_0000">
      <Border Background="#FFFFFFFF" CornerRadius="2" Height="4" Width="1" cs:id="Border_0000" x:Uid="@Background=Foreground.Alt" />
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="VerticalThumbResizerTemplate" TargetType="dxc:AxisScrollBarThumbResizer">
    <Grid Background="Transparent" cs:id="Grid_0000">
      <Border Background="#FFFFFFFF" CornerRadius="2" Height="1" Width="4" cs:id="Border_0000" x:Uid="@Background=Foreground.Alt" />
    </Grid>
  </ControlTemplate>
  <!-- Scroll -->
  <ControlTemplate x:Key="{dxct:ChartControlThemeKey ResourceKey=AxisScrollBarTemplate}" TargetType="dxc:AxisScrollBar">
    <Grid x:Name="Root">
      <Grid x:Name="PART_HorizontalRoot">
        <dxc:ScrollBarBorder x:Name="PART_HorizontalBorder" Annotations="{TemplateBinding Annotations}" Background="#FFFFFFFF" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" cs:id="Border_0000" x:Uid="@Background=Editor.Background" />
        <Border Background="Transparent" Opacity="0" cs:id="Border_0000" />
        <Grid x:Name="PART_HorizontalContainer">
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="Auto" />
          </Grid.ColumnDefinitions>
          <dxc:ScrollBarRepeatButton x:Name="PART_HorizontalSmallDecrease" AutomationProperties.Name="HorizontalScrollbarSmallDecrement" Grid.Column="0" Interval="50" IsTabStop="False" Margin="0" Padding="4,0,4,0" Template="{StaticResource HorizontalDecrementTemplate}" Width="16" />
          <dxc:ScrollBarRepeatButton x:Name="PART_HorizontalLargeDecrease" AutomationProperties.Name="HorizontalScrollbarLargeDecrement" Grid.Column="1" Interval="50" IsTabStop="False" Padding="4,0,4,0" Template="{StaticResource RepeatButtonTemplate}" />
          <dxc:AxisScrollBarThumb x:Name="PART_HorizontalThumb" Grid.Column="2" Height="6" Margin="0,1,0,1" MinWidth="22" Template="{StaticResource HorizontalThumbTemplate}" />
          <dxc:AxisScrollBarThumbResizer x:Name="PART_HorizontalThumbDecrease" Cursor="SizeWE" Grid.Column="2" HorizontalAlignment="Left" MinWidth="4" Template="{StaticResource HorizontalThumbResizerTemplate}" />
          <dxc:AxisScrollBarThumbResizer x:Name="PART_HorizontalThumbIncrease" Cursor="SizeWE" Grid.Column="2" HorizontalAlignment="Right" MinWidth="4" Template="{StaticResource HorizontalThumbResizerTemplate}" />
          <dxc:ScrollBarRepeatButton x:Name="PART_HorizontalLargeIncrease" AutomationProperties.Name="HorizontalScrollbarLargeIncrement" Grid.Column="3" Interval="50" IsTabStop="False" Padding="4,0,4,0" Template="{StaticResource RepeatButtonTemplate}" />
          <dxc:ScrollBarRepeatButton x:Name="PART_HorizontalSmallIncrease" AutomationProperties.Name="HorizontalScrollbarSmallIncrement" Grid.Column="4" Interval="50" IsTabStop="False" Margin="0" Padding="4,0,4,0" Template="{StaticResource HorizontalIncrementTemplate}" Width="16" />
        </Grid>
      </Grid>
      <Grid x:Name="PART_VerticalRoot" Visibility="Collapsed">
        <dxc:ScrollBarBorder x:Name="PART_VerticalBorder" Annotations="{TemplateBinding Annotations}" Background="#FFFFFFFF" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" cs:id="Border_0001" x:Uid="@Background=Editor.Background" />
        <Border Background="Transparent" Opacity="0" cs:id="Border_0001" />
        <Grid x:Name="PART_VerticalContainer">
          <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
          </Grid.RowDefinitions>
          <dxc:ScrollBarRepeatButton x:Name="PART_VerticalSmallIncrease" AutomationProperties.Name="VerticalScrollbarSmallIncrement" Grid.Row="0" Height="16" Interval="50" IsTabStop="False" Margin="0" Padding="0,4,0,4" Template="{StaticResource VerticalIncrementTemplate}" />
          <dxc:ScrollBarRepeatButton x:Name="PART_VerticalLargeIncrease" AutomationProperties.Name="VerticalScrollbarLargeIncrement" Grid.Row="1" Interval="50" IsTabStop="False" Margin="0,0,1,0" Padding="0,4,0,4" Template="{StaticResource RepeatButtonTemplate}" />
          <dxc:AxisScrollBarThumb x:Name="PART_VerticalThumb" Grid.Row="2" Margin="1,0,1,0" MinHeight="22" Template="{StaticResource VerticalThumbTemplate}" Width="6" />
          <dxc:AxisScrollBarThumbResizer x:Name="PART_VerticalThumbIncrease" Cursor="SizeNS" Grid.Row="2" MinHeight="4" Template="{StaticResource VerticalThumbResizerTemplate}" VerticalAlignment="Top" />
          <dxc:AxisScrollBarThumbResizer x:Name="PART_VerticalThumbDecrease" Cursor="SizeNS" Grid.Row="2" MinHeight="4" Template="{StaticResource VerticalThumbResizerTemplate}" VerticalAlignment="Bottom" />
          <dxc:ScrollBarRepeatButton x:Name="PART_VerticalLargeDecrease" AutomationProperties.Name="VerticalScrollbarLargeDecrement" Grid.Row="3" Interval="50" IsTabStop="False" Margin="0,0,1,0" Padding="0,4,0,4" Template="{StaticResource RepeatButtonTemplate}" />
          <dxc:ScrollBarRepeatButton x:Name="PART_VerticalSmallDecrease" AutomationProperties.Name="VerticalScrollbarSmallDecrement" Grid.Row="4" Height="16" Interval="50" IsTabStop="False" Margin="0" Padding="0,4,0,4" Template="{StaticResource VerticalDecrementTemplate}" />
        </Grid>
      </Grid>
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="CommonStates">
          <VisualState x:Name="Normal" />
          <VisualState x:Name="MouseOver" />
          <VisualState x:Name="Disabled">
            <Storyboard>
              <DoubleAnimation Duration="0" Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="0.5" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
</ResourceDictionary>