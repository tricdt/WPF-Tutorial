        <DataTemplate>
          <Border Background="#FF444444" BorderBrush="#30000000" BorderThickness="0,1,0,1" Height="3" Margin="1,0,1,0" Width="32" />
        </DataTemplate>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>