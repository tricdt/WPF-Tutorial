<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="{dx:SchedulingResource Generic/DefaultUIPreset.xaml}"/>
    </ResourceDictionary.MergedDictionaries>

    <Thickness x:Key="AppointmentControl.BorderThickness">1</Thickness>

    <Thickness x:Key="AppointmentControl.StatusContainer.Margin">0,0,-2,0</Thickness>

    <Thickness x:Key="HeaderControl.Padding">5,3,5,3</Thickness>

    <sys:Boolean x:Key="DateHeaderUseUpperCase">False</sys:Boolean>

    <sys:Double x:Key="GroupSeparatorSize">0</sys:Double>

    <Thickness x:Key="HeaderControl.BorderThickness">1,0,1,0</Thickness>

    <Thickness x:Key="DateHeaderControl.TimelineView.BorderThickness.NewStyle">1,0,1,0</Thickness>

    <GridLength x:Key="DateHeaderControl.TimelineView.VerticalBorderSize.NewStyle">1*</GridLength>

    <CornerRadius x:Key="NavigationButton.Backward.CornerRadius">16</CornerRadius>

    <CornerRadius x:Key="NavigationButton.Forward.CornerRadius">16</CornerRadius>

    <CornerRadius x:Key="ResourceNavigatorButton.CornerRadius">2</CornerRadius>

    <Thickness x:Key="ResourceNavigatorButton.BorderThickness">1</Thickness>

    <sys:Double x:Key="ResourceNavigatorButton.Width">18</sys:Double>

    <Thickness x:Key="ResourceNavigator.Padding">0,1,0,2</Thickness>

    <sys:Double x:Key="ResourceNavigator.MinSize">18</sys:Double>

    <CornerRadius x:Key="Scheduler.CornerRadius">4</CornerRadius>

</ResourceDictionary>