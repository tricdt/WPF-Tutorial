<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduling"
                    xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduling/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="{dx:SchedulingResource Generic/DefaultUICore.xaml}"/>
    </ResourceDictionary.MergedDictionaries>

    <Color x:Key="BrushSet.Resource1.Color">#F0F9E5</Color>

    <Color x:Key="BrushSet.Resource2.Color">#EDF7F9</Color>

    <Color x:Key="BrushSet.Resource3.Color">#FEF1E6</Color>

    <Color x:Key="BrushSet.Resource4.Color">#F7F1FF</Color>

    <Color x:Key="BrushSet.Resource5.Color">#E4FEEA</Color>

    <Color x:Key="BrushSet.Resource6.Color">#FFEFF7</Color>

    <Color x:Key="BrushSet.Resource7.Color">#F7F4EB</Color>

    <Color x:Key="BrushSet.Resource8.Color">#FFF1EF</Color>

    <Color x:Key="BrushSet.Resource9.Color">#E6FAFC</Color>

    <SolidColorBrush x:Key="BrushSet.LabelNone" Color="Transparent"/>

    <SolidColorBrush x:Key="BrushSet.LabelImportant" Color="#FFFFAFB3"/>

    <SolidColorBrush x:Key="BrushSet.LabelBusiness" Color="#FFAFCAFF"/>

    <SolidColorBrush x:Key="BrushSet.LabelPersonal" Color="#FFC4F7C1"/>

    <SolidColorBrush x:Key="BrushSet.LabelVacation" Color="#FFFFD5AF"/>

    <SolidColorBrush x:Key="BrushSet.LabelMustAttend" Color="#FFFFC5AF"/>

    <SolidColorBrush x:Key="BrushSet.LabelTravelRequired" Color="#FF8FE2FA"/>

    <SolidColorBrush x:Key="BrushSet.LabelNeedsPreparation" Color="#FFD2E186"/>

    <SolidColorBrush x:Key="BrushSet.LabelBirthday" Color="#FFEBB5D9"/>

    <SolidColorBrush x:Key="BrushSet.LabelAnniversary" Color="#FF8CE4C6"/>

    <SolidColorBrush x:Key="BrushSet.LabelPhoneCall" Color="#FFF7DD93"/>

    <SolidColorBrush x:Key="CellInplaceEditorControl.Background" Color="#FF6589BD"/>

    <SolidColorBrush x:Key="ResourceForeground" Color="Black"/>

    <SolidColorBrush x:Key="DefaultHeaderForeground" Color="Black"/>

    <SolidColorBrush x:Key="DefaultLabelColor" Color="White"/>

    <SolidColorBrush x:Key="LightCellBackground" Color="#ffffff"/>

    <SolidColorBrush x:Key="LightCellLightBorder" Color="#ececec"/>

    <SolidColorBrush x:Key="LightCellDarkBorder" Color="#cbcbcb"/>

    <SolidColorBrush x:Key="DarkCellBackground" Color="#f1f1f1"/>

    <SolidColorBrush x:Key="DarkCellLightBorder" Color="#e1e1e1"/>

    <SolidColorBrush x:Key="DarkCellDarkBorder" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="Transparent" Color="Transparent"/>

    <SolidColorBrush x:Key="AppointmentControl.ResizeGlyph.Background" Color="#ffffff"/>

    <SolidColorBrush x:Key="AppointmentControl.Foreground" Color="#FF000000"/>

    <SolidColorBrush x:Key="HeaderControl.Foreground" Color="#d7d7d7"/>

    <SolidColorBrush x:Key="HeaderControl.TodayForeground" Color="#d7d7d7"/>

    <SolidColorBrush x:Key="CellControl.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="TimeRulerCellControl.Foreground" Color="#8b8989"/>

    <SolidColorBrush x:Key="TimeRulerHeaderControl.Foreground" Color="#8b8989"/>

    <SolidColorBrush x:Key="MoreButton.Normal.Foreground" Color="#ffffff"/>

    <SolidColorBrush x:Key="MoreButton.Hover.Foreground" Color="#ffffff"/>

    <SolidColorBrush x:Key="MoreButton.Pressed.Foreground" Color="#ffffff"/>

    <SolidColorBrush x:Key="NavigationButton.Normal.Foreground" Color="#ffffff"/>

    <SolidColorBrush x:Key="NavigationButton.Hover.Foreground" Color="#ffffff"/>

    <SolidColorBrush x:Key="NavigationButton.Pressed.Foreground" Color="#ffffff"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.Normal.Foreground" Color="#656565"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.Hover.Foreground" Color="#656565"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.Pressed.Foreground" Color="#656565"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.Normal.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.Hover.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.Pressed.Foreground" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="AllDayAreaCorner.Background" Color="#f1f1f1"/>

    <SolidColorBrush x:Key="AllDayAreaCorner.BorderBrush" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="CellControl.SelectionBackground"
                     Color="#bad8fc"
                     Opacity="0.5"/>

    <SolidColorBrush x:Key="MonthCellControl.TodayBackground" Color="#9e9e9e"/>

    <!-- AppointmentControl -->

    <SolidColorBrush x:Key="AppointmentControl.StatusContainer.Background" Color="#ffffff"/>

    <SolidColorBrush x:Key="AppointmentControl.StatusContainer.BorderBrush" Color="#cfcfcf"/>

    <SolidColorBrush x:Key="AppointmentControl.BorderBrush" Color="#cfcfcf"/>

    <SolidColorBrush x:Key="AppointmentControl.SelectedBorderBrush" Color="#6e6e6e"/>

    <!-- Separators -->

    <SolidColorBrush x:Key="GroupSeparator.BorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="GroupSeparator.Background" Color="#FFFFFF"/>

    <SolidColorBrush x:Key="AllDaySeparator.BorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="AllDaySeparator.Background" Color="#FFFFFF"/>

    <!-- HeaderControl -->

    <SolidColorBrush x:Key="HeaderControl.BorderBrush" Color="#a1a1a1"/>

    <SolidColorBrush x:Key="HeaderControl.Background" Color="#6e6e6e"/>

    <SolidColorBrush x:Key="HeaderControl.TodayBackground" Color="#9e9e9e"/>

    <!-- Buttons & ResourceNavigator -->

    <SolidColorBrush x:Key="MoreButton.NormalBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="MoreButton.NormalBackground" Color="#FFF0F0F0"/>

    <SolidColorBrush x:Key="MoreButton.HoverBackground" Color="#FFD3D3D3"/>

    <SolidColorBrush x:Key="MoreButton.HoverBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="MoreButton.PressedBackground" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="MoreButton.PressedBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="MoreButton.DisabledBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="MoreButton.DisabledBackground" Color="#FFF0F0F0"/>

    <SolidColorBrush x:Key="NavigationButton.NormalBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="NavigationButton.NormalBackground"
                     Color="#FD6E6E6E"
                     Opacity="0.5"/>

    <SolidColorBrush x:Key="NavigationButton.HoverBackground"
                     Color="#FD6E6E6E"
                     Opacity="0.75"/>

    <SolidColorBrush x:Key="NavigationButton.HoverBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="NavigationButton.PressedBackground"
                     Color="#FD6E6E6E"
                     Opacity="1"/>

    <SolidColorBrush x:Key="NavigationButton.PressedBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="NavigationButton.DisabledBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="NavigationButton.DisabledBackground"
                     Color="#FD6E6E6E"
                     Opacity="0.175"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.NormalBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.NormalBackground" Color="Transparent"/>

    <LinearGradientBrush x:Key="ResourceNavigatorButton.HoverBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFFFDF3" Offset="0"/>
        <GradientStop Color="#FFC28F36" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ResourceNavigatorButton.HoverBorderBrush"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF9B9B9B" Offset="0"/>
        <GradientStop Color="#FF7E7E7E" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ResourceNavigatorButton.PressedBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFB8B8B8" Offset="0"/>
        <GradientStop Color="#FFA7A7A7" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ResourceNavigatorButton.PressedBorderBrush"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF8F8F8F" Offset="0"/>
        <GradientStop Color="#FF707070" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ResourceNavigatorButton.DisabledBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="ResourceNavigatorButton.DisabledBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="ResourceNavigator.BorderBrush" Color="#c5c5c5"/>

    <SolidColorBrush x:Key="ResourceNavigator.Background" Color="Transparent"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.NormalBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.NormalBackground" Color="#FFF0F0F0"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.HoverBackground" Color="#FFD3D3D3"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.HoverBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.PressedBackground" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.PressedBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.DisabledBorderBrush" Color="#FFA2A2A2"/>

    <SolidColorBrush x:Key="DateNavigationPanelButton.DisabledBackground" Color="#FFF0F0F0"/>

    <!-- TimeRuler -->

    <SolidColorBrush x:Key="TimeIndicatorControl.BorderBrush" Color="#6fa2dd"/>

    <SolidColorBrush x:Key="TimeIndicatorControl.Background" Color="#bdd9f8"/>

    <SolidColorBrush x:Key="TimeMarkerControl.BorderBrush" Color="#6fa2dd"/>

    <SolidColorBrush x:Key="TimeMarkerControl.Background" Color="#bdd9f8"/>

    <SolidColorBrush x:Key="TimeRulerCellControl.Delimiter.Long.Background" Color="#c5c5c5"/>

    <SolidColorBrush x:Key="TimeRulerCellControl.Delimiter.Short.Background" Color="#c5c5c5"/>

    <SolidColorBrush x:Key="TimeRulerHeaderSeparator.BorderBrush" Color="#c5c5c5"/>

    <!-- Scheduler -->

    <SolidColorBrush x:Key="Scheduler.Background" Color="#ededed"/>

    <SolidColorBrush x:Key="Scheduler.Foreground" Color="#000000"/>

    <SolidColorBrush x:Key="Scheduler.Background.Legacy" Color="#ededed"/>

    <SolidColorBrush x:Key="Scheduler.BorderBrush" Color="#6e6e6e"/>

    <SolidColorBrush x:Key="Scheduler.Corner.Background" Color="#ededed"/>

</ResourceDictionary>