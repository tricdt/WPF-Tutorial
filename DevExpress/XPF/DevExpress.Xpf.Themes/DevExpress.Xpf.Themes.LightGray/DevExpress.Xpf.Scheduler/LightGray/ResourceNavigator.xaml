            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxscht:ResourceNavigatorThemeKey ResourceKey=VerticalResourceNavigatorTemplate}" TargetType="{x:Type dxschd:ResourceNavigatorControl}">
        <Grid x:Name="vert_navigator">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <ScrollBar Grid.Column="0"
                       Grid.Row="0"
                       IsEnabled="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ScrollBarEnabled}"
                       LargeChange="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ResourcesPerPage}"
                       Maximum="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Maximum}"
                       Minimum="0"
                       Orientation="Vertical"
                       SmallChange="1"
                       Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=FirstVisibleResourceIndex, Mode=TwoWay}"
                       ViewportSize="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ViewportSize}">
            </ScrollBar>
            <Border BorderBrush="#FFC5C5C5"
                    BorderThickness="1,1,0,1"
                    Grid.Row="1">
                <Border.Background>
                    <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                        <GradientStop Color="#FFF0F0F0" Offset="0"/>
                        <GradientStop Color="#FFDEDEDE" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
                <StackPanel Grid.Row="1"
                            HorizontalAlignment="Stretch"
                            SnapsToDevicePixels="True"
                            UseLayoutRounding="True">
                    <Button x:Name="first_vert"
                            Command="{TemplateBinding CommandFirst}"
                            CommandParameter="{TemplateBinding SchedulerControl}"
                            MinHeight="20"
                            Padding="0"
                            Style="{TemplateBinding Property=ButtonFirstStyle}"
                            Visibility="{Binding SchedulerControl.ResourceNavigator.ButtonFirst.Visible, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent} }">
                        <ToolTipService.ToolTip>
                            <ToolTip Content="{Binding Source={StaticResource SchedulerStringIdConverter}, ConverterParameter=Caption_FirstVisibleResources, Converter={StaticResource SchedulerStringIdConverter}}"/>
                        </ToolTipService.ToolTip>
                        <Grid x:Name="first_vert_glyph"
                              Height="10"
                              HorizontalAlignment="Center"
                              VerticalAlignment="Center"
                              Width="8">
                            <Path Data="F1 M 1468,936.999L 1472,933L 1468,933L 1468,932L 1476,932L 1476,933L 1472,933L 1476,936.999L 1472,936.999L 1476,940.999L 1468,940.999L 1472,936.999L 1468,936.999 Z "
                                  Fill="White"
                                  Height="9"
                                  Margin="0,1,0,0"
                                  Opacity="0.35"
                                  Stretch="Fill"
