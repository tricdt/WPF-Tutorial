                              Width="4">
                            <Path Data="F1 M 1423,919L 1427,923L 1423,927L 1423,919 Z "
                                  Fill="White"
                                  Height="8"
                                  Margin="0,1,0,0"
                                  Opacity="0.35"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="4"/>
                            <Path Data="F1 M 1423,918L 1427,922L 1423,926L 1423,918 Z "
                                  Fill="#FF656565"
                                  Height="8"
                                  Margin="0,0,0,1"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="4"/>
                        </Grid>
                    </Button>
                    <Button x:Name="next"
                            Command="{TemplateBinding CommandNextPage}"
                            CommandParameter="{TemplateBinding SchedulerControl}"
                            MinWidth="20"
                            Padding="0"
                            Style="{TemplateBinding Property=ButtonNextPageStyle}"
                            Visibility="{Binding SchedulerControl.ResourceNavigator.ButtonNextPage.Visible, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent} }">
                        <ToolTipService.ToolTip>
                            <ToolTip Content="{Binding Source={StaticResource SchedulerStringIdConverter}, ConverterParameter=Caption_NextVisibleResourcesPage, Converter={StaticResource SchedulerStringIdConverter}}"/>
                        </ToolTipService.ToolTip>
                        <Grid x:Name="next_glyph"
                              Height="9"
                              HorizontalAlignment="Center"
                              VerticalAlignment="Center"
                              Width="8">
                            <Path Data="F1 M 1406,919L 1410,923L 1406,927L 1406,923L 1402,927L 1402,919L 1406,923L 1406,919 Z "
                                  Fill="White"
                                  Height="8"
                                  Margin="0,1,0,0"
                                  Opacity="0.35"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="8"/>
                            <Path Data="F1 M 1406,918L 1410,922L 1406,926L 1406,922L 1402,926L 1402,918L 1406,922L 1406,918 Z "
                                  Fill="#FF656565"
                                  Height="8"
                                  Margin="0,0,0,1"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="8"/>
                        </Grid>
                    </Button>
                    <Button x:Name="last"
                            Command="{TemplateBinding CommandLast}"
                            CommandParameter="{TemplateBinding SchedulerControl}"
                            MinWidth="20"
                            Padding="0"
                            Style="{TemplateBinding Property=ButtonLastStyle}"
                            Visibility="{Binding SchedulerControl.ResourceNavigator.ButtonLast.Visible, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent} }">
                        <ToolTipService.ToolTip>
                            <ToolTip Content="{Binding Source={StaticResource SchedulerStringIdConverter}, ConverterParameter=Caption_LastVisibleResources, Converter={StaticResource SchedulerStringIdConverter}}"/>
                        </ToolTipService.ToolTip>
                        <Grid x:Name="last_glyph"
                              Height="9"
                              HorizontalAlignment="Center"
                              Margin="1,0,0,0"
                              VerticalAlignment="Center"
                              Width="9">
                            <Path Data="F1 M 1386,919L 1390,923L 1390,919L 1391,919L 1391,927L 1390,927L 1390,923L 1386,927L 1386,923L 1382,927L 1382,919L 1386,923L 1386,919 Z "
                                  Fill="White"
                                  Height="8"
                                  Margin="0,1,0,0"
                                  Opacity="0.35"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="9"/>
                            <Path Data="F1 M 1386,918L 1390,922L 1390,918L 1391,918L 1391,926L 1390,926L 1390,922L 1386,926L 1386,922L 1382,926L 1382,918L 1386,922L 1386,918 Z "
                                  Fill="#FF656565"
                                  Height="8"
                                  Margin="0,0,0,1"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="9"/>
                        </Grid>
                    </Button>
                    <Button x:Name="plus"
                            Command="{TemplateBinding CommandIncCount}"
                            CommandParameter="{TemplateBinding SchedulerControl}"
                            MinWidth="20"
                            Padding="0"
                            Style="{TemplateBinding Property=ButtonIncCountStyle}"
                            Visibility="{Binding SchedulerControl.ResourceNavigator.ButtonIncCount.Visible, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent} }">
                        <ToolTipService.ToolTip>
                            <ToolTip Content="{Binding Source={StaticResource SchedulerStringIdConverter}, ConverterParameter=Caption_IncreaseVisibleResourcesCount, Converter={StaticResource SchedulerStringIdConverter}}"/>
                        </ToolTipService.ToolTip>
