                            <Path Data="F1 M 1314,926L 1310,922L 1314,918L 1314,926 Z "
                                  Fill="#FF656565"
                                  Height="8"
                                  Margin="0,0,0,1"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="4"/>
