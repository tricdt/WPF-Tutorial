                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFF0F0F0" Offset="0"/>
                        <GradientStop Color="#FFDEDEDE" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
                <StackPanel Grid.Column="1"
                            Orientation="Horizontal"
                            VerticalAlignment="Stretch">
                    <Button x:Name="first"
                            Command="{TemplateBinding CommandFirst}"
                            CommandParameter="{TemplateBinding SchedulerControl}"
                            MinWidth="20"
                            Padding="0"
                            Style="{TemplateBinding Property=ButtonFirstStyle}"
                            Visibility="{Binding SchedulerControl.ResourceNavigator.ButtonFirst.Visible, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent} }">
                        <ToolTipService.ToolTip>
                            <ToolTip Content="{Binding Source={StaticResource SchedulerStringIdConverter}, ConverterParameter=Caption_FirstVisibleResources, Converter={StaticResource SchedulerStringIdConverter}}"/>
                        </ToolTipService.ToolTip>
                        <Grid x:Name="first_glyph"
                              Height="9"
                              HorizontalAlignment="Center"
                              Margin="0,0,1,0"
                              VerticalAlignment="Center"
                              Width="9">
                            <Path Data="F1 M 1351,927L 1347,923L 1347,927L 1346,927L 1346,919L 1347,919L 1347,923L 1351,919L 1351,923L 1355,919L 1355,927L 1351,923L 1351,927 Z "
                                  Fill="White"
                                  Height="8"
                                  Margin="0,1,0,0"
                                  Opacity="0.35"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="9"/>
                            <Path Data="F1 M 1351,926L 1347,922L 1347,926L 1346,926L 1346,918L 1347,918L 1347,922L 1351,918L 1351,922L 1355,918L 1355,926L 1351,922L 1351,926 Z "
                                  Fill="#FF656565"
                                  Height="8"
                                  Margin="0,0,0,1"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="9"/>
                        </Grid>
                    </Button>
                    <Button x:Name="prev"
                            Command="{TemplateBinding CommandPrevPage}"
                            CommandParameter="{TemplateBinding SchedulerControl}"
                            MinWidth="20"
                            Padding="0"
                            Style="{TemplateBinding Property=ButtonPrevPageStyle}"
                            Visibility="{Binding SchedulerControl.ResourceNavigator.ButtonPrevPage.Visible, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent} }">
                        <ToolTipService.ToolTip>
                            <ToolTip Content="{Binding Source={StaticResource SchedulerStringIdConverter}, ConverterParameter=Caption_PrevVisibleResourcesPage, Converter={StaticResource SchedulerStringIdConverter}}"/>
                        </ToolTipService.ToolTip>
                        <Grid x:Name="prev_glyph"
                              Height="9"
                              HorizontalAlignment="Center"
                              VerticalAlignment="Center"
                              Width="8">
                            <Path Data="F1 M 1331,927L 1327,923L 1331,919L 1331,923L 1335,919L 1335,927L 1331,923L 1331,927 Z "
                                  Fill="White"
                                  Height="8"
                                  Margin="0,1,0,0"
                                  Opacity="0.35"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="8"/>
                            <Path Data="F1 M 1331,926L 1327,922L 1331,918L 1331,922L 1335,918L 1335,926L 1331,922L 1331,926 Z "
                                  Fill="#FF656565"
                                  Height="8"
                                  Margin="0,0,0,1"
                                  Stretch="Fill"
                                  UseLayoutRounding="False"
                                  Width="8"/>
                        </Grid>
                    </Button>
                    <Button x:Name="left"
                            Command="{TemplateBinding CommandPrev}"
                            CommandParameter="{TemplateBinding SchedulerControl}"
                            MinWidth="20"
                            Padding="0"
                            Style="{TemplateBinding Property=ButtonPrevStyle}"
                            Visibility="{Binding SchedulerControl.ResourceNavigator.ButtonPrev.Visible, Converter={StaticResource boolToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent} }">
