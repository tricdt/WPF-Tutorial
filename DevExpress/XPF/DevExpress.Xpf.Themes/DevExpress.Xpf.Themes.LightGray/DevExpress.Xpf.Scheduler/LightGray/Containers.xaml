    <dxschd:ElementPosition x:Key="{dxscht:TimelineViewThemeKey ResourceKey=BottomLeftCornerGroupByDateElementPosition}">Start,Start,Start,Both</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:TimelineViewThemeKey ResourceKey=ContentElementPosition}">Standalone,End,End,End,Start|NoEnd,NoEnd</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:TimelineViewThemeKey ResourceKey=ResourceHeadersElementPosition}">Start,Standalone,Both,End,NotDefined,NoStart|NoEnd</dxschd:ElementPosition>

    <SolidColorBrush x:Key="{dxscht:SchedulerControlThemeKey ResourceKey=DefaultBorderBrush}" Color="#6E6E6E"/>

    <SolidColorBrush x:Key="{dxscht:WeekViewThemeKey ResourceKey=CellBorderBrush}" Color="White"/>

    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=MoreButtonContainerStyle}" TargetType="{x:Type Canvas}">
        <Setter Property="Width" Value="20"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
    </Style>

    <Thickness x:Key="{dxscht:MonthViewThemeKey ResourceKey=HorizontalWeekAppointmentPanelMargin}" Left="1"/>

    <Thickness x:Key="{dxscht:DayViewThemeKey ResourceKey=VerticalScrollBarMargin}"/>

    <Thickness x:Key="{dxscht:DayViewThemeKey ResourceKey=HorizontalScrollBarMargin}"/>

    <Thickness x:Key="{dxscht:DayViewThemeKey ResourceKey=ResourceNavigatorMargin}"/>

