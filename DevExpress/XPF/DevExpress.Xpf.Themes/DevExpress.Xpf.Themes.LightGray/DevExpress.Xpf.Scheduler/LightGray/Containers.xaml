    <Visibility x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderCornerVisibility}">Collapsed</Visibility>

    <dxschd:ElementPosition x:Key="{dxscht:DayViewThemeKey ResourceKey=TopHeadersElementPosition}">Start,Start,Start,None,NoStart,NotDefined</dxschd:ElementPosition>

