        <Setter Property="PairMargin">
            <Setter.Value>
                <Thickness>0,0,0,0</Thickness>
            </Setter.Value>
        </Setter>
        <Setter Property="VisibleBorderMargin">
            <Setter.Value>
                <Thickness>-1,0,0,0</Thickness>
