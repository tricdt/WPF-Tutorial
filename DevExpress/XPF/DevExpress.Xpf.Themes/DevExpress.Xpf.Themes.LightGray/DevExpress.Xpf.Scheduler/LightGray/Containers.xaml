    <dxschd:ElementPosition x:Key="{dxscht:TimelineViewThemeKey ResourceKey=ScaleTopLevelHeaderGroupByDateDefaultElementPosition}">End,Start,End,Both,None,NotDefined</dxschd:ElementPosition>

    <dxschd:OuterSeparatorPosition x:Key="{dxscht:TimelineViewThemeKey ResourceKey=ScaleTopLevelHeaderGroupByDateRightCornerSeparatorPosition}">None,NotDefined</dxschd:OuterSeparatorPosition>

    <dxschd:ElementPosition x:Key="{dxscht:TimelineViewThemeKey ResourceKey=ScaleOtherLevelHeaderGroupByDateDefaultElementPosition}">End,Middle,End,End,None,NotDefined</dxschd:ElementPosition>

    <dxschd:OuterSeparatorPosition x:Key="{dxscht:TimelineViewThemeKey ResourceKey=ScaleOtherLevelHeaderGroupByDateRightCornerSeparatorPosition}">None,NotDefined</dxschd:OuterSeparatorPosition>

