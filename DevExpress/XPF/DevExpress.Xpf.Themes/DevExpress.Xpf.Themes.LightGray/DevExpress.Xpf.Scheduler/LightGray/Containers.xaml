    <dxschd:ElementPosition x:Key="{dxscht:WeekViewThemeKey ResourceKey=TopLeftCornerGroupByDateElementPosition}">Start,Start,Both,Both</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:WeekViewThemeKey ResourceKey=TopRightCornerGroupByDateElementPosition}">End,Start,Both,Both</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:WeekViewThemeKey ResourceKey=BottomLeftCornerGroupByDateElementPosition}">End,Start,End,Both</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:WeekViewThemeKey ResourceKey=GroupByResourceElementPosition}">Standalone,End,Start,None,NoStart,None</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:WeekViewThemeKey ResourceKey=ResourceHeadersGroupByResourceElementPosition}">Standalone,Start,Start,End,NoStart|End,NotDefined</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:WeekViewThemeKey ResourceKey=TopRightHeaderCornerGroupByResourceElementPosition}">End,Start,End,Both</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:DayViewThemeKey ResourceKey=AllDayAreaElementPosition}">Standalone, Standalone, Start, End, Start, End</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:DayViewThemeKey ResourceKey=TopRightHeaderCornerPosition}">End,Start,Both,Both</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:MonthViewThemeKey ResourceKey=DayOfWeekHeadersGroupByNoneElementPosition}">Start,Start,Start,Both,None,NotDefined</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:MonthViewThemeKey ResourceKey=ResourceHeadersGroupByResourceElementPosition}">Start,Start,Start,Both,None,NotDefined,Start,NotDefined</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:MonthViewThemeKey ResourceKey=DayOfWeekHeadersGroupByResourceElementPosition}">Start,Middle,Start,End,None,NotDefined</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:MonthViewThemeKey ResourceKey=TopRightHeaderCornerGroupByNoneElementPosition}">End,Start,Both,Both</dxschd:ElementPosition>

    <dxschd:ElementPosition x:Key="{dxscht:MonthViewThemeKey ResourceKey=TopRightHeaderCornerGroupByResourceElementPosition}">End,Start,Both,Both</dxschd:ElementPosition>

