    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=TimeRulerHeadersContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Grid VerticalAlignment="Bottom">
                        <dxschd:SharedSizePanel IsBaseSizePanel="False"
                                                Orientation="Vertical"
                                                SharedSizeGroup="AllDayAreaVerticalGroup"
                                                SnapsToDevicePixels="True">
                            <Border Background="#FFEDEDED" CornerRadius="3,0,0,0"/>
                        </dxschd:SharedSizePanel>
                        <ContentPresenter/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=TimeRulersContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Grid>
                        <Border Background="#FFEDEDED"
                                BorderThickness="0"
                                CornerRadius="0,0,0,0"
                                Grid.RowSpan="2"
                                Margin="0,0,0,0"
                                SnapsToDevicePixels="True"
                                UseLayoutRounding="True"/>
                        <ContentPresenter/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=GroupByNoneHeadersStyle}" TargetType="{x:Type ItemsControl}">
        <Setter Property="Margin" Value="0,5,0,4"/>
    </Style>

    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=TopHeadersStyle}" TargetType="{x:Type ItemsControl}">
        <Setter Property="Margin" Value="0,5,0,0"></Setter>
    </Style>

    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=BottomHeadersStyle}" TargetType="{x:Type ItemsControl}">
        <Setter Property="Margin" Value="0,0,0,4"></Setter>
    </Style>

    <Style x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=DayViewHorizontalHeadersContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <ContentPresenter Margin="0,5,0,4"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HorizontalHeadersContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <ContentPresenter Margin="0,5,0,4"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=VerticalHeadersContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <ContentPresenter Margin="5,0,4,0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=VerticalContentContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Grid>
                        <ContentPresenter Content="{TemplateBinding Content}"
                                          ContentTemplate="{TemplateBinding ContentTemplate}"
                                          SnapsToDevicePixels="True"
                                          UseLayoutRounding="True"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=AllDayAreaContentContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Grid>
                        <dxschd:RoundedBorderControl BorderBrush="Transparent"
                                                     CornerRadius="0,3,0,0"
                                                     RoundBorderThickness="0,0,0,0"
                                                     SnapsToDevicePixels="True"
                                                     UseLayoutRounding="True">
                            <ContentPresenter Content="{TemplateBinding Content}"
                                              ContentTemplate="{TemplateBinding ContentTemplate}"
                                              SnapsToDevicePixels="True"
                                              UseLayoutRounding="True"/>
                        </dxschd:RoundedBorderControl>
                        <!--<Border Background="Transparent" BorderBrush="#FF6E6E6E" IsHitTestVisible="False" BorderThickness="1,0,1,0" />						
                        <Rectangle Fill="#FFC5C5C5" Width="1" VerticalAlignment="Stretch" HorizontalAlignment="Left"/>-->
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxscht:WeekViewThemeKey ResourceKey=WeekViewGroupByNoneContentContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxscht:WeekViewThemeKey ResourceKey=WeekViewGroupByResourceContentContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <!--<Grid>
                        <Border BorderThickness="1,1,1,1" BorderBrush="#FF6E6E6E" Grid.RowSpan="2" CornerRadius="0,0,4,4" SnapsToDevicePixels="True" UseLayoutRounding="True" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                        <dxschd:RoundedBorderControl RoundBorderThickness="0,0,1,1" BorderBrush="#FF6E6E6E" CornerRadius="0,0,4,4" SnapsToDevicePixels="True" UseLayoutRounding="True">
                            <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" />
                        </dxschd:RoundedBorderControl>
                    </Grid>-->
                    <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxscht:WeekViewThemeKey ResourceKey=ScrollBarStyle}" TargetType="{x:Type ScrollBar}"></Style>

    <Style x:Key="{dxscht:WeekViewThemeKey ResourceKey=WeekViewGroupByDateContentContainerStyle}" TargetType="{x:Type ContentControl}">
