    <ControlTemplate x:Key="{dxt:CarouselNavigatorThemeKey ResourceKey=NavigatorTemplate}" TargetType="{x:Type local:CarouselNavigator}">
        <Border x:Name="navigator" d:LayoutOverrides="Width, Height">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <RepeatButton Name="FirstItem"
                              Command="dxc:CarouselPanel.FirstItem"
                              CommandTarget="{TemplateBinding Carousel}"
                              Grid.Column="0"
                              Interval="{Binding Path=Carousel.AnimationTime, RelativeSource={RelativeSource TemplatedParent}}"
                              Margin="0,0,1,0">
                    <Grid Height="9"
                          HorizontalAlignment="Center"
                          VerticalAlignment="Center"
                          Width="19">
                        <Path Data="F1 M 588,565L 588,574L 580,570L 580,569L 588,565 Z M 597,565L 597,574L 589,570L 589,569L 597,565 Z "
                              HorizontalAlignment="Right"
                              Margin="0,0,0,0"
                              SnapsToDevicePixels="True"
                              Stretch="Fill"
                              VerticalAlignment="Stretch"
                              Width="17">
                            <Path.Fill>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FF041C43" Offset="0"/>
                                    <GradientStop Color="#FF1B4FA3" Offset="1"/>
                                </LinearGradientBrush>
                            </Path.Fill>
                        </Path>
                        <Rectangle Height="Auto"
                                   HorizontalAlignment="Left"
                                   Margin="0,0,0,0"
                                   SnapsToDevicePixels="True"
                                   Stroke="{x:Null}"
                                   VerticalAlignment="Stretch"
                                   Width="1">
                            <Rectangle.Fill>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FF041C43" Offset="0"/>
                                    <GradientStop Color="#FF1B4FA3" Offset="1"/>
                                </LinearGradientBrush>
                            </Rectangle.Fill>
                        </Rectangle>
                    </Grid>
                </RepeatButton>
                <RepeatButton Name="PrevPage"
                              Command="dxc:CarouselPanel.PreviousPage"
                              CommandTarget="{TemplateBinding Carousel}"
                              Grid.Column="1"
