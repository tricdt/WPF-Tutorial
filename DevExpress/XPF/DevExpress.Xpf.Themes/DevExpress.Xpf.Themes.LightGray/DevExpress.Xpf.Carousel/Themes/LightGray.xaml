                              Interval="{Binding Path=Carousel.AnimationTime, RelativeSource={RelativeSource TemplatedParent}}"
                              Margin="0,0,1,0">
                    <Path Data="F1 M 589,574L 589,565L 597,569L 597,570L 589,574 Z M 580,574L 580,565L 588,569L 588,570L 580,574 Z "
                          Height="9"
