                              Interval="{Binding Path=Carousel.AnimationTime, RelativeSource={RelativeSource TemplatedParent}}"
                              Margin="0,0,1,0">
                    <Path Data="F1 M 590,589L 598,585L 598,584L 590,580L 590,589 Z "
                          Height="9"
                          HorizontalAlignment="Center"
                          SnapsToDevicePixels="True"
                          Stretch="Fill"
                          VerticalAlignment="Center"
                          Width="8">
                        <Path.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF041C43" Offset="0"/>
                                <GradientStop Color="#FF1B4FA3" Offset="1"/>
                            </LinearGradientBrush>
                        </Path.Fill>
                    </Path>
                </RepeatButton>
                <RepeatButton Name="NextPage"
                              Command="dxc:CarouselPanel.NextPage"
                              CommandTarget="{TemplateBinding Carousel}"
