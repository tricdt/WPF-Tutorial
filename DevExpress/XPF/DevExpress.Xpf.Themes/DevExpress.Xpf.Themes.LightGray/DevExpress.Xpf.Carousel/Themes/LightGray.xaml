                              Margin="0,0,1,0">
                    <Path Data="F1 M 588,565L 588,574L 580,570L 580,569L 588,565 Z M 597,565L 597,574L 589,570L 589,569L 597,565 Z "
                          Height="9"
                          HorizontalAlignment="Center"
                          SnapsToDevicePixels="True"
                          Stretch="Fill"
                          VerticalAlignment="Center"
                          Width="17">
                        <Path.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF041C43" Offset="0"/>
                                <GradientStop Color="#FF1B4FA3" Offset="1"/>
                            </LinearGradientBrush>
                        </Path.Fill>
                    </Path>
                </RepeatButton>
                <RepeatButton Name="PrevItem"
                              Command="dxc:CarouselPanel.Previous"
                              CommandTarget="{TemplateBinding Carousel}"
                              Grid.Column="2"
                              Interval="{Binding Path=Carousel.AnimationTime, RelativeSource={RelativeSource TemplatedParent}}"
                              Margin="0,0,1,0">
                    <Path Data="F1 M 588,565L 580,569L 580,570L 588,574L 588,565 Z "
                          Height="9"
                          HorizontalAlignment="Center"
                          Margin="0,0,1,0"
                          SnapsToDevicePixels="True"
                          Stretch="Fill"
                          VerticalAlignment="Center"
                          Width="8">
                        <Path.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF041C43" Offset="0"/>
                                <GradientStop Color="#FF1B4FA3" Offset="1"/>
                            </LinearGradientBrush>
                        </Path.Fill>
                    </Path>
                </RepeatButton>
                <RepeatButton Name="NextItem"
                              Command="dxc:CarouselPanel.Next"
                              CommandTarget="{TemplateBinding Carousel}"
