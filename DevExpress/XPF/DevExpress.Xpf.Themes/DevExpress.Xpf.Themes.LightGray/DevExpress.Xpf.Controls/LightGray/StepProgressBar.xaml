<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxcot="http://schemas.devexpress.com/winfx/2008/xaml/controls/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Duration x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressAnimationDuration}">0:0:0.5</Duration>

    <sys:Boolean x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ShowOuterBorder}">False</sys:Boolean>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderBrush}" Color="Transparent"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterIndentBrush}" Color="Transparent"/>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderIndent}">0</sys:Double>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderSize}">0</sys:Double>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBarThickness}">4</sys:Double>

    <Thickness x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBorderThickness}">1</Thickness>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBackground}" Color="#FFBEBEBE"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressForeground}" Color="#5EC3F2"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressAccentBorderBrush}" Color="#55A3C6"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressNeutralBorderBrush}" Color="#787878"/>

    <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorBorderSize}">1</sys:Double>

    <Thickness x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorPanelMargin}">3</Thickness>

    <EllipseGeometry x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorGeometry}"
                     Center="20,20"
                     RadiusX="20"
                     RadiusY="20"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedItemForeground}" Color="White"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedIndicatorFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop x:Name="gs1"
                      Color="#FF959595"
                      cs:id="GradientStop_0000"/>
        <GradientStop x:Name="gs2"
                      Color="#FF7B7B7B"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedIndicatorStroke}" Color="#596271"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedMouseOverFill}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF7A7A7A" cs:id="GradientStop_0000"/>
        <GradientStop Color="#FF646464"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedMouseOverStroke}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF596372" cs:id="GradientStop_0000"/>
        <GradientStop Color="#FF535962"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FF414346"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedItemForeground}" Color="#363636"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedIndicatorFill}"
                         EndPoint="0.5,0"
                         StartPoint="0.5,1">
        <GradientStop Color="#FFC0D7F4"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FFE2F0FD"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FFDBECFF"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
        <GradientStop Color="#FFE4F1FF"
                      Offset="0.5"
                      cs:id="GradientStop_0003"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedIndicatorStroke}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF596372" cs:id="GradientStop_0000"/>
        <GradientStop Color="#FF535962"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FF414346"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedMouseOverFill}" Color="#BBD2EE"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedMouseOverStroke}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF596372" cs:id="GradientStop_0000"/>
        <GradientStop Color="#FF535962"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FF414346"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateItemForeground}" Color="#363636"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateIndicatorFill}" Color="White"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateIndicatorStroke}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF596372" cs:id="GradientStop_0000"/>
        <GradientStop Color="#FF535962"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FF414346"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateMouseOverFill}" Color="White"/>

    <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateMouseOverStroke}"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF596372" cs:id="GradientStop_0000"/>
        <GradientStop Color="#FF535962"
                      Offset="1"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="#FF414346"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledNotStartedIndicatorFill}" Color="#C8C8C8"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledNotStartedIndicatorStroke}" Color="#B9BBBF"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledIndeterminateIndicatorFill}" Color="#FAFAFA"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledIndeterminateIndicatorStroke}" Color="#B5B8BB"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledCompletedIndicatorFill}" Color="#E4EFFB"/>

    <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledCompletedIndicatorStroke}" Color="#C0C4CA"/>

</ResourceDictionary>