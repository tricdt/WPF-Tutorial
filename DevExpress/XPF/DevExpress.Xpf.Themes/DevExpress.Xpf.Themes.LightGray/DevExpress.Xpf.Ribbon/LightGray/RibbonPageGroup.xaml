<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
                    xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys"
                    xmlns:interop="clr-namespace:System.Windows.Interop;assembly=PresentationFramework"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:syscm="clr-namespace:System.ComponentModel;assembly=PresentationFramework"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <dxb:FontSettings x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupButtonFontSettings}"/>

    <sys:Double x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupVerticalOffset}">-7</sys:Double>

    <sys:Double x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupHorizontalOffset}">0</sys:Double>

    <Thickness x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupBorderMargin}">0,20,0,20</Thickness>

    <Thickness x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=ContentMargin}">1,1,2,2</Thickness>

    <Style x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=ContentStyle}" TargetType="{x:Type ItemsPresenter}">
        <Setter Property="Margin" Value="{StaticResource {dxrt:RibbonPageGroupThemeKey ResourceKey=ContentMargin}}"/>
    </Style>

    <Style x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=CaptionButtonStyle}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
        <Setter Property="VerticalAlignment" Value="Bottom"/>
    </Style>

    <Style x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=CaptionPanelStyle}" TargetType="{x:Type dxb:SplitLayoutPanel}">
        <Setter Property="MaximizeContent1" Value="True"/>
        <Setter Property="LeftContent1Margin" Value="4,0,0,0"/>
        <Setter Property="SingleContent1Margin" Value="4,0,4,0"/>
    </Style>

    <!-- LightGray: Ribbon Page Group Border -->

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
        <Grid Margin="0,2,0,1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="3"/>
            </Grid.ColumnDefinitions>
            <ContentPresenter Content="{TemplateBinding Content}"
                              Grid.Column="0"
                              SnapsToDevicePixels="True"/>
            <TextBlock Name="PART_DesignTimeCaption"
                       Grid.Column="1"
                       Margin="2,0"
                       Text="[PageGroup]"
                       VerticalAlignment="Center"
                       Visibility="Collapsed"/>
            <Border Name="PART_Separator"
                    BorderThickness="0,0,1,0"
                    Grid.Column="2"
                    Margin="1,0,0,0"
                    Opacity="0.5"
                    SnapsToDevicePixels="True">
                <Border.BorderBrush>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFF2F2F2" Offset="0"/>
                        <GradientStop Color="#7FFFFFFF" Offset="1"/>
                    </LinearGradientBrush>
                </Border.BorderBrush>
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFDADADA" Offset="0"/>
                        <GradientStop Color="#FF545454" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=IsDesignTimeCaptionVisible, RelativeSource={RelativeSource AncestorType=dxr:RibbonPageGroupControl}}" Value="True">
                <Setter Property="Visibility"
                        TargetName="PART_DesignTimeCaption"
                        Value="Visible"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=IsSeparatorVisible, RelativeSource={RelativeSource AncestorType=dxr:RibbonPageGroupControl}}" Value="False">
                <Setter Property="Visibility"
                        TargetName="PART_Separator"
                        Value="Collapsed"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!-- LightGray: Ribbon Page Group Popup Border -->

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=BorderTemplateInPopup}" TargetType="{x:Type ContentControl}">
        <Border BorderBrush="#FF4C4D4C" BorderThickness="1">
            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFF0F0F0"/>
                    <GradientStop Color="#FFE0E1DF" Offset="1"/>
                </LinearGradientBrush>
            </Border.Background>
            <Grid Margin="1,2,1,4">
                <ContentPresenter Content="{TemplateBinding Content}"
                                  Grid.Column="0"
                                  SnapsToDevicePixels="True"/>
                <Grid Grid.Column="1">
                    <Rectangle HorizontalAlignment="Left"
                               SnapsToDevicePixels="True"
                               Stretch="Fill"
                               Width="1">
                        <Rectangle.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#335A7DAD" Offset="0"/>
                                <GradientStop Color="#FF5A7DAD" Offset="0.5"/>
                            </LinearGradientBrush>
                        </Rectangle.Fill>
                    </Rectangle>
                </Grid>
            </Grid>
        </Border>
    </ControlTemplate>

    <!-- LightGray: Caption Button -->

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=CaptionButtonTemplate}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
        <Grid>
            <Grid x:Name="PART_Normal"
                  Opacity="0.0"
                  Visibility="Visible">
                <Border Background="Transparent"
                        CornerRadius="4,4,4,4"
                        Height="Auto"
                        Margin="0,1,0,1"
                        Padding="1,1,1,1"
                        SnapsToDevicePixels="True"
                        Width="Auto">
                    <Border Background="Transparent"
                            CornerRadius="3,3,3,3"
                            Height="Auto"
                            Padding="1,1,1,1"
                            SnapsToDevicePixels="True"
                            Width="Auto">
                        <Border Background="Transparent"
                                CornerRadius="2,2,2,2"
                                Height="Auto"
                                Padding="3,3,3,3"
                                SnapsToDevicePixels="True"
                                Width="Auto">
                        </Border>
                    </Border>
                </Border>
                <Border Background="Transparent"
                        CornerRadius="4,4,4,4"
                        Height="Auto"
                        Margin="0,1,0,1"
                        Padding="1,1,1,1"
                        SnapsToDevicePixels="True"
                        Width="Auto">
                    <Border Background="Transparent"
                            CornerRadius="3,3,3,3"
                            Height="Auto"
                            Padding="1,1,1,1"
                            SnapsToDevicePixels="True"
                            Width="Auto">
                        <Border Background="Transparent"
                                CornerRadius="2,2,2,2"
                                Height="Auto"
                                Padding="3,3,3,3"
                                SnapsToDevicePixels="True"
                                Width="Auto">
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PART_Hover" Visibility="Collapsed">
                <Border CornerRadius="4,4,4,4"
                        Height="Auto"
                        Margin="0,1,0,1"
                        Padding="1,1,1,1"
                        SnapsToDevicePixels="True"
                        Width="Auto">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                            <GradientStop Color="#19000000"/>
                            <GradientStop Color="#00000000" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="3,3,3,3"
                            Height="Auto"
                            Padding="1,1,1,1"
                            SnapsToDevicePixels="True"
                            Width="Auto">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                                <GradientStop Color="#FF7E7E7E"/>
                                <GradientStop Color="#FF9D9D9D" Offset="1"/>
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="2,2,2,2"
                                Height="Auto"
                                Padding="3,3,3,3"
                                SnapsToDevicePixels="True"
                                Width="Auto">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                                    <GradientStop Color="#FFDBDBDB"/>
                                    <GradientStop Color="#FFF7F7F7" Offset="1"/>
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
                <Border CornerRadius="4,4,4,4"
                        Height="Auto"
                        Margin="0,1,0,1"
                        Padding="1,1,1,1"
                        SnapsToDevicePixels="True"
                        Width="Auto">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                            <GradientStop Color="#19000000"/>
                            <GradientStop Color="#00000000" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="3,3,3,3"
                            Height="Auto"
                            Padding="1,1,1,1"
                            SnapsToDevicePixels="True"
                            Width="Auto">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                                <GradientStop Color="#807E7E7E"/>
                                <GradientStop Color="#809D9D9D" Offset="1"/>
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="2,2,2,2"
                                Height="Auto"
                                Padding="3,3,3,3"
                                SnapsToDevicePixels="True"
                                Width="Auto">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                                    <GradientStop Color="#80DBDBDB"/>
                                    <GradientStop Color="#80F7F7F7" Offset="1"/>
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PART_Pressed" Visibility="Collapsed">
                <Border CornerRadius="4,4,4,4"
                        Height="Auto"
                        Margin="0,1,0,1"
                        Padding="1,1,1,1"
                        SnapsToDevicePixels="True"
                        Width="Auto">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                            <GradientStop Color="#7FFFFFFF"/>
                            <GradientStop Color="#00FFFFFF" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="3,3,3,3"
                            Height="Auto"
                            Padding="1,1,1,1"
                            SnapsToDevicePixels="True"
                            Width="Auto">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                                <GradientStop Color="#FF707070"/>
                                <GradientStop Color="#FF929292" Offset="1"/>
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="2,2,2,2"
                                Height="Auto"
                                Padding="3,3,3,3"
                                SnapsToDevicePixels="True"
                                Width="Auto">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                                    <GradientStop Color="#FFA9A9AA"/>
                                    <GradientStop Color="#FFBABAB9" Offset="1"/>
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PART_Focused" Visibility="Collapsed">
                <Border CornerRadius="4,4,4,4"
                        Height="Auto"
                        Margin="0,1,0,1"
                        Padding="1,1,1,1"
                        SnapsToDevicePixels="True"
                        Width="Auto">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                            <GradientStop Color="#19000000"/>
                            <GradientStop Color="#00000000" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="3,3,3,3"
                            Height="Auto"
                            Padding="1,1,1,1"
                            SnapsToDevicePixels="True"
                            Width="Auto">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                                <GradientStop Color="#FF7E7E7E"/>
                                <GradientStop Color="#FF9D9D9D" Offset="1"/>
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="2,2,2,2"
                                Height="Auto"
                                Padding="3,3,3,3"
                                SnapsToDevicePixels="True"
                                Width="Auto">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                                    <GradientStop Color="#FFDBDBDB"/>
                                    <GradientStop Color="#FFF7F7F7" Offset="1"/>
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
                <Border CornerRadius="4,4,4,4"
                        Height="Auto"
                        Margin="0,1,0,1"
                        Padding="1,1,1,1"
                        SnapsToDevicePixels="True"
                        Width="Auto">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                            <GradientStop Color="#19000000"/>
                            <GradientStop Color="#00000000" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="3,3,3,3"
                            Height="Auto"
                            Padding="1,1,1,1"
                            SnapsToDevicePixels="True"
                            Width="Auto">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                                <GradientStop Color="#807E7E7E"/>
                                <GradientStop Color="#809D9D9D" Offset="1"/>
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="2,2,2,2"
                                Height="Auto"
                                Padding="3,3,3,3"
                                SnapsToDevicePixels="True"
                                Width="Auto">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                                    <GradientStop Color="#80DBDBDB"/>
                                    <GradientStop Color="#80F7F7F7" Offset="1"/>
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid Height="8"
                  HorizontalAlignment="Center"
                  Margin="4,5,4,5"
                  SnapsToDevicePixels="True"
                  VerticalAlignment="Center"
                  Width="8">
                <Path Data="F1 M 1,6L 2,6L 2,2.00003L 6,2L 6,1L 1,1.00003L 1,6 Z "
                      Fill="#7FFFFFFF"
                      Margin="1,1,2,2"
                      SnapsToDevicePixels="True"
                      Stretch="Fill"/>
                <Rectangle Fill="#7FFFFFFF"
                           Height="1"
                           HorizontalAlignment="Right"
                           Margin="0,3,3,0"
                           SnapsToDevicePixels="True"
                           Stretch="Fill"
                           VerticalAlignment="Top"
                           Width="1"/>
                <Path Data="F1 M 5.00006,8L 4.00006,8L 4,7L 5,7L 5,5L 7,5L 7,3L 8,3L 8,8L 5.00006,8 Z "
                      Fill="#7FFFFFFF"
                      HorizontalAlignment="Right"
                      Margin="0,3,0,0"
                      SnapsToDevicePixels="True"
                      Stretch="Fill"
                      Width="4"/>
                <Path Data="F1 M 6.10352e-005,6L 1.00006,6L 1,1L 6,1L 6,0L 0,0L 6.10352e-005,6 Z "
                      Fill="#FF656565"
                      Margin="0,0,2,2"
                      SnapsToDevicePixels="True"
                      Stretch="Fill"/>
                <Path Data="F1 M 3.00006,3L 3.00006,4L 4,4.00003L 4,3.00003L 3.00006,3 Z "
                      Fill="#FF656565"
                      Height="1"
                      HorizontalAlignment="Left"
                      Margin="3,3,0,0"
                      SnapsToDevicePixels="True"
                      Stretch="Fill"
                      VerticalAlignment="Top"
                      Width="1"/>
                <Path Data="F1 M 4.00006,7L 3.00006,7L 3,6L 4,6L 4,4L 6,4L 6,3L 7,3L 7,7L 4.00006,7 Z "
                      Fill="#FF656565"
                      Margin="3,3,1,1"
                      SnapsToDevicePixels="True"
                      Stretch="Fill"/>
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Hover" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Pressed" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Focused" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Hover" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Pressed" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Focused" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Hover" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Pressed" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Focused" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Hover" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Pressed" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Focused" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Hover" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Pressed" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Focused" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

    <!-- LightGray: Text Area Background Template -->

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=CaptionBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border>
            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFD4D4D4" Offset="0"/>
                    <GradientStop Color="#FFC8C8C8" Offset="1"/>
                </LinearGradientBrush>
            </Border.Background>
            <ContentPresenter Content="{TemplateBinding Content}"
                              MinHeight="20"
                              SnapsToDevicePixels="True"/>
        </Border>
    </ControlTemplate>

    <!-- LightGray: Popup Button -->

    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=PopupButtonBorderTemplate}" TargetType="{x:Type dxb:ItemBorderControl}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="2"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="21"/>
            </Grid.RowDefinitions>
            <Border x:Name="PART_Hover"
                    BorderThickness="0"
                    Grid.ColumnSpan="2"
                    Grid.RowSpan="2"
                    Margin="0,1,0,1"
                    Opacity="0">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFF3F3F3" Offset="0"/>
                        <GradientStop Color="#FFEFEFEF" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border x:Name="PART_Pressed"
                    BorderThickness="0"
                    Grid.ColumnSpan="2"
                    Grid.RowSpan="2"
                    Margin="0,1,0,1"
                    Opacity="0">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFE3E3E3" Offset="0"/>
                        <GradientStop Color="#FFCECECD" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border x:Name="PART_Separator"
                    BorderThickness="0,0,1,0"
                    Grid.Column="1"
                    Grid.RowSpan="2"
                    Margin="0,1,0,1"
                    Opacity="0.5">
                <Border.BorderBrush>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFF2F2F2" Offset="0"/>
                        <GradientStop Color="#7FFFFFFF" Offset="1"/>
                    </LinearGradientBrush>
                </Border.BorderBrush>
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFDADADA" Offset="0"/>
                        <GradientStop Color="#FF545454" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Grid HorizontalAlignment="Center"
                  Margin="0"
                  VerticalAlignment="Center">
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.656*"/>
                    <RowDefinition Height="0.344*"/>
                </Grid.RowDefinitions>
                <ContentPresenter Grid.Row="1"
                                  HorizontalAlignment="Center"
                                  Margin="6,0,6,0"
