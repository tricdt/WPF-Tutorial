    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=RightRepeatButtonTemplate}" TargetType="{x:Type RepeatButton}">
        <Grid x:Name="grid"
              Background="{x:Null}"
              VerticalAlignment="Center">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="IsMouseOver"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="IsDefault"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="IsPressed"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0"/>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="path"
                                            Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)"
                                            To="#FF636363"
                                            cs:id="ColorAnimation_0000"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="IsPressed"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="IsDefault"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0"/>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="IsMouseOver"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0"/>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="path"
                                            Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)"
                                            To="#FF434343"
                                            cs:id="ColorAnimation_0001"/>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Shadow"
                                            Storyboard.TargetProperty="(Border.Background).(GradientBrush.GradientStops)[1].(GradientStop.Color)"
                                            To="#26000000"
                                            cs:id="ColorAnimation_0002"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="Shadow"
                    CornerRadius="32"
                    Margin="-1">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                         cs:id="LinearGradientBrush_0000">
                        <GradientStop Offset="0"/>
                        <GradientStop Color="#3F000000"
                                      Offset="0.992"
                                      cs:id="GradientStop_0000"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border x:Name="IsDefault" CornerRadius="32">
                <Border.BorderBrush>
                    <LinearGradientBrush EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                         cs:id="LinearGradientBrush_0001">
                        <GradientStop Color="#FF535353"
                                      Offset="0"
                                      cs:id="GradientStop_0001"/>
                        <GradientStop Color="#FF535353"
                                      Offset="1"
                                      cs:id="GradientStop_0002"/>
                    </LinearGradientBrush>
                </Border.BorderBrush>
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                         cs:id="LinearGradientBrush_0002">
                        <GradientStop Color="#FFEFEFEF"
                                      Offset="0"
                                      cs:id="GradientStop_0003"/>
                        <GradientStop Color="#FFDFE0DE"
                                      Offset="1"
                                      cs:id="GradientStop_0004"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border x:Name="IsMouseOver"
                    CornerRadius="32"
                    Opacity="0">
                <Border.BorderBrush>
                    <LinearGradientBrush EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                         cs:id="LinearGradientBrush_0003">
                        <GradientStop Color="#FF535353"
                                      Offset="0"
                                      cs:id="GradientStop_0005"/>
                        <GradientStop Color="#FF535353"
                                      Offset="1"
                                      cs:id="GradientStop_0006"/>
                    </LinearGradientBrush>
                </Border.BorderBrush>
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                         cs:id="LinearGradientBrush_0004">
                        <GradientStop Color="White"
                                      Offset="0"
                                      cs:id="GradientStop_0007"/>
                        <GradientStop Color="#FFEFF0EE"
                                      Offset="1"
                                      cs:id="GradientStop_0008"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border x:Name="IsPressed"
                    CornerRadius="32"
                    Opacity="0">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1"
                                         StartPoint="0.5,0"
                                         cs:id="LinearGradientBrush_0005">
                        <GradientStop Color="#FFA7A7A8"
                                      Offset="1"
                                      cs:id="GradientStop_0009"/>
                        <GradientStop Color="#FFB8B8B7" cs:id="GradientStop_0010"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Path x:Name="path"
                  Data="F1 M 28,60L 28,68L 36,64L 28,60"
                  Fill="#FF535353"
                  Height="8"
                  HorizontalAlignment="Center"
                  Margin="4,2,2,2"
                  Stretch="Fill"
                  UseLayoutRounding="False"
                  VerticalAlignment="Center"
                  Width="8"
                  cs:id="Path_0000"/>
        </Grid>
    </ControlTemplate>

