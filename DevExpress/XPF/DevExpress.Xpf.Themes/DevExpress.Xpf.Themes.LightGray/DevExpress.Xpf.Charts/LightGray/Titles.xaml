<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/charts/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- AxisTitle -->

    <Thickness x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisTitlePadding}">5</Thickness>

    <!-- Title -->

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=TitleForeground}" Color="#FF000000"/>

    <sys:Double x:Key="{dxt:ChartControlThemeKey ResourceKey=TitleFontSize}">21</sys:Double>

    <Thickness x:Key="{dxt:ChartControlThemeKey ResourceKey=TitleMarginValue}">0,0,0,10</Thickness>

    <!-- PaneTitle -->

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=PaneTitleForeground}">#FF000000</SolidColorBrush>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=PaneTitleForegroundHot}">#FF73A5DE</SolidColorBrush>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=PaneTitleForegroundDisabled}">#3F000000</SolidColorBrush>

    <sys:Double x:Key="{dxt:ChartControlThemeKey ResourceKey=PaneTitleFontSize}">12</sys:Double>

    <FontWeight x:Key="{dxt:ChartControlThemeKey ResourceKey=PaneTitleFontWeight}">Bold</FontWeight>

    <Thickness x:Key="{dxt:ChartControlThemeKey ResourceKey=PaneTitleMarginValue}">0,10,0,10</Thickness>

</ResourceDictionary>