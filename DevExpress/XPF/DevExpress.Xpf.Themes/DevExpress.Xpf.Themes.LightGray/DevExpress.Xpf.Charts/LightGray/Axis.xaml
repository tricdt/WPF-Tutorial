<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/charts/themekeys"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <sys:Double x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisLabelFontSize}">12</sys:Double>

    <sys:Double x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisTitleFontSize}">16</sys:Double>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=AxisBrush}" Color="#FF6E6E6E"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=Axis2DInterlacedBrush}" Color="#0F000000"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=StripBrush}" Color="#2EBB002F"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=ConstantLineBrush}" Color="#FFBB002F"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=ConstantLineTitleForeground}" Color="#FFBB002F"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=GridLineBrush}" Color="#2B000000"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=MinorGridLineBrush}" Color="#14000000"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=AxisLabelForeground}" Color="#FF5E5E5E"/>

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=AxisTitleForeground}" Color="#FF3B3B3B"/>

    <Color x:Key="{dxt:ChartBrushesThemeKey ResourceKey=StripBorderColor}">#1FBB002F</Color>

    <Thickness x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisLabelPadding}">5,1</Thickness>

    <DataTemplate x:Key="{dxt:ChartControlThemeKey ResourceKey=AxisLabelTemplate}">
        <ContentPresenter Content="{Binding Path=Content}"/>
    </DataTemplate>

    <!--BEGIN WPF-->

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=Axis3DInterlacedBrush}" Color="#112D4F85"/>

    <!--END WPF-->

</ResourceDictionary>