    <SolidColorBrush x:Key="{dxtEx:ThemeColorsThemeKey ResourceKey=BlackForegroundColor}" Color="#FF000000"/>

    <SolidColorBrush x:Key="{dxtEx:ThemeColorsThemeKey ResourceKey=DXMessageBoxForegroundColor}" Color="#FF000000"/>

    <Color x:Key="{dxet:SpinEditThemeKey ResourceKey=ArrowBrushColor}">#FF201F35</Color>

    <SolidColorBrush x:Key="{dxet:ButtonsThemeKey ResourceKey=Foreground}" Color="#FFFFFFFF"/>

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=CheckEditPadding}">0,0,0,0</Thickness>

