                                <GradientStop Color="#FFC0D7F4"
                                              Offset="0"
                                              cs:id="GradientStop_0003"/>
                                <GradientStop Color="#FFE2F0FD"
                                              Offset="1"
                                              cs:id="GradientStop_0004"/>
                                <GradientStop Color="#FFDBECFF"
                                              Offset="0.531"
                                              cs:id="GradientStop_0005"/>
                                <GradientStop Color="#FFE4F1FF"
                                              Offset="0.557"
                                              cs:id="GradientStop_0006"/>
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="2">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,0"
                                                     StartPoint="0.5,1"
                                                     cs:id="LinearGradientBrush_0002">
                                    <GradientStop Color="#FFBED9F7"
                                                  Offset="0"
                                                  cs:id="GradientStop_0007"/>
                                    <GradientStop Color="#FFD7E9FB"
                                                  Offset="1"
                                                  cs:id="GradientStop_0008"/>
                                    <GradientStop Color="#FFA9C9F7"
                                                  Offset="0.531"
                                                  cs:id="GradientStop_0009"/>
                                    <GradientStop Color="#FFC8DEF5"
                                                  Offset="0.531"
