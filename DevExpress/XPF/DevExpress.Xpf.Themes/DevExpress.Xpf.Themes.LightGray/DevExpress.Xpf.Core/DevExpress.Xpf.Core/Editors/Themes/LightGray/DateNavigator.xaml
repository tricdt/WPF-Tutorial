                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                    <Border x:Name="MouseOverState"
                            CornerRadius="2"
                            Opacity="0"
                            Padding="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,0"
                                                 StartPoint="0.5,1"
                                                 cs:id="LinearGradientBrush_0003">
                                <GradientStop Color="#FFE0EEFF"
                                              Offset="0"
                                              cs:id="GradientStop_0011"/>
                                <GradientStop Color="#FFEEF6FF"
                                              Offset="1"
                                              cs:id="GradientStop_0012"/>
                                <GradientStop Color="#FFE4F1FF"
                                              Offset="0.531"
                                              cs:id="GradientStop_0013"/>
                                <GradientStop Color="#FFF1F8FF"
                                              Offset="0.532"
                                              cs:id="GradientStop_0014"/>
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="2">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,0"
                                                     StartPoint="0.5,1"
                                                     cs:id="LinearGradientBrush_0004">
                                    <GradientStop Color="#FFDDEDFF"
                                                  Offset="0"
                                                  cs:id="GradientStop_0015"/>
                                    <GradientStop Color="#FFE4F1FF"
                                                  Offset="1"
                                                  cs:id="GradientStop_0016"/>
                                    <GradientStop Color="#FFCCE0FD"
                                                  Offset="0.531"
                                                  cs:id="GradientStop_0017"/>
                                    <GradientStop Color="#FFDCEBFB"
                                                  Offset="0.532"
                                                  cs:id="GradientStop_0018"/>
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                    <Border x:Name="PressedState"
                            CornerRadius="2"
                            Opacity="0"
                            Padding="1">
