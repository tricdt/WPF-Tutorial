    <Style x:Key="{dxet:DateNavigatorThemeKey ResourceKey=WeekdayAbbreviationStyle}" TargetType="{x:Type TextBlock}">
        <Setter Property="Foreground"
                Value="#FF000000"
                cs:id="Setter_0000"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0,0,0,4"/>
    </Style>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=MonthInfoDelimeter}">
        <Border Background="#FFBFC2C8"
                Height="1"
                SnapsToDevicePixels="True"
                cs:id="Border_0000"/>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=WeekNumberDelimeter}">
        <Border SnapsToDevicePixels="True" Width="1"/>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=CellButtonTemplate}" TargetType="{x:Type Button}">
        <dxednc:DateNavigatorCalendarCellButtonContent x:Name="PART_Content"
                                                       Cursor="Hand"
                                                       DisabledForegroundSolidColor="#FFE3E3E3"
                                                       HolidayForegroundSolidColor="#FFCC3333"
                                                       InactiveForegroundSolidColor="#FFB3B3B3"
                                                       MouseOverForegroundSolidColor="White"
                                                       SelectedForegroundSolidColor="White"
                                                       TodayForegroundSolidColor="Black"
                                                       cs:id="DateNavigatorCalendarCellButtonContent_0001">
            <dxednc:DateNavigatorCalendarCellButtonContent.Template>
                <ControlTemplate>
                    <Grid Background="Transparent" cs:id="Grid_0000">
                        <Border x:Name="PART_Today"
                                BorderBrush="#FF7F8693"
                                BorderThickness="1"
                                CornerRadius="2"
                                Visibility="Collapsed"
                                cs:id="Border_0000"/>
                        <Border x:Name="PART_Focused"
                                CornerRadius="2"
                                Padding="1"
                                Visibility="Collapsed">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,0"
                                                     StartPoint="0.5,1"
                                                     cs:id="LinearGradientBrush_0000">
                                    <GradientStop Color="#FF6FA2DD" cs:id="GradientStop_0000"/>
                                    <GradientStop Color="#FF8FB8E5"
                                                  Offset="1"
                                                  cs:id="GradientStop_0001"/>
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                        <Border x:Name="PART_MouseOver"
                                CornerRadius="2"
                                Visibility="Collapsed">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                     cs:id="LinearGradientBrush_0001">
                                    <GradientStop Color="#FFADD7F5"
                                                  Offset="0"
                                                  cs:id="GradientStop_0002"/>
                                    <GradientStop Color="#FF90C2FD"
                                                  Offset="1"
                                                  cs:id="GradientStop_0003"/>
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                        <Border x:Name="PART_Selected"
                                CornerRadius="2"
                                Visibility="Collapsed">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                     cs:id="LinearGradientBrush_0002">
                                    <GradientStop Color="#FF8DB7E5"
                                                  Offset="0"
                                                  cs:id="GradientStop_0004"/>
                                    <GradientStop Color="#FF70A2DD"
                                                  Offset="1"
                                                  cs:id="GradientStop_0005"/>
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                        <TextBlock x:Name="PART_Text"
                                   HorizontalAlignment="Center"
                                   Margin="1,0,1,1"
                                   VerticalAlignment="Center"/>
                    </Grid>
                </ControlTemplate>
            </dxednc:DateNavigatorCalendarCellButtonContent.Template>
        </dxednc:DateNavigatorCalendarCellButtonContent>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=YearInfoTemplate}">
        <Grid x:Name="PART_ContentGrid" SnapsToDevicePixels="True">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:DateNavigatorThemeKey ResourceKey=MonthInfoTemplate}">
        <Grid x:Name="PART_ContentGrid" SnapsToDevicePixels="True">
            <Grid.Resources>
                <dxc:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
            </Grid.Resources>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <ContentControl dxc:FocusHelper2.Focusable="False"
                            Grid.Column="2"
                            Grid.ColumnSpan="7"
                            Grid.Row="1"
                            Margin="0,0,0,5"
                            Template="{DynamicResource {dxet:DateNavigatorThemeKey IsVisibleInBlend=True, ResourceKey=MonthInfoDelimeter}}"/>
            <ContentControl x:Name="WeekNumberDelimeter"
                            dxc:FocusHelper2.Focusable="False"
                            Grid.Column="1"
                            Grid.Row="2"
                            Grid.RowSpan="6"
                            Template="{DynamicResource {dxet:DateNavigatorThemeKey IsVisibleInBlend=True, ResourceKey=WeekNumberDelimeter}}"
                            Visibility="{Binding Path=(dxedn:DateNavigator.Navigator).ShowWeekNumbers, RelativeSource={RelativeSource Self}, Converter={StaticResource BoolToVisibilityConverter}}"/>
        </Grid>
    </ControlTemplate>

    <Style x:Key="{dxet:DateNavigatorThemeKey IsVisibleInBlend=True, ResourceKey=CalendarCurrentDateStyle}" TargetType="TextBlock">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

</ResourceDictionary>