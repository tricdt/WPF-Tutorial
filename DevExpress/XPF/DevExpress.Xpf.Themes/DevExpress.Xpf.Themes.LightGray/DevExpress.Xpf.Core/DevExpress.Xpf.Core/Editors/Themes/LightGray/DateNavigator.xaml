                            <LinearGradientBrush EndPoint="0.5,0"
                                                 StartPoint="0.5,1"
                                                 cs:id="LinearGradientBrush_0005">
                                <GradientStop Color="#FFC0D7F4"
                                              Offset="0"
                                              cs:id="GradientStop_0019"/>
                                <GradientStop Color="#FF7297C2"
                                              Offset="1"
                                              cs:id="GradientStop_0020"/>
