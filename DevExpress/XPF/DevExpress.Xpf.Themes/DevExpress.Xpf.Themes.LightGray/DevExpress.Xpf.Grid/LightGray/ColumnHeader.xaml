    <SolidColorBrush x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=HeadersVerticalLineBrush}" Color="White"/>

    <SolidColorBrush x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=HeaderCaptionForeground}" Color="White"/>

    <SolidColorBrush x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=HeaderHoverCaptionForeground}" Color="White"/>

    <SolidColorBrush x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=HeaderPressedCaptionForeground}" Color="White"/>

    <SolidColorBrush x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=GroupHeaderCaptionForeground}" Color="#FF6E6E6E"/>

    <dx:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>

    <dxmvvm:ObjectToObjectConverter x:Key="IntToVisibilityConverter" DefaultTarget="Visible">
        <dxmvvm:MapItem Source="0" Target="Collapsed"/>
    </dxmvvm:ObjectToObjectConverter>

    <dxmvvm:ObjectToObjectConverter x:Key="IntToVisibilityConverterInvert" DefaultTarget="Collapsed">
        <dxmvvm:MapItem Source="0" Target="Visible"/>
    </dxmvvm:ObjectToObjectConverter>

    <dxgcore:DoubleToVisibilityConverter x:Key="DoubleToVisibilityConverter"/>

    <ControlTemplate x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=SortArrowUpTemplate}">
        <StackPanel SnapsToDevicePixels="True" VerticalAlignment="Center">
            <Border x:Name="SortBySummaryIndicator"
                    Background="{TemplateBinding Foreground}"
                    BorderBrush="{x:Null}"
                    Height="1"
                    Margin="0,0,0,1"
                    Visibility="{Binding IsSortedBySummary, Converter={StaticResource BooleanToVisibilityConverter}}"
                    Width="7"/>
            <Path Data="F1 M 685,527L 682,521L 681,521L 678,527L 685,527 Z "
                  Fill="{TemplateBinding Foreground}"
                  Height="6"
                  Stretch="Fill"
                  Width="7"/>
        </StackPanel>
    </ControlTemplate>

    <Style x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=SortArrowUpStyle}" TargetType="{x:Type Control}">
        <Setter Property="Width" Value="12"/>
        <Setter Property="Margin" Value="2,0,-1,1"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
    </Style>

    <ControlTemplate x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=SortArrowDownTemplate}">
        <StackPanel SnapsToDevicePixels="True" VerticalAlignment="Center">
            <Path Data="F1 M 678,521L 681,527L 682,527L 685,521L 678,521 Z "
                  Fill="{TemplateBinding Foreground}"
                  Height="6"
                  Stretch="Fill"
                  Width="7"/>
            <Border x:Name="SortBySummaryIndicator"
                    Background="{TemplateBinding Foreground}"
                    BorderBrush="{x:Null}"
                    Height="1"
                    Margin="0,1,0,0"
                    Visibility="{Binding IsSortedBySummary, Converter={StaticResource BooleanToVisibilityConverter}}"
                    Width="7"/>
        </StackPanel>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding IsSortedBySummary}" Value="true">
                <Setter Property="Visibility"
                        TargetName="SortBySummaryIndicator"
                        Value="Visible"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="{dxgt:GridColumnHeaderThemeKey ResourceKey=SortArrowDownStyle}" TargetType="{x:Type Control}">
        <Setter Property="Width" Value="12"/>
        <Setter Property="Margin" Value="2,0,-1,0"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
    </Style>

