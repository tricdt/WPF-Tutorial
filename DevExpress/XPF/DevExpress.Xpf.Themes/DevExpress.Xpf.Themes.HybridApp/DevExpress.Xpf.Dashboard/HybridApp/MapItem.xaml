<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxdasht="http://schemas.devexpress.com/winfx/2008/xaml/dashboard/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:wn="clr-namespace:System.Windows;assembly=PresentationCore"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.HybridApp.v24.1;component/DevExpress.Xpf.Dashboard/Generic/MapItem.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=GradientColorStart}">#FF5F8195</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=GradientColorEnd}">#FFB55951</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=DeltaColorGood}">#9AB57E</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=DeltaColorBad}">#E55253</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=DeltaColorWarning}">#E0C23A</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorTens}">#FF0000FF</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorHundreds}">#FF008000</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorThousands}">#FFFFA500</Color>

    <Color x:Key="{dxdasht:MapItemThemeKey ResourceKey=ClusterColorMillions}">#FFFF0000</Color>

    <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointBackgroundBrush}" Color="Red"/>

    <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointHighLightedBackgroundBrush}" Color="Blue"/>

    <SolidColorBrush x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointBorderBrush}" Color="Black"/>

    <sys:Double x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointDisplayTextFontSize}">12</sys:Double>

    <wn:FontWeight x:Key="{dxdasht:MapItemThemeKey ResourceKey=GeoPointDisplayTextFontWeight}">Bold</wn:FontWeight>

</ResourceDictionary>