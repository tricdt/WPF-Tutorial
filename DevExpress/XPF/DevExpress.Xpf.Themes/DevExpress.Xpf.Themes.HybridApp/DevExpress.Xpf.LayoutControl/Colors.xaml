<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Text.Foreground" Color="#FF000000"/>

    <Color x:Key="LayerSeparator.NormalColor">#FFC3C3C3</Color>

    <Color x:Key="LayerSeparator.MouseOverColor">#FF2F51A5</Color>

    <Color x:Key="ElementSizer.GlyphNormalColor">#FFC3C3C3</Color>

    <Color x:Key="ElementSizer.GlyphMouseOverColor">#FFC3C3C3</Color>

    <Color x:Key="ElementSizer.GlyphBackgroundColor">#FFC3C3C3</Color>

    <Color x:Key="GroupBoxButton.NormalColor">#FF404040</Color>

    <Color x:Key="GroupBoxButton.MouseOverColor">#FF2F51A5</Color>

    <Color x:Key="GroupBoxButton.PressedColor">#FF2F51A5</Color>

    <SolidColorBrush x:Key="GroupBox.Background" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="GroupBox.BorderBrush" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="GroupBox.SeparatorBrush" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="GroupBox.TitleBackground" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="GroupSeparator.Brush" Color="#FFC3C3C3"/>

</ResourceDictionary>