<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=TextEditPadding}">3,2,3,2</Thickness>

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=ValidationErrorPadding}">3,0,1,0</Thickness>

    <Thickness x:Key="leftButtonsMargin">1,1,0,1</Thickness>

    <Thickness x:Key="rightButtonsMargin">0,1,1,1</Thickness>

    <Color x:Key="{dxet:SpinEditThemeKey ResourceKey=ArrowBrushColor}">#FF7F7F7F</Color>

    <SolidColorBrush x:Key="{dxet:ButtonsThemeKey ResourceKey=Foreground}" Color="#FF191919"/>

    <Thickness x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=CheckEditPadding}">0,0,0,0</Thickness>

    <Thickness x:Key="{dxet:ComboBoxEditThemeKey ResourceKey=SelectedItemImagePadding}">3,0,0,0</Thickness>

    <Thickness x:Key="{dxet:ComboBoxEditThemeKey ResourceKey=SelectedItemContentPadding}">3,8,3,8</Thickness>

</ResourceDictionary>