                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Path"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF2F51A5"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Path"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FF2F51A5"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Path x:Name="Path"
                  Data="F1 M 247,75L 248,75L 248,74L 249,74L 249,73L 250,73L 250,72L 251,72L 251,73L 251,74L 251,75L 251,76L 251,77L 251,78L 251,79L 250,79L 250,78L 249,78L 249,77L 248,77L 248,76L 247,76L 247,75 Z "
                  Fill="#FF404040"
