<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.HybridApp.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/TimePicker.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="{dxet:TimePickerThemeKey ResourceKey=TimeEditBoxStyle}" TargetType="{x:Type dxe:ButtonEdit}">
        <Setter Property="Margin" Value="0,12,0,12"/>
        <Setter Property="MinWidth" Value="100"/>
    </Style>

    <SolidColorBrush x:Key="{dxet:TimePickerThemeKey ResourceKey=BackgroundBrush}" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="{dxet:TimePickerThemeKey ResourceKey=ClockBorderBrush}" Color="Black"/>

    <SolidColorBrush x:Key="{dxet:TimePickerThemeKey ResourceKey=ClockForeground}" Color="Black"/>

    <system:Double x:Key="{dxet:TimePickerThemeKey ResourceKey=ClockSize}">120</system:Double>

    <SolidColorBrush x:Key="{dxet:TimePickerThemeKey ResourceKey=ClockFillBrush}" Color="Transparent"/>

    <system:Double x:Key="{dxet:TimePickerThemeKey ResourceKey=ClockStrokeThickness}">1</system:Double>

    <system:Double x:Key="{dxet:TimePickerThemeKey ResourceKey=ClockShaftSize}">10</system:Double>

    <system:Double x:Key="{dxet:TimePickerThemeKey ResourceKey=ClockLongTickOffset}">5</system:Double>

    <system:Double x:Key="{dxet:TimePickerThemeKey ResourceKey=ClockShortTickOffset}">5</system:Double>

    <system:Double x:Key="{dxet:TimePickerThemeKey ResourceKey=ClockLongTickLength}">8</system:Double>

    <system:Double x:Key="{dxet:TimePickerThemeKey ResourceKey=ClockShortTickLength}">4</system:Double>

</ResourceDictionary>