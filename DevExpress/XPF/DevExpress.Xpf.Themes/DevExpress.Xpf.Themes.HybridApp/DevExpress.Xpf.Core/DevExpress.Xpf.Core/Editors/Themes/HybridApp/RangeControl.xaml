<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxer="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.HybridApp.v24.1;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/RangeControl.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarClientForegroundBrush}" Color="#FFFFFFFF"/>

    <Thickness x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftLabelMargin}"
               Bottom="1"
               Left="1"
               Right="0"
               Top="1"/>

    <Thickness x:Key="{dxt:RangeControlThemeKey ResourceKey=RightLabelMargin}"
               Bottom="1"
               Left="0"
               Right="1"
               Top="1"/>

    <!-- CalendarSelectionMarker -->

    <DataTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarSelectionMarkerTemplate}">
        <Ellipse Fill="{DynamicResource {dxt:CalendarClientThemeKey ResourceKey=CalendarClientForegroundBrush}}"
                 Height="20"
                 HorizontalAlignment="Center"
                 VerticalAlignment="Center"
                 Width="20"/>
    </DataTemplate>

    <!-- ToolTips -->

    <DataTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LabelTemplate}">
        <Border Background="#FFC3C3C3"
                Margin="2,0,1,0"
                MinHeight="22"
                MinWidth="80"
                VerticalAlignment="Top">
            <TextBlock Foreground="#FFFFFFFF"
                       HorizontalAlignment="Center"
                       Margin="4,0,4,0"
                       Text="{Binding}"
                       VerticalAlignment="Center"/>
        </Border>
    </DataTemplate>

    <!-- Left & Right Navigation Buttons -->

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftNavigationButtonTemplate}" TargetType="{x:Type Button}">
        <Grid Background="Transparent" Margin="16,0,0,0">
            <Ellipse x:Name="Ellipse"
                     Fill="#FFC3C3C3"
                     Height="32"
                     Stretch="Fill"
                     Stroke="#7FFFFFFF"
                     StrokeThickness="1"
                     Width="32"/>
            <Path x:Name="Glyph"
                  Data="F1 M 0,5L 6,0L 6,10L 0,5"
                  Fill="#BFFFFFFF"
                  Height="10"
                  Margin="-1,0,1,0"
                  Stretch="Fill"
                  Width="6"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RightNavigationButtonTemplate}" TargetType="{x:Type Button}">
        <Grid Background="Transparent" Margin="0,0,16,0">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Ellipse"
                                            Storyboard.TargetProperty="(Ellipse.Fill).(SolidColorBrush.Color)"
                                            To="#FFB9BAC3"/>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#BFFFFFFF"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Ellipse"
                                            Storyboard.TargetProperty="(Ellipse.Fill).(SolidColorBrush.Color)"
                                            To="#cbcbd2"/>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#FFFFFFFF"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Ellipse"
                                            Storyboard.TargetProperty="(Ellipse.Fill).(SolidColorBrush.Color)"
                                            To="#adadb8"/>
                            <ColorAnimation Duration="0"
                                            Storyboard.TargetName="Glyph"
                                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"
                                            To="#7FFFFFFF"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Ellipse x:Name="Ellipse"
                     Fill="#FFB9BAC3"
                     Height="32"
                     Stretch="Fill"
                     Stroke="#7FFFFFFF"
                     StrokeThickness="1"
                     Width="32"/>
            <Path x:Name="Glyph"
                  Data="F1 M 0,0L 0,10L 6,5L 0,0 Z "
                  Fill="#BFFFFFFF"
                  Height="10"
                  Margin="1,0,-1,0"
                  Stretch="Fill"
                  Width="6"/>
        </Grid>
    </ControlTemplate>

    <!-- Calendar -->

    <ControlTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarGroupItemTemplate}" TargetType="{x:Type dxer:CalendarGroupItem}">
        <Border Background="#FFB9BAC3">
            <TextBlock Margin="20,0"
                       Text="{TemplateBinding Text}"
                       TextAlignment="Center"
                       VerticalAlignment="Center"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarItemTemplate}" TargetType="{x:Type dxer:CalendarItem}">
        <Border x:Name="Border"
                Background="#FFB9BAC3"
                BorderThickness="0">
            <TextBlock x:Name="Text"
                       HorizontalAlignment="Center"
                       Margin="0,10,0,10"
                       Text="{TemplateBinding Text}"
                       TextAlignment="Center"/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Background"
                        TargetName="Border"
                        Value="#FFDCB1F0"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!-- Range Bar -->

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FF9DA0AA"
                BorderThickness="1">
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarRootBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF"
                BorderBrush="#FF9DA0AA"
                BorderThickness="0,1,0,0">
            <ContentPresenter/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarLeftResizeThumbTemplate}">
        <Border BorderBrush="#FFB6B8BF"
                BorderThickness="0,0,1,0"
                Width="12">
            <Border BorderBrush="#FFB6B8BF"
                    BorderThickness="1,0,1,0"
                    Margin="6,2,2,2"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarRightResizeThumbTemplate}">
        <Border BorderBrush="#FFB6B8BF"
                BorderThickness="1,0,0,0"
                Width="12">
            <Border BorderBrush="#FFB6B8BF"
                    BorderThickness="1,0,1,0"
                    Margin="2,2,6,2"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftSelectionThumbTemplate}" TargetType="{x:Type Thumb}">
        <Grid Background="Transparent"
              HorizontalAlignment="Left"
              Margin="3,0,-3,0"
              Width="23">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="Border1"
                    Background="#7F808080"
                    Grid.Column="1"
                    HorizontalAlignment="Center"
                    Margin="0,0,-1,0"
                    Width="1"/>
            <Border x:Name="Border2"
                    Background="#FFFFFFFF"
                    BorderBrush="#7F808080"
                    BorderThickness="1"
                    Grid.Column="1"
                    Padding="5"
                    VerticalAlignment="Center">
                <Path x:Name="Glyph"
                      Data="F1 M 8,9.53674e-007L 9,9.53674e-007L 9,16L 8,16L 8,9.53674e-007 Z M 6,7.62939e-006L 7,7.62939e-006L 7,16L 6,16L 6,7.62939e-006 Z M 4,6.67572e-006L 5,6.67572e-006L 5,16L 4,16L 4,6.67572e-006 Z M 2,4.76837e-006L 3,4.76837e-006L 3,16L 2,16L 2,4.76837e-006 Z M 6.35783e-007,9.53674e-007L 1,9.53674e-007L 1,16L 6.35783e-007,16L 6.35783e-007,9.53674e-007 Z "
                      Fill="#7F808080"
                      Height="16"
                      Stretch="Fill"
                      Width="9"/>
            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RightSelectionThumbTemplate}" TargetType="{x:Type Thumb}">
        <Grid Background="Transparent"
              HorizontalAlignment="Right"
              Margin="-3,0,3,0"
              Width="23">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="Border1"
                    Background="#7F808080"
                    Grid.Column="1"
                    HorizontalAlignment="Center"
                    Margin="-2,0,0,0"
                    Width="1"/>
            <Border x:Name="Border2"
                    Background="#FFFFFFFF"
                    BorderBrush="#7F808080"
                    BorderThickness="1"
                    Grid.Column="1"
                    Padding="5"
                    VerticalAlignment="Center">
                <Path x:Name="Glyph"
                      Data="F1 M 8,9.53674e-007L 9,9.53674e-007L 9,16L 8,16L 8,9.53674e-007 Z M 6,7.62939e-006L 7,7.62939e-006L 7,16L 6,16L 6,7.62939e-006 Z M 4,6.67572e-006L 5,6.67572e-006L 5,16L 4,16L 4,6.67572e-006 Z M 2,4.76837e-006L 3,4.76837e-006L 3,16L 2,16L 2,4.76837e-006 Z M 6.35783e-007,9.53674e-007L 1,9.53674e-007L 1,16L 6.35783e-007,16L 6.35783e-007,9.53674e-007 Z "
                      Fill="#7F808080"
                      Height="16"
                      Stretch="Fill"
                      Width="9"/>
            </Border>
        </Grid>
    </ControlTemplate>

    <!-- Styles -->

    <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=SelectionRectangleStyle}" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="#7FFFFFFF"/>
    </Style>

    <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=OutOfRangeAreaBorderStyle}" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="#CFFFFFFF"/>
    </Style>

    <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarOutOfRangeBorderStyle}" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="#3FC0C0C0"/>
    </Style>

    <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarOutOfSelectionBorderStyle}" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="#FFF0F1F2"/>
    </Style>

    <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarSelectionBorderStyle}" TargetType="{x:Type Border}">
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Background" Value="#FFFFFFFF"/>
    </Style>

    <Style x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarBottomBorderStyle}" TargetType="{x:Type Border}">
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Background" Value="#FFFFFFFF"/>
        <Setter Property="Height" Value="13"/>
    </Style>

</ResourceDictionary>