<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <system:Double x:Key="{dxt:FloatingContainerThemeKey ResourceKey=GlobalFontSize}">18</system:Double>

    <FontFamily x:Key="{dxt:FloatingContainerThemeKey ResourceKey=GlobalFontFamily}">Segoe UI</FontFamily>

    <!-- General -->

    <Color x:Key="DefaultBackgroundColor">#FFFFFFFF</Color>

    <Color x:Key="DefaultForegroundColor">#FF191919</Color>

    <SolidColorBrush x:Key="{dxt:FloatingContainerThemeKey ResourceKey=FloatingContainerBackground}" Color="{StaticResource DefaultBackgroundColor}"/>

    <SolidColorBrush x:Key="{dxt:ThemeColorsThemeKey ResourceKey=WhiteForegroundColor}" Color="#FF191919"/>

    <SolidColorBrush x:Key="{dxt:ThemeColorsThemeKey ResourceKey=BlackForegroundColor}" Color="#FF191919"/>

    <SolidColorBrush x:Key="{dxt:ThemeColorsThemeKey ResourceKey=DXMessageBoxForegroundColor}" Color="#FF191919"/>

    <SolidColorBrush x:Key="Page.Background" Color="{StaticResource DefaultBackgroundColor}"/>

    <SolidColorBrush x:Key="Text.Foreground" Color="{StaticResource DefaultForegroundColor}"/>

    <SolidColorBrush x:Key="Text.DisabledState.Foreground" Color="#7F191919"/>

    <SolidColorBrush x:Key="Shadow.Background" Color="#00000000"/>

    <SolidColorBrush x:Key="Text.Selection.Background" Color="#FF2F51A5"/>

    <SolidColorBrush x:Key="Text.CaretBrush" Color="#FF191919"/>

    <!-- Button -->

    <SolidColorBrush x:Key="Button.DefaultState.Border1" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="Button.DefaultState.Border2" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="Button.HoverState.Border1" Color="#FFC3C3C3"/>

    <LinearGradientBrush x:Key="Button.HoverState.Border2"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFF6F6F6" Offset="0"/>
        <GradientStop Color="#FFF6F6F6" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="Button.PressedState.Border1" Color="#FFC3C3C3"/>

    <LinearGradientBrush x:Key="Button.PressedState.Border2"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE3E3E3" Offset="0"/>
        <GradientStop Color="#FFE3E3E3" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="Button.CheckedState.Border1" Color="#FFC3C3C3"/>

    <LinearGradientBrush x:Key="Button.CheckedState.Border2"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE3E3E3" Offset="0"/>
        <GradientStop Color="#FFE3E3E3" Offset="1"/>
    </LinearGradientBrush>

    <!-- ComboBox -->

    <SolidColorBrush x:Key="ComboBox.Path.DefaultState" Color="#FF7F7F7F"/>

    <SolidColorBrush x:Key="ComboBox.Path.DisabledState" Color="#7F7F7F7F"/>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.DefaultState.Border" Color="#FFC3C3C3"/>

    <LinearGradientBrush x:Key="ComboBox.ToggleButton.DefaultState.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFF0F0F0" Offset="0"/>
        <GradientStop Color="#FFE5E5E5" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.HoverState.Border" Color="#FFC3C3C3"/>

    <LinearGradientBrush x:Key="ComboBox.ToggleButton.HoverState.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFF6F6F6" Offset="0"/>
        <GradientStop Color="#FFF6F6F6" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.PressedState.Border" Color="#FFC3C3C3"/>

    <LinearGradientBrush x:Key="ComboBox.ToggleButton.PressedState.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFE3E3E3" Offset="0"/>
        <GradientStop Color="#FFE3E3E3" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ComboBox.ToggleButton.DisabledState.Border" Color="#FFEAEAEA"/>

    <LinearGradientBrush x:Key="ComboBox.ToggleButton.DisabledState.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFFFFFFF" Offset="0"/>
        <GradientStop Color="#FFFFFFFF" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ComboBox.DropDownBorder.BorderBrush" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="ComboBox.DropDownBorder.Background" Color="#FFFFFFFF"/>

    <!-- CheckBox -->

    <SolidColorBrush x:Key="CheckBox.DefaultState.Border1" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="CheckBox.DefaultState.Border2" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="CheckBox.HoverState.Border1" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="CheckBox.HoverState.Border2" Color="#FFF6F6F6"/>

    <SolidColorBrush x:Key="CheckBox.PressedState.Border1" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="CheckBox.PressedState.Border2" Color="#FFE3E3E3"/>

    <SolidColorBrush x:Key="CheckBox.IndeterminateState.Border" Color="#FF424242"/>

    <!-- RadioButton -->

    <SolidColorBrush x:Key="RadioButton.DefaultState.Border1" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="RadioButton.DefaultState.Border2" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="RadioButton.HoverState.Border1" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="RadioButton.HoverState.Border2" Color="#FFF6F6F6"/>

    <SolidColorBrush x:Key="RadioButton.PressedState.Border1" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="RadioButton.PressedState.Border2" Color="#FFE3E3E3"/>

    <SolidColorBrush x:Key="RadioButton.Icon.Background" Color="#FF424242"/>

    <!-- TextBox -->

    <SolidColorBrush x:Key="TextBox.Background" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="TextBox.BorderBrush" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="TextBox.ReadOnly.Background" Color="#FFDDDDDD"/>

    <!-- ListBox -->

    <SolidColorBrush x:Key="ListBox.Background" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="ListBox.BorderBrush" Color="#FFC3C3C3"/>

    <!-- ListBoxItem -->

    <SolidColorBrush x:Key="ListBoxItem.Background" Color="#00000000"/>

    <SolidColorBrush x:Key="ListBoxItem.BorderBrush" Color="#00000000"/>

    <SolidColorBrush x:Key="ListBoxItem.Foreground" Color="#FF191919"/>

    <SolidColorBrush x:Key="ListBoxItem.DisabledState.Foreground" Color="#7F191919"/>

    <SolidColorBrush x:Key="ListBoxItem.Selection.Border" Color="#FFE3E3E3"/>

    <SolidColorBrush x:Key="ListBoxItem.HoverSelection.Border" Color="#FFE0E5F2"/>

    <SolidColorBrush x:Key="ListBoxItem.InactiveSelection.Border" Color="#FFDDDDDD"/>

    <!-- Label -->

    <SolidColorBrush x:Key="Label.Background" Color="#00000000"/>

    <SolidColorBrush x:Key="Label.Foreground" Color="#FF000000"/>

    <!-- ProgressBar -->

    <SolidColorBrush x:Key="ProgressBar.BorderBrush" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="ProgressBar.Background" Color="#FFE5E5E5"/>

    <SolidColorBrush x:Key="ProgressBar.IndicatorBackground" Color="#FF2F51A5"/>

    <SolidColorBrush x:Key="ProgressBar.IndicatorAnimatedFill" Color="#FF2F51A5"/>

    <!-- ScrollViewer -->

    <SolidColorBrush x:Key="ScrollViewer.BorderBrush" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="ScrollViewer.Corner.Background" Color="#FFFFFFFF"/>

    <!-- ScrollBar -->

    <SolidColorBrush x:Key="ScrollBar.Background" Color="#FFF0F0F0"/>

    <SolidColorBrush x:Key="ScrollBar.BorderBrush" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="ScrollBar.PageButton.Background" Color="#00000000"/>

    <SolidColorBrush x:Key="ScrollBar.Thumb.Path.Background" Color="#FFEAEAEA"/>

    <SolidColorBrush x:Key="ScrollBar.Path.Background" Color="#FF404040"/>

    <SolidColorBrush x:Key="ScrollBar.DefaultState.Border" Color="#FFC0C0C0"/>

    <SolidColorBrush x:Key="ScrollBar.HoverState.Border" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="ScrollBar.DraggingState.Border" Color="#FF404040"/>

    <SolidColorBrush x:Key="ScrollBar.Button.HoverState" Color="#FFDDDDDD"/>

    <!-- Separator -->

    <SolidColorBrush x:Key="Separator.BorderBrush" Color="#17000000"/>

    <!-- TabControl -->

    <SolidColorBrush x:Key="TabControl.BorderBrush" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="TabControl.Background" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="TabControl.Foreground" Color="#FF000000"/>

    <!-- TabItem -->

    <SolidColorBrush x:Key="TabItem.Foreground" Color="#FF000000"/>

    <SolidColorBrush x:Key="TabItem.HoverState.Foreground" Color="#FF000000"/>

    <SolidColorBrush x:Key="TabItem.SelectedState.Foreground" Color="#FF2F51A5"/>

    <SolidColorBrush x:Key="TabItem.BorderBrush" Color="#FFC3C3C3"/>

    <LinearGradientBrush x:Key="TabItem.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFF0F0F0" Offset="0"/>
        <GradientStop Color="#FFE5E5E5" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="TabItem.HoverState.BorderBrush" Color="#FFC3C3C3"/>

    <LinearGradientBrush x:Key="TabItem.HoverState.Background"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFF6F6F6" Offset="0"/>
        <GradientStop Color="#FFF6F6F6" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="TabItem.SelectedState.Background" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Top" Color="#FF2F51A5"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Bottom" Color="#FF2F51A5"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Left" Color="#FF2F51A5"/>

    <SolidColorBrush x:Key="TabItem.FocusedState.Right" Color="#FF2F51A5"/>

    <!-- Slider -->

    <SolidColorBrush x:Key="Slider.Track.Background" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="Slider.Tick.Background" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="Slider.SelectionRange.Background" Color="#FF2F51A5"/>

    <SolidColorBrush x:Key="SliderThumb.DefaultState" Color="#FF191919"/>

    <SolidColorBrush x:Key="SliderThumb.HoverState" Color="#FF2F51A5"/>

    <SolidColorBrush x:Key="SliderThumb.PressedState" Color="#FF2F51A5"/>

    <SolidColorBrush x:Key="SliderThumb.DisabledState" Color="#FFEAEAEA"/>

    <!-- GridSplitter -->

    <SolidColorBrush x:Key="GridSplitter.Background" Color="#FFDDDDDD"/>

    <SolidColorBrush x:Key="GridSplitter.PreviewBrush" Color="#FFC3C3C3"/>

    <!-- ToolTip -->

    <SolidColorBrush x:Key="ToolTip.BorderBrush" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="ToolTip.Background" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="ToolTip.Foreground" Color="{StaticResource DefaultForegroundColor}"/>

    <!-- Expander -->

    <SolidColorBrush x:Key="Expander.Background" Color="#00000000"/>

    <SolidColorBrush x:Key="Expander.BorderBrush" Color="#00000000"/>

</ResourceDictionary>