                      Fill="{StaticResource RulerIndentHotZoneBorderBrush}"
                      Stretch="Fill"
                      UseLayoutRounding="False"/>
                <Path Data="F1 M 571,353L 568,356L 567,356L 564,353L 564,351L 571,351L 571,353 Z"
