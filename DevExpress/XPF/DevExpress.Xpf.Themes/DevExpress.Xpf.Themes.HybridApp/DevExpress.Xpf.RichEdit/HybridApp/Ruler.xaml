    <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=HorizontalTableHotZoneControlTemplate}" TargetType="dxrec:HorizontalTableHotZoneControl">
        <Border Background="{StaticResource RulerSpaceAreaBrush}"
                BorderBrush="{StaticResource RulerBorderBrush}"
                BorderThickness="1"
                Padding="4,0,4,0">
            <Grid Margin="0,2,0,2">
                <Grid Height="9"
                      Visibility="{TemplateBinding EnabledVisibility}"
                      Width="7">
                    <Rectangle Fill="{StaticResource RulerTableHotZoneBackgroundBrush}" Stretch="Fill"/>
                    <Path Data="F1 M 66,139L 67,139L 67,148L 66,148L 66,139 Z M 68,139L 69,139L 69,148L 68,148L 68,139 Z M 70,139L 71,139L 71,148L 70,148L 70,139 Z"
                          Fill="{StaticResource RulerTableHotZoneBorderBrush}"
                          Margin="1,0"
                          Stretch="Fill"/>
                    <Path Data="F1 M 65,141L 72,141L 72,142L 65,142L 65,141 Z M 72,143L 72,144L 65,144L 65,143L 72,143 Z M 65,145L 72,145L 72,146L 65,146L 65,145 Z M 65,147L 72,147L 72,148L 65,148L 65,147 Z M 65,139L 72,139L 72,140L 65,140L 65,139 Z"
                          Fill="{StaticResource RulerTableHotZoneBorderBrush}"
                          Stretch="Fill"/>
                </Grid>
                <Grid Height="9"
                      Visibility="{TemplateBinding DisabledVisibility}"
                      Width="7">
                    <Rectangle Fill="{StaticResource RulerTableHotZoneBackgroundBrush}" Stretch="Fill"/>
                    <Path Data="F1 M 66,139L 67,139L 67,148L 66,148L 66,139 Z M 68,139L 69,139L 69,148L 68,148L 68,139 Z M 70,139L 71,139L 71,148L 70,148L 70,139 Z"
                          Fill="{StaticResource RulerTableHotZoneBorderBrush}"
                          Margin="1,0"
                          Stretch="Fill"/>
                    <Path Data="F1 M 65,141L 72,141L 72,142L 65,142L 65,141 Z M 72,143L 72,144L 65,144L 65,143L 72,143 Z M 65,145L 72,145L 72,146L 65,146L 65,145 Z M 65,147L 72,147L 72,148L 65,148L 65,147 Z M 65,139L 72,139L 72,140L 65,140L 65,139 Z"
                          Fill="{StaticResource RulerTableHotZoneBorderBrush}"
                          Stretch="Fill"/>
                </Grid>
            </Grid>
        </Border>
    </ControlTemplate>

