              <dxe:OfficeZoomTrackBarBehavior Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(dxsps:SpreadsheetControl.Spreadsheet).ActiveViewZoom, Mode=TwoWay}" Minimum="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(dxsps:SpreadsheetControl.Spreadsheet).Options.Behavior.MinZoomFactor, Mode=OneWay, Converter={StaticResource MinZoomFactorToZoomTrackBarMinimumConverter}}" Maximum="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(dxsps:SpreadsheetControl.Spreadsheet).Options.Behavior.MaxZoomFactor, Mode=OneWay, Converter={StaticResource MaxZoomFactorToZoomTrackBarMaximumConverter}}" />
