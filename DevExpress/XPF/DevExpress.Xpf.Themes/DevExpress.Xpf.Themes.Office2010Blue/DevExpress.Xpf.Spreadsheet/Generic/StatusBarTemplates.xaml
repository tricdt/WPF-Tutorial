      <dxmvvm:Interaction.Behaviors>
        <dxb:ControllerBehavior Actions="{Binding Path=(dxsps:SpreadsheetControl.Spreadsheet).StatusBarActions, RelativeSource={RelativeSource TemplatedParent}}" ExecutionMode="OnEvent">
          <dxb:ControllerBehavior.Triggers>
            <dxb:ActionTrigger EventName="Loaded" ExecuteOnce="True" />
          </dxb:ControllerBehavior.Triggers>
        </dxb:ControllerBehavior>
      </dxmvvm:Interaction.Behaviors>
      <dxr:RibbonStatusBarControl.LeftItems>
        <dxb:BarEditItem dx:XamlHelper.Name="{x:Static dxsps:DefaultBarItemNames.StatusBarItem_Progress}" Style="{DynamicResource {themes:BarItemStyleKey ResourceKey=StatusBarItem_Progress}}" />
        <!--<dxb:BarButtonItem dx:XamlHelper.Name="{x:Static dxsps:DefaultBarItemNames.StatusBarItem_CancelOperation}" Style="{DynamicResource {themes:BarItemStyleKey ResourceKey=StatusBarItem_CancelOperation}}"/>-->
        <dxspsi:EndModeStaticItem dx:XamlHelper.Name="{x:Static dxsps:DefaultBarItemNames.StatusBarItem_EndMode}" Style="{DynamicResource {themes:BarItemStyleKey ResourceKey=StatusBarItem_EndMode}}" />
      </dxr:RibbonStatusBarControl.LeftItems>
