  <Style x:Key="{themes:SpreadsheetThemeKey ResourceKey=ResizeThumbStyle}" TargetType="{x:Type Thumb}">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Thumb}">
          <Grid x:Name="Root" Background="#FFCFDDEE" Margin="0,0,-1,0" Width="11">
            <Border Background="{StaticResource TabPanel.Background}" BorderBrush="{StaticResource TabPanel.BorderBrush}" BorderThickness="0,1,1,0" />
            <Border x:Name="NormalState" BorderBrush="{StaticResource TabPanel.BorderBrush}" BorderThickness="1,1,1,0">
              <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                  <GradientStop Color="#FFE1EDFA" Offset="0" />
                  <GradientStop Color="#FFD1E0EF" Offset="1" />
                </LinearGradientBrush>
              </Border.Background>
              <Grid>
                <Border BorderBrush="#FF94ADCC" BorderThickness="1,0,1,0" Margin="1,2,1,2" Width="3" />
                <Border BorderBrush="#3FFFFFFF" BorderThickness="1,1,0,0" />
              </Grid>
            </Border>
            <Border x:Name="HoverState" BorderBrush="{StaticResource TabPanel.BorderBrush}" BorderThickness="1,1,1,0" Opacity="0">
              <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                  <GradientStop Color="#FFF0F4FA" Offset="0" />
                  <GradientStop Color="#FFD1E0EF" Offset="1" />
                </LinearGradientBrush>
              </Border.Background>
              <Grid>
                <Border BorderBrush="#FF94ADCC" BorderThickness="1,0,1,0" Margin="1,2,1,2" Width="3" />
                <Border BorderBrush="#3FFFFFFF" BorderThickness="1,1,0,0" />
              </Grid>
            </Border>
            <Border x:Name="PressedState" BorderBrush="{StaticResource TabPanel.BorderBrush}" BorderThickness="1,1,1,0" Opacity="0">
              <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                  <GradientStop Color="#FFCFDDEE" Offset="0" />
                  <GradientStop Color="#FFCFDDEE" Offset="1" />
                </LinearGradientBrush>
              </Border.Background>
              <Grid>
                <Border BorderBrush="#FF94ADCC" BorderThickness="1,0,1,0" Margin="1,2,1,2" Width="3" />
                <Border BorderBrush="#3FFFFFFF" BorderThickness="1,1,0,0" />
              </Grid>
            </Border>
          </Grid>
          <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
              <Setter Property="Opacity" TargetName="HoverState" Value="1" />
              <Setter Property="Opacity" TargetName="NormalState" Value="0" />
              <Setter Property="Opacity" TargetName="PressedState" Value="0" />
            </Trigger>
            <Trigger Property="IsDragging" Value="True">
              <Setter Property="Opacity" TargetName="HoverState" Value="0" />
              <Setter Property="Opacity" TargetName="NormalState" Value="0" />
              <Setter Property="Opacity" TargetName="PressedState" Value="1" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
              <Setter Property="Opacity" TargetName="Root" Value="0.5" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
