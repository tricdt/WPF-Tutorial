  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftSelectionThumbTemplate}" TargetType="{x:Type Thumb}">
    <Grid Background="Transparent" HorizontalAlignment="Left" Width="15">
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="Auto" />
      </Grid.ColumnDefinitions>
      <Grid Grid.Column="1" HorizontalAlignment="Right" Margin="2,0,-2,0" Opacity="0.5" Width="2">
        <Border Background="#FF000000" Opacity="0.1" />
        <Border Background="#FF000000" Margin="0,0,1,0" Opacity="0.2" />
      </Grid>
      <Border x:Name="Border1" Background="#FFCFDDEE" BorderBrush="#FF94ADCC" BorderThickness="1,0,1,0" Grid.Column="1" HorizontalAlignment="Right" Width="4">
        <Border BorderBrush="#1FFFFFFF" BorderThickness="1,0,0,0" />
      </Border>
      <Border x:Name="Border2" Background="#FF94ADCC" Grid.Column="1" Height="16" Margin="0,0,3,0" Padding="1,1,0,1" Width="7">
        <Border x:Name="Border3" Background="#FFCFDDEE" Margin="0,0,-1,0">
          <Grid>
            <Border x:Name="Border4" BorderBrush="#FF94ADCC" BorderThickness="1,0,1,0" Margin="1,2,1,2" Width="3" />
            <Border BorderBrush="#1FFFFFFF" BorderThickness="1,1,0,0" />
          </Grid>
        </Border>
      </Border>
    </Grid>
    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding Path=(dxer:RangeControl.OwnerRangeControl).IsSelectionMoving, RelativeSource={RelativeSource Self}}" Value="True">
        <Setter Property="Background" TargetName="Border1" Value="#FF909090" />
        <Setter Property="BorderBrush" TargetName="Border1" Value="#FF666666" />
        <Setter Property="Background" TargetName="Border2" Value="#FF666666" />
        <Setter Property="Background" TargetName="Border3" Value="#FF909090" />
        <Setter Property="BorderBrush" TargetName="Border4" Value="#FF666666" />
      </DataTrigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RightSelectionThumbTemplate}" TargetType="{x:Type Thumb}">
    <Grid Background="Transparent" HorizontalAlignment="Right" Width="15">
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>
      <Grid Grid.Column="0" HorizontalAlignment="Left" Margin="-2,0,2,0" Opacity="0.5" Width="2">
        <Border Background="#FF000000" Opacity="0.1" />
        <Border Background="#FF000000" Margin="1,0,0,0" Opacity="0.2" />
