  <!-- Left & Right Navigation Buttons -->
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=LeftNavigationButtonTemplate}" TargetType="{x:Type Button}">
    <Grid x:Name="Root" Height="24" Margin="12,0,0,0" Width="24">
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="CommonStates">
          <VisualState x:Name="Disabled">
            <Storyboard>
              <DoubleAnimation Duration="0" Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="0" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Normal">
            <Storyboard>
              <DoubleAnimation Duration="0" Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="0.75" />
              <DoubleAnimation Duration="0" Storyboard.TargetName="NormalState" Storyboard.TargetProperty="Opacity" To="1" />
              <ColorAnimation Duration="0" Storyboard.TargetName="GlyphBackward" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="#FF515D7A" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="MouseOver">
            <Storyboard>
              <DoubleAnimation Duration="0" Storyboard.TargetName="HoverState" Storyboard.TargetProperty="Opacity" To="1" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Pressed">
            <Storyboard>
              <DoubleAnimation Duration="0" Storyboard.TargetName="PressedState" Storyboard.TargetProperty="Opacity" To="1" />
              <DoubleAnimation Duration="0" Storyboard.TargetName="Shadow" Storyboard.TargetProperty="Opacity" To="0" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
      <Grid x:Name="Shadow">
        <Grid.OpacityMask>
          <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#59000000" Offset="0" />
            <GradientStop Color="#FF000000" Offset="1" />
          </LinearGradientBrush>
        </Grid.OpacityMask>
        <Border Background="#0C000000" CornerRadius="48" Margin="-2" />
        <Border Background="#07000000" CornerRadius="48" Margin="-1" />
      </Grid>
      <Border x:Name="NormalState" Background="#FF94ADCC" CornerRadius="64">
        <Border CornerRadius="64" Margin="1">
          <Border.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
              <GradientStop Color="#FFE1EDFA" Offset="0" />
              <GradientStop Color="#FFD8E4F2" Offset="1" />
            </LinearGradientBrush>
          </Border.Background>
        </Border>
      </Border>
      <Border x:Name="HoverState" CornerRadius="24" Opacity="0">
        <Border.Background>
          <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#BFD7A820" Offset="0" />
            <GradientStop Color="#BFB89407" Offset="1" />
          </LinearGradientBrush>
        </Border.Background>
        <Border CornerRadius="64" Margin="1">
          <Border.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
              <GradientStop Color="#FFFFFAE6" Offset="0" />
              <GradientStop Color="#FFFFFFED" Offset="1" />
            </LinearGradientBrush>
          </Border.Background>
          <Border CornerRadius="64" Margin="1">
            <Border.Background>
              <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FFFFEEA8" Offset="0" />
                <GradientStop Color="#FFFFFCE0" Offset="1" />
                <GradientStop Color="#FFFFE277" Offset="0.5" />
              </LinearGradientBrush>
            </Border.Background>
          </Border>
        </Border>
      </Border>
      <Border x:Name="PressedState" CornerRadius="64" Opacity="0">
        <Border.Background>
          <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FFC28F36" Offset="0" />
            <GradientStop Color="#FFC2A34C" Offset="1" />
          </LinearGradientBrush>
        </Border.Background>
        <Border CornerRadius="64" Margin="1">
          <Border.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
              <GradientStop Color="#FFFFE575" Offset="0" />
              <GradientStop Color="#FFFFF37F" Offset="1" />
              <GradientStop Color="#FFFFD86C" Offset="0.5" />
            </LinearGradientBrush>
          </Border.Background>
        </Border>
      </Border>
      <Path x:Name="GlyphBackward" Data="F1 M 204,6.99994L 204,14.9999L 200,10.9999L 204,6.99994 Z" Fill="#FF515D7A" Height="8" HorizontalAlignment="Center" Margin="0,0,2,0" Stretch="Fill" VerticalAlignment="Center" Width="4" />
    </Grid>
  </ControlTemplate>
