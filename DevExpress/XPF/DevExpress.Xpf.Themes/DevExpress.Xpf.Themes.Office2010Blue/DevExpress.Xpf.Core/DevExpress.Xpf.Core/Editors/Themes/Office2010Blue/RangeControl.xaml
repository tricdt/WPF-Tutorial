      <Path x:Name="GlyphForward" Data="F1 M 206,7.00006L 206,15.0001L 210,11.0001L 206,7.00006 Z" Fill="#FF515D7A" Height="8" HorizontalAlignment="Center" Margin="2,0,0,0" Stretch="Fill" VerticalAlignment="Center" Width="4" />
    </Grid>
  </ControlTemplate>
  <!-- Calendar -->
  <ControlTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarGroupItemTemplate}" TargetType="{x:Type dxer:CalendarGroupItem}">
    <Border Background="#FF94ADCC">
      <TextBlock Margin="20,0" Text="{TemplateBinding Text}" TextAlignment="Center" VerticalAlignment="Center" />
    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:CalendarClientThemeKey ResourceKey=CalendarItemTemplate}" TargetType="{x:Type dxer:CalendarItem}">
    <Border x:Name="Border" Background="#FF94ADCC" BorderThickness="0">
      <TextBlock x:Name="text" HorizontalAlignment="Center" Margin="0,10" Text="{TemplateBinding Text}" TextAlignment="Center" />
    </Border>
    <ControlTemplate.Triggers>
      <Trigger Property="IsSelected" Value="True">
        <Setter Property="Background" TargetName="Border" Value="#FFFFA500" />
        <Setter Property="Foreground" TargetName="text" Value="#EF000000" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <!-- Range Bar -->
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
    <Border Background="#FFFFFFFF" BorderBrush="#FF94ADCC" BorderThickness="{Binding Path=(dxer:RangeControl.OwnerRangeControl).BorderThickness, RelativeSource={RelativeSource Self}}">
      <ContentPresenter />
    </Border>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:RangeControlThemeKey ResourceKey=RangeBarRootBorderTemplate}" TargetType="{x:Type ContentControl}">
    <Border Background="#BF000000" BorderBrush="#FF94ADCC" BorderThickness="0,1,0,0">
      <ContentPresenter />
    </Border>
  </ControlTemplate>
