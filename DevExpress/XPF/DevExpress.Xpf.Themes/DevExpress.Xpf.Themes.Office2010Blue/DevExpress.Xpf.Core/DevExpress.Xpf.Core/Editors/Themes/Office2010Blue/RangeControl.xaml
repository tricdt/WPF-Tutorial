      <Border x:Name="Border1" Background="#FFCFDDEE" BorderBrush="#FF94ADCC" BorderThickness="1,0,1,0" Grid.Column="0" HorizontalAlignment="Left" Width="4">
        <Border BorderBrush="#1FFFFFFF" BorderThickness="1,0,0,0" />
      </Border>
      <Border x:Name="Border2" Background="#FF94ADCC" Grid.Column="0" Height="16" Margin="3,0,0,0" Padding="0,1,1,1" Width="7">
        <Border x:Name="Border3" Background="#FFCFDDEE" Margin="-1,0,0,0">
          <Grid>
            <Border x:Name="Border4" BorderBrush="#FF94ADCC" BorderThickness="1,0,1,0" Margin="1,2,1,2" Width="3" />
            <Border BorderBrush="#1FFFFFFF" BorderThickness="0,1,0,0" />
          </Grid>
        </Border>
      </Border>
    </Grid>
    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding Path=(dxer:RangeControl.OwnerRangeControl).IsSelectionMoving, RelativeSource={RelativeSource Self}}" Value="True">
        <Setter Property="Background" TargetName="Border1" Value="#FF909090" />
        <Setter Property="BorderBrush" TargetName="Border1" Value="#FF666666" />
        <Setter Property="Background" TargetName="Border2" Value="#FF666666" />
        <Setter Property="Background" TargetName="Border3" Value="#FF909090" />
        <Setter Property="BorderBrush" TargetName="Border4" Value="#FF666666" />
      </DataTrigger>
    </ControlTemplate.Triggers>
