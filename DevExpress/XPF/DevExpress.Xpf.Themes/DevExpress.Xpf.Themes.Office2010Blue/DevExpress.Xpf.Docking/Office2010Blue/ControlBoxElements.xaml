                    <ColorAnimation Duration="0" Storyboard.TargetName="UnpinnedGlyph" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="#FF515D7A" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Disabled" />
              </VisualStateGroup>
              <VisualStateGroup x:Name="PinnedStates">
