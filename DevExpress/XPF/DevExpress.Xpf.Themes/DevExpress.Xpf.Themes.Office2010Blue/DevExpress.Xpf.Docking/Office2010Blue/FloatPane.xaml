<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/docking/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:system="clr-namespace:System;assembly=mscorlib" xmlns:ve="http://schemas.devexpress.com/winfx/2008/xaml/docking/visualelements" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Thickness x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=BorderMargin}">0</Thickness>
  <Thickness x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=ShadowMargin}">8,8,3,3</Thickness>
  <Thickness x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=FormBorderMargin}">3</Thickness>
  <Thickness x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=SingleBorderMargin}">0</Thickness>
  <Thickness x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=MaximizedBorderMargin}">0</Thickness>
  <system:Double x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=ResizingSideLength}">6</system:Double>
  <system:Double x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=ResizingCornerLength}">10</system:Double>
  <system:Double x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=MinHeight}">84</system:Double>
  <system:Double x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=MinWidth}">84</system:Double>
  <Point x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=WindowDraggingOffset}" X="4" Y="2" />
  <ControlTemplate x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=EmptyBorderTemplate}" TargetType="ve:FormBorderControl">
    <Grid>
      <Grid>
        <Grid x:Name="PART_Content" Margin="{TemplateBinding ActualContentMargin}">
          <ve:LayoutItemsControl ItemsSource="{Binding Path=ItemsInternal}" Margin="{Binding Path=ActualMargin}" Orientation="{Binding Path=Orientation}" ve:DockPane.HitTestType="Content" />
        </Grid>
        <ve:ResizeBoundsControl x:Name="PART_ResizeBorders" />
      </Grid>
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="SizeStates">
          <VisualState x:Name="EmptySizeState" />
          <VisualState x:Name="Maximized">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="PART_ResizeBorders" Storyboard.TargetProperty="Visibility" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=SingleBorderTemplate}" TargetType="ve:FormBorderControl">
    <Grid>
      <Border x:Name="Shadow" Background="#FF000000" CornerRadius="2" Margin="2,2,0,0" Opacity="0.12" />
      <Grid x:Name="grid" Margin="0,0,3,3">
        <Grid>
          <Border Background="#FF515D7A" Margin="{TemplateBinding ActualBorderMargin}" ve:DockPane.HitTestType="Border" />
        </Grid>
        <Grid x:Name="PART_Content" Margin="{TemplateBinding ActualContentMargin}">
          <ve:LayoutItemsControl x:Name="layoutItemsControl" ItemsSource="{Binding Path=ItemsInternal}" Margin="{Binding Path=ActualMargin}" Orientation="{Binding Path=Orientation}" ve:DockPane.HitTestType="Content" />
        </Grid>
        <ve:ResizeBoundsControl x:Name="PART_ResizeBorders" />
      </Grid>
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="SizeStates">
          <VisualState x:Name="EmptySizeState" />
          <VisualState x:Name="Maximized">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="PART_ResizeBorders" Storyboard.TargetProperty="Visibility" />
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0" Storyboard.TargetName="layoutItemsControl" Storyboard.TargetProperty="Margin" />
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0" Storyboard.TargetName="grid" Storyboard.TargetProperty="Margin" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=FormBorderTemplate}" TargetType="ve:FormBorderControl">
    <Grid>
      <Border Background="#FF000000" CornerRadius="4" IsHitTestVisible="False" Margin="{TemplateBinding ActualShadowMargin}" Opacity="0.15" />
      <ve:FormBorderContentControl Padding="{TemplateBinding ActualBorderMargin}">
        <ve:LayoutItemsControl ItemsSource="{Binding Path=ItemsInternal}" Margin="{Binding Path=ActualMargin}" Orientation="{Binding Path=Orientation}" ve:DockPane.HitTestType="Content" />
      </ve:FormBorderContentControl>
      <ve:ResizeBoundsControl x:Name="PART_ResizeBorders" Canvas.ZIndex="1" />
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="SizeStates">
          <VisualState x:Name="EmptySizeState" />
          <VisualState x:Name="Maximized">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="PART_ResizeBorders" Storyboard.TargetProperty="Visibility" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:FloatPaneElementsThemeKey ResourceKey=FormBorderContentTemplate}" TargetType="ve:FormBorderContentControl">
    <Grid>
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="ActiveStates">
          <VisualState x:Name="Active">
            <Storyboard>
              <ColorAnimation Duration="0" Storyboard.TargetName="PART_CaptionControlPresenter" Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" To="#FF1E395B" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Inactive" />
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
      <Border Background="#FF515D7A" ve:DockPane.HitTestType="Border">
        <Border Background="#FF515D7A" Margin="3" Padding="1">
          <Grid MinHeight="22">
            <Grid.RowDefinitions>
              <RowDefinition Height="Auto" />
              <RowDefinition />
            </Grid.RowDefinitions>
            <ve:FloatingDragWidget x:Name="PART_Header" Background="#FFA7BFD9" ve:DockPane.HitTestType="Header">
              <Grid Margin="4,1,1,2">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition />
                  <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <ve:TemplatedCaptionControl x:Name="PART_CaptionControlPresenter" Foreground="#FF1E395B" LayoutItem="{Binding}" />
                <ve:WindowControlBoxControl x:Name="PART_ControlBox" ButtonHeight="17" ButtonStyle="{DynamicResource {dxt:FloatPaneElementsThemeKey ResourceKey=FloatPaneButtonStyle}}" ButtonWidth="17" CloseButtonTemplate="{DynamicResource {dxt:FloatPaneElementsThemeKey ResourceKey=CloseButton}}" Grid.Column="1" LayoutItem="{Binding}" MaximizeButtonTemplate="{DynamicResource {dxt:FloatPaneElementsThemeKey ResourceKey=MaximizeButton}}" MinimizeButtonTemplate="{DynamicResource {dxt:FloatPaneElementsThemeKey ResourceKey=MinimizeButton}}" RestoreButtonTemplate="{DynamicResource {dxt:FloatPaneElementsThemeKey ResourceKey=RestoreButton}}" />
              </Grid>
            </ve:FloatingDragWidget>
            <Border x:Name="PART_Content" Background="#FFCFDDEE" Grid.Row="1" ve:DockPane.HitTestType="Content">
              <ContentPresenter />
            </Border>
          </Grid>
        </Border>
      </Border>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:MinimizedFloatGroupsItemElementsThemeKey ResourceKey=Template}" TargetType="{x:Type ve:MinimizedFloatGroupsItem}">
    <Border x:Name="PART_CaptionBackground" Background="#FF515D7A" Margin="0,1,1,0" Padding="2" UseLayoutRounding="True" ve:DockPane.HitTestType="Header">
      <Grid UseLayoutRounding="True">
        <Grid.ColumnDefinitions>
          <ColumnDefinition />
          <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>
        <Border Background="#FFCFDDEE" BorderBrush="#FF515D7A" BorderThickness="2" Grid.ColumnSpan="2" />
        <ve:TemplatedCaptionControl DataContext="{TemplateBinding ActualLayoutItem}" Foreground="#FF1E395B" LayoutItem="{Binding}" Padding="5,3" />
        <ve:MinimizedItemControlBoxControl x:Name="PART_ControlBox" ButtonHeight="17" ButtonStyle="{DynamicResource {dxt:FloatPaneElementsThemeKey ResourceKey=FloatPaneButtonStyle}}" ButtonWidth="17" CloseButtonTemplate="{DynamicResource {dxt:DockPaneElementsThemeKey ResourceKey=CloseButton}}" Grid.Column="1" LayoutItem="{TemplateBinding ActualLayoutItem}" Margin="0,0,1,0" MaximizeButtonTemplate="{DynamicResource {dxt:DockPaneElementsThemeKey ResourceKey=MaximizeButton}}" RestoreButtonTemplate="{DynamicResource {dxt:DockPaneElementsThemeKey ResourceKey=RestoreButton}}" />
      </Grid>
    </Border>
  </ControlTemplate>
</ResourceDictionary>