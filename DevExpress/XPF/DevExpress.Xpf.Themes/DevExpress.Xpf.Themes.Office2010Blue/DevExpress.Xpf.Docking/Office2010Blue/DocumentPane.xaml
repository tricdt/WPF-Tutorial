            <Storyboard>
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0" Storyboard.TargetName="BorderHighlight" Storyboard.TargetProperty="Margin" />
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0" Storyboard.TargetName="BorderBackground" Storyboard.TargetProperty="Margin" />
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0" Storyboard.TargetName="PART_Content" Storyboard.TargetProperty="Margin" />
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Thickness="0" Storyboard.TargetName="PART_Content" Storyboard.TargetProperty="BorderThickness" />
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="PART_Header" Storyboard.TargetProperty="Visibility" />
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="PART_ResizeBounds" Storyboard.TargetProperty="Visibility" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="Minimized">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Collapsed" Storyboard.TargetName="PART_ResizeBounds" Storyboard.TargetProperty="Visibility" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:DocumentPaneElementsThemeKey ResourceKey=MDIContainerTemplate}" TargetType="ve:DocumentMDIContainer">
    <Grid DataContext="{Binding LayoutItem}" SnapsToDevicePixels="True" ve:DockPane.HitTestType="Bounds">
      <Grid.RowDefinitions>
        <RowDefinition Height="Auto" />
        <RowDefinition />
      </Grid.RowDefinitions>
      <Border x:Name="barPanel" Background="#FFE1EDFA" BorderBrush="#FF94ADCC" BorderThickness="0,0,0,1" Visibility="Collapsed">
        <Grid>
          <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition Width="Auto" />
          </Grid.ColumnDefinitions>
          <ContentPresenter Content="{Binding SelectedItem.Caption}" ContentTemplate="{Binding LayoutItem.SelectedItem.CaptionTemplate, RelativeSource={RelativeSource TemplatedParent}}" ContentTemplateSelector="{Binding LayoutItem.SelectedItem.CaptionTemplateSelector, RelativeSource={RelativeSource TemplatedParent}}" Margin="2" TextElement.Foreground="#FF1E395B" />
          <ve:TabHeaderControlBoxControl x:Name="PART_ControlBox" ButtonHeight="18" ButtonStyle="{DynamicResource {dxt:DocumentPaneElementsThemeKey ResourceKey=MDIContainerButtonStyle}}" ButtonWidth="19" CloseButtonTemplate="{DynamicResource {dxt:DocumentPaneElementsThemeKey ResourceKey=CloseButton}}" DropDownButtonTemplate="{DynamicResource {dxt:DocumentPaneElementsThemeKey ResourceKey=DropDownButton}}" Grid.Column="1" HorizontalAlignment="Right" LayoutItem="{Binding}" Margin="2" RestoreButtonTemplate="{DynamicResource {dxt:DocumentPaneElementsThemeKey ResourceKey=RestoreButton}}" ScrollNextButtonTemplate="{DynamicResource {dxt:DocumentPaneElementsThemeKey ResourceKey=ScrollNextButton}}" ScrollPrevButtonTemplate="{DynamicResource {dxt:DocumentPaneElementsThemeKey ResourceKey=ScrollPrevButton}}" />
        </Grid>
      </Border>
      <ScrollViewer x:Name="scroller" Background="{TemplateBinding ActualBackground}" BorderThickness="0" Grid.Row="1" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
        <ItemsPresenter x:Name="PART_MDIPanelPresenter" HorizontalAlignment="Left" VerticalAlignment="Top" />
      </ScrollViewer>
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="HeaderStates">
          <VisualState x:Name="HeaderVisible">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible" Storyboard.TargetName="barPanel" Storyboard.TargetProperty="Visibility" />
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.HorizontalAlignment="Stretch" Storyboard.TargetName="PART_MDIPanelPresenter" Storyboard.TargetProperty="HorizontalAlignment" />
              <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.VerticalAlignment="Stretch" Storyboard.TargetName="PART_MDIPanelPresenter" Storyboard.TargetProperty="VerticalAlignment" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="HeaderHidden" />
        </VisualStateGroup>
        <VisualStateGroup x:Name="MaximizedStates">
          <VisualState x:Name="EmptyMaximizedState" />
          <VisualState x:Name="Maximized">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames Storyboard.TargetName="scroller" Storyboard.TargetProperty="HorizontalScrollBarVisibility">
                <DiscreteObjectKeyFrame KeyTime="0">
                  <DiscreteObjectKeyFrame.Value>
                    <ScrollBarVisibility>Disabled</ScrollBarVisibility>
                  </DiscreteObjectKeyFrame.Value>
                </DiscreteObjectKeyFrame>
              </ObjectAnimationUsingKeyFrames>
              <ObjectAnimationUsingKeyFrames Storyboard.TargetName="scroller" Storyboard.TargetProperty="VerticalScrollBarVisibility">
                <DiscreteObjectKeyFrame KeyTime="0">
                  <DiscreteObjectKeyFrame.Value>
                    <ScrollBarVisibility>Disabled</ScrollBarVisibility>
                  </DiscreteObjectKeyFrame.Value>
                </DiscreteObjectKeyFrame>
              </ObjectAnimationUsingKeyFrames>
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:DocumentElementsThemeKey ResourceKey=FloatDocument}" TargetType="ve:FloatDocument">
    <ve:DocumentFormBorderContentControl IsActive="{Binding IsActive}">
      <Border Background="{Binding Background}">
        <ve:DocumentContentPresenter x:Name="PART_ContentPresenter" Content="{Binding LayoutItemData}" ve:DockPane.HitTestType="Content" />
      </Border>
    </ve:DocumentFormBorderContentControl>
  </ControlTemplate>
</ResourceDictionary>