      <Border Background="#FFB1C0D6" CornerRadius="0,2,2,0" Padding="1">
        <Border x:Name="GalleryBackground" Background="#FFEDF4FD" CornerRadius="0,1,1,0"></Border>
      </Border>
      <ContentPresenter />
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="IsMouseOver" Value="True">
        <Setter Property="Background" TargetName="GalleryBackground" Value="#FFFFFFFF" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
