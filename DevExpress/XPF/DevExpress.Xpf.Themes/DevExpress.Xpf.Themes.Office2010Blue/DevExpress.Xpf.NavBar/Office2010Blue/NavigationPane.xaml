    <ControlTemplate.Triggers>
      <DataTrigger Binding="{Binding Source={x:Static interop:BrowserInteropHelper.IsBrowserHosted}}" Value="True">
        <Setter Property="Visibility" TargetName="Shadow" Value="Collapsed" />
        <Setter Property="Margin" TargetName="PopupBorder" Value="0" />
        <Setter Property="CornerRadius" TargetName="PopupBorder" Value="0" />
        <Setter Property="CornerRadius" TargetName="Background" Value="0" />
