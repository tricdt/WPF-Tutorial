<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dxcot="http://schemas.devexpress.com/winfx/2008/xaml/controls/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Duration x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressAnimationDuration}">0:0:0.5</Duration>
  <sys:Boolean x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ShowOuterBorder}">False</sys:Boolean>
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderBrush}" Color="Transparent" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterIndentBrush}" Color="Transparent" />
  <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderIndent}">0</sys:Double>
  <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=OuterBorderSize}">0</sys:Double>
  <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBarThickness}">6</sys:Double>
  <Thickness x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBorderThickness}">0</Thickness>
  <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressBackground}" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FF94ADCC" Offset="0" />
    <GradientStop Color="#FFEFF5FB" Offset="0.5" />
    <GradientStop Color="#FFEFF5FB" Offset="1" />
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressForeground}" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FFC28E35" Offset="0" />
    <GradientStop Color="#FFF8E088" Offset="0.5" />
    <GradientStop Color="#FFF8E088" Offset="1" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressAccentBorderBrush}" Color="Transparent" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=ProgressNeutralBorderBrush}" Color="Transparent" />
  <sys:Double x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorBorderSize}">1</sys:Double>
  <Thickness x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorPanelMargin}">3</Thickness>
  <EllipseGeometry x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndicatorGeometry}" Center="20,20" RadiusX="20" RadiusY="20" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedItemForeground}" Color="#363636" />
  <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedIndicatorFill}" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#CFDDEE" Offset="0" />
    <GradientStop Color="#CFDDEE" Offset="1" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedIndicatorStroke}" Color="#94ADCC" />
  <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedMouseOverFill}" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#E7EDF4" Offset="0" />
    <GradientStop Color="#E7EDF4" Offset="1" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=NotStartedMouseOverStroke}" Color="#94ADCC" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateItemForeground}" Color="#363636" />
  <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateIndicatorFill}" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FFF8EBB2" Offset="0" />
    <GradientStop Color="#FFFCF7E4" Offset="1" />
    <GradientStop Color="#FFF8E088" Offset="0.35" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateIndicatorStroke}" Color="#FFEBC448" />
  <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateMouseOverFill}" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FFF8EBB2" Offset="0" />
    <GradientStop Color="#FFFCF7E4" Offset="1" />
    <GradientStop Color="#FFF8E088" Offset="0.35" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=IndeterminateMouseOverStroke}" Color="#FFEBC448" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedItemForeground}" Color="#FF000000" />
  <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedIndicatorFill}" EndPoint="0.5,1" StartPoint="0.5,0">
    <GradientStop Color="#FFF8EBB2" Offset="0" />
    <GradientStop Color="#FFF1D449" Offset="1" />
    <GradientStop Color="#FFF1D449" Offset="0.5" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedIndicatorStroke}" Color="#FFEBC448" />
  <LinearGradientBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedMouseOverFill}" EndPoint="0.5,1" StartPoint="0.5,0">
    <GradientStop Color="#FFF8EBB2" Offset="0" />
    <GradientStop Color="#FFFCF7E4" Offset="1" />
    <GradientStop Color="#FFFFE575" Offset="0.5" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=CompletedMouseOverStroke}" Color="#FFEBC448" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledNotStartedIndicatorFill}" Color="#CFDDEE" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledNotStartedIndicatorStroke}" Color="#B7CAE0" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledIndeterminateIndicatorFill}" Color="#DFE0CB" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledIndeterminateIndicatorStroke}" Color="#DAD3AC" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledCompletedIndicatorFill}" Color="#DEDDBC" />
  <SolidColorBrush x:Key="{dxcot:StepProgressBarThemeKey ResourceKey=DisabledCompletedIndicatorStroke}" Color="#DAD3AC" />
</ResourceDictionary>